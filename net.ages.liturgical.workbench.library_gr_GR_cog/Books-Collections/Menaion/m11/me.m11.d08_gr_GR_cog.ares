meVE.Stichera01.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.Stichera01.greekmelody = he.a.m4_gr_GR_cog.heAU.OsGennaionEnMartysi.greekname
meVE.Stichera01.melody = he.a.m4_gr_GR_cog.heAU.OsGennaionEnMartysi.name
meVE.Stichera01.incipit = "Τρισηλίου Θεότητος…"
meVE.Stichera01.text = "Τρισηλίου Θεότητος, παραστάτης φαιδρότατος, Μιχαὴλ δεικνύμενος Ἀρχιστράτηγε, μετὰ τῶν ἄνω Δυνάμεων, κραυγάζεις γηθόμενος· Ἅγιος εἶ ὁ Πατήρ, Ἅγιος ὁ συνάναρχος, Λόγος Ἅγιος, καὶ τὸ Ἅγιον Πνεῦμα, μία δόξα, βασιλεία μία φύσις, μία Θεότης καὶ δύναμις."

meVE.Stichera02.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.Stichera02.melody = he.a.m4_gr_GR_cog.heAU.OsGennaionEnMartysi.name
meVE.Stichera02.incipit = "Καὶ τὸ εἶδός σου πύρινον…"
meVE.Stichera02.text = "Καὶ τὸ εἶδός σου πύρινον, καὶ τὸ κάλλος θαυμάσιον, Μιχαὴλ πρωτάγγελε· τῇ ἀΰλῳ γάρ, φύσει διέρχῃ τὰ πέρατα, πληρῶν τὰ προστάγματα, τοῦ τῶν ὅλων Ποιητοῦ, δυνατὸς ἐν ἰσχύϊ σου, γνωριζόμενος, καὶ πηγὴν ἰαμάτων τὸν ναόν σου, ἐργαζόμενος τῇ κλήσει, σοῦ τῇ ἁγίᾳ τιμώμενον."

meVE.Stichera03.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.Stichera03.melody = he.a.m4_gr_GR_cog.heAU.OsGennaionEnMartysi.name
meVE.Stichera03.incipit = "Ὁ ποιῶν τοὺς Ἀγγέλους σου…"
meVE.Stichera03.text = "Ὁ ποιῶν τοὺς Ἀγγέλους σου, ὥσπερ γέγραπται πνεύματα, καὶ τοὺς λειτουργοῦντάς σοι φλόγα πύρινον, μέσον ταγμάτων πρωτεύοντα, ἀνέδειξας Κύριε, σοῦ τῶν ἀρχαγγελικῶν, Μιχαὴλ Ἀρχιστράτηγον, σοῦ τοῖς νεύμασι, καθυπείκοντα Λόγε, καὶ τὸν ὕμνον, τὸν τρισάγιον ἐν φόβῳ, ἀναφωνοῦντα τῇ δόξῃ σου."

meVE.Stichera04.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.Stichera04.greekmelody = he.a.m4_gr_GR_cog.heAU.EdokasSimeiosin.greekname
meVE.Stichera04.melody = he.a.m4_gr_GR_cog.heAU.EdokasSimeiosin.name
meVE.Stichera04.incipit = "Νοῦς ὁ προαιώνιος…"
meVE.Stichera04.text = "Νοῦς ὁ προαιώνιος, φῶς ὑπεστήσατο δεύτερον, Γαβριήλ σε μεθέξεσιν, ἐνθέοις φωτίζοντα, πᾶσαν οἰκουμένην, καὶ τὸ ἀπʼ αἰῶνος, ἀνακαλύπτοντα ἡμῖν, θεῖον καὶ μέγα ὄντως μυστήριον, ἐν μήτρᾳ σωματούμενον, παρθενικῇ τὸν ἀσώματον, καὶ γενόμενον ἄνθρωπον, εἰς τὸ σῶσαι τὸν ἄνθρωπον."

meVE.Stichera05.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.Stichera05.melody = he.a.m4_gr_GR_cog.heAU.EdokasSimeiosin.name
meVE.Stichera05.incipit = "Θρόνῳ παριστάμενος…"
meVE.Stichera05.text = "Θρόνῳ παριστάμενος, τῆς τρισηλίου Θεότητος, καὶ πλουσίως λαμπόμενος, ταῖς θείαις λαμπρότησι, ταῖς ἐκπεμπομέναις, ἀπαύστως ἐκεῖθεν, τοὺς ἐπὶ γῆς χαρμονικῶς, χοροστατοῦντας καὶ εὐφημοῦντάς σε, παθῶν ἀχλύος λύτρωσαι, καὶ φωτισμῷ καταλάμπρυνον, Γαβριὴλ Ἀρχιστράτηγε, πρεσβευτὰ τῶν ψυχῶν ἡμῶν."

meVE.Stichera06.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.Stichera06.melody = he.a.m4_gr_GR_cog.heAU.EdokasSimeiosin.name
meVE.Stichera06.incipit = "Θραῦσον τὰ φρυάγματα…"
meVE.Stichera06.text = "Θραῦσον τὰ φρυάγματα, τῶν ἐκ τῆς Ἄγαρ τῇ ποίμνῃ σου, συνεχῶς ἐπερχόμενα. Κόπασον τὰ σχίσματα, τὰ τῆς Ἐκκλησίας. Πράϋνον τὸν σάλον, τῶν ἀμετρήτων πειρασμῶν. Ῥῦσαι κινδύνων καὶ περιστάσεων, τοὺς πόθῳ σε γεραίροντας, καὶ σοῦ τῇ σκέπῃ προστρέχοντας, Γαβριὴλ Ἀρχιστράτηγε, πρεσβευτὰ τῶν ψυχῶν ἡμῶν."

meVE.SticGlory.poet = poets_gr_GR_cog.po.Vyzantiou
meVE.SticGlory.mode = miscellanea_gr_GR_cog.misc.Mode6
meVE.SticGlory.melody = ""
meVE.SticGlory.incipit = "Συγχάρητε ἡμῖν…"
meVE.SticGlory.text = "Συγχάρητε ἡμῖν, ἅπασαι αἱ τῶν Ἀγγέλων ταξιαρχίαι· ὁ πρωτοστάτης γὰρ ὑμῶν, καὶ ἡμέτερος προστάτης, ὁ μέγας Ἀρχιστράτηγος, τὴν σήμερον ἡμέραν, ἐν τῷ σεπτῷ αὐτοῦ τεμένει, παραδόξως ἐποπτανόμενος ἁγιάζει. Ὅθεν κατὰ χρέος, ἀνυμνοῦντες αὐτὸν βοήσωμεν· Σκέπασον ἡμᾶς ἐν τῇ σκέπῃ τῶν πτερύγων σου, μέγιστε Μιχαὴλ Ἀρχάγγελε."

meVE.SticTheotokion.mode = miscellanea_gr_GR_cog.misc.Mode6
meVE.SticTheotokion.melody = ""
meVE.SticTheotokion.incipit = "Συγχάρητε ἡμῖν, ἅπασαι αἱ τῶν παρθένων…"
meVE.SticTheotokion.text = "Συγχάρητε ἡμῖν, ἅπασαι αἱ τῶν παρθένων χοροστασίαι· ἡ προστασία γὰρ ἡμῶν, καὶ μεσίτρια καὶ σκέπη, καὶ μέγα καταφύγιον, τὴν σήμερον ἡμέραν, ἐν τῷ σεπτῷ αὐτῆς τεμένει, παραδόξως ἐποπτανομένη ἁγιάζει. Ὅθεν κατὰ χρέος, ἀνυμνοῦντες αὐτὴν βοήσωμεν· Σκέπασον ἡμᾶς, ἐν τῇ σκέπῃ τῶν πτερύγων σου, ἄχραντε Θεοτόκε Δέσποινα."

meVE.Lity1.poet = poets_gr_GR_cog.po.Kyprianou
meVE.Lity1.mode = miscellanea_gr_GR_cog.misc.Mode1
meVE.Lity1.melody = ""
meVE.Lity1.incipit = "Τῶν νοερῶν Δυνάμεων Ἀρχιστράτηγοι…"
meVE.Lity1.text = "Τῶν νοερῶν Δυνάμεων Ἀρχιστράτηγοι, οἱ παρεστῶτες ἀπαύστως, τῷ δεσποτικῷ θρόνῳ, πρεσβεύσατε πρὸς Κύριον, εἰρήνην τῷ κόσμῳ δωρήσασθαι, καὶ ταῖς ψυχαῖς ἡμῶν τὸ μέγα ἔλεος."

meVE.Lity2.poet = poets_gr_GR_cog.po.Arseniou
meVE.Lity2.mode = miscellanea_gr_GR_cog.misc.Mode1
meVE.Lity2.melody = ""
meVE.Lity2.incipit = "Ὁ Ταξιάρχης τῶν ἄνω Δυνάμεων…"
meVE.Lity2.text = "Ὁ Ταξιάρχης τῶν ἄνω Δυνάμεων, Μιχαὴλ ὁ πρωτοστάτης τῶν θείων ταγμάτων, σήμερον ἡμᾶς πρὸς πανήγυριν συνεκάλεσεν, ὁ καθʼ ἑκάστην μεθʼ ἡμῶν πορευόμενος, καὶ φυλάττων τοὺς πάντας, ἐκ πάσης τοῦ διαβόλου περιστάσεως. Δεῦτε οὖν φιλέορτοι καὶ φιλόχριστοι, τὰ ἄνθη τῶν ἀρετῶν δρεψάμενοι, καθαραῖς ἐννοίαις, καὶ συνειδότι εὐστάθμῳ, τοῦ Ἀρχαγγέλου τὴν Σύναξιν τιμήσωμεν· αὐτὸς γὰρ ἀδιαλείπτως τῷ Θεῷ παριστάμενος, καὶ τὸν τρισάγιον ὕμνον ἀναμέλπων, πρεσβεύει σωθῆναι τὰς ψυχάς ἡμῶν."

meVE.Lity3.poet = poets_gr_GR_cog.po.Stouditou
meVE.Lity3.mode = miscellanea_gr_GR_cog.misc.Mode2
meVE.Lity3.melody = ""
meVE.Lity3.incipit = "Τῆς ἀΰλου οὐσίας…"
meVE.Lity3.text = "Τῆς ἀΰλου οὐσίας, τῶν νοερῶν Δυνάμεων προστατεύοντες, καὶ τῆς Τρισηλίου δόξης ταῖς μαρμαρυγαῖς, φωτοδοτοῦντες τὴν οἰκουμένην, Ἀρχιστράτηγοι, ἀλήκτῳ φωνῇ, τὸν τρισάγιον ὕμνον ἐξᾴδετε· διὸ πρεσβεύσατε σωθῆναι τὰς ψυχὰς ἡμῶν."

meVE.Lity4.poet = poets_gr_GR_cog.po.Arseniou
meVE.Lity4.mode = miscellanea_gr_GR_cog.misc.Mode2
meVE.Lity4.melody = ""
meVE.Lity4.incipit = "Τῷ ἀΰλω θρόνῳ περικυκλοῦντες…"
meVE.Lity4.text = "Τῷ ἀΰλω θρόνῳ περικυκλοῦντες, νοεραὶ οὐσίαι θεῖοι Ἀσώματοι, τὴν τρισάγιον ᾠδήν, τῷ πρύτανι Θεῷ, φλογεροῖς τοῖς χείλεσιν ᾄδετε, Ἅγιος ὁ Θεός, ὁ Πατὴρ ὁ ἄναρχος, Ἅγιος ἰσχυρός, ὁ Υἱὸς ὁ συνάναρχος, Ἅγιος ἀθάνατος, τὸ ὁμοούσιον Πνεῦμα, τὸ σὺν Πατρὶ καὶ Υἱῷ συνδοξαζόμενον."

meVE.Lity5.poet = poets_gr_GR_cog.po.Anatoliou
meVE.Lity5.mode = miscellanea_gr_GR_cog.misc.Mode2
meVE.Lity5.melody = ""
meVE.Lity5.incipit = "Ἀσωμάτοις χείλεσι…"
meVE.Lity5.text = "Ἀσωμάτοις χείλεσι, καὶ νοεροῖς στόμασι, τῶν Ἀγγέλων αἱ τάξεις, τὸν ἀκατάπαυστον ὕμνον προσφέρουσι, τῇ σῇ ἀπροσίτῳ Θεότητι Κύριε, καὶ νόες καθαροὶ καὶ λειτουργοὶ τῆς σῆς δόξης αἰνοῦσί σε Δέσποτα· Σὺν αὐτοῖς δὲ Μιχαὴλ ὁ ἀσώματος, καὶ πρωτοστάτης τῶν ἄνω Δυνάμεων, ὁ Ἀρχιστράτηγος τῶν Ἀγγέλων, ἑστιάτωρ ἡμῖν σήμερον γέγονε, τὰ τῶν ἀσωμάτων ᾄσματα προτρεπόμενος ἡμᾶς ᾄδειν, τῇ ἀπροσίτῳ σου δόξῃ Φιλάνθρωπε, ἵνα πρεσβεύῃ ἀπαύστως, ὑπὲρ τῶν ψυχῶν ἡμῶν."

meVE.Lity6.poet = poets_gr_GR_cog.po.EfraimKarias
meVE.Lity6.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.Lity6.melody = ""
meVE.Lity6.incipit = "Τῆς ἡλιακῆς ἀκτῖνος…"
meVE.Lity6.text = "Τῆς ἡλιακῆς ἀκτῖνος τοῦ θείου φωτός, καταλαμπόμενοι Ἀρχιστράτηγοι, τὰς τῶν ἀΰλων καταυγάζετε ταξιαρχίας, ὡς φῶς λευχειμονοῦντες, ἐν ὑψίστοις τὸ πῦρ καταυγάζετε ἐν κόσμῳ, τῆς ἀπροσίτου θεότητος· ὅθεν καὶ τὸν τρισάγιον ὕμνον, ἐν φλογεροῖς τοῖς χείλεσιν ἀκαταπαύστως ᾄδετε· Ἅγιος, Ἅγιος, Ἅγιος εἶ, ὁ Θεὸς ἡμῶν δόξα σοι."

meVE.Lity7.mode = miscellanea_gr_GR_cog.misc.Mode6
meVE.Lity7.melody = ""
meVE.Lity7.incipit = "Οἱ Ἄγγελοί σου Χριστέ…"
meVE.Lity7.text = "Οἱ Ἄγγελοί σου Χριστέ, τῷ θρόνῳ τῆς μεγαλωσύνης, ἐν φόβῳ παρεστῶτες, καὶ τῇ σῇ φωτοχυσίᾳ ἀεὶ λαμπόμενοι, ἐν τοῖς ὑψίστοις ὑμνῳδοί σου, καὶ τῆς βουλῆς σου λειτουργοί, ὑπὸ σοῦ ἐκπεμπόμενοι, νέμουσι φωτισμὸν ταῖς ψυχαῖς ἡμῶν."

meVE.LityGlory.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.LityGlory.melody = ""
meVE.LityGlory.incipit = "Πυρίνοις χείλεσιν…"
meVE.LityGlory.text = "Πυρίνοις χείλεσιν, ὑμνεῖ σε τὰ Χερσυβίμ, Χριστὲ ὁ Θεός, ἀσωμάτοις δὲ στόμασιν ἀσιγήτως, δοξολογεῖ σε τῶν Ἀρχαγγέλων ὁ χορός, Μιχαὴλ δὲ ὁ Ἀρχιστράτηγος, τῶν ἄνω δυνάμεων, τὸν ἐπινίκιον ὕμνον, ἀκαταπαύστως προσφέρει τῇ δόξῃ σου· Αὐτὸς γὰρ ἡμᾶς σήμερον κατηύγασε, πρὸς πανήγυριν φαιδράν, κροτῆσαι ἐπαξίως ψαλμικῶς, αἶνον τὸν τρισάγιον, τοῖς πηλίνοις χείλεσιν, ὅτι τὰ πάντα τῆς σῆς αἰνέσεως ἐπλήσθη, καὶ παρέχεις τῷ κόσμῳ διʼ αὐτοῦ τὸ μέγα ἔλεος."

meVE.LityBoth.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.LityBoth.melody = ""
meVE.LityBoth.incipit = "Σήμερον ὁ θεοχώρητος ναός…"
meVE.LityBoth.text = "Σήμερον ὁ θεοχώρητος ναός, ἡ Θεοτόκος, ἐν ναῷ Κυρίου προσάγεται, καὶ Ζαχαρίας ταύτην ὑποδέχεται. Σήμερον τὰ τῶν Ἁγίων Ἅγια ἀγάλλονται, καὶ ὁ χορὸς τῶν Ἀγγέλων, μυστικῶς πανηγυρίζει· μεθʼ ὧν καὶ ἡμεῖς ἑορτάζοντες σήμερον, σὺν τῷ Γαβριὴλ ἐκβοήσωμεν· Χαῖρε Κεχαριτωμένη, ὁ Κύριος μετὰ σοῦ, ὁ ἔχων τὸ μέγα ἔλεος."

meVE.Aposticha1.mode = miscellanea_gr_GR_cog.misc.Mode1
meVE.Aposticha1.greekmelody = he.a.m1_gr_GR_cog.heAU.TonOuranionTagmaton.greekname
meVE.Aposticha1.melody = he.a.m1_gr_GR_cog.heAU.TonOuranionTagmaton.name
meVE.Aposticha1.incipit = "Ἀγγελικῶς οἱ ἐν κόσμῳ…"
meVE.Aposticha1.text = "Ἀγγελικῶς οἱ ἐν κόσμῳ πανηγυρίζοντες, τῷ ἐπὶ θρόνου δόξης, Θεῷ ἐποχουμένῳ, βοήσωμεν τὸν ὕμνον, Ἅγιος εἶ, ὁ Πατὴρ ὁ οὐράνιος· ὁ συναΐδιος Λόγος Ἅγιος εἶ, καὶ τὸ Πνεῦμα τὸ Πανάγιον."

meVE.Aposticha2.mode = miscellanea_gr_GR_cog.misc.Mode1
meVE.Aposticha2.melody = he.a.m1_gr_GR_cog.heAU.TonOuranionTagmaton.name
meVE.Aposticha2.incipit = "Τῶν οὐρανίων πρωτεύων…"
meVE.Aposticha2.text = "Τῶν οὐρανίων πρωτεύων, ἐν παῤῥησίᾳ πολλῇ, καὶ παρεστὼς τῷ θρόνῳ, τῷ ἀστέκτῳ ἐν δόξῃ, αὐτόπτα τῶν ἀῤῥήτων, σῶσον ἡμᾶς, Μιχαὴλ Ἀρχιστράτηγε, τοὺς ἐν ἀνάγκαις κινδύνων καὶ πειρασμῶν, ταῖς πρεσβείαις σου δεόμεθα."

meVE.Aposticha3.mode = miscellanea_gr_GR_cog.misc.Mode1
meVE.Aposticha3.melody = he.a.m1_gr_GR_cog.heAU.TonOuranionTagmaton.name
meVE.Aposticha3.incipit = "Τῶν ἀσωμάτων Ἀγγέλων…"
meVE.Aposticha3.text = "Τῶν ἀσωμάτων Ἀγγέλων, πρῶτος ὑπάρχεις σαφῶς, καὶ λειτουργὸς τῆς θείας, φωταυγείας ἐκείνης, αὐτόπτης τε καὶ μύστης, σῶσον ἡμᾶς, Μιχαὴλ Ἀρχιστράτηγε, τοὺς ἐτησίως τιμῶντάς σε εὐσεβῶς, καὶ ὑμνοῦντας τὴν Τριάδα πιστῶς."

meVE.AposGlory.poet = poets_gr_GR_cog.po.IoannouMonachou
meVE.AposGlory.mode = miscellanea_gr_GR_cog.misc.Mode8
meVE.AposGlory.melody = ""
meVE.AposGlory.incipit = "Ὡς ταξιάρχης καὶ πρόμαχος…"
meVE.AposGlory.text = "Ὡς ταξιάρχης καὶ πρόμαχος, καὶ τῶν Ἀγγέλων Ἀρχηγὸς Ἀρχιστράτηγε, πάσης ἀνάγκης καὶ θλίψεως, νόσου καὶ δεινῶν ἁμαρτημάτων ἐλευθέρωσον, τοὺς εἰλικρινῶς ἀνυμνοῦντας, καὶ αἰτουμένους σε Ἔνδοξε, ἐναργῶς ὡς ἄϋλος τὸν ἄϋλον καθορῶν, καὶ τῷ ἀπροσίτῳ φωτί, τῆς τοῦ Δεσπότου δόξης καταλαμπόμενος· αὐτὸς γὰρ φιλανθρώπως, σάρκα διʼ ἡμᾶς ἐκ Παρθένου προσείληφε, σῶσαι βουληθεὶς τὸ ἀνθρώπινον."

meVE.Apolytikion1.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.Apolytikion1.greekmelody = he.a.m4_gr_GR_cog.heAU.OYpsotheisEnToStavro.greekname
meVE.Apolytikion1.melody = he.a.m4_gr_GR_cog.heAU.OYpsotheisEnToStavro.name
meVE.Apolytikion1.incipit = "Τῶν οὐρανίων στρατιῶν…"
meVE.Apolytikion1.text = "Τῶν οὐρανίων στρατιῶν Ἀρχιστράτηγοι, δυσωποῦμεν ὑμᾶς ἡμεῖς οἱ ἀνάξιοι, ἵνα ταῖς ὑμῶν δεήσεσι, τειχίσητε ἡμᾶς, σκέπῃ τῶν πτερύγων, τῆς ἀΰλου ὑμῶν δόξης, φρουροῦντες ἡμᾶς προσπίπτοντας, ἐκτενῶς καὶ βοῶντας· Ἐκ τῶν κινδύνων λυτρώσασθε ἡμᾶς, ὡς Ταξιάρχαι τῶν ἄνω Δυνάμεων."

meMA.Kathisma11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Kathisma11.greekmelody = he.a.m4_gr_GR_cog.heAU.OYpsotheisEnToStavro.greekname
meMA.Kathisma11.melody = he.a.m4_gr_GR_cog.heAU.OYpsotheisEnToStavro.name
meMA.Kathisma11.incipit = "Τῶν ἀσωμάτων λειτουργῶν Ἀρχιστράτηγε, ὁ…"
meMA.Kathisma11.text = "Τῶν ἀσωμάτων λειτουργῶν Ἀρχιστράτηγε, ὁ κατʼ ἐνώπιον Θεοῦ παριστάμενος, καὶ ταῖς ἐκεῖθεν λάμψεσι λαμπόμενος, φαίδρυνον ἁγίασον, τοὺς πιστῶς σε ὑμνοῦντας, πάσης ἀπολύτρωσαι, τοῦ ἐχθροῦ τυραννίδος, καὶ εἰρηναίαν αἴτησαι ζωήν, τοῖς βασιλεῦσι, καὶ πᾶσι τοῖς πέρασιν."

meMA.Kathisma1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Kathisma1T.melody = he.a.m4_gr_GR_cog.heAU.OYpsotheisEnToStavro.name
meMA.Kathisma1T.incipit = "Οὐ σιωπήσομεν ποτὲ Θεοτόκε, τοῦ…"
meMA.Kathisma1T.text = "Οὐ σιωπήσομεν ποτὲ Θεοτόκε, τοῦ εὐχαρίστως ἀνυμνεῖν ἐκ καρδίας, τὰ σὰ ἐλέη Δέσποινα οἱ δοῦλοί σου, κράζοντες καὶ λέγοντες· Παναγία Παρθένε, πρόφθασον καὶ λύτρωσαι, ἐξ ἐχθρῶν ἀοράτων, καὶ ὁρατῶν καὶ πάσης ἀπειλῆς· σοὺς γὰρ δούλους σῴζεις ἀεί, ἐκ παντοίων δεινῶν."

meMA.Kathisma21.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Kathisma21.greekmelody = he.a.m4_gr_GR_cog.heAU.OYpsotheisEnToStavro.greekname
meMA.Kathisma21.melody = he.a.m4_gr_GR_cog.heAU.OYpsotheisEnToStavro.name
meMA.Kathisma21.incipit = "Τὰ Χερουβὶμ καὶ Σεραφὶμ πολυόμματα…"
meMA.Kathisma21.text = "Τὰ Χερουβὶμ καὶ Σεραφὶμ πολυόμματα, τῶν Ἀρχαγγέλων λειτουργῶν τὰ στρατεύματα, σὺν Ἐξουσίαις Θρόνοι Κυριότητες, Ἄγγελοι Δυνάμεις τε, καὶ Ἀρχαὶ δυσωποῦσι, σὲ τὸν Ποιητὴν ἡμῶν, καὶ Θεὸν καὶ Δεσπότην· Μὴ ὑπερίδῃς δέησιν λαοῦ, ἡμαρτηκότος, Χριστὲ πολυέλεε."

meMA.Kathisma2T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Kathisma2T.melody = he.a.m4_gr_GR_cog.heAU.TachyProkatalave.melody
meMA.Kathisma2T.incipit = he.a.m4_gr_GR_cog.heAU.TachyProkatalave.incipit
meMA.Kathisma2T.text = he.a.m4_gr_GR_cog.heAU.TachyProkatalave.text

meMA.Kathisma31.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Kathisma31.greekmelody = he.a.m8_gr_GR_cog.heAU.ToProstachthen.greekname
meMA.Kathisma31.melody = he.a.m8_gr_GR_cog.heAU.ToProstachthen.name
meMA.Kathisma31.incipit = "Τῶν οὐρανίων ἀρχηγοὶ καὶ πρωτοστάται…"
meMA.Kathisma31.text = "Τῶν οὐρανίων ἀρχηγοὶ καὶ πρωτοστάται, τῶν ὑψιθρόνων καὶ φρικτῶν τῆς θείας δόξης, Μιχαὴλ καὶ Γαβριὴλ Ἀρχιστράτηγοι, σὺν πᾶσι τοῖς ἀσωμάτοις ὦ Λειτουργοί, Δεσπότου ὑπὲρ τοῦ κόσμου διαπαντός, πρεσβεύοντες αἰτήσασθε τὴν ἄφεσιν, τῶν ἐπταισμένων ἡμῖν, καὶ ἔλεος καὶ χάριν εὑρεῖν, ἐν ἡμέρᾳ τῆς κρίσεως."

meMA.Kathisma3T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Kathisma3T.melody = he.a.m8_gr_GR_cog.heAU.ToProstachthen.name
