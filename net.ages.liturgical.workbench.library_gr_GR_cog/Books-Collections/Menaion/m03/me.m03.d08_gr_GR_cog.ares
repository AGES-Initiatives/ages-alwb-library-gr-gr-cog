meMA.Ode1C11.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C11.greekmelody = he.h.m8_gr_GR_cog.heHE.AsomenToKyrio.greekname
meMA.Ode1C11.melody = he.h.m8_gr_GR_cog.heHE.AsomenToKyrio.name
meMA.Ode1C11.incipit = "Θρόνῳ τῷ τοῦ Δεσπότου, Πάτερ…"
meMA.Ode1C11.text = "Θρόνῳ τῷ τοῦ Δεσπότου, Πάτερ θεοφόρε Θεοφύλακτε, παριστάμενος πίστει, τοὺς τελοῦντας τὴν μνήμην σου φύλαττε."

meMA.Ode1C12.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C12.melody = he.h.m8_gr_GR_cog.heHE.AsomenToKyrio.name
meMA.Ode1C12.incipit = "Ἔχρισέ σε ἀξίως, χάρις ἡ…"
meMA.Ode1C12.text = "Ἔχρισέ σε ἀξίως, χάρις ἡ τοῦ Πνεύματος Πανόλβιε, ἕνεκεν ἀληθείας, καὶ τὸν τρόπον σου χάριν χρηστότητος."

meMA.Ode1C13.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C13.melody = he.h.m8_gr_GR_cog.heHE.AsomenToKyrio.name
meMA.Ode1C13.incipit = "Ἵνα τῶν οὐρανίων, Μάκαρ ἐπιτύχῃς…"
meMA.Ode1C13.text = "Ἵνα τῶν οὐρανίων, Μάκαρ ἐπιτύχῃς τὰ ἐπίγεια, κατεπάτησας πάντα, καὶ Χριστῷ τῷ Θεῷ ἠκολούθησας."

meMA.Ode1C1T.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C1T.melody = he.h.m8_gr_GR_cog.heHE.AsomenToKyrio.name
meMA.Ode1C1T.incipit = "Ὅλος ἐπιθυμίᾳ, ὁ ἐκ τῆς…"
meMA.Ode1C1T.text = "Ὅλος ἐπιθυμίᾳ, ὁ ἐκ τῆς γαστρός σου Θεονύμφευτε, εὐδοκήσας τεχθῆναι, γλυκασμός τε καὶ φῶς Ἁγνὴ ἄδυτον."

meMA.Ode3C1H.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C1H.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C1H.greekmelody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.greekname
meMA.Ode3C1H.melody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.melody
meMA.Ode3C1H.incipit = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.incipit
meMA.Ode3C1H.text = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.text

meMA.Ode3C11.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C11.greekmelody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.greekname
meMA.Ode3C11.melody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.name
meMA.Ode3C11.incipit = "Νόμοις σὺ πειθόμενος, τοῖς τοῦ…"
meMA.Ode3C11.text = "Νόμοις σὺ πειθόμενος, τοῖς τοῦ Δεσπότου σου ἔνδοξε, Ἀρχιερεῦ, νόμων παρανόμων, βασιλέων ἠλόγησας."

meMA.Ode3C12.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C12.melody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.name
meMA.Ode3C12.incipit = "Θεῖον ἐξεικόνισμα, Χριστοῦ καὶ ἄχραντον…"
meMA.Ode3C12.text = "Θεῖον ἐξεικόνισμα, Χριστοῦ καὶ ἄχραντον ἔσεβες, τὰς τῶν ἀσεβούντων ἀσεβείας ἀρνούμενος."

meMA.Ode3C13.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C13.melody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.name
meMA.Ode3C13.incipit = "Ἔτλης τὸ μαρτύριον, τῆς συνειδήσεως…"
meMA.Ode3C13.text = "Ἔτλης τὸ μαρτύριον, τῆς συνειδήσεως πρότερον, ἀσκητικῶς, καὶ νῦν τῶν Μαρτύρων, ἀνεδήσω τὸν στέφανον."

meMA.Ode3C1T.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C1T.melody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.name
meMA.Ode3C1T.incipit = "Ἵνα τρίβον Δέσποτα, πρὸς ἁγιότητα…"
meMA.Ode3C1T.text = "Ἵνα τρίβον Δέσποτα, πρὸς ἁγιότητα φέρουσαν, δείξῃς ἡμῖν, μήτραν παναγίαν, παναγίως κατῴκησας."

meMA.Ode3Kathisma1.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3Kathisma1.greekmelody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.greekname
meMA.Ode3Kathisma1.melody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.name
meMA.Ode3Kathisma1.incipit = "Ἐξασκήσας ὁσίως Πάτερ σοφέ, Ἱεράρχης…"
meMA.Ode3Kathisma1.text = "Ἐξασκήσας ὁσίως Πάτερ σοφέ, Ἱεράρχης ἐχρίσθης ἱερουργεῖν, τὰ θεῖα μυστήρια, εἰς ψυχῶν περιποίησιν, τὴν Χριστοῦ δὲ εἰκόνα, τιμήσας ὑπήνεγκας, ἐξορίας θλίψεις, καὶ χρόνιον κάθειρξιν· ὅθεν μετὰ τέλος, ἀναβλύζων ἰάσεις, ἰᾶσαι τοὺς κάμνοντας, καὶ φωτίζεις τοὺς ψάλλοντας, Ἱεράρχα Θεοφύλακτε· πρέσβευε Χριστῷ τῷ Θεῷ, τῶν πταισμάτων ἄφεσιν δωρήσασθαι, τοῖς ἑορτάζουσι πόθῳ, τὴν ἁγίαν μνήμην σου."

meMA.Ode3KathismaT.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3KathismaT.melody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.name
meMA.Ode3KathismaT.incipit = "Τοῦ Ἡλίου νεφέλη τοῦ νοητοῦ…"
meMA.Ode3KathismaT.text = "Τοῦ Ἡλίου νεφέλη τοῦ νοητοῦ, θείου φέγγους λυχνία χρυσοφαής, ἄσπιλε, ἀμόλυντε, ὑπεράμωμε Δέσποινα, τὴν σκοτεινὴν ψυχήν μου, τυφλώττουσαν πάθεσι, τῆς ἀπαθείας αἴγλῃ καταύγασον δέομαι, καὶ μεμολυσμένην, τὴν καρδίαν μου, πλῦνον ῥοαῖς κατανύξεως, μετανοίας τε δάκρυσι, ἵνα πόθῳ κραυγάζω σοι· Πρέσβευε τῷ σῷ Υἱῷ καὶ Θεῷ, τῶν πταισμάτων ἄφεσιν δοθῆναί μοι· σὲ γὰρ ἔχω ἐλπίδα, ὁ ἀνάξιος δοῦλός σου."

meMA.Ode3KathismaSt.mode = miscellanea_gr_GR_cog.misc.Mode8
