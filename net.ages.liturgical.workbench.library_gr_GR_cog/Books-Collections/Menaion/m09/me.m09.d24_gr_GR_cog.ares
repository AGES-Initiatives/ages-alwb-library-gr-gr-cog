meLI.Kontakion1.incipit = meMA.Kontakion1.incipit
meLI.Kontakion1.text = meMA.Kontakion1.text

meMegalynarion.text = "Παύλου λαμπρυνθεῖσα ταῖς ἀστραπαῖς, ὅλη καλὴ ὤφθης, ὅλη ἄμωμος τῷ Χριστῷ, Θέκλα Πρωτομάρτυς, ὑπερφυέσιν ἄθλοις,ὧν φαίδρυνον τῇ δόξῃ τοὺς σὲ γεραίροντας."

alt1.meDA.date = "Τῇ ΚΔ΄ Σεπτεμβρίου"
alt1.meDA.commemoration.full = "Ἀνάμνησης ποιούμεθα τῆς θαυμαστῆς ἐν Μυρτιδίοις τῶν Κυθήρων εὑρέσεως τῆς ἱερᾶς καὶ σεβασμίας Εἰκόνος τῆς Παναγίας" //full title of day
alt1.meDA.commemoration.first_short = "Ἡ Σύναξις τῆς Ὑπεραγίας Θεοτόκου τῆς \"Μυρτιδιωτίσσης\""
