peMA.Ode3C1H.text = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.text

peMA.Ode3C11.ode = miscellanea_gr_GR_cog.misc.Ode3
peMA.Ode3C11.mode = miscellanea_gr_GR_cog.misc.Mode4
peMA.Ode3C11.greekmelody = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.greekname
peMA.Ode3C11.melody = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.name
peMA.Ode3C11.incipit = "Ναμάτων ζωοποιῶν…"
peMA.Ode3C11.text = "Ναμάτων ζωοποιῶν, τῇ Ἐκκλησίᾳ τὰς πηγὰς ἤνοιξας· Εἴ τις διψᾷ, πρόθυμος, ἴτω καὶ πινέτω, βοῶν Ἀγαθέ."

peMA.Ode3C12.ode = miscellanea_gr_GR_cog.misc.Ode3
peMA.Ode3C12.mode = miscellanea_gr_GR_cog.misc.Mode4
peMA.Ode3C12.melody = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.name
peMA.Ode3C12.incipit = "Ἐκ γῆς μὲν πρὸς οὐρανόν…"
peMA.Ode3C12.text = "Ἐκ γῆς μὲν πρὸς οὐρανόν, ἀνυψωθῆναι προφανῶς ἔλεγες· ἐξ οὐρανοῦ Πνεῦμα δέ, πέμπειν ἐπηγγείλω τὸ ἅγιον."

peMA.Ode3C1T.ode = miscellanea_gr_GR_cog.misc.Ode3
peMA.Ode3C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
peMA.Ode3C1T.melody = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.name
peMA.Ode3C1T.incipit = "Ὁ φύσει ζωοποιός…"
peMA.Ode3C1T.text = "Ὁ φύσει ζωοποιός, καὶ ἐκ Παρθένου γεννηθεὶς Κύριος, πᾶσι πιστοῖς δεδώρηται, τὴν ἀθανασίαν ὡς εὔσπλαγχνος."

peMA.Ode3C2H.ode = miscellanea_gr_GR_cog.misc.Ode3
peMA.Ode3C2H.mode = miscellanea_gr_GR_cog.misc.Mode8
peMA.Ode3C2H.greekmelody = he.h.m8_gr_GR_cog.heHE.EstereothiIKardiaMouEnKyrio.greekname
peMA.Ode3C2H.melody = he.h.m8_gr_GR_cog.heHE.EstereothiIKardiaMouEnKyrio.melody
peMA.Ode3C2H.incipit = he.h.m8_gr_GR_cog.heHE.EstereothiIKardiaMouEnKyrio.incipit
peMA.Ode3C2H.text = he.h.m8_gr_GR_cog.heHE.EstereothiIKardiaMouEnKyrio.text

peMA.Ode3C21.ode = miscellanea_gr_GR_cog.misc.Ode3
peMA.Ode3C21.mode = miscellanea_gr_GR_cog.misc.Mode8
peMA.Ode3C21.greekmelody = he.h.m8_gr_GR_cog.heHE.EstereothiIKardiaMouEnKyrio.greekname
peMA.Ode3C21.melody = he.h.m8_gr_GR_cog.heHE.EstereothiIKardiaMouEnKyrio.name
peMA.Ode3C21.incipit = "Μὴ τὴν κατʼ ὄψιν κρίσιν…"
peMA.Ode3C21.text = "Μὴ τὴν κατʼ ὄψιν κρίσιν κρίνετε Ἰουδαῖοι, διδάσκων ἔλεγεν ὁ Δεσπότης· ὡς ἐπέστη τῷ ἱερῷ, καθὼς γέγραπται, μεσούσης τῆς νομικῆς ἑορτῆς."

peMA.Ode3C22.ode = miscellanea_gr_GR_cog.misc.Ode3
peMA.Ode3C22.mode = miscellanea_gr_GR_cog.misc.Mode8
peMA.Ode3C22.melody = he.h.m8_gr_GR_cog.heHE.EstereothiIKardiaMouEnKyrio.name
peMA.Ode3C22.incipit = "Μὴ τὴν κατʼ ὄψιν κρίσιν…"
peMA.Ode3C22.text = "Μὴ τὴν κατʼ ὄψιν κρίσιν κρίνετε Ἰουδαῖοι· Χριστὸς γὰρ ἦλθεν, ὅνπερ ἐκάλουν οἱ Προφῆται, ἐκ Σιὼν ἐλευσόμενον, καὶ κόσμον ἀνακαλούμενον."

peMA.Ode3C23.ode = miscellanea_gr_GR_cog.misc.Ode3
peMA.Ode3C23.mode = miscellanea_gr_GR_cog.misc.Mode8
peMA.Ode3C23.melody = he.h.m8_gr_GR_cog.heHE.EstereothiIKardiaMouEnKyrio.name
peMA.Ode3C23.incipit = "Εἰ καὶ τοῖς λόγοις οὐ…"
peMA.Ode3C23.text = "Εἰ καὶ τοῖς λόγοις οὐ πιστεύετε Ἰουδαῖοι, τοῖς ἔργοις πείσθητε τοῦ Δεσπότου· τί πλανᾶσθε ἀθετοῦντες τὸν ἅγιον, ὃν ἔγραψεν ἐν τῷ νόμῳ Μωσῆς;"

peMA.Ode3C24.ode = miscellanea_gr_GR_cog.misc.Ode3
peMA.Ode3C24.mode = miscellanea_gr_GR_cog.misc.Mode8
peMA.Ode3C24.melody = he.h.m8_gr_GR_cog.heHE.EstereothiIKardiaMouEnKyrio.name
peMA.Ode3C24.incipit = "Εἰ τὸν Μεσσίαν πάντως δεῖ…"
peMA.Ode3C24.text = "Εἰ τὸν Μεσσίαν πάντως δεῖ ἐλθεῖν Ἰουδαῖοι, Χριστὸς δὲ ἦλθε ὁ νῦν Μεσσίας, τί πλανᾶσθε ἀθετοῦντες τὸν Δίκαιον, ὃν ἔγραψεν ἐν τῷ νόμῳ Μωσῆς;"

peMA.Ode3C2Tr.ode = miscellanea_gr_GR_cog.misc.Ode3
peMA.Ode3C2Tr.mode = miscellanea_gr_GR_cog.misc.Mode8
peMA.Ode3C2Tr.melody = he.h.m8_gr_GR_cog.heHE.EstereothiIKardiaMouEnKyrio.name
peMA.Ode3C2Tr.incipit = "Σὲ προσκυνοῦμεν Πάτερ ἄναρχε τῇ…"
peMA.Ode3C2Tr.text = "Σὲ προσκυνοῦμεν Πάτερ ἄναρχε τῇ οὐσίᾳ, ὑμνοῦμεν ἄναρχον τὸν Υἱόν σου, καὶ τὸ Πνεῦμα εὐσεβῶς τὸ πανάγιον, ὡς ἕνα τὰ Τρία φύσει Θεόν."

peMA.Ode3C2T.ode = miscellanea_gr_GR_cog.misc.Ode3
peMA.Ode3C2T.mode = miscellanea_gr_GR_cog.misc.Mode8
peMA.Ode3C2T.melody = he.h.m8_gr_GR_cog.heHE.EstereothiIKardiaMouEnKyrio.name
peMA.Ode3C2T.incipit = "Εἷς τῆς Τριάδος ὤν, γενόμενος…"
peMA.Ode3C2T.text = "Εἷς τῆς Τριάδος ὤν, γενόμενος σάρξ ὡράθης, οὐ τρέψας Κύριε τὴν οὐσίαν, οὐδὲ φλέξας τῆς τεκούσης τὴν ἄφθορον γαστέρα, Θεὸς ὢν ὅλος καὶ πῦρ."

peMA.Ode3Kathisma1.mode = miscellanea_gr_GR_cog.misc.Mode8
peMA.Ode3Kathisma1.greekmelody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.greekname
peMA.Ode3Kathisma1.melody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.name
peMA.Ode3Kathisma1.incipit = "Ἑστηκὼς ἐν τῷ μέσῳ…"
peMA.Ode3Kathisma1.text = "Ἑστηκὼς ἐν τῷ μέσῳ τοῦ ἱεροῦ, μεσαζούσης ἐνθέως τῆς ἑορτῆς· Ὁ διψῶν, ἀνέκραζες, ἐρχέσθω πρός με καὶ πινέτω· ὁ γὰρ πίνων ἐκ τούτου, τοῦ θείου νάματος, ποταμοὺς ἐκ κοιλίας, ἐκρεύσει δογμάτων μου· ὅστις δὲ πιστεύει, εἰς ἐμὲ τὸν σταλέντα, ἐκ θείου Γεννήτορος, μετʼ ἐμοῦ δοξασθήσεται. Διὰ τοῦτο βοῶμέν σοι· Δόξα σοι, Χριστὲ ὁ Θεός, ὅτι πλουσίως ἐξέχεας τὰ νάματα, τῆς σῆς φιλανθρωπίας τοῖς δούλοις σου."

peMA.Ode3Kathisma2.mode = miscellanea_gr_GR_cog.misc.Mode8
peMA.Ode3Kathisma2.greekmelody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.greekname
peMA.Ode3Kathisma2.melody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.name
peMA.Ode3Kathisma2.incipit = "Τῆς σοφίας τὸ ὕδωρ…"
peMA.Ode3Kathisma2.text = "Τῆς σοφίας τὸ ὕδωρ καὶ τῆς ζωῆς, ἀναβρύζων τῷ κόσμῳ, πάντας Σωτήρ, καλεῖς τοῦ ἀρύσασθαι, σωτηρίας τὰ νάματα· τὸν γὰρ θεῖον νόμον σου, δεχόμενος ἄνθρωπος, ἐν αὐτῷ σβεννύει, τῆς πλάνης τοὺς ἄνθρακας· ὅθεν εἰς αἰῶνας, οὐ διψήσει, οὐ λήξει, τοῦ κόρου σου Δέσποτα Βασιλεῦ ἐπουράνιε. Διὰ τοῦτο δοξάζομεν, τὸ κράτος σου, Χριστὲ ὁ Θεός, τῶν πταισμάτων ἄφεσιν αἰτούμενοι, καταπέμψαι πλουσίως τοῖς δούλοις σου."

peMA.Ode4C1H.ode = miscellanea_gr_GR_cog.misc.Ode4
peMA.Ode4C1H.mode = miscellanea_gr_GR_cog.misc.Mode4
peMA.Ode4C1H.greekmelody = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.greekname
peMA.Ode4C1H.melody = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.melody
peMA.Ode4C1H.incipit = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.incipit
peMA.Ode4C1H.text = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.text

peMA.Ode4C11.ode = miscellanea_gr_GR_cog.misc.Ode4
peMA.Ode4C11.mode = miscellanea_gr_GR_cog.misc.Mode4
peMA.Ode4C11.greekmelody = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.greekname
peMA.Ode4C11.melody = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.name
peMA.Ode4C11.incipit = "Ῥήξας θανάτου τὰς πύλας…"
peMA.Ode4C11.text = "Ῥήξας θανάτου τὰς πύλας τῇ σῇ δυνάμει, ὁδοὺς ζωῆς ἐγνώρισας τῆς ἀθανασίας· πύλας δὲ διήνοιξας, τοῖς πίστει κραυγάζουσι· Δόξα τῇ δυνάμει σου, Κύριε."

peMA.Ode4C12.ode = miscellanea_gr_GR_cog.misc.Ode4
peMA.Ode4C12.mode = miscellanea_gr_GR_cog.misc.Mode4
peMA.Ode4C12.melody = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.name
peMA.Ode4C12.incipit = "Τὴν μεσότητα τῶν ὅλων…"
peMA.Ode4C12.text = "Τὴν μεσότητα τῶν ὅλων, καὶ τέλος ἔχων, καὶ τῆς ἀρχῆς ὡς ἄναρχος περιδεδραγμένος, ἔστης ἐν τῷ μέσῳ, βοῶν· Τῶν θείων θεόφρονες, δεῦτε δωρεῶν ἀπολαύσατε."

peMA.Ode4C13.ode = miscellanea_gr_GR_cog.misc.Ode4
peMA.Ode4C13.mode = miscellanea_gr_GR_cog.misc.Mode4
peMA.Ode4C13.melody = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.name
peMA.Ode4C13.incipit = "Ὡς Θεὸς καὶ πάντων…"
peMA.Ode4C13.text = "Ὡς Θεὸς καὶ πάντων ἔχων τὴν ἐξουσίαν, ὡς δυνατὸς τὸ κράτος καθελὼν τοῦ θανάτου, πέμπειν ἐπηγγείλω Χριστέ, τὸ Πνεῦμα τὸ ἅγιον, τὸ ἐκ Πατρὸς προερχόμενον."

peMA.Ode4C1T.ode = miscellanea_gr_GR_cog.misc.Ode4
peMA.Ode4C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
peMA.Ode4C1T.melody = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.name
peMA.Ode4C1T.incipit = "Νέμεις πλουσίως τὴν χάριν…"
peMA.Ode4C1T.text = "Νέμεις πλουσίως τὴν χάριν, τοῖς σὲ ὑμνοῦσι, καὶ τὸν ἐκ σοῦ τεχθέντα προαιώνιον Λόγον, Μῆτερ ἀπειρόγαμε, πταισμάτων τὴν ἄφεσιν, τούτοις αἰτουμένη πανάχραντε."

peMA.Ode4C2H.ode = miscellanea_gr_GR_cog.misc.Ode4
peMA.Ode4C2H.mode = miscellanea_gr_GR_cog.misc.Mode8
peMA.Ode4C2H.greekmelody = he.h.m8_gr_GR_cog.heHE.OProfitisAvvakoum.greekname
peMA.Ode4C2H.melody = he.h.m8_gr_GR_cog.heHE.OProfitisAvvakoum.melody
peMA.Ode4C2H.incipit = he.h.m8_gr_GR_cog.heHE.OProfitisAvvakoum.incipit
peMA.Ode4C2H.text = he.h.m8_gr_GR_cog.heHE.OProfitisAvvakoum.text

peMA.Ode4C21.ode = miscellanea_gr_GR_cog.misc.Ode4
peMA.Ode4C21.mode = miscellanea_gr_GR_cog.misc.Mode8
peMA.Ode4C21.greekmelody = he.h.m8_gr_GR_cog.heHE.OProfitisAvvakoum.greekname
peMA.Ode4C21.melody = he.h.m8_gr_GR_cog.heHE.OProfitisAvvakoum.name
peMA.Ode4C21.incipit = "Εἰ Μεσσίαν δεῖ ἐλθεῖν…"
peMA.Ode4C21.text = "Εἰ Μεσσίαν δεῖ ἐλθεῖν, ὁ δὲ Μεσσίας Χριστός ἐστι, παράνομοι, τί ἀπιστεῖτε αὐτῷ; ἰδοὺ παραγέγονε, καὶ μαρτυρεῖ ἃ αὐτὸς ποιεῖ· τὸ ὕδωρ οἶνον ἐποίησε, Παράλυτον λόγῳ συνέσφιγξεν."

peMA.Ode4C22.ode = miscellanea_gr_GR_cog.misc.Ode4
peMA.Ode4C22.mode = miscellanea_gr_GR_cog.misc.Mode8
peMA.Ode4C22.melody = he.h.m8_gr_GR_cog.heHE.OProfitisAvvakoum.name
peMA.Ode4C22.incipit = "Μὴ συνιέντες τὰς Γραφάς…"
peMA.Ode4C22.text = "Μὴ συνιέντες τὰς Γραφάς, πλανᾶσθε πάντες ὑμεῖς, Ἑβραῖοι ἄνομοι· ὄντως γὰρ ἦλθε Χριστός, καὶ πάντας ἐφώτισε, καὶ ἐν ὑμῖν ἔδειξε πολλὰ σημεῖα καὶ τεράστια· καὶ μάτην ἀρνεῖσθε τὴν ὄντως ζωήν."

peMA.Ode4C23.ode = miscellanea_gr_GR_cog.misc.Ode4
peMA.Ode4C23.mode = miscellanea_gr_GR_cog.misc.Mode8
peMA.Ode4C23.melody = he.h.m8_gr_GR_cog.heHE.OProfitisAvvakoum.name
peMA.Ode4C23.incipit = "Ἓν ἔργον ἔδειξα ἡμῖν…"
peMA.Ode4C23.text = "Ἓν ἔργον ἔδειξα ἡμῖν, καὶ πάντες ἤδη θαυμάζετε, ἀνέκραζε τοῖς Ἰουδαίοις Χριστός· ὑμεῖς περιτέμνετε, καὶ ἐν Σαββάτῳ ἄνθρωπον, φησίν· ἐμοὶ δὲ τί ἐγκαλεῖτε λοιπόν, ἐγείραντι λόγῳ Παράλυτον;"

peMA.Ode4C24.ode = miscellanea_gr_GR_cog.misc.Ode4
peMA.Ode4C24.mode = miscellanea_gr_GR_cog.misc.Mode8
peMA.Ode4C24.melody = he.h.m8_gr_GR_cog.heHE.OProfitisAvvakoum.name
peMA.Ode4C24.incipit = "Ἔργα ἐποίησα πολλά…"
peMA.Ode4C24.text = "Ἔργα ἐποίησα πολλά, καὶ διὰ ποῖον ἔργον λιθάζετέ με; τοῖς Ἰουδαίοις Χριστός, ἐλέγχων ἀνέκραζεν· ὅτι ἄνθρωπον ὑγιῆ ὅλον λόγῳ ἐποίησα; μὴ κρίνετε κατʼ ὄψιν ἄνθρωποι."

peMA.Ode4C25.ode = miscellanea_gr_GR_cog.misc.Ode4
peMA.Ode4C25.mode = miscellanea_gr_GR_cog.misc.Mode8
peMA.Ode4C25.melody = he.h.m8_gr_GR_cog.heHE.OProfitisAvvakoum.name
peMA.Ode4C25.incipit = "Ἐν Ἀποστόλοις ἐνεργῶν…"
peMA.Ode4C25.text = "Ἐν Ἀποστόλοις ἐνεργῶν, καὶ ἐν Προφήταις αὐτὸς ἐπαναπαυόμενος, μετὰ τοῦ Πνεύματος, τὸ ἄχραντον γέννημα, τῆς πατρικῆς φύσεως Χριστέ, τὰ ἔθνη πρὸς σὴν ἐπίγνωσιν, ἀνῆγες τῶν σημείων σου."

peMA.Ode4C2Tr.ode = miscellanea_gr_GR_cog.misc.Ode4
peMA.Ode4C2Tr.mode = miscellanea_gr_GR_cog.misc.Mode8
peMA.Ode4C2Tr.melody = he.h.m8_gr_GR_cog.heHE.OProfitisAvvakoum.name
peMA.Ode4C2Tr.incipit = "Τριὰς ἀμέριστε μονάς…"
peMA.Ode4C2Tr.text = "Τριὰς ἀμέριστε μονάς, ἄναρχε Πάτερ, Υἱέ, καὶ Πνεῦμα ἅγιον, ἡ ἐν μονάδι Τριάς· ὁμότιμε, σύνθρονε, ζωοποιέ, ἄκτιστε Θεέ, σῷζε τοὺς ἀνυμνοῦντάς σε, καὶ ῥῦσαι κινδύνων καὶ θλίψεων."

peMA.Ode4C2T.ode = miscellanea_gr_GR_cog.misc.Ode4
peMA.Ode4C2T.mode = miscellanea_gr_GR_cog.misc.Mode8
peMA.Ode4C2T.melody = he.h.m8_gr_GR_cog.heHE.OProfitisAvvakoum.name
peMA.Ode4C2T.incipit = "Ἡ ἐν γαστρί σου τὸν Θεόν…"
peMA.Ode4C2T.text = "Ἡ ἐν γαστρί σου τὸν Θεόν, ἀπεριγράπτως χωρήσασα Θεόνυμφε, Παρθενομῆτορ ἁγνή, μὴ παύσῃ πρεσβεύουσα ὑπὲρ ἡμῶν, ὅπως διὰ σοῦ ῥυσθῶμεν τῶν περιστάσεων· πρὸς σὲ γὰρ ἀεὶ καταφεύγομεν."

peMA.Ode5C1H.ode = miscellanea_gr_GR_cog.misc.Ode5
peMA.Ode5C1H.mode = miscellanea_gr_GR_cog.misc.Mode4
peMA.Ode5C1H.greekmelody = he.h.m4_gr_GR_cog.heHE.SyKyrieMouFos.greekname
