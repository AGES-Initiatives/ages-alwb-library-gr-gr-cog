meVE.Stichera01.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.Stichera01.greekmelody = he.a.m4_gr_GR_cog.heAU.EdokasSimeiosin.greekname
meVE.Stichera01.melody = he.a.m4_gr_GR_cog.heAU.EdokasSimeiosin.name
meVE.Stichera01.incipit = "Νόμος ὁ ἐν γράμματι, παιδαγωγός…"
meVE.Stichera01.text = "Νόμος ὁ ἐν γράμματι, παιδαγωγός σοι γεγένηται, εἰς Χριστὸν Ἐπιφάνιε, δεικνὺς τὴν ἀστράψασαν, τοῦ Σωτῆρος χάριν, τῆς θεογνωσίας, προγραφομένην μυστικῶς, καὶ τυπουμένην διαφανέστατα· ἥν περ καὶ ἐφανέρωσας, ταῖς διδαχαῖς σου πανεύφημε, τηλαυγῶς ἐκτιθέμενος, τὰ θεόπνευστα δόγματα."

meVE.Stichera02.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.Stichera02.melody = he.a.m4_gr_GR_cog.heAU.EdokasSimeiosin.name
meVE.Stichera02.incipit = "Νόμου τοῦ τῆς χάριτος, καταγγελεὺς…"
meVE.Stichera02.text = "Νόμου τοῦ τῆς χάριτος, καταγγελεὺς ἐχρημάτισας, θεοφάντορ μακάριε, φωτίζων τὰ πέρατα, ταῖς τῶν σῶν δογμάτων, θείαις δᾳδουχίαις, καὶ τῶν αἱρέσεων θερμῶς, τὰς γλωσσαλγίας καταστρεψάμενος, πανσόφως ἐστηλίτευσας, διδασκαλίας πανόσιε, καὶ τὸ κάλλος τὸ ἄχραντον, τὸ τῆς πίστεως ἔδειξας."

meVE.Stichera03.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.Stichera03.melody = he.a.m4_gr_GR_cog.heAU.EdokasSimeiosin.name
meVE.Stichera03.incipit = "Ὁ βίος ὁ ἔνθεος…"
meVE.Stichera03.text = "Ὁ βίος ὁ ἔνθεος, τῷ ὀρθοδόξῳ φρονήματι, συνδραμὼν παρεσκεύασε, τελεῖν τὰ θαυμάσια, καὶ δαιμόνων θράσος, Πάτερ ἀπελαύνειν, καὶ νοσημάτων χαλεπῶν, τὰς ἀλγηδόνας παύειν ἐντεύξεσι· καὶ χάριτι κοσμούμενον, ἱεραρχίας σε ἔδειξεν, Ἰησοῦς ὁ φιλάνθρωπος, καὶ Σωτὴρ τῶν ψυχῶν ἡμῶν."

meVE.Stichera04.mode = miscellanea_gr_GR_cog.misc.Mode1
meVE.Stichera04.greekmelody = he.a.m1_gr_GR_cog.heAU.PaneffimoiMartyres.greekname
meVE.Stichera04.melody = he.a.m1_gr_GR_cog.heAU.PaneffimoiMartyres.name
meVE.Stichera04.incipit = "Λεόντιον φρόνημα στεῤῥῶς, Γερμανὲ κατέβαλες…"
meVE.Stichera04.text = "Λεόντιον φρόνημα στεῤῥῶς, Γερμανὲ κατέβαλες, τὸ δυσσεβείας ἀνάμεστον· τὴν γὰρ προσκύνησιν, τῶν σεπτῶν εἰκόνων, τοῦ Χριστοῦ ὁ δείλαιος, καὶ πάντων τῶν Ἁγίων ἠρνήσατο, ἀλλὰ τοῖς λόγοις σου, θεοφάντορ ἐξελήλεγκται, καὶ ὡς ἄνους, ἀνόητος ἔμεινεν."

meVE.Stichera05.mode = miscellanea_gr_GR_cog.misc.Mode1
meVE.Stichera05.melody = he.a.m1_gr_GR_cog.heAU.PaneffimoiMartyres.name
meVE.Stichera05.incipit = "Ὁ λέων ὁ ἄθεος, ὁ…"
meVE.Stichera05.text = "Ὁ λέων ὁ ἄθεος, ὁ θήρ, ὁ Θεῷ μισούμενος, τοῦ Ἀντιχρίστου ὁ πρόδρομος, ὁ τὴν προσκύνησιν, τῆς Χριστοῦ εἰκόνος, ἀθετήσας Ὅσιε, τοῦ κλήρου τῶν πιστῶν ἐξωστράκισται· διὸ αἰτοῦμέν σε· Ὡς ἐκεῖνον ταῖς πρεσβείαις σου, τὸν παρόντα, κατεύνασον τάραχον."

meVE.Stichera06.mode = miscellanea_gr_GR_cog.misc.Mode1
meVE.Stichera06.melody = he.a.m1_gr_GR_cog.heAU.PaneffimoiMartyres.name
meVE.Stichera06.incipit = "Ἐφέσεως ἔτυχες τῆς σοί, θεοφάντορ…"
meVE.Stichera06.text = "Ἐφέσεως ἔτυχες τῆς σοί, θεοφάντορ πόῤῥωθεν, πεποθημένης σαφέστατα· τῷ γὰρ Δεσπότῃ σου, μετὰ παῤῥησίας, Γερμανὲ μακάριε, νῦν ἱεροπρεπῶς προσεχώρησας, ᾧ παριστάμενος, ἀπολαύεις τῆς θεώσεως, τὴν εἰρήνην, τῷ κόσμῳ αἰτούμενος."

meVE.SticGlory.mode = miscellanea_gr_GR_cog.misc.Mode2
meVE.SticGlory.melody = ""
meVE.SticGlory.incipit = "Εἰ καὶ Μωσαϊκοῖς νόμοις ὑπέκυψας…"
meVE.SticGlory.text = "Εἰ καὶ Μωσαϊκοῖς νόμοις ὑπέκυψας, τῆς ἰουδαϊκῆς λατρείας ὑπάρχων βλάστημα, ἀλλʼ ἡ χάρις Πάτερ ἡ τοῦ Χριστοῦ ἐν σοὶ ἐπιλάμψασα, μαθητὴν τῆς εἰρήνης τοῦ Εὐαγγελίου σαφῶς ἀπειργάσατο· ἐντεῦθεν ὡς ἀγχίνους, τοῖς ἀμφοῖν διαθηκῶν, κατὰ νοῦν εἰληφὼς ἔκβασιν, ἀπὸ δουλείας, εἰς ἐλευθερίαν μεταβέβηκας. Διὸ ἱκέτευε ὑπὲρ ἡμῶν, λυτρωθῆναι τῆς τυραννίδος τοῦ ἐχθροῦ, καὶ σωθῆναι τὰς ψυχὰς ἡμῶν."

meVE.AposGlory.mode = miscellanea_gr_GR_cog.misc.Mode5
meVE.AposGlory.melody = ""
meVE.AposGlory.incipit = "Τῆς ὀρθοδόξου πίστεως, διὰ τὴν…"
meVE.AposGlory.text = "Τῆς ὀρθοδόξου πίστεως, διὰ τὴν πνευματικὴν σοφίαν, διδάσκαλος ὤφθης, Ἐπιφάνιε μακάριε· καὶ Χριστὸν εὑρὼν ποδηγέτην, τὰς βουλὰς τῶν κακοδόξων αἱρετικῶν διεσκέδασας. Διὸ αἰτοῦμέν σε Ἅγιε, πρεσβεύειν ἄχρι τέλους, τὴν ὑπὸ σοῦ βεβαιωθεῖσαν ὀρθὴν πίστιν, ἀσάλευτον φυλαχθῆναι ἡμῖν, καὶ τὸ μέγα ἔλεος παρʼ αὐτοῦ τοῦ Σωτῆρος τῶν ὅλων, δωρηθῆναι ταῖς ψυχαῖς ἡμῶν."

meVE.Apolytikion1.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.Apolytikion1.melody = ""
meVE.Apolytikion1.incipit = he.ga_gr_GR_cog.heGA.OTheosTonPateronImon.incipit
meVE.Apolytikion1.text = he.ga_gr_GR_cog.heGA.OTheosTonPateronImon.text

meMA.C1.acrostic.title = "Κανὼν τοῦ Ἁγίου Ἐπιφανίου,"
meMA.C1.acrostic.notice1 = "οὗ ἡ Ἀκροστιχίς (ἄνευ τῶν Θεοτοκίων)·"
meMA.C1.acrostic.text = "Ἐπιφανίου τὸ κῦδος ἔπλησε χθόνα."
meMA.C1.poet = poets_gr_GR_cog.po.IoannouTouDamaskinou
meMA.C1.title = "Κανὼν τοῦ Ἁγ. Ἐπιφανίου."
meMA.C1.verse = prayers_gr_GR_cog.pr.verse_SaintM.text

meMA.C2.acrostic.title = "Ἕτερος Κανὼν τοῦ Ἁγίου Γερμανοῦ,"
meMA.C2.acrostic.notice1 = "οὗ ἡ Ἀκροστιχίς·"
meMA.C2.acrostic.text = "Τοῦ ποιμενάρχου Γερμανοῦ μέλπω κλέος."
meMA.C2.poet = poets_gr_GR_cog.po.Theofanous
meMA.C2.title = "Κανὼν τοῦ Ἁγ. Γερμανοῦ."
meMA.C2.verse = prayers_gr_GR_cog.pr.verse_SaintM.text

meMA.Ode1C11.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C11.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode1C11.melody = he.h.m1_gr_GR_cog.heHE.SouITropaiouchos.name
meMA.Ode1C11.incipit = "Ἐν τῷ τοῦ θεράποντος πιστῶς…"
meMA.Ode1C11.text = "Ἐν τῷ τοῦ θεράποντος πιστῶς, πολιτευσάμενος νόμῳ τοῦ γράμματος, τῷ δεσποτικῷ ὑποκύψας ζυγῷ, διέπρεψας Πανόλβιε, καὶ φωστὴρ ἐδείχθης, τῆς γῆς φωτίζων τὰ πέρατα."

meMA.Ode1C12.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C12.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode1C12.melody = he.h.m1_gr_GR_cog.heHE.SouITropaiouchos.name
meMA.Ode1C12.incipit = "Πόθῳ θείου νόμου συσχεθείς, καὶ…"
meMA.Ode1C12.text = "Πόθῳ θείου νόμου συσχεθείς, καὶ τοῦ ἀδίκου προκρίνας τὸ δίκαιον, Πάτερ Ἐπιφάνιε, συμβολικῶς ἐδέξω τὴν σωτήριον, πίστιν τῆς Τριάδος, ἐκ θείας ὥσπερ προγνώσεως."

meMA.Ode1C13.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C13.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode1C13.melody = he.h.m1_gr_GR_cog.heHE.SouITropaiouchos.name
meMA.Ode1C13.incipit = "Ἰατὴρ σαρκός τε καὶ ψυχῆς…"
meMA.Ode1C13.text = "Ἰατὴρ σαρκός τε καὶ ψυχῆς, θεοειδὴς σοι ἐγένετο ἄνθρωπος, τῷ ὑποζυγίῳ σου, τὸ τῶν παθῶν συναποκτείνας ὅρμημα, καὶ συνθεραπεύσας, ψυχῆς τὸ ἄπιστον φρόνημα."

meMA.Ode1C1T.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C1T.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode1C1T.melody = he.h.m1_gr_GR_cog.heHE.SouITropaiouchos.name
meMA.Ode1C1T.incipit = "Ὅλην τὴν μορφήν μου προσλαβών…"
meMA.Ode1C1T.text = "Ὅλην τὴν μορφήν μου προσλαβών, ὁ ἐν μορφῇ Θεοῦ πρὶν καθορώμενος, ὅλην ἀνεκαίνισε, τῇ σῇ γαστρὶ θεοπρεπῶς σκηνώσας Ἁγνή· ὅθεν Θεοτόκον πιστοὶ σε πάντες δοξάζομεν."

meMA.Ode1C21.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C21.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode1C21.melody = he.h.m1_gr_GR_cog.heHE.SouITropaiouchos.name
meMA.Ode1C21.incipit = "Τῆς Ἱερωσύνης τῇ στολῇ, κατακοσμούμενος…"
meMA.Ode1C21.text = "Τῆς Ἱερωσύνης τῇ στολῇ, κατακοσμούμενος Μάκαρ διέπρεψας, λόγῳ δὲ τῆς χάριτος, τὰ τῶν πιστῶν πανευσεβῶς συστήματα, ὤφθης θεοφάντορ, καταφωτίζων τοῖς δόγμασιν."

meMA.Ode1C22.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C22.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode1C22.melody = he.h.m1_gr_GR_cog.heHE.SouITropaiouchos.name
meMA.Ode1C22.incipit = "Ὄχημα ὑπῆρξας ἀρετῶν, οὗ ἐπιβὰς…"
meMA.Ode1C22.text = "Ὄχημα ὑπῆρξας ἀρετῶν, οὗ ἐπιβὰς τῷ Θεῷ προσεχώρησας, κόσμου τὴν τερπνότητα, καταλιπών, καὶ τὸ σαθρὸν δοξάριον, καὶ οὐρανοφοίτης, τῇ σῇ σοφίᾳ γενόμενος."

meMA.Ode1C23.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C23.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode1C23.melody = he.h.m1_gr_GR_cog.heHE.SouITropaiouchos.name
meMA.Ode1C23.incipit = "Ὑπὸ τῆς προνοίας προγνωσθείς, ὡς…"
meMA.Ode1C23.text = "Ὑπὸ τῆς προνοίας προγνωσθείς, ὡς Ἱερεὺς θεηγόρος ἐσόμενος, Πάτερ ὑπεξέφυγας, τυραννικὴν μιαιφονίαν Ὅσιε, τοῦ Θεοῦ φρουροῦντος, σὲ δεξιᾷ παντοκράτορι."

meMA.Ode1C2T.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C2T.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode1C2T.melody = he.h.m1_gr_GR_cog.heHE.SouITropaiouchos.name
meMA.Ode1C2T.incipit = "Ποίαν σοι ἐπάξιον ᾠδήν, ἡ…"
meMA.Ode1C2T.text = "Ποίαν σοι ἐπάξιον ᾠδήν, ἡ ἡμετέρα προσοίσει ἀσθένεια, εἰμὴ τὴν χαρμόσυνον, ἣν Γαβριὴλ ἡμᾶς ἐμυσταγώγησε; Χαῖρε Θεοτόκε, Παρθένε Μῆτερ ἀνύμφευτε."

meMA.Ode3C11.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C11.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode3C11.melody = he.h.m1_gr_GR_cog.heHE.OMonosEidos.name
meMA.Ode3C11.incipit = "Φαιὸν περιβόλαιον ἰδών, αἰτήσαντι τὸν…"
meMA.Ode3C11.text = "Φαιὸν περιβόλαιον ἰδών, αἰτήσαντι τὸν ἔλεον, μεταδοθὲν τοῖς σώματος ὄμμασι, λαμπρὰν ἐσθῆτα περιβαλλόμενον, νοητῶς τεθέασαι, καὶ τῇ θείᾳ χάριτι, ἐφωτίσθης πρὸς πίστιν ἀμώμητον."

meMA.Ode3C12.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C12.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode3C12.melody = he.h.m1_gr_GR_cog.heHE.OMonosEidos.name
meMA.Ode3C12.incipit = "Ἀκούσας ῥημάτων εὐσεβῶν, τὴν ὕλην…"
meMA.Ode3C12.text = "Ἀκούσας ῥημάτων εὐσεβῶν, τὴν ὕλην ἑβδελύξω μέν, καὶ βίον ἐπόθησας ἄϋλον, ὡς αἵματος δὲ συμμετασχοῦσάν σοι, καὶ τῆς γνώμης μέτοχον, προσλαβὼν τὴν ὅμαιμον, ἀνεδείχθητε ζεῦγος μακάριον."

meMA.Ode3C13.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C13.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode3C13.melody = he.h.m1_gr_GR_cog.heHE.OMonosEidos.name
meMA.Ode3C13.incipit = "Νεκροὺς λογισμοὺς ἀποστραφείς, Παμμάκαρ Ἐπιφάνιε…"
meMA.Ode3C13.text = "Νεκροὺς λογισμοὺς ἀποστραφείς, Παμμάκαρ Ἐπιφάνιε, ἐπιποθήσας ζῶντι λατρεύειν Θεῷ, τῷ θείῳ δρόμῳ πιστῶς προσέδραμες, καὶ ἐδέξω σύμβολον, τηλαυγὲς πανόλβιε, τῶν νεκρῶν σου πεδίλων τὴν ἔκπτωσιν."

meMA.Ode3C1T.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C1T.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode3C1T.melody = he.h.m1_gr_GR_cog.heHE.OMonosEidos.name
meMA.Ode3C1T.incipit = "Ἁγίων τὸν Ἅγιον Χριστόν, ἁγίως…"
meMA.Ode3C1T.text = "Ἁγίων τὸν Ἅγιον Χριστόν, ἁγίως ἀπεκύησας, ἁγιωσύνης ἅγιον σκήνωμα, τὸν ἐν Ἁγίοις ἀναπαυόμενον· ᾧ βοῶμεν, Ἅγιος, ὁ ναὸς ὁ ἔμψυχος, τῆς ἀφράστου σου δόξης φιλάνθρωπε."

meMA.Ode3C21.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C21.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode3C21.melody = he.h.m1_gr_GR_cog.heHE.OMonosEidos.name
meMA.Ode3C21.incipit = "Ὁ θείᾳ προγνώσει προειδώς, σῆς…"
meMA.Ode3C21.text = "Ὁ θείᾳ προγνώσει προειδώς, σῆς γνώμης τὸ ἐλεύθερον, καὶ τῆς ψυχῆς Σοφὲ τὴν εὐγένειαν, χερσὶν ἀνόμοις ἀναιρεθῆναί σε, ὡς Θεὸς ἑκώλυσε, καὶ παρθένων τάγματι, εὐνουχίᾳ πανσόφως κατέλεξεν."

meMA.Ode3C22.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C22.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode3C22.melody = he.h.m1_gr_GR_cog.heHE.OMonosEidos.name
meMA.Ode3C22.incipit = "Ἰδεῖν ἐφιέμενος σαφῶς, Παμμάκαρ τὴν…"
meMA.Ode3C22.text = "Ἰδεῖν ἐφιέμενος σαφῶς, Παμμάκαρ τὴν λαμπρότητα, τὴν νοητὴν τῆς θείας θεώσεως, ἱεραρχίαν τὴν σὴν ἐκόσμησας, πολιτείᾳ σώφρονι, καὶ σοφοῖς διδάγμασιν, ὀρθοδόξως ἰθύνας τὸ ποίμνιον."

meMA.Ode3C23.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C23.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode3C23.melody = he.h.m1_gr_GR_cog.heHE.OMonosEidos.name
meMA.Ode3C23.incipit = "Μεγάλης καθέδρας ἐπιβάς, μεγάλως κατελάμπρυνας…"
meMA.Ode3C23.text = "Μεγάλης καθέδρας ἐπιβάς, μεγάλως κατελάμπρυνας, τῆς Ἐκκλησίας Πάτερ τὸ πλήρωμα, διδασκαλίαις καὶ μελῳδήμασι, Γερμανὲ πανεύφημε, Ἱεράρχα Ὅσιε, καὶ φωστήρ οἰκουμένης ὑπέρτιμε."

meMA.Ode3C2T.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C2T.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode3C2T.melody = he.h.m1_gr_GR_cog.heHE.OMonosEidos.name
meMA.Ode3C2T.incipit = "Ἐκ σοῦ ἀνεβλάστησεν Ἁγνή, τὸ…"
meMA.Ode3C2T.text = "Ἐκ σοῦ ἀνεβλάστησεν Ἁγνή, τὸ ἄνθος τὸ ἀμάραντον, εὐωδιάζον τὴν ἀνθρωπότητα, τῷ θείῳ μύρῳ τῆς αὐτοῦ φύσεως, ὁ Πατρὶ συνάναρχος, καὶ ἐκ σοῦ γενόμενος, ὑπὸ χρόνον, Παρθένε πανάμωμε."

meMA.Ode3H.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3H.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode3H.melody = he.h.m1_gr_GR_cog.heHE.OMonosEidos.melody
meMA.Ode3H.incipit = he.h.m1_gr_GR_cog.heHE.OMonosEidos.incipit
meMA.Ode3H.text = he.h.m1_gr_GR_cog.heHE.OMonosEidos.text

meMA.Ode3Kathisma1.mode = miscellanea_gr_GR_cog.misc.Mode3
meMA.Ode3Kathisma1.melody = he.a.m3_gr_GR_cog.heAU.TheiasPisteos.name
meMA.Ode3Kathisma1.incipit = "Θείοις δόγμασι, τὴν Ἐκκλησίαν, Πάτερ…"
meMA.Ode3Kathisma1.text = "Θείοις δόγμασι, τὴν Ἐκκλησίαν, Πάτερ ἔπλησας, ὀρθοδοξίας, ταῖς διδαχαῖς σου αἱρέσεις τρεψάμενος, τῆς εὐσεβείας τὸν δρόμον ἐπλήρωσας, καὶ σὺν τῷ Παύλῳ τὴν πίστιν ἐτήρησας, Ἐπιφάνιε, Χριστὸν τὸν Θεὸν ἱκέτευε, δωρήσασθαι ἡμῖν τὸ μέγα ἔλεος."

meMA.Ode3Kathisma2.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3Kathisma2.melody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.name
meMA.Ode3Kathisma2.incipit = "Ἱερὰν ἀμπεχόμενος τὴν στολήν, ἀνεδείχθης…"
meMA.Ode3Kathisma2.text = "Ἱερὰν ἀμπεχόμενος τὴν στολήν, ἀνεδείχθης ὑπέρμαχος τοῦ Χριστοῦ, μὴ πτήξας παράνομον, Βασιλέα, Πατὴρ ἡμῶν, τὸν αὐχένα μὴ κλίνας, ἀθέῳ προστάγματι, ῥαπισμούς ὑποφέρων, ὀνείδη καὶ σκώμματα· ὅθεν ὁ ἀλάστωρ, τῷ θυμῷ ὑπερζέσας, φρουρᾷ σε κατέκλεισε, μέχρι τέλους μακάριε· διὰ τοῦτο βοῶμέν σοι· Πρέσβευε Χριστῷ τῷ Θεῷ, τῶν πταισμάτων ἄφεσιν δωρήσασθαι, τοῖς ἑορτάζουσι πόθῳ, τὴν ἁγίαν μνήμην σου."

meMA.Ode4C11.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C11.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode4C11.melody = he.h.m1_gr_GR_cog.heHE.OrosSeTiChariti.name
meMA.Ode4C11.incipit = "Ἱκέτης ὡς δοῦλος προσελθὼν τῷ…"
meMA.Ode4C11.text = "Ἱκέτης ὡς δοῦλος προσελθὼν τῷ Βαπτίσματι, υἱοθετήθης διʼ αὐτοῦ, καὶ κληρονομος ἀληθῶς, Θεοῦ Ἐπιφάνιε, καὶ τοῦ Χριστοῦ συγκληρονόμος γεγένησαι, θεοπρεπῶς αὐτῷ πολιτευσάμενος."

meMA.Ode4C12.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C12.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode4C12.melody = he.h.m1_gr_GR_cog.heHE.OrosSeTiChariti.name
meMA.Ode4C12.incipit = "Ὁρᾷ σου πολλῇ ἠγλαϊσμένον τὸ…"
meMA.Ode4C12.text = "Ὁρᾷ σου πολλῇ ἠγλαϊσμένον τὸ πρόσωπον, καὶ ἀποῤῥήτῳ ὁ ποιμήν, δόξῃ καὶ στέφει εὐπρεπεῖ, τὴν ἔνδοξον κάραν σου ὑπερφυῶς, κατεστεμμένην ἀοίδιμε, τῆς καθαρᾶς σου καρδίας τὰ σύμβολα."

meMA.Ode4C13.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C13.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode4C13.melody = he.h.m1_gr_GR_cog.heHE.OrosSeTiChariti.name
meMA.Ode4C13.incipit = "Ὑπήρξας δοχεῖον θείας χάριτος Ὅσιε…"
meMA.Ode4C13.text = "Ὑπήρξας δοχεῖον θείας χάριτος Ὅσιε, καὶ διεσκόρπισας σαφῶς πλοῦτον τὸν πρόσκαιρον Χριστῷ καὶ ὄλβον αἰώνιον, ἐν οὐρανοῖς ἀσύλητον ἐθησαύρισας, ὥσπερ ἀγχίνους κριτὴς Ἐπιφάνιε."

meMA.Ode4C14.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C14.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode4C14.melody = he.h.m1_gr_GR_cog.heHE.OrosSeTiChariti.name
meMA.Ode4C14.incipit = "Τῶν νέων ἀμείψας ἐν νεότητι…"
meMA.Ode4C14.text = "Τῶν νέων ἀμείψας ἐν νεότητι φρόνημα, στοιχειωθεὶς δὲ πρεσβυτῶν, παρὰ σοφῶν καὶ συνετῶν, τῷ φόβῳ Μακάριε, τῷ θεϊκῷ, μελέτῃ λόγων τοῦ Πνεύματος, σὺ ἐραστής τῆς σοφίας γεγένησαι."

meMA.Ode4C1T.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C1T.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode4C1T.melody = he.h.m1_gr_GR_cog.heHE.OrosSeTiChariti.name
meMA.Ode4C1T.incipit = "Ἁγία Ἁγίων Θεοτοκε πανύμνητε, ἡ…"
meMA.Ode4C1T.text = "Ἁγία Ἁγίων Θεοτοκε πανύμνητε, ἡ προσδοκία τῶν ἐθνῶν, καὶ σωτηρία τῶν πιστῶν, ἐκ σοῦ ἀνεβλάστησεν, ὁ λυτρωτὴς καὶ ζωοδότης καὶ Κύριος, εἰς σωτηρίαν ἡμῶν τῶν ὑμνούντων σε."

meMA.Ode4C21.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C21.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode4C21.melody = he.h.m1_gr_GR_cog.heHE.OrosSeTiChariti.name
meMA.Ode4C21.incipit = "Νεκρώσας πανσόφως τῆς σαρκὸς τὰ…"
meMA.Ode4C21.text = "Νεκρώσας πανσόφως τῆς σαρκὸς τὰ σκιρτήματα, Ἱερομύστα Γερμανέ, φιλοσοφίας φωτισμῷ, τὸν νοῦν κατελάμπρυνας, πνευματικῇ μυσταγωγίᾳ κοσμούμενος, Χριστῷ κραυγάζων· Δόξα τῇ δυνάμει σου."

meMA.Ode4C22.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C22.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode4C22.melody = he.h.m1_gr_GR_cog.heHE.OrosSeTiChariti.name
meMA.Ode4C22.incipit = "Ἀρνεῖσθαι τὸ πάνσεπον Χριστοῦ ἐξεικόνισμα…"
meMA.Ode4C22.text = "Ἀρνεῖσθαι τὸ πάνσεπον Χριστοῦ ἐξεικόνισμα, ὁ δυσμενέστατος ἐχθρός, παραφροσύνῃ προκληθείς, ταῖς σαῖς ἐξελήλεγκται, περιφανῶς διδασκαλίαις ἀοίδιμε, ἀλλʼ ἀνιάτως νοσῶν οὐκ ᾐσθάνετο."

meMA.Ode4C23.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C23.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode4C23.melody = he.h.m1_gr_GR_cog.heHE.OrosSeTiChariti.name
meMA.Ode4C23.incipit = "Ῥημάτων τὸν ἔλεγχον τῶν σῶν…"
meMA.Ode4C23.text = "Ῥημάτων τὸν ἔλεγχον τῶν σῶν ὑφορώμενος, ὁ θηριώνυμος, Σοφέ, καὶ θηριότροπος τὸν νοῦν, τοῦ θρόνου κατάγει σε, τοῦ ἱεροῦ, ἀλλʼ ἀντὶ τούτου ἐπλούτησας, Ἱερουργίαν Παμμάκαρ οὐράνιον."

meMA.Ode4C2T.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C2T.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode4C2T.melody = he.h.m1_gr_GR_cog.heHE.OrosSeTiChariti.name
meMA.Ode4C2T.incipit = "Χαράν σοι σαφῶς ἀπʼ οὐρανοῦ…"
meMA.Ode4C2T.text = "Χαράν σοι σαφῶς ἀπʼ οὐρανοῦ ἀφικόμενος, ὁ Ἀρχιστράτηγος Ἁγνή, εὐηγγελίσατο, εἰπών· Θεὸς ἐξελεύσεται, μετὰ σαρκός, ἐκ σοῦ Παρθένε πανάχραντε, εἰς σωτηρίαν τῶν πόθῳ ὑμνούντων σε."

meMA.Ode5C11.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C11.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode5C11.melody = he.h.m1_gr_GR_cog.heHE.OFotisasTiEllampsei.name
meMA.Ode5C11.incipit = "Ὁσίως σου τὴν γαστέρα, ἐν…"
meMA.Ode5C11.text = "Ὁσίως σου τὴν γαστέρα, ἐν ἄρτῳ καὶ ὕδατι, ἐστένωσας, κεχρημένος ὡς ὄψῳ τῷ ἅλατι, καὶ παθῶν ἐκράτησας σαρκός, ἐν σώματι τὸν βίον, τῶν Ἀσωμάτων μιμούμενος."

meMA.Ode5C12.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C12.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode5C12.melody = he.h.m1_gr_GR_cog.heHE.OFotisasTiEllampsei.name
meMA.Ode5C12.incipit = "Κατῴκτιρας τρυχομένους, τῷ δίψει ἐν…"
meMA.Ode5C12.text = "Κατῴκτιρας τρυχομένους, τῷ δίψει ἐν καύσωνι, καὶ ἤμειψας εὐπρεπῶς τῶν σημείων ἀρχόμενος ὡς θεράπων ἄριστος, τὸν σὸν μιμούμενος Δεσπότην οἶνον εἰς ὕδωρ Μακάριε."

meMA.Ode5C13.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C13.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode5C13.melody = he.h.m1_gr_GR_cog.heHE.OFotisasTiEllampsei.name
meMA.Ode5C13.incipit = "Ὑπέδρασας τὸν κενόν, τῶν ἀνθρώπων…"
meMA.Ode5C13.text = "Ὑπέδρασας τὸν κενόν, τῶν ἀνθρώπων δοξάριον, καὶ ἔρημον, ὥσπερ ὁ σὸς Δεσπότης κατῴκησας, πειρασμοῖς δὲ ἐχώρησας δαιμονικήν, πρὸς πάλην ἀοράτως, τὸ ἀσθενές σου νευρούμενος."

meMA.Ode5C1T.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C1T.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode5C1T.melody = he.h.m1_gr_GR_cog.heHE.OFotisasTiEllampsei.name
meMA.Ode5C1T.incipit = "Λόγον θεῖον, διὰ λόγου ἐν…"
meMA.Ode5C1T.text = "Λόγον θεῖον, διὰ λόγου ἐν μήτρᾳ ἐχώρησας, ὑπὲρ λόγον Θεοτόκε, αὐτὸν οὖν δυσώπησον, ἐξ ἀλόγων πράξεων, καὶ ἡδονῶν θανατηφόρων, ἐλευθερῶσαι τοὺς δούλους σου."

meMA.Ode5C21.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C21.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode5C21.melody = he.h.m1_gr_GR_cog.heHE.OFotisasTiEllampsei.name
meMA.Ode5C21.incipit = "Ὁλοκλήρως τὸν Χριστόν, ἀγαπήσας θεόληπτε…"
meMA.Ode5C21.text = "Ὁλοκλήρως τὸν Χριστόν, ἀγαπήσας θεόληπτε, ἠγαπήθης παρʼ αὐτοῦ, ἐπαξίως καὶ εἴληφας, χαρισμάτων πέλαγος· καὶ γὰρ σοφῶς ταῖς διδαχαῖς, σου, τὴν Ἐκκλησίαν ἐφώτισας."

meMA.Ode5C22.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C22.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode5C22.melody = he.h.m1_gr_GR_cog.heHE.OFotisasTiEllampsei.name
meMA.Ode5C22.incipit = "Ὑμνῳδίαις Γερμανέ, θεοπνεύστοις ἐφαίδρυνας, τὰς…"
meMA.Ode5C22.text = "Ὑμνῳδίαις Γερμανέ, θεοπνεύστοις ἐφαίδρυνας, τὰς χορείας τῶν πιστῶν, καὶ τὰ θεῖα πληρώματα· ἡ γὰρ παναρμόνιος νευρά, Πάτερ τῆς σῆς καρδίας, τους Ὀρθοδόξους ἐλάμπρυνε."

meMA.Ode5C23.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C23.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode5C23.melody = he.h.m1_gr_GR_cog.heHE.OFotisasTiEllampsei.name
meMA.Ode5C23.incipit = "Γηθόμενος, τῶν Μαρτύρων τοὺς ἄθλους…"
meMA.Ode5C23.text = "Γηθόμενος, τῶν Μαρτύρων τοὺς ἄθλους καὶ τρόπαια, στηλιτεύσας ἐγκωμίων καὶ λόγων συνθέσεσι, καὶ αὖθις Ἀοίδιμε, εἰκονικαῖς χρωματουργίαις, εἰς ἓν συνάπτων ἀμφότερα."

meMA.Ode5C2T.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C2T.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode5C2T.melody = he.h.m1_gr_GR_cog.heHE.OFotisasTiEllampsei.name
meMA.Ode5C2T.incipit = "Εὐφραίνονται, οὐρανῶν αἱ δυνάμεις ὁρῶσαί…"
meMA.Ode5C2T.text = "Εὐφραίνονται, οὐρανῶν αἱ δυνάμεις ὁρῶσαί σε ἀγάλλονται, σὺν αὐτοῖς τῶν βροτῶν τὰ συστήματα· τῷ γὰρ τόκῳ ἥνωνται τῷ σῷ, Παρθένε Θεοτόκε, ὃν ἐπαξίως δοξάζομεν."

meMA.Ode6C11.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C11.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode6C11.melody = he.h.m1_gr_GR_cog.heHE.EkyklosenImas.name
meMA.Ode6C11.incipit = "Δαιμόνων σε οἱ μίμηται ἐκύκλωσαν…"
meMA.Ode6C11.text = "Δαιμόνων σε οἱ μίμηται ἐκύκλωσαν, τῆς Ἄγαρ οἱ ἀπόγονοι, τὸν Δεσπότην δʼ ἐκμιμούμενος τὸν σόν, τὸν ἀντιδικοῦντα εὐηργέτησας, καὶ διφαῆ, ἔδειξας Πάτερ τὸν ἑτερόφθαλμον."

meMA.Ode6C12.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C12.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode6C12.melody = he.h.m1_gr_GR_cog.heHE.EkyklosenImas.name
meMA.Ode6C12.incipit = "Ὁ πλάστης σου ἐν σοὶ…"
meMA.Ode6C12.text = "Ὁ πλάστης σου ἐν σοὶ ἀναπαυσάμενος, τοῦ Πνεύματός σε ὄργανον, καὶ Θεὸν ὡς Μωϋσῆν τῷ Φαραώ, ἔδειξεν Ἀράβων τοῖς σκηνώμασι, παναληθῶς, ὤφθης τὸν τρόπον τούτου μιμούμενος."

meMA.Ode6C13.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C13.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode6C13.melody = he.h.m1_gr_GR_cog.heHE.EkyklosenImas.name
meMA.Ode6C13.incipit = "Συνέσεισε τῶν Ἀσσυρίων ἄνακτα, τὸ…"
meMA.Ode6C13.text = "Συνέσεισε τῶν Ἀσσυρίων ἄνακτα, τὸ πνεῦμα διωκόμενον, ἐχρημάτισε δὲ ἄγγελος τῆς σῆς, Πάτερ ἀρετῆς καὶ θείας χάριτος· τοὺς γὰρ αὐτὸν οἶδε δοξάζειν Θεὸς δοξάζοντας."

meMA.Ode6C1T.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C1T.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode6C1T.melody = he.h.m1_gr_GR_cog.heHE.EkyklosenImas.name
meMA.Ode6C1T.incipit = "Εὐφραίνονται ἐν σοὶ Παρθένε ἄχραντε…"
meMA.Ode6C1T.text = "Εὐφραίνονται ἐν σοὶ Παρθένε ἄχραντε, τοῦ γένους οἱ προπάτορες, τὴν Ἐδὲμ ἀπολαβόντες διὰ σοῦ, ἣν ἐκ παραβάσεως ἀπώλεσαν· σὺ γὰρ ἁγνή, καὶ πρὸ τόκου καὶ μετὰ γέννησιν."

meMA.Ode6C21.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C21.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode6C21.melody = he.h.m1_gr_GR_cog.heHE.EkyklosenImas.name
meMA.Ode6C21.incipit = "Ῥυθμίζεται ταῖς διδαχαῖς σου Πάνσοφε…"
meMA.Ode6C21.text = "Ῥυθμίζεται ταῖς διδαχαῖς σου Πάνσοφε, καὶ λόγοις τὸ βασίλειον ἱεράτευμα, λαός τε τοῦ Χριστοῦ, ὁ ἡγιασμένος καὶ θεόκλητος, καὶ πρὸς Θεόν, καθοδηγεῖται ταῖς ὑποθήκαις σου."

meMA.Ode6C22.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C22.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode6C22.melody = he.h.m1_gr_GR_cog.heHE.EkyklosenImas.name
meMA.Ode6C22.incipit = "Μνημόνευε τῶν σὲ ὑμνούντων Ὅσιε…"
meMA.Ode6C22.text = "Μνημόνευε τῶν σὲ ὑμνούντων Ὅσιε, Θεῷ νῦν παριστάμενος, καὶ πληρούμενος τῆς θείας καλλονῆς, καὶ τῆς ἀκηράτου ὡραιότητος, πρὸς τὴν ζωήν, καθοδηγῆσαι ταῖς ἱκεσίαις σου."

meMA.Ode6C23.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C23.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode6C23.melody = he.h.m1_gr_GR_cog.heHE.EkyklosenImas.name
meMA.Ode6C23.incipit = "Ἀνάβασιν σοφῶς τοῦ βίου τέθεικας…"
meMA.Ode6C23.text = "Ἀνάβασιν σοφῶς τοῦ βίου τέθεικας, ἐκ δόξης καὶ δυνάμεως, πρὸς οὐράνιον σὺ δόξαν καὶ ἰσχύν, Ἔνδοξε προβαίνων ταῖς ἀσκήσεσιν, ἕως Χριστῷ, τῷ ποθουμένῳ χαίρων ὡμίλησας."

meMA.Ode6C2T.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C2T.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode6C2T.melody = he.h.m1_gr_GR_cog.heHE.EkyklosenImas.name
meMA.Ode6C2T.incipit = "Νῦν Πάναγνε τὸν σὸν οἰκέτην…"
meMA.Ode6C2T.text = "Νῦν Πάναγνε τὸν σὸν οἰκέτην λύτρωσαι, ἐκ πάσης περιστάσεως, τρικυμίας τε καὶ ζάλης τῶν παθῶν, καὶ πρὸς σὸν λιμένα καθοδήγησον· σὺ γὰρ ἐμοῦ, καὶ προστασία καὶ ἐπανόρθωσις."

meMA.Ode6H.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6H.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode6H.melody = he.h.m1_gr_GR_cog.heHE.EkyklosenImas.melody
meMA.Ode6H.incipit = he.h.m1_gr_GR_cog.heHE.EkyklosenImas.incipit
meMA.Ode6H.text = he.h.m1_gr_GR_cog.heHE.EkyklosenImas.text

meMA.Kontakion1.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Kontakion1.melody = he.a.m4_gr_GR_cog.heAU.OYpsotheisEnToStavro.name
meMA.Kontakion1.incipit = "Ἱεραρχῶν τὴν θαυμαστὴν ξυνωρίδα, ἀνευφημήσωμεν…"
meMA.Kontakion1.text = "Ἱεραρχῶν τὴν θαυμαστὴν ξυνωρίδα, ἀνευφημήσωμεν πιστοὶ κατὰ χρέος, σὺν Γερμανῷ τὸν θεῖον Ἐπιφάνιον· οὗτοι γὰρ κατέφλεξαν, τῶν ἀθέων τὰς γλώσσας, δόγματα σοφώτατα, διαθέμενοι πᾶσι, τοῖς ὀρθοδόξως μέλπουσιν ἀεί, τῆς εὐσεβείας τὸ μέγα μυστήριον."

meMA.Oikos1.text = "Ὡς ἱερέων καλλονήν, καὶ δόξαν ὀρθοδόξων, δυάδα τὴν ἁγίαν τιμήσωμεν ἀξίως· οὗτοι γὰρ ὤφθησαν ἡμῖν πύργοι ἀσφαλείας, κατʼ ἐχθρῶν τῶν ὁρατῶν καὶ ἀοράτων πάντοτε, τῆς πίστεως στῦλοι ἀκλόνητοι, καὶ τῆς Ἐκκλησίας θεῖος κόσμος, ὃν φοροῦσα ὀρθοτομεῖ, εὐθύμως βοῶσα· Εὐφράνθη ἐν ἐμοὶ τὸ πνεῦμά μου ὄντως, τὴν στολὴν μου ὥσπερ πρὶν ἀπολαβούσῃ, ἣν αἱρετικοὶ διέῤῥηξαν, μὴ ὑμνοῦντες τῆς εὐσεβείας τὸ μέγα μυστήριον."

meMA.Ode7C11.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C11.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode7C11.melody = he.h.m1_gr_GR_cog.heHE.SeNoitin.name
meMA.Ode7C11.incipit = "Ἐκ θεϊκῆς, δεδεγμένος χάριτος, τὴν…"
meMA.Ode7C11.text = "Ἐκ θεϊκῆς, δεδεγμένος χάριτος, τὴν τῶν θαυμάτων δωρεάν, τοῖς αἰτοῦσί σε δωρεάν, Πάτερ τὰ ἰάματα, ἅπασι διένειμας, ὡς ὁ Δεσπότης προσέταξεν, ὁ αἰνετὸς τῶν Πατέρων, Θεὸς καὶ ὑπερένδοξος."

meMA.Ode7C12.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C12.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode7C12.melody = he.h.m1_gr_GR_cog.heHE.SeNoitin.name
meMA.Ode7C12.incipit = "Πίπτει ποδῶν, σοῦ προκυλινδούμενος, ὁ…"
meMA.Ode7C12.text = "Πίπτει ποδῶν, σοῦ προκυλινδούμενος, ὁ Ἀσσυρίων βασιλεύς· εὐλαβεῖσθε γὰρ ἀρετήν, οἶδε καὶ πολέμιος· ἅπας ὑποπίπτει δέ, οἷς ἐνοικῶν Χριστὸς ἀναπαύεται, ὁ αἰνετὸς τῶν Πατέρων, Θεὸς καὶ ὑπερένδοξος."

meMA.Ode7C13.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C13.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode7C13.melody = he.h.m1_gr_GR_cog.heHE.SeNoitin.name
meMA.Ode7C13.incipit = "Λόγος σιγᾷ, τῶν κατορθωμάτων σου…"
meMA.Ode7C13.text = "Λόγος σιγᾷ, τῶν κατορθωμάτων σου, καὶ τῶν θαυμάτων τὴν πληθύν, τὸ κατʼ ἔπος οὐ δυνηθείς, φράσαι Παμμακάριστε· ψάμμον ὑπερέβη γάρ, ὧν ὁ Χριστὸς σοι δεδώρηται, ὁ αἰνετὸς τῶν Πατέρων, Θεὸς καὶ ὑπερένδοξος."

meMA.Ode7C1Tr.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C1Tr.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode7C1Tr.melody = he.h.m1_gr_GR_cog.heHE.SeNoitin.name
meMA.Ode7C1Tr.incipit = "Ἡ τριφεγγής, ἑνιαία ἄληπτος, ἀπείρου…"
meMA.Ode7C1Tr.text = "Ἡ τριφεγγής, ἑνιαία ἄληπτος, ἀπείρου ἄβυσσος φωτός, ἐποπτεύοις σοὺς ὑμνητάς, ὁ ὑπερυψούμενος, ἄναρχος ἀρχή, Πατήρ, Υἱὸς καὶ Πνεῦμα συναΐδιον, ὁ αἰνετὸς τῶν Πατέρων, Θεὸς καὶ ὑπερένδοξος."

meMA.Ode7C1T.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C1T.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode7C1T.melody = he.h.m1_gr_GR_cog.heHE.SeNoitin.name
meMA.Ode7C1T.incipit = "Σὲ Ἰακώβ, Θεοτόκε κλίμακα, προφητικῶς…"
meMA.Ode7C1T.text = "Σὲ Ἰακώβ, Θεοτόκε κλίμακα, προφητικῶς κατανοεῖ· διὰ σοῦ γὰρ ἐπὶ τῆς γῆς, ὁ ὑπερυψούμενος, ὤφθη τοῖς ἀνθρώποις τε, συνανεστράφη ὡς ηὐδόκησεν, ὁ αἰνετὸς τῶν Πατέρων, Θεὸς καὶ ὑπερένδοξος."

meMA.Ode7C21.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C21.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode7C21.melody = he.h.m1_gr_GR_cog.heHE.SeNoitin.name
meMA.Ode7C21.incipit = "Ὅλην ἐν σοί, τὴν αὐγὴν…"
meMA.Ode7C21.text = "Ὅλην ἐν σοί, τὴν αὐγὴν τοῦ Πνεύματος, εἰσδεδεγμένος ἀληθῶς, ἀναβρύεις ὡς ποταμός, Πάτερ ἀξιάγαστε, δόγματα σωτήρια, θεοτερπῆ τε διδάγματα, τὸν αἰνετόν μεγαλύνων, Θεὸν καὶ ὑπερένδοξον."

meMA.Ode7C22.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C22.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode7C22.melody = he.h.m1_gr_GR_cog.heHE.SeNoitin.name
meMA.Ode7C22.incipit = "Ὑμνολογῶν, εὐσεβῶς ἐφαίδρυνας, τὰς τοῦ…"
meMA.Ode7C22.text = "Ὑμνολογῶν, εὐσεβῶς ἐφαίδρυνας, τὰς τοῦ Δεσπότου ἑορτάς, ἀναμέλπων μελῳδικῶς, ἔνθεος, γενόμενος, πάντας τοὺς Ἁγίους τε, σαῖς μουσικαῖς ἐμεγάλυνας, πρὸς ἀρετήν ἐπαλείφων, σοφῶς τοὺς πίστει ψάλλοντας."

meMA.Ode7C2Tr.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C2Tr.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode7C2Tr.melody = he.h.m1_gr_GR_cog.heHE.SeNoitin.name
meMA.Ode7C2Tr.incipit = "Μίαν ἀρχήν, ἑνιαίαν ἄτμητον, ἐν…"
meMA.Ode7C2Tr.text = "Μίαν ἀρχήν, ἑνιαίαν ἄτμητον, ἐν ὑποστάσεσι τρισί, θεολόγῳ γλώσσῃ βοῶν, πᾶσιν ἀνεκήρυξας, ἄναρχον Γεννήτορα, Μονογενῆ τε συνάναρχον, καὶ συμφυὲς θεῖον Πνεῦμα, τρανῶς καὶ ὁμοούσιον."

meMA.Ode7C2T.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C2T.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode7C2T.melody = he.h.m1_gr_GR_cog.heHE.SeNoitin.name
meMA.Ode7C2T.incipit = "Ἐν σοὶ ἁγνή, Θεοτόκε τίθημι…"
meMA.Ode7C2T.text = "Ἐν σοὶ ἁγνή, Θεοτόκε τίθημι, πᾶσαν ἐλπίδα, καὶ πρὸς σέ, καταφεύγω διὰ παντός. Σῶσόν με πανάμωμε, ῥῦσαι ταῖς πρεσβείαις σου, τῆς κατεχούσης συγχύσεως, καὶ ταραχῆς, τῶν δεινῶν μου παθῶν καὶ παραπτώσεων."

meMA.Ode8C11.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C11.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode8C11.melody = he.h.m1_gr_GR_cog.heHE.EnKaminoPaides.name
meMA.Ode8C11.incipit = "Σὺ τῷ λόγῳ ἤλεγξας σαφῶς…"
meMA.Ode8C11.text = "Σὺ τῷ λόγῳ ἤλεγξας σαφῶς, αἱρέσεων τὴν πλάνην, καὶ φέγγει τῆς εὐσεβείας, ἐφυγάδευσας αὐτήν, βοῶν Ἐπιφάνιε· Εὐλογεῖτε πάντα τὰ ἔργα Κυρίου, τὸν Κύριον ὑμνεῖτε, καὶ ὑπερυψοῦτε εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8C12.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C12.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode8C12.melody = he.h.m1_gr_GR_cog.heHE.EnKaminoPaides.name
meMA.Ode8C12.incipit = "Ἐπὶ θρόνου ἤρθης ὑψηλοῦ· παθῶν…"
meMA.Ode8C12.text = "Ἐπὶ θρόνου ἤρθης ὑψηλοῦ· παθῶν γὰρ βασιλεύσας, σαρκός τε ἐπικρατήσας, σὺ ἐποίμανας Χριστοῦ, κραυγάζων τὸ ποίμνιον. Εὐλογεῖτε πάντα τὰ ἔργα Κυρίου, τὸν Κύριον ὑμνεῖτε, καὶ ὑπερυψοῦτε, εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8C13.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C13.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode8C13.melody = he.h.m1_gr_GR_cog.heHE.EnKaminoPaides.name
meMA.Ode8C13.incipit = "Χορευέτω πᾶσα νοερά, καὶ λογικὴ…"
meMA.Ode8C13.text = "Χορευέτω πᾶσα νοερά, καὶ λογικὴ οὐσία, τῇ μνήμῃ τοῦ Ἱεράρχου, καὶ θεράποντος Χριστοῦ, πιστῶς ἀνακράζουσα· Εὐλογεῖτε πάντα τὰ ἔργα Κυρίου, τὸν Κύριον ὑμνεῖτε, καὶ ὑπερυψοῦτε, εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8C14.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C14.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode8C14.melody = he.h.m1_gr_GR_cog.heHE.EnKaminoPaides.name
meMA.Ode8C14.incipit = "Θεαρέστως σὺ θεολογῶν, ὡς ἄγκυραν…"
meMA.Ode8C14.text = "Θεαρέστως σὺ θεολογῶν, ὡς ἄγκυραν δογμάτων, τοῖς πᾶσι θεολογίαν, καταλέλοιπας τὴν σήν, διʼ ἧς ἀναμέλπομεν· Εὐλογεῖτε πάντα τὰ ἔργα Κυρίου, τὸν Κύριον ὑμνεῖτε, καὶ ὑπερυψοῦτε, εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8C1T.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C1T.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode8C1T.melody = he.h.m1_gr_GR_cog.heHE.EnKaminoPaides.name
meMA.Ode8C1T.incipit = "Χαῖρε θρόνε ἔνδοξε Θεοῦ, πιστῶν…"
meMA.Ode8C1T.text = "Χαῖρε θρόνε ἔνδοξε Θεοῦ, πιστῶν χαῖρε τὸ τεῖχος, διʼ ἧς φῶς τοῖς ἐν σκότει, ἐξανέτειλε Χριστός, τοῖς σὲ μακαρίζουσι καὶ βοῶσιν· Εὐλογεῖτε πάντα τὰ ἔργα Κυρίου, τὸν Κύριον ὑμνεῖτε, καὶ ὑπερυψοῦτε, εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8C21.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C21.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode8C21.melody = he.h.m1_gr_GR_cog.heHE.EnKaminoPaides.name
meMA.Ode8C21.incipit = "Λαμπηδόσι καὶ μαρμαρυγαῖς, φωτὸς τῆς…"
meMA.Ode8C21.text = "Λαμπηδόσι καὶ μαρμαρυγαῖς, φωτὸς τῆς τρισηλίου, λαμπάδος Ἱεροφάντορ, ἐνηδόμενος τανῦν, κραυγάζεις γηθόμενος· Εὐλογεῖτε πάντα τὰ ἔργα Κυρίου, τὸν Κύριον ὑμνεῖτε, καὶ ὑπερυψοῦτε, εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8C22.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C22.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode8C22.melody = he.h.m1_gr_GR_cog.heHE.EnKaminoPaides.name
meMA.Ode8C22.incipit = "Πολυφώτου καὶ πλησιφαοῦς, Πάτερ ἠξιωμένος…"
meMA.Ode8C22.text = "Πολυφώτου καὶ πλησιφαοῦς, Πάτερ ἠξιωμένος, χορείας ἐπουρανίου, ἐν σκηνώμασιν ἡμῶν, βοώντων μνημόνευε· Εὐλογεῖτε πάντα τὰ ἔργα Κυρίου, τὸν Κύριον ὑμνεῖτε, καὶ ὑπερυψοῦτε, εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8C23.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C23.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode8C23.melody = he.h.m1_gr_GR_cog.heHE.EnKaminoPaides.name
meMA.Ode8C23.incipit = "Ὡς τηρήσας τὸ θεοειδές, καὶ…"
meMA.Ode8C23.text = "Ὡς τηρήσας τὸ θεοειδές, καὶ Θεοῦ κατʼ εἰκόνα, εἰκόνας τὰς τῶν Ἁγίων, ἀνεστήλωσας εἰδώς, ἐπὶ τὸ πρωτότυπον, διαβαίνειν πᾶσαν τιμήν τε καὶ δόξαν, πανεύφημε καὶ γέρας. Κύριον ὑμνεῖτε, βοῶν εἰς τοὺς αἰῶνας."

meMA.Ode8C2T.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C2T.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode8C2T.melody = he.h.m1_gr_GR_cog.heHE.EnKaminoPaides.name
meMA.Ode8C2T.incipit = "Καινῷ τρόπῳ καὶ ὑπερφυεῖ, Παρθένε…"
meMA.Ode8C2T.text = "Καινῷ τρόπῳ καὶ ὑπερφυεῖ, Παρθένε συλλαβοῦσα, τὸν Λόγον τὸν τῷ Πατρί, συγκαθορώμενον Υἱόν, Παρθένος διέμεινας· Σὲ ὑμνοῦμεν πάντα τὰ ἔργα, τὸ Χαῖρέ σοι βοῶντες, καὶ ὑπερυψοῦμεν, εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8H.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8H.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode8H.melody = he.h.m1_gr_GR_cog.heHE.EnKaminoPaides.melody
meMA.Ode8H.incipit = he.h.m1_gr_GR_cog.heHE.EnKaminoPaides.incipit
meMA.Ode8H.text = he.h.m1_gr_GR_cog.heHE.EnKaminoPaides.text

meMA.Ode9C11.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C11.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode9C11.melody = he.h.m1_gr_GR_cog.heHE.TyponTisAgnis.name
meMA.Ode9C11.incipit = "Ὅρος πρακτικῆς ὁ βίος σου…"
meMA.Ode9C11.text = "Ὅρος πρακτικῆς ὁ βίος σου, καὶ θεωρίας θείας Πάτερ οἱ λόγοι σου, τῷ θεόφρονι, ὄντως λαῷ κατελείφθησαν, Ἐπιφάνιε μάκαρ ἀοίδιμε· διὸ σε ἐπαξίως, ἀνευφημοῦντες μακαρίζομεν."

meMA.Ode9C12.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C12.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode9C12.melody = he.h.m1_gr_GR_cog.heHE.TyponTisAgnis.name
meMA.Ode9C12.incipit = "Νύμφης τοῦ Χριστοῦ προΐστασο, τῆς…"
meMA.Ode9C12.text = "Νύμφης τοῦ Χριστοῦ προΐστασο, τῆς Ἐκκλησίας ἔνδοξε ἐπιφάνιε, καὶ κατεύνασον, τὸν κατʼ αὐτῆς ἐγειρόμενον, ταῖς πρεσβείαις σου ἄγριον κλύδωνα, ὡς ἔχων παῤῥησίαν, πρὸς τὸν Δεσπότην καὶ φιλάνθρωπον."

meMA.Ode9C13.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C13.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode9C13.melody = he.h.m1_gr_GR_cog.heHE.TyponTisAgnis.name
meMA.Ode9C13.incipit = "Ἄναξ τῶν ἀνακτων Ἅγιε, μοναρχικὴ…"
meMA.Ode9C13.text = "Ἄναξ τῶν ἀνακτων Ἅγιε, μοναρχικὴ Τριάς, ἡ πάντων δεσπόζουσα, τοῖς ὑμνοῦσί σε, τὴν τῶν πταισμάτων συγχώρησιν, καὶ τοῦ βίου παντὸς τὴν κατάστασιν, ἀπείραστον παράσχου, Ἐπιφανίου ταῖς ἐντεύξεσι."

meMA.Ode9C1T.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C1T.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode9C1T.melody = he.h.m1_gr_GR_cog.heHE.TyponTisAgnis.name
meMA.Ode9C1T.incipit = "Ἅπας ἐγκωμίων πάναγνε, νόμος ἡττᾶται…"
meMA.Ode9C1T.text = "Ἅπας ἐγκωμίων πάναγνε, νόμος ἡττᾶται τῷ μεγέθει τῆς δόξης σου· ἀλλὰ Δέσποινα, παρʼ οἰκετῶν ἀναξίων σου, ἐξ εὐνοίας δὲ σοι προσφερόμενον, προσδέχου Θεοτόκε, μέτʼ εὐμενείας τὸ ἐφύμνιον."

meMA.Ode9C21.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C21.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode9C21.melody = he.h.m1_gr_GR_cog.heHE.TyponTisAgnis.name
meMA.Ode9C21.incipit = "Λύειν καὶ δεσμεῖν τὰ πταίσματα…"
meMA.Ode9C21.text = "Λύειν καὶ δεσμεῖν τὰ πταίσματα, τὴν ἐξουσίαν οὐρανόθεν δεξάμενος, ἐννομώτατος, ὡς Ἱερεὺς παμμακάριστε, τοῖς ὑμνοῦσί σε Πάτερ τὴν ἄφεσιν, πρεσβείαις σου παράσχου, ἵνα σε πάντες μακαρίζωμεν."

meMA.Ode9C22.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C22.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode9C22.melody = he.h.m1_gr_GR_cog.heHE.TyponTisAgnis.name
meMA.Ode9C22.incipit = "Εὗρες ἀμοιβὰς τῶν πόνων σου…"
meMA.Ode9C22.text = "Εὗρες ἀμοιβὰς τῶν πόνων σου, Ἱεροφάντορ Γερμανὲ παμμακάριστε, τὸ μακάριον, ἀπειληφὼς ἐνδιαίτημα, καὶ ζωῆς αἰωνίου λαβόμενος, καὶ θείας θεωρίας, ἀκαταπαύστως ἐμφορούμενος."

meMA.Ode9C23.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C23.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode9C23.melody = he.h.m1_gr_GR_cog.heHE.TyponTisAgnis.name
meMA.Ode9C23.incipit = "Ὅλον σεαυτὸν ἀνέθηκας, τῷ παντεπόπτῃ…"
meMA.Ode9C23.text = "Ὅλον σεαυτὸν ἀνέθηκας, τῷ παντεπόπτῃ Λόγῳ καὶ παντοκράτορι, καλλιέρημα, ἀναδειχθεὶς ἱερώτατον, καὶ θυσίαν δεκτὴν καὶ εὐάρεστον, προσάγων τῷ Κυρίῳ, σῶν πονημάτων τὰ ἐφύμνια."

meMA.Ode9C2T.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C2T.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode9C2T.melody = he.h.m1_gr_GR_cog.heHE.TyponTisAgnis.name
meMA.Ode9C2T.incipit = "Στῆσον τῆς σαρκὸς τὸ φρόνημα…"
meMA.Ode9C2T.text = "Στῆσον τῆς σαρκὸς τὸ φρόνημα, καὶ τῶν παθῶν Παρθένε παῦσον τὸν τάραχον, τοῦ προσφεύγοντος, Θεογεννῆτορ τῇ σκέπῃ σου, καὶ ἐλπίδος κρηπῖδά σε ἔχοντος, καὶ θείαν προστασίαν, πεπλουτηκότος καὶ ἀντίληψιν."

