trMA.Ode6C11.ode = miscellanea_gr_GR_cog.misc.Ode6
trMA.Ode6C11.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode6C11.greekmelody = he.h.m8_gr_GR_cog.heHE.TinDeisinEkcheo.greekname
trMA.Ode6C11.melody = he.h.m8_gr_GR_cog.heHE.TinDeisinEkcheo.name
trMA.Ode6C11.incipit = "Οἱ Ἅγιοι διὰ πλείστων βασάνων…"
trMA.Ode6C11.text = "Οἱ Ἅγιοι διὰ πλείστων βασάνων, τὰς πολλὰς τῶν δυσμενῶν μυριάδας, καταβαλεῖν, ἠξιώθησαν Σῶτερ, καὶ τῶν πολλῶν ἀγαθῶν σου ἐπέτυχον, εὐχαῖς αὐτῶν ὡς ἀγαθός, τὰ πολλά μου ἐξάλειψον πταίσματα."

trMA.Ode6C12.ode = miscellanea_gr_GR_cog.misc.Ode6
trMA.Ode6C12.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode6C12.melody = he.h.m8_gr_GR_cog.heHE.TinDeisinEkcheo.name
trMA.Ode6C12.incipit = "Ὑμνήσωμεν τοὺς Χριστοῦ Ἀθλοφόρους, καὶ…"
trMA.Ode6C12.text = "Ὑμνήσωμεν τοὺς Χριστοῦ Ἀθλοφόρους, καὶ βοήσωμεν αὐτοῖς ὁμοφρόνως, Δεσποτικῶν, μιμηταὶ παθημάτων, τὰ τῶν ψυχῶν ἡμῶν πάθη ἰάσατε, νηστεύειν τε ἀπὸ κακῆς, συνηθείας ἡμᾶς ἐνισχύσατε."

trMA.Ode6C13.ode = miscellanea_gr_GR_cog.misc.Ode6
trMA.Ode6C13.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode6C13.melody = he.h.m8_gr_GR_cog.heHE.TinDeisinEkcheo.name
trMA.Ode6C13.incipit = "Μετέβαλες τοῦ θανάτου εἰς ὕπνον…"
trMA.Ode6C13.text = "Μετέβαλες τοῦ θανάτου εἰς ὕπνον, τὸ ἐπίπονον ἐν τάφῳ ὑπνώσας, καὶ τοῖς νεκροῖς, τὴν ζωὴν ἐδωρήσω, τοὺς μεταστάντας οὖν Σῶτερ ἀξίωσον, τῆς στάσεως τῶν ἐκλεκτῶν, Ἀθλοφόρων ἁγίων δεήσεσι."

trMA.Ode6C1T.ode = miscellanea_gr_GR_cog.misc.Ode6
trMA.Ode6C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode6C1T.melody = he.h.m8_gr_GR_cog.heHE.TinDeisinEkcheo.name
trMA.Ode6C1T.incipit = "Νηστεύειν με καὶ ἀπέχεσθαι πάσης…"
trMA.Ode6C1T.text = "Νηστεύειν με καὶ ἀπέχεσθαι πάσης, ἁμαρτίας ἐνδυνάμωσον Κόρη, τὸν δυνατόν, ἀναμάρτητον Λόγον, σωματωθέντα ἀῤῥήτως κυήσασα, καὶ δίδου μοι καθαρτικόν, μολυσμάτων Πανάμωμε δάκρυον."

trMA.Ode6C21.ode = miscellanea_gr_GR_cog.misc.Ode6
trMA.Ode6C21.mode = miscellanea_gr_GR_cog.misc.Mode3
trMA.Ode6C21.greekmelody = he.h.m3_gr_GR_cog.heHE.VythosMoiTonPathon.greekname
trMA.Ode6C21.melody = he.h.m3_gr_GR_cog.heHE.VythosMoiTonPathon.name
trMA.Ode6C21.incipit = "Μαρτύρων μνήμην ἐπιτελοῦμεν ἐνθέως, φιλομάρτυρες…"
trMA.Ode6C21.text = "Μαρτύρων μνήμην ἐπιτελοῦμεν ἐνθέως, φιλομάρτυρες δεῦτε, ἀγαλλιασώμεθα, ὕμνοις αὐτοὺς καταστέφοντες, τὸν ἀθλοθέτην Χριστὸν εὐλογοῦντες."

trMA.Ode6C22.ode = miscellanea_gr_GR_cog.misc.Ode6
trMA.Ode6C22.mode = miscellanea_gr_GR_cog.misc.Mode3
trMA.Ode6C22.melody = he.h.m3_gr_GR_cog.heHE.VythosMoiTonPathon.name
trMA.Ode6C22.incipit = "Μαστίγων πρῶτον πεῖραν λαβόντες, εἶθ…"
trMA.Ode6C22.text = "Μαστίγων πρῶτον πεῖραν λαβόντες, εἶθ οὕτω λιθασθέντες πρισθέντες, θηρσὶ βορὰ δίδοσθε, σφαγιάζεσθε ὡς ἄρνες Χριστοῦ, ἀλλὰ ζῆτε εἰς ἀεὶ Ἀθλοφόροι."

trMA.Ode6C2Tr.ode = miscellanea_gr_GR_cog.misc.Ode6
trMA.Ode6C2Tr.mode = miscellanea_gr_GR_cog.misc.Mode3
trMA.Ode6C2Tr.melody = he.h.m3_gr_GR_cog.heHE.VythosMoiTonPathon.name
trMA.Ode6C2Tr.incipit = "Τὰ τρία μιᾷ φύσει συνάπτων…"
trMA.Ode6C2Tr.text = "Τὰ τρία μιᾷ φύσει συνάπτων, τὸ ἓν δέ, τρισὶ τέμνων προσώποις, ἃ ἡ Θεότης ἐστί, Σαβέλλιόν τε καὶ τὸν Ἄρειον, τοὺς ἰσομέτρους κρημνοὺς διαφεύγω."

trMA.Ode6C2T.ode = miscellanea_gr_GR_cog.misc.Ode6
trMA.Ode6C2T.mode = miscellanea_gr_GR_cog.misc.Mode3
trMA.Ode6C2T.melody = he.h.m3_gr_GR_cog.heHE.VythosMoiTonPathon.name
trMA.Ode6C2T.incipit = "Παρθένος ὤφθης, καὶ μετὰ τόκον…"
trMA.Ode6C2T.text = "Παρθένος ὤφθης, καὶ μετὰ τόκον ἁγνεύεις· ἄμφω γὰρ καὶ λοχεύεις, ξενοπρεπὲς θέαμα, λεγόμενον, καὶ νοούμενον, ἐν σοὶ Θεομῆτορ."

trMA.Ode6C2A1.ode = miscellanea_gr_GR_cog.misc.Ode6
trMA.Ode6C2A1.mode = miscellanea_gr_GR_cog.misc.Mode3
trMA.Ode6C2A1.melody = he.h.m3_gr_GR_cog.heHE.VythosMoiTonPathon.name
trMA.Ode6C2A1.incipit = "Τὸ αἷμα τῆς ὑμῶν καρτερίας…"
trMA.Ode6C2A1.text = "Τὸ αἷμα τῆς ὑμῶν καρτερίας, Κυρίῳ ἱκετεύει ἀπαύστως, ὑπὲρ ὑμῶν Μάρτυρες· διὸ καὶ νῦν, ὑπερεύξασθε νηστεύειν ἡμᾶς, ἐκ παθῶν ἀτιμίας."

trMA.Ode6C2A2.ode = miscellanea_gr_GR_cog.misc.Ode6
trMA.Ode6C2A2.mode = miscellanea_gr_GR_cog.misc.Mode3
trMA.Ode6C2A2.melody = he.h.m3_gr_GR_cog.heHE.VythosMoiTonPathon.name
trMA.Ode6C2A2.incipit = "Ὁ πάντα μέλλων κρῖναι τὸν…"
trMA.Ode6C2A2.text = "Ὁ πάντα μέλλων κρῖναι τὸν Κόσμον, παρόντων μυριάδων Ἀγγέλων, ἀκαταγνώστως τότε παραστῆναί σοι καταξίωσον, τοὺς κοιμηθέντας, πιστῶς Πανοικτίρμον."

trMA.Ode6C2H.ode = miscellanea_gr_GR_cog.misc.Ode6
trMA.Ode6C2H.mode = miscellanea_gr_GR_cog.misc.Mode3
trMA.Ode6C2H.greekmelody = he.h.m3_gr_GR_cog.heHE.VythosMoiTonPathon.greekname
trMA.Ode6C2H.melody = he.h.m3_gr_GR_cog.heHE.VythosMoiTonPathon.melody
trMA.Ode6C2H.incipit = he.h.m3_gr_GR_cog.heHE.VythosMoiTonPathon.incipit
trMA.Ode6C2H.text = he.h.m3_gr_GR_cog.heHE.VythosMoiTonPathon.text

trMA.Ode7C11.ode = miscellanea_gr_GR_cog.misc.Ode7
trMA.Ode7C11.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode7C11.greekmelody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.greekname
trMA.Ode7C11.melody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.name
trMA.Ode7C11.incipit = "Ὄμβροις ἐσβέσατε αἱμάτων, παναοίδιμοι τὴν…"
trMA.Ode7C11.text = "Ὄμβροις ἐσβέσατε αἱμάτων, παναοίδιμοι τὴν φλόγα τῆς ἀπάτης, τοῦ πυρὸς οὖν ἡμᾶς, τοῦ μέλλοντος πρεσβείαις, ταῖς πρὸς Χριστὸν λυτρώσατε, τοῦ Κυρίου Ἀθλοφόροι."

trMA.Ode7C12.ode = miscellanea_gr_GR_cog.misc.Ode7
trMA.Ode7C12.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode7C12.melody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.name
trMA.Ode7C12.incipit = "Στόματα φράξαντες λεόντων, τῶν βασάνων…"
trMA.Ode7C12.text = "Στόματα φράξαντες λεόντων, τῶν βασάνων τὴν πυρὰν ὑπενεγκόντες, Ἀθλοφόροι τρυφήν, τὴν κρείττονα κληροῦσθε, ἧς περ τυχεῖν δεήθητε, καὶ ἡμᾶς εἰς τοὺς αἰῶνας."

trMA.Ode7C13.ode = miscellanea_gr_GR_cog.misc.Ode7
trMA.Ode7C13.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode7C13.melody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.name
trMA.Ode7C13.incipit = "Ὅλῳ φωτὶ περιλαμφθέντες, τῷ τοῦ…"
trMA.Ode7C13.text = "Ὅλῳ φωτὶ περιλαμφθέντες, τῷ τοῦ πνεύματος γενναῖοι Ἀθλοφόροι, μεταστᾶσι πιστοῖς, τὴν ἄνεσιν αἰτεῖτε, καὶ Παραδείσου εἴσοδον, καὶ ζωῆς τὴν μετουσίαν."

trMA.Ode7C1T.ode = miscellanea_gr_GR_cog.misc.Ode7
trMA.Ode7C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode7C1T.melody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.name
trMA.Ode7C1T.incipit = "Ὕμνον προσάγω σοι Παρθένε, μὴ…"
trMA.Ode7C1T.text = "Ὕμνον προσάγω σοι Παρθένε, μὴ παρίδῃς με κακοῖς κεκρατημένον· ἀλλὰ δίδου Ἁγνή, διόρθωσιν τελείαν, διὰ νηστείας πάσης τε, ἀγωγῆς ἀγαθοτρόπου."

trMA.Ode7C21.ode = miscellanea_gr_GR_cog.misc.Ode7
trMA.Ode7C21.mode = miscellanea_gr_GR_cog.misc.Mode3
trMA.Ode7C21.greekmelody = he.h.m3_gr_GR_cog.heHE.OTinFloga.greekname
trMA.Ode7C21.melody = he.h.m3_gr_GR_cog.heHE.OTinFloga.name
trMA.Ode7C21.incipit = "Πῦρ καὶ ξίφος καὶ θήρας…"
trMA.Ode7C21.text = "Πῦρ καὶ ξίφος καὶ θήρας Ἀθλοφόροι, ὡς τρυφὴν λογισθέντες κατεπλήξατε, τοὺς φονεῖς ὑμῶν, ἀνυμνοῦντες Θεόν, τὸν τῶν Πατέρων ἡμῶν."

trMA.Ode7C22.ode = miscellanea_gr_GR_cog.misc.Ode7
trMA.Ode7C22.mode = miscellanea_gr_GR_cog.misc.Mode3
trMA.Ode7C22.melody = he.h.m3_gr_GR_cog.heHE.OTinFloga.name
trMA.Ode7C22.incipit = "Μεληδὸν συγκοπέντες καὶ καέντες, τῷ…"
trMA.Ode7C22.text = "Μεληδὸν συγκοπέντες καὶ καέντες, τῷ Χριστῷ, ἀνηνέχθητε θυσία, εὐωδεστάτη Χριστοῦ Μάρτυρες, ἀλλʼ αὐτὸν ὑπὲρ ἡμῶν, ἀεὶ πρεσβεύσατε."

trMA.Ode7C2Tr.ode = miscellanea_gr_GR_cog.misc.Ode7
trMA.Ode7C2Tr.mode = miscellanea_gr_GR_cog.misc.Mode3
trMA.Ode7C2Tr.melody = he.h.m3_gr_GR_cog.heHE.OTinFloga.name
trMA.Ode7C2Tr.incipit = "Ἐν Τριάδι ἕνα Θεὸν δοξάζω…"
trMA.Ode7C2Tr.text = "Ἐν Τριάδι ἕνα Θεὸν δοξάζω, τὸν Πατέρα, Υἱόν τε καὶ τὸ Πνεῦμα, Μονάδα ἁπλήν, Τριάδα σεπτήν, ἀρχὴν προάναρχον, καὶ ὁμοούσιον."

trMA.Ode7C2T.ode = miscellanea_gr_GR_cog.misc.Ode7
trMA.Ode7C2T.mode = miscellanea_gr_GR_cog.misc.Mode3
trMA.Ode7C2T.melody = he.h.m3_gr_GR_cog.heHE.OTinFloga.name
trMA.Ode7C2T.incipit = "Παναγία Δέσποινα Θεοτόκε, τὰς εὐχὰς…"
trMA.Ode7C2T.text = "Παναγία Δέσποινα Θεοτόκε, τὰς εὐχὰς τῶν δούλων σου δεχομένη, προσένεγκε τῷ πάντων Θεῷ, ἵνα σώσῃ ἡμᾶς, ἀπὸ παντὸς πειρασμοῦ."

trMA.Ode7C2A1.ode = miscellanea_gr_GR_cog.misc.Ode7
trMA.Ode7C2A1.mode = miscellanea_gr_GR_cog.misc.Mode3
trMA.Ode7C2A1.melody = he.h.m3_gr_GR_cog.heHE.OTinFloga.name
trMA.Ode7C2A1.incipit = "Τῶν Μαρτύρων οἱ δῆμοι οὐρανόθεν…"
trMA.Ode7C2A1.text = "Τῶν Μαρτύρων οἱ δῆμοι οὐρανόθεν, τοὺς ὑμᾶς ἀνυμνοῦντας ἐπιστάντες, εὐλογήσατε, ἁγιάσατε προθύμως, τῶν Νηστειῶν, διατελεῖν τὸν καιρόν."

trMA.Ode7C2A2.ode = miscellanea_gr_GR_cog.misc.Ode7
trMA.Ode7C2A2.mode = miscellanea_gr_GR_cog.misc.Mode3
trMA.Ode7C2A2.melody = he.h.m3_gr_GR_cog.heHE.OTinFloga.name
trMA.Ode7C2A2.incipit = "Τῶν ἐν πίστει πρὸς σὲ…"
trMA.Ode7C2A2.text = "Τῶν ἐν πίστει πρὸς σὲ μετατεθέντων ὁ συνιὼν τὰ ἔργα τῶν ἀνθρώπων, τὰ ἑκούσια, καὶ ἀκούσια πταίσματα συγχωρῶν, ἀνάπαυσον ὡς Θεός."

trMA.Ode7C2H.ode = miscellanea_gr_GR_cog.misc.Ode7
trMA.Ode7C2H.mode = miscellanea_gr_GR_cog.misc.Mode3
trMA.Ode7C2H.greekmelody = he.h.m3_gr_GR_cog.heHE.OTinFloga.greekname
trMA.Ode7C2H.melody = he.h.m3_gr_GR_cog.heHE.OTinFloga.melody
trMA.Ode7C2H.incipit = he.h.m3_gr_GR_cog.heHE.OTinFloga.incipit
trMA.Ode7C2H.text = he.h.m3_gr_GR_cog.heHE.OTinFloga.text

trMA.Ode8C11.ode = miscellanea_gr_GR_cog.misc.Ode8
trMA.Ode8C11.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode8C11.greekmelody = he.h.m8_gr_GR_cog.heHE.NikitaiTyrannou.greekname
trMA.Ode8C11.melody = he.h.m8_gr_GR_cog.heHE.NikitaiTyrannou.name
trMA.Ode8C11.incipit = "Τῶν βασάνων εἴδη, ἀσθενείᾳ σώματος…"
trMA.Ode8C11.text = "Τῶν βασάνων εἴδη, ἀσθενείᾳ σώματος ὑπενεγκόντες, ἰατροὶ νοσούντων, Ἀθλοφόροι ὤφθητε· ὅθεν κραυγάζω. Τὴν ἐμὴν νοσοῦσαν ψυχὴν ἰατρεύσατε, χρόνῳ τῆς Νηστείας, διὰ τῆς μετανοίας."

trMA.Ode8C12.ode = miscellanea_gr_GR_cog.misc.Ode8
trMA.Ode8C12.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode8C12.melody = ""
trMA.Ode8C12.incipit = "Οἴμοι! πῶς παρῆλθον, πᾶσαι αἱ…"
trMA.Ode8C12.text = "Οἴμοι! πῶς παρῆλθον, πᾶσαι αἱ ἡμέραι μου ἐν ἀμελείᾳ, καὶ ἰδοὺ τὸ τέλος, φθάνει ἀτελείωτον, καλῶν λαβεῖν με. Οἱ καλῶς τὸν δρόμον, τελέσαντες Μάρτυρες, τέλος ἀγαθόν μοι, γενέσθαι δυσωπεῖτε."

trMA.Ode8C13.ode = miscellanea_gr_GR_cog.misc.Ode8
trMA.Ode8C13.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode8C13.melody = ""
trMA.Ode8C13.incipit = "Σταλαγμοῖς αἱμάτων, τὴν πυρὰν ἐσβέσατε…"
trMA.Ode8C13.text = "Σταλαγμοῖς αἱμάτων, τὴν πυρὰν ἐσβέσατε τῆς ἀθεΐας, θεῖοι Ἀθλοφόροι, καὶ τοῖς ἐκδημήσασι τοῦδε τοῦ βίου, ἐξαιτεῖτε ὄντως, τὴν θείαν ἀνάπαυσιν, καὶ τῶν πεπραγμένων, τὴν λύσιν εἰς αἰῶνας."

trMA.Ode8C1T.ode = miscellanea_gr_GR_cog.misc.Ode8
trMA.Ode8C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode8C1T.melody = ""
trMA.Ode8C1T.incipit = "Ἰεζεκιὴλ σε, πύλην ἀδιόδευτον Ἁγνὴ…"
trMA.Ode8C1T.text = "Ἰεζεκιὴλ σε, πύλην ἀδιόδευτον Ἁγνὴ προεῖδε, μετανοίας πύλας, πᾶσιν ὑπανοίγουσαν ἀπεγνωσμένοις· ὅθεν δυσωπῶ σε, τρίβους μοι ὑπάνοιξον, τὰς πρὸς τὰς ἐκεῖθεν, φερούσας καταπαύσεις."

trMA.Ode8C21.ode = miscellanea_gr_GR_cog.misc.Ode8
trMA.Ode8C21.mode = miscellanea_gr_GR_cog.misc.Mode3
trMA.Ode8C21.greekmelody = he.h.m3_gr_GR_cog.heHE.TonExAnarchou.greekname
trMA.Ode8C21.melody = he.h.m3_gr_GR_cog.heHE.TonExAnarchou.name
trMA.Ode8C21.incipit = "Τοὺς πολυάθλους αἰκισμούς, διαφόρως ὑπομείναντες…"
trMA.Ode8C21.text = "Τοὺς πολυάθλους αἰκισμούς, διαφόρως ὑπομείναντες, οἱ μὲν καέντες, ἕτεροι πρισθέντες, ἄλλοι δὲ τεμνόμενοι, εὐφραίνεσθε Ἀθληταί, ἀναμέλποντες Χριστῷ· Ὑμνεῖτε, εὐλογεῖτε, καὶ ὑπερυψοῦτε εἰς πάντας τοὺς αἰῶνας."

trMA.Ode8C22.ode = miscellanea_gr_GR_cog.misc.Ode8
trMA.Ode8C22.mode = miscellanea_gr_GR_cog.misc.Mode3
trMA.Ode8C22.melody = he.h.m3_gr_GR_cog.heHE.TonExAnarchou.name
trMA.Ode8C22.incipit = "Ἐν τοῖς αἵμασιν ὑμῶν, ἁγιάζετε…"
trMA.Ode8C22.text = "Ἐν τοῖς αἵμασιν ὑμῶν, ἁγιάζετε τὰ πέρατα, μικραῖς ῥανίσι, ῥεῖθρα ἰαμάτων πᾶσιν ἀναβλύζοντες, πανεύφημοι Μάρτυρες· διὸ βοᾶτε ἀεί· Ὑμνεῖτε, εὐλογεῖτε, καὶ ὑπερυψοῦτε εἰς πάντας τοὺς αἰῶνας."

trMA.Ode8C2Tr.ode = miscellanea_gr_GR_cog.misc.Ode8
trMA.Ode8C2Tr.mode = miscellanea_gr_GR_cog.misc.Mode3
trMA.Ode8C2Tr.melody = he.h.m3_gr_GR_cog.heHE.TonExAnarchou.name
trMA.Ode8C2Tr.incipit = "Ἡ τρισυπόστατος Μονάς, ὁ Πατὴρ…"
trMA.Ode8C2Tr.text = "Ἡ τρισυπόστατος Μονάς, ὁ Πατὴρ ὁ Υἱός, καὶ Πνεῦμα τὸ ζῶν, μία Θεότης, μία Βασιλεία, σὲ αἰνεῖ τὸ ἄδυτον φῶς, Ἀγγέλων αἱ στρατιαί, καὶ ἡμεῖς οἱ ἐπὶ γῆς, ὑμνοῦμεν, εὐλογοῦμεν, καὶ ὑπερυψοῦμεν εἰς πάντας τοὺς αἰῶνας."

trMA.Ode8C2T.ode = miscellanea_gr_GR_cog.misc.Ode8
trMA.Ode8C2T.mode = miscellanea_gr_GR_cog.misc.Mode3
trMA.Ode8C2T.melody = he.h.m3_gr_GR_cog.heHE.TonExAnarchou.name
trMA.Ode8C2T.incipit = "Ἰδοὺ πᾶσαι γενεαί, μακαρίζομέν σε…"
trMA.Ode8C2T.text = "Ἰδοὺ πᾶσαι γενεαί, μακαρίζομέν σε Ἄχραντε, τὰ μεγαλεῖα τὰ σὰ καθορῶντες· σὺ γὰρ τίκτεις ὑπερφυῶς, τὸν τοῦ παντὸς Ποιητήν, Θεὸν ὄντα καὶ βροτόν· διό σε εὐλογοῦμεν, καὶ ὑπερυψοῦμεν εἰς πάντας τοὺς αἰῶνας."

trMA.Ode8C2A1.ode = miscellanea_gr_GR_cog.misc.Ode8
trMA.Ode8C2A1.mode = miscellanea_gr_GR_cog.misc.Mode3
trMA.Ode8C2A1.melody = he.h.m3_gr_GR_cog.heHE.TonExAnarchou.name
trMA.Ode8C2A1.incipit = "Τῶν Ἀθλοφόρων ὁ χορός, δυσωπήθητι…"
trMA.Ode8C2A1.text = "Τῶν Ἀθλοφόρων ὁ χορός, δυσωπήθητι τοῦ σῶσαι ἡμᾶς, πρὸς τὸν Σωτῆρα, τὰς ἱκετηρίας, καὶ τανῦν ποιούμενοι, λατρεύειν εἰλικρινῶς, διʼ ἐγκρατείας αὐτῷ· Ὑμνεῖτε, εὐλογεῖτε, καὶ ὑπερυψοῦτε αὐτὸν εἰς τοὺς αἰῶνας."

trMA.Ode8C2A2.ode = miscellanea_gr_GR_cog.misc.Ode8
trMA.Ode8C2A2.mode = miscellanea_gr_GR_cog.misc.Mode3
trMA.Ode8C2A2.melody = he.h.m3_gr_GR_cog.heHE.TonExAnarchou.name
trMA.Ode8C2A2.incipit = "Τοὺς Κοιμηθέντας εὐσεβῶς, ἐλπίδι ἀναστάσεως…"
trMA.Ode8C2A2.text = "Τοὺς Κοιμηθέντας εὐσεβῶς, ἐλπίδι ἀναστάσεως, πρὸς αἰωνίαν ζωὴν ἐγερθῆναι, Κύριε, ἀξίωσον αἰνεῖν σε εἰλικρινῶς, καὶ δοξάζειν ψαλμικῶς· Ὑμνεῖτε, εὐλογεῖτε, καὶ ὑπερυψοῦτε εἰς πάντας τοὺς αἰῶνας."

trMA.Ode8C2H.ode = miscellanea_gr_GR_cog.misc.Ode8
trMA.Ode8C2H.mode = miscellanea_gr_GR_cog.misc.Mode3
trMA.Ode8C2H.greekmelody = he.h.m3_gr_GR_cog.heHE.TonExAnarchou.greekname
trMA.Ode8C2H.melody = he.h.m3_gr_GR_cog.heHE.TonExAnarchou.melody
trMA.Ode8C2H.incipit = he.h.m3_gr_GR_cog.heHE.TonExAnarchou.incipit
trMA.Ode8C2H.text = he.h.m3_gr_GR_cog.heHE.TonExAnarchou.text

trMA.Ode9C11.ode = miscellanea_gr_GR_cog.misc.Ode9
trMA.Ode9C11.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode9C11.greekmelody = he.h.m8_gr_GR_cog.heHE.OrousParilthes.greekname
trMA.Ode9C11.melody = he.h.m8_gr_GR_cog.heHE.OrousParilthes.name
trMA.Ode9C11.incipit = "Ὤφθητε θεῖοι ὡς ἄνθρακες, ὕλην…"
trMA.Ode9C11.text = "Ὤφθητε θεῖοι ὡς ἄνθρακες, ὕλην πονηρὰν ἀθεΐας συμφλέξαντες, καὶ στίφη τῶν δαιμόνων ξίφει συγκόψαντες, γενναῖοι Ἀθλοφόροι, φωταγωγοὶ τῶν καρδιῶν ἡμῶν."

trMA.Ode9C12.ode = miscellanea_gr_GR_cog.misc.Ode9
trMA.Ode9C12.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode9C12.melody = he.h.m8_gr_GR_cog.heHE.OrousParilthes.name
trMA.Ode9C12.incipit = "Σκότος βασάνων διήλθετε, καὶ πρὸς…"
trMA.Ode9C12.text = "Σκότος βασάνων διήλθετε, καὶ πρὸς φωτισμὸν νοητὸν μετοικίσθητε, γενναῖοι Ἀθλοφόροι· διὸ φωτίσατε τὴν ταπεινὴν ψυχήν μου, ἐσκοτισμένην πλημμελήμασιν."

trMA.Ode9C13.ode = miscellanea_gr_GR_cog.misc.Ode9
trMA.Ode9C13.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode9C13.melody = he.h.m8_gr_GR_cog.heHE.OrousParilthes.name
trMA.Ode9C13.incipit = "Ἤλγησαν πόνους προσφέροντες, οἱ πανευκλεεῖς…"
trMA.Ode9C13.text = "Ἤλγησαν πόνους προσφέροντες, οἱ πανευκλεεῖς Ἀθλοφόροι τοῦ σώματος, καὶ νῦν τοῖς μεταστᾶσι πιστοῖς τὴν ἄπονον, ἀνάπαυσιν αἰτοῦνται, καὶ παραδείσου τὴν ἀπόλαυσιν."

trMA.Ode9C1T.ode = miscellanea_gr_GR_cog.misc.Ode9
trMA.Ode9C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode9C1T.melody = he.h.m8_gr_GR_cog.heHE.OrousParilthes.name
trMA.Ode9C1T.incipit = "Φάνηθι ἄχραντε Δέσποινα, θεῖος συνεργὸς…"
trMA.Ode9C1T.text = "Φάνηθι ἄχραντε Δέσποινα, θεῖος συνεργὸς τοῖς ἀχρείοις οἰκέταις σου, καιρῷ τῆς ἐγκρατείας, τὰς προσευχὰς ἡμῶν προσάγουσα Κυρίῳ, τῷ τῶν αἰώνων βασιλεύοντι."

trMA.Ode9C21.ode = miscellanea_gr_GR_cog.misc.Ode9
trMA.Ode9C21.mode = miscellanea_gr_GR_cog.misc.Mode3
trMA.Ode9C21.greekmelody = he.h.m3_gr_GR_cog.heHE.SeTinAthanaton.greekname
trMA.Ode9C21.melody = he.h.m3_gr_GR_cog.heHE.SeTinAthanaton.name
trMA.Ode9C21.incipit = "Ὡς πῦρ ἐμβληθέντες ἐκ Θεοῦ…"
trMA.Ode9C21.text = "Ὡς πῦρ ἐμβληθέντες ἐκ Θεοῦ, εἰς τὴν γῆν καταφλέξατε, πᾶσαν πλάνην εἰδώλων, ἀνάψαντες πυρσόν, εἰς τὰ πέρατα εὐσεβείας, Ἀθλοφόροι Χριστοῦ."

trMA.Ode9C22.ode = miscellanea_gr_GR_cog.misc.Ode9
trMA.Ode9C22.mode = miscellanea_gr_GR_cog.misc.Mode3
trMA.Ode9C22.melody = he.h.m3_gr_GR_cog.heHE.SeTinAthanaton.name
trMA.Ode9C22.incipit = "Ὑμᾶς οὔτε φλόγες οὐ σφαγαί…"
trMA.Ode9C22.text = "Ὑμᾶς οὔτε φλόγες οὐ σφαγαί, οὐ τροχοὶ οὐκ ἀρθρέμβολα, καταπέλται, πριστῆρες, οὐκ ἄλλη βάσανος πικρὰ διεχώρισε, τῆς ἀγάπης Χριστοῦ Ἀθληταί."

trMA.Ode9C2Tr.ode = miscellanea_gr_GR_cog.misc.Ode9
trMA.Ode9C2Tr.mode = miscellanea_gr_GR_cog.misc.Mode3
trMA.Ode9C2Tr.melody = he.h.m3_gr_GR_cog.heHE.SeTinAthanaton.name
trMA.Ode9C2Tr.incipit = "Ἓν καὶ τρία ἀνυμνῶ, εὐσεβῶς…"
trMA.Ode9C2Tr.text = "Ἓν καὶ τρία ἀνυμνῶ, εὐσεβῶς τὴν Θεότητα, τῇ μορφὴ τοῖς προσώποις, συνάπτων καὶ διαιρῶν, ὁ Πατήρ, ὁ Υἱός, καὶ τὸ ἅγιον Πνεῦμα· ταῦτα Ἓν εἰσι."

trMA.Ode9C2T.ode = miscellanea_gr_GR_cog.misc.Ode9
trMA.Ode9C2T.mode = miscellanea_gr_GR_cog.misc.Mode3
trMA.Ode9C2T.melody = he.h.m3_gr_GR_cog.heHE.SeTinAthanaton.name
trMA.Ode9C2T.incipit = "Σὲ τὴν ἐκ ῥίζης Ἱεσσαί…"
trMA.Ode9C2T.text = "Σὲ τὴν ἐκ ῥίζης Ἱεσσαί, καὶ Δαυῒδ τοῦ προπάτορος, ἐκβλαστήσασαν ῥάβδον, πανάχραντε Ἁγνὴ μεγαλύνομεν, ὅτι σῴζεις τὰς ψυχὰς ἡμῶν."

trMA.Ode9C2A1.ode = miscellanea_gr_GR_cog.misc.Ode9
trMA.Ode9C2A1.mode = miscellanea_gr_GR_cog.misc.Mode3
trMA.Ode9C2A1.melody = he.h.m3_gr_GR_cog.heHE.SeTinAthanaton.name
trMA.Ode9C2A1.incipit = "Πληθὺς Μαρτύρων ἡ σεπτή, τὸν…"
trMA.Ode9C2A1.text = "Πληθὺς Μαρτύρων ἡ σεπτή, τὸν Χριστὸν ἐκδυσώπησον, ἐν εἰρήνῃ πληροῦντας τὸν δρόμον τῶν Νηστειῶν, κατιδεῖν ἡμᾶς, καὶ προσκυνῆσαι καὶ τὰ Πάθη αὐτοῦ."

trMA.Ode9C2A2.ode = miscellanea_gr_GR_cog.misc.Ode9
trMA.Ode9C2A2.mode = miscellanea_gr_GR_cog.misc.Mode3
trMA.Ode9C2A2.melody = he.h.m3_gr_GR_cog.heHE.SeTinAthanaton.name
trMA.Ode9C2A2.incipit = "Νεκρῶν καὶ ζώντων ὡς Θεός…"
trMA.Ode9C2A2.text = "Νεκρῶν καὶ ζώντων ὡς Θεός, ὁ νεκρώσας τὸν θάνατον, καὶ ζωὴν τῇ Ἐγέρσει σου πᾶσι παρασχών, Χριστὲ ἀνάπαυσον, καὶ τοὺς δούλους σου, οὓς μετέστησας."

