meVE.SticStavrotheotokion.text = me.m01.d15_gr_GR_cog.meVE.AposStavrotheotokion.text

meVE.AposGlory.poet = poets_gr_GR_cog.po.IoannouMonachou
meVE.AposGlory.mode = miscellanea_gr_GR_cog.misc.Mode6
meVE.AposGlory.melody = ""
meVE.AposGlory.incipit = "Ἐν τῷ Σταυρῷ παρεστηκώς, καὶ…"
meVE.AposGlory.text = "Ἐν τῷ Σταυρῷ παρεστηκώς, καὶ τὰ γενόμενα σκοπῶν, τὸν σταυρωθέντα ἐπὶ ξύλου, Θεὸν καὶ ἄνθρωπον ἑωρακώς, ἐβόας πρὸς αὐτόν· Ἐν τῇ βασιλείᾳ σου μνήσθητί μου Κύριε. Διὸ καὶ ὁ Σωτὴρ ἐφώνει σοι· Μακάριος εἶ Λογγῖνε, καὶ τὸ μνημόσυνόν σου εἰς γενεὰν καὶ γενεάν."

meVE.AposTheotokion.mode = miscellanea_gr_GR_cog.misc.Mode6
meVE.AposTheotokion.greekmelody = he.a.m6_gr_GR_cog.heAU.TriimerosAnestis.greekname
