ocMA.Kathisma13.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.Kathisma13.melody = he.a.m8_gr_GR_cog.heAU.ToProstachthen.name
ocMA.Kathisma13.incipit = "Τὸν ἐξ αἱμάτων σου ἁγνῶν…"
ocMA.Kathisma13.text = "Τὸν ἐξ αἱμάτων σου ἁγνῶν σωματωθέντα, καὶ ὑπὲρ ἔννοιαν ἐκ σοῦ, σεμνὴ τεχθέντα, ἐπὶ ξύλου κρεμάμενον, μέσον τῶν κακούργων, ὁρῶσα, τὰ σπλάγχνα ἤλγεις, καὶ μητρικῶς θρηνοῦσα, ἐβόας· Οἴμοι! Τέκνον ἐμόν, τίς ἡ θεία καὶ ἄφατος, οἰκονομία σου διʼ ἧς, ἐζώωσας τὸ πλάσμα σου! Ἀνυμνῶ σου τὸ εὔσπλαγχνον."

ocMA.Kathisma21.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.Kathisma21.melody = he.a.m8_gr_GR_cog.heAU.AnestisEkNekron.name
ocMA.Kathisma21.incipit = oc.m8.d4_gr_GR_cog.ocMA.Kathisma21.incipit
ocMA.Kathisma21.text = oc.m8.d4_gr_GR_cog.ocMA.Kathisma21.text

ocMA.Kathisma21b.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.Kathisma21b.melody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.name
ocMA.Kathisma21b.incipit = "Ὁ Σταυρὸν ὑπομείνας, ὡς ἀγαθός…"
ocMA.Kathisma21b.text = "Ὁ Σταυρὸν ὑπομείνας, ὡς ἀγαθός, τοῦ Ἀδὰμ τὴν πτωχείαν, ἀναλαβών, μόνε Ὑπεράγαθε, ἀναμάρτητε Κύριε, τοῦ θανάτου εὔσπλαγχνε, τὸ κράτος κατέλυσας, καὶ τῷ πάθει σου Δέσποτα, τὸν κόσμον διέσωσας· ὅθεν Ζωοδότα, τὴν πεσοῦσαν ψυχήν μου, οἰκτείρησον δέομαι, ὅταν κρίνῃς τὰ ἔργα μου, Δικαιοκρῖτα φιλάνθρωπε, καὶ δός μοι πανοικτίρμον ὁ Θεός, τῶν πταισμάτων ἄφεσιν ὡς εὔσπλαγχνος, ὅτι μόνος ὑπάρχεις ἀναμάρτητος."

ocMA.Kathisma22.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.Kathisma22.melody = he.a.m8_gr_GR_cog.heAU.ToProstachthen.name
ocMA.Kathisma22.incipit = oc.m8.d4_gr_GR_cog.ocMA.Kathisma22.incipit
ocMA.Kathisma22.text = oc.m8.d4_gr_GR_cog.ocMA.Kathisma22.text

ocMA.Kathisma22b.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.Kathisma22b.melody = he.a.m8_gr_GR_cog.heAU.AnestisEkNekron.name
ocMA.Kathisma22b.incipit = oc.m8.d4_gr_GR_cog.ocMA.Kathisma21.incipit
ocMA.Kathisma22b.text = oc.m8.d4_gr_GR_cog.ocMA.Kathisma21.text

ocMA.Kathisma23.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.Kathisma23.melody = he.a.m8_gr_GR_cog.heAU.ToProstachthen.name
ocMA.Kathisma23.incipit = "Ἐκ τῶν αἱμάτων τῶν ἐμῶν…"
ocMA.Kathisma23.text = "Ἐκ τῶν αἱμάτων τῶν ἐμῶν, βροτὸς γεγέννησαι, ἐκ τῶν αἱμάτων δὲ τῶν σῶν, βροτοὺς ἐθέωσας, τοὺς βρώσει δὲ φθαρέντας, ζητῶν κατῆλθες, ὡς ὑπεράγαθος, ἐβόα πάλαι ἡ ἄμεμπτος, ἐν κλαυθμῷ, Χριστὸν θρηνοῦσα κρεμάμενον, ἐν Σταυρῷ, ὃν ὁρῶσα νεκρούμενον, δακρύων ἔῤῥει κρουνούς, τὰς ὄψεις καταξαίνουσα, καὶ αὐτὸν μεγαλύνουσα."

ocMA.Kathisma31.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.Kathisma31.melody = he.a.m8_gr_GR_cog.heAU.AnestisEkNekron.name
ocMA.Kathisma31.incipit = "Θεώσεως ψευσθείς, ὁ Γενάρχης ἐλπίδος…"
ocMA.Kathisma31.text = "Θεώσεως ψευσθείς, ὁ Γενάρχης ἐλπίδος, φθορᾶς τοῖς ἐξ αὐτοῦ, πᾶσιν αἴτιος ὤφθη. Σὺ δέ διὰ Σταυροῦ σου, ζωὴν πηγάζεις ὡς Ὑπεράγαθος· θέλων καὶ γὰρ ἐπάγης, ἵνα τὸ πρῶτον λύσῃς κατάκριμα· διὸ ὑμνοῦμέν σου Χριστέ, τὸ πάθος τὸ ἑκούσιον."

ocMA.Kathisma32.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.Kathisma32.melody = ""
ocMA.Kathisma32.incipit = "Φωτὶ οὐρανίῳ καταλάμπεται, σήμερον αὕτη…"
ocMA.Kathisma32.text = "Φωτὶ οὐρανίῳ καταλάμπεται, σήμερον αὕτη ἡ σκηνή· ἐν αὐτῇ γὰρ στρατιαὶ Ἀγγέλων ἀγάλλονται, σὺν αὐταῖς καὶ Μαρτύρων χοροὶ εὐφραίνονται, ἐπὶ τῇ μνήμῃ τῶν Ἀθλοφόρων. Αὐτῶν ταῖς παρακλήσεσι, κατάπεμψον τῷ κόσμῳ σου, τὴν εἰρήνην Χριστέ, καὶ τὸ μέγα ἔλεος."

ocMA.Kathisma33.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.Kathisma33.melody = he.a.m8_gr_GR_cog.heAU.AnestisEkNekron.name
ocMA.Kathisma33.incipit = "Ὢ θαύματος καινοῦ! ὢ φρικτοῦ…"
ocMA.Kathisma33.text = "Ὢ θαύματος καινοῦ! ὢ φρικτοῦ Μυστηρίου! ἐβόα ἡ Ἀμνάς, τὸν Υἱὸν καθορῶσα, ξύλῳ Σταυροῦ παγέντα. Τί τοῦτο Λόγε Θεοῦ ἀθάνατε; πῶς νεκρὸς ὁρᾶσαι, καὶ γῆν σαλεύεις ὡς παντοδύναμος; ἀλλὰ ὑμνῶ σου τὴν φρικτήν, καὶ θείαν συγκατάβασιν."

ocMA.C1.acrostic.title = "Κανὼν Σταυρώσιμος,"
ocMA.C1.acrostic.notice1 = "οὗ ἡ ἀκροστιχίς·"
ocMA.C1.acrostic.text = "Πλήρωμα πάντων τῶν καλῶν Σταυρὸς πέλει, Ἀμήν. Ἰωσήφ."

ocMA.CA.notice = ""

