meVE.Stichera01.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.Stichera01.greekmelody = he.a.m4_gr_GR_cog.heAU.EdokasSimeiosin.greekname
meVE.Stichera01.melody = he.a.m4_gr_GR_cog.heAU.EdokasSimeiosin.name
meVE.Stichera01.incipit = "Σήμερον ὑπέδειξας, τὴν σην Θεότητα…"
meVE.Stichera01.text = "Σήμερον ὑπέδειξας, τὴν σην Θεότητα Κύριε, τοῖς σεπτοῖς Ἀποστόλοις σου, Μωσεῖ σὺν Ἠλίᾳ τε, ὡς Θεὸς τοῦ νόμου, καὶ χάριτος πέλων, ὁ λυτρωσάμενος ἡμᾶς, ἐκ τοῦ θανάτου νῦν τῆς χειρώσεως, μεθʼ ὧν σου τὴν φιλάνθρωπον, οἰκονομίαν δοξάζομεν, Ἰησοῦ παντοδύναμε, ὁ Σωτὴρ τῶν ψυχῶν ἡμῶν."

meVE.Stichera02.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.Stichera02.melody = he.a.m4_gr_GR_cog.heAU.EdokasSimeiosin.name
meVE.Stichera02.incipit = "Σήμερον ἐξέλαμψας, φωτοειδὴς ὑπὲρ ἥλιον…"
meVE.Stichera02.text = "Σήμερον ἐξέλαμψας, φωτοειδὴς ὑπὲρ ἥλιον, τηλαυγῶς ἐν ὄρει Θαβώρ, δεικνύων τοῖς φίλοις σου, ὅτι σὺ ὑπάρχεις, ἀπαύγασμα δόξης, ὁ τὴν οὐσίαν τῶν βροτῶν, διʼ εὐσπλαγχνίαν φορέσας Κύριε· διό σου τὴν φιλάνθρωπον, οἰκονομίαν δοξάζομεν, Ἰησοῦ παντοδύναμε, ὁ Σωτὴρ τῶν ψυχῶν ἡμῶν."

meVE.Stichera03.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.Stichera03.melody = he.a.m4_gr_GR_cog.heAU.EdokasSimeiosin.name
meVE.Stichera03.incipit = "Σήμερον ἀγάλλονται, οἱ ἐπουράνιοι Ἄγγελοι…"
meVE.Stichera03.text = "Σήμερον ἀγάλλονται, οἱ ἐπουράνιοι Ἄγγελοι, σὺν βροτοῖς ἑορτάζοντες, Χριστέ σου τὴν ἔλλαμψιν, τὴν φρικτὴν καὶ θείαν, τὴν ἐν Θαβωρίῳ ἐν ᾧ παρέστησας Μωσῆν, καὶ τὸν Ἠλίαν Σωτὴρ φιλάνθρωπε, καὶ Πέτρον καὶ Ἰάκωβον, καὶ Ἰωάννην ὑμνοῦντάς σε, Ἰησοῦ Παντοδύναμε, ὁ Σωτὴρ καὶ Θεὸς ἡμῶν."

meVE.Stichera04.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.Stichera04.greekmelody = he.a.m4_gr_GR_cog.heAU.OExYpsistouKlitheis.greekname
meVE.Stichera04.melody = he.a.m4_gr_GR_cog.heAU.OExYpsistouKlitheis.name
meVE.Stichera04.incipit = "Ὁ τὴν Εἰκόνα Χριστοῦ σαφῶς…"
meVE.Stichera04.text = "Ὁ τὴν Εἰκόνα Χριστοῦ σαφῶς τιμήσας, ὅτε τὸ λεόντειον θράσος ἐμαίνετο, καὶ τοὺς λαοὺς διεκλόνησεν, ἀνακαινίζον, τοῦ Κοπρωνύμου ἄθεον αἵρεσιν, τότε τοῦτο Πάνσοφε, σαφῶς διήλεγξας, σῇ παῤῥησίᾳ χρησάμενος, συνηθροισμένης, Ἀρχιερέων τῆς συνελεύσεως· ὅθεν ἐγνώσθης γενναιότατος, στρατιώτης Χριστοῦ παμμακάριστε, ὃν ἱκέτευε σῶσαι, καὶ φωτίσαι τὰς ψυχὰς ἡμῶν."

meVE.Stichera05.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.Stichera05.melody = he.a.m4_gr_GR_cog.heAU.OExYpsistouKlitheis.name
meVE.Stichera05.incipit = "Ὁ οὐρανίαις μοναῖς ἐνῳκισμένος, ὅτε…"
meVE.Stichera05.text = "Ὁ οὐρανίαις μοναῖς ἐνῳκισμένος, ὅτε τὴν ἀτίθασον γνώμην καὶ ἄθεον, ὁ δυσσεβὴς ἐνεδείξατο, ὑπερορίαις, καταδικάσας τὴν σὴν στεῤῥότητα, τότε τὸν Παράδεισον· ὅθεν πεπτώκαμεν μόνην πατρίδα θεσπέσιε, σαφῶς γινώσκων, μέτʼ εὐφροσύνης χαίρων ὑπέμεινας· ὅθεν δικαίως τὴν ἀκήρατον, ἠξιώθης ζωὴν κατοπτεῦσαι Χριστόν, ὃν ἱκέτευε σῶσαι, καὶ φωτίσαι τὰς ψυχὰς ἡμῶν."

meVE.Stichera06.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.Stichera06.melody = he.a.m4_gr_GR_cog.heAU.OExYpsistouKlitheis.name
meVE.Stichera06.incipit = "Πυρίνῃ γλώσσῃ πανσόφως κεχρημένος, ὅτε…"
meVE.Stichera06.text = "Πυρίνῃ γλώσσῃ πανσόφως κεχρημένος, ὅτε μιξοβάρβαρον νόμον ἐθέσπισε, μὴ προσκυνεῖσθαι κελεύοντα, τὴν σεβασμίαν, Χριστοῦ Εἰκόνα Λέων ὁ τύραννος, τότε σὺ τὸν ἔνθεον νόμον ἀντέθηκας, ὡς διαβαίνει προσκύνησις, ἡ τῆς εἰκόνος, ὣς πού τις ἔφη πρὸς τὸ ἀρχέτυπον, καὶ τῷ σῷ λόγῳ ὡς ἐμβρόντητος, ἀπεφάνθη ὁ θὴρ ὁ δυσώνυμος, πανσεβάσμιε Πάτερ, θεοῤῥῆμον ἱερώτατε."

meVE.SticGlory.mode = miscellanea_gr_GR_cog.misc.Mode1
meVE.SticGlory.melody = ""
meVE.SticGlory.incipit = me.m08.d06_gr_GR_cog.meVE.Aposticha1.incipit
meVE.SticGlory.text = me.m08.d06_gr_GR_cog.meVE.Aposticha1.text

meVE.Aposticha1.mode = miscellanea_gr_GR_cog.misc.Mode6
meVE.Aposticha1.greekmelody = he.a.m6_gr_GR_cog.heAU.TriimerosAnestis.greekname
meVE.Aposticha1.melody = he.a.m6_gr_GR_cog.heAU.TriimerosAnestis.name
meVE.Aposticha1.incipit = "Μορφούμενος διʼ οἶκτον Χριστέ, Ἀδὰμ…"
meVE.Aposticha1.text = "Μορφούμενος διʼ οἶκτον Χριστέ, Ἀδὰμ τὸν πρῶτον ἄνθρωπον, ἐκ Παρθένου, ὤφθης δεύτερος, Ἀδάμ, ἐν ὄρει δὲ Σωτήρ μου, Θαβὼρ μετεμορφώθης, παραγυμνῶν σου τὴν Θεότητα."

meVE.Aposticha2.mode = miscellanea_gr_GR_cog.misc.Mode6
meVE.Aposticha2.melody = he.a.m6_gr_GR_cog.heAU.TriimerosAnestis.name
meVE.Aposticha2.incipit = "Ἐξέστησαν ὁρῶντες τὴν σήν, τῆς…"
meVE.Aposticha2.text = "Ἐξέστησαν ὁρῶντες τὴν σήν, τῆς χάριτος οἱ πρόκριτοι, καὶ τοῦ νόμου Μεταμόρφωσιν Χριστέ, ἐν ὄρει Θαβωρίῳ, μεθʼ ὧν σε προσκυνοῦμεν, σὺν τῷ Πατρί σου καὶ τῷ Πνεύματι."

meVE.Aposticha3.mode = miscellanea_gr_GR_cog.misc.Mode6
meVE.Aposticha3.melody = he.a.m6_gr_GR_cog.heAU.TriimerosAnestis.name
meVE.Aposticha3.incipit = "Λαμπρύνεται ἡ κτίσις φαιδρῶς, τῇ…"
meVE.Aposticha3.text = "Λαμπρύνεται ἡ κτίσις φαιδρῶς, τῇ σῇ Μεταμορφώσει Χριστέ, ἣν τοῖς θείοις Ἀποστόλοις ἐν Θαβώρ, Μωσεῖ τε καὶ Ἠλίᾳ, ὑπέδειξας Θεὸς ὤν, ἀῤῥήτως λάμψας ὑπὲρ ἥλιον."

meVE.AposGlory.mode = miscellanea_gr_GR_cog.misc.Mode1
meVE.AposGlory.melody = ""
meVE.AposGlory.incipit = me.m08.d06_gr_GR_cog.meVE.Aposticha2.incipit
meVE.AposGlory.text = me.m08.d06_gr_GR_cog.meVE.Aposticha2.text

meVE.Apolytikion1ger.poet = poets_gr_GR_cog.po.Gerasimou
meVE.Apolytikion1ger.mode = miscellanea_gr_GR_cog.misc.Mode1
meVE.Apolytikion1ger.greekmelody = he.a.m1_gr_GR_cog.heAU.TisErimouPolitis.greekname
meVE.Apolytikion1ger.melody = he.a.m1_gr_GR_cog.heAU.TisErimouPolitis.name
meVE.Apolytikion1ger.incipit = "Τῆς τοῦ Λόγου είκόνος διαγράψας…"
meVE.Apolytikion1ger.text = "Τῆς τοῦ Λόγου είκόνος διαγράψας τὴν ἔλλαμψιν, Αἰμιλιανὲ ἱεράρχα διὰ βίου ὀρθότητος, τὴν ἔνσωμον Εἰκόνα τοῦ Χριστοῦ, ἐδίδαξας τιμᾶσθαι εὐσεβῶς· διὰ τοῦτο ὡς ποιμένα καὶ ἀθλητήν, τιμῶμέν σε κραυγάζοντες· δόξα τῷ παρασχόντι σοι ἰσχήν, δόξα τῷ σὲ στεφανώσαντι, δόξα τῷ δωρουμένῳ διὰ σοῦ, πᾶσι τὴν ἄφεσιν."

meMA.Kathisma11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Kathisma11.greekmelody = he.a.m4_gr_GR_cog.heAU.KateplagiIosif.greekname
meMA.Kathisma11.melody = he.a.m4_gr_GR_cog.heAU.KateplagiIosif.name
meMA.Kathisma11.incipit = me.m08.d06_gr_GR_cog.meMA.Kathisma11.incipit
meMA.Kathisma11.text = me.m08.d06_gr_GR_cog.meMA.Kathisma11.text

meMA.Kathisma21.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Kathisma21.greekmelody = he.a.m4_gr_GR_cog.heAU.KateplagiIosif.greekname
meMA.Kathisma21.melody = he.a.m4_gr_GR_cog.heAU.KateplagiIosif.name
meMA.Kathisma21.incipit = "Ἐν τῷ ὄρει τῷ Θαβώρ…"
meMA.Kathisma21.text = "Ἐν τῷ ὄρει τῷ Θαβώρ, μεταμορφούμενος Σωτήρ, τοῖς προκρίτοις Μαθηταῖς, δείξας τὴν δόξαν σου Χριστέ, τὸ ἀναλλοίωτον ἤστραψας τῆς Θεότητος. Νεφέλῃ δὲ φωτὸς συνεκάλεσας, Ἠλίαν καὶ Μωσῆν συλλαλοῦντάς σοι· διὸ καὶ Πέτρος ἔλεγεν· Οἰκτίρμον, καλόν ἐστιν ὧδε εἶναι σὺν σοί. Ὁ τότε τούτοις, τὸ φῶς σου λάμψας, φώτισον τὰς ψυχὰς ἡμῶν."

meMA.C1.acrostic.title = "Ὁ Κανὼν τοῦ Ἁγίου,"
meMA.C1.acrostic.notice1 = "οὗ ἡ Ἀκροστιχίς·"
meMA.C1.acrostic.text = "Τὸν μέγαν Αἰμιλιανὸν ἐν ᾄσμασιν ἔξοχα μέλπω."
meMA.C1.poet = poets_gr_GR_cog.po.Theofanous
meMA.C1.title = titles_gr_GR_cog.ti.CanonSaintM
meMA.C1.verse = prayers_gr_GR_cog.pr.verse_SaintM.text

meMA.Ode1C11.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C11.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode1C11.greekmelody = he.h.m1_gr_GR_cog.heHE.SouITropaiouchos.greekname
meMA.Ode1C11.melody = he.h.m1_gr_GR_cog.heHE.SouITropaiouchos.name
meMA.Ode1C11.incipit = "Ταῖς ὑπερκοσμίαις συνοικῶν, ταξιαρχίαις θεόφρον…"
meMA.Ode1C11.text = "Ταῖς ὑπερκοσμίαις συνοικῶν, ταξιαρχίαις θεόφρον μακάριε, τοὺς ἐπιτελοῦντάς σου, χαρμονικῶς τὴν ποθητὴν πανήγυριν, Πάτερ ἐποπτεύων, τῆς σωτηρίας ἀξίωσον."

meMA.Ode1C12.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C12.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode1C12.melody = he.h.m1_gr_GR_cog.heHE.SouITropaiouchos.name
meMA.Ode1C12.incipit = "Ὅλος τῷ Δεσπότῃ συγκραθείς, καὶ…"
meMA.Ode1C12.text = "Ὅλος τῷ Δεσπότῃ συγκραθείς, καὶ τοῖς ἐκεῖθεν ἀστράψας πυρσεύμασι, πᾶσαν κατεφώτισας, σαῖς διδαχαῖς θεοσεβῆ πανήγυριν, ὡς ἱεροφάντωρ, φανεὶς θεόφρον θεόληπττε."

meMA.Ode1C13.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C13.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode1C13.melody = he.h.m1_gr_GR_cog.heHE.SouITropaiouchos.name
meMA.Ode1C13.incipit = "Νοῦν καθηγεμόνα τῶν παθῶν, δι’…"
meMA.Ode1C13.text = "Νοῦν καθηγεμόνα τῶν παθῶν, διʼ ἀρετῆς Ἱεράρχα κατέστησας, οἷα δικαιότατος, διαιτητὴς τῇ τε ψυχῇ καὶ τῷ σώματι, Πάτερ διανέμων, τοῖς ἑκατέροις τὰ πρόσφορα."

meMA.Ode1C14.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C14.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode1C14.melody = he.h.m1_gr_GR_cog.heHE.SouITropaiouchos.name
meMA.Ode1C14.incipit = "Μύστης ἀκριβὴς τῶν ὑπὲρ νοῦν…"
meMA.Ode1C14.text = "Μύστης ἀκριβὴς τῶν ὑπὲρ νοῦν, τῆς Ἐκκλησίας σαφῶς προϊστάμενος, ὤφθης παναοίδιμε, καὶ κραταιὸς τῆς ἀληθείας πρόμαχος, μύλας τῶν λεόντων, συνθλῶν καὶ φράττων τὰ στόματα."

meMA.Ode1C1T.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C1T.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode1C1T.melody = he.h.m1_gr_GR_cog.heHE.SouITropaiouchos.name
meMA.Ode1C1T.incipit = "Ἕνα ἑκατέραις εὐσεβῶς, τὸν ἐκ…"
meMA.Ode1C1T.text = "Ἕνα ἑκατέραις εὐσεβῶς, τὸν ἐκ γαστρός σου τεχθέντα γινώσκομεν, φύσεσι Πανάμωμε, Θεὸν ὁμοῦ ὑπερφυῶς καὶ ἄνθρωπον, ἔχοντα τελείως ἑκάστης τὰ ἰδιώματα."

meMA.Ode3C11.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C11.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode3C11.greekmelody = he.h.m1_gr_GR_cog.heHE.OMonosEidos.greekname
meMA.Ode3C11.melody = he.h.m1_gr_GR_cog.heHE.OMonosEidos.name
meMA.Ode3C11.incipit = "Ἱκέτην θεόφρονα Χριστοῦ, πιστοῖς σε…"
meMA.Ode3C11.text = "Ἱκέτην θεόφρονα Χριστοῦ, πιστοῖς σε προβαλλόμεθα, τὸν διʼ αὐτὸν λαμπρῶς ἀριστεύσαντα, καὶ τὴν εἰκόνα τούτου τιμήσαντα, τοῦ σεπτοῦ κηρύγματος, καὶ τὴν ἄνωθεν πίστιν φυλάξαντα."

meMA.Ode3C12.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C12.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode3C12.melody = he.h.m1_gr_GR_cog.heHE.OMonosEidos.name
meMA.Ode3C12.incipit = "Μαρτύρων στεῤῥότητα δεικνύς, θεόφρον ἀξιάγαστε…"
meMA.Ode3C12.text = "Μαρτύρων στεῤῥότητα δεικνύς, θεόφρον ἀξιάγαστε, τὴν δυσσεβῆ ὀφρὺν ἐξεφαύλισας, τῆς Ἐκκλησίας ἐκφανεὶς πρόβολος, ἀῤῥαγὴς ἀκλόνητος, καὶ μεσίτης ἔνθεος, καρτερῶς διελέγχων τοὺς ἄφρονας."

meMA.Ode3C13.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C13.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode3C13.melody = he.h.m1_gr_GR_cog.heHE.OMonosEidos.name
meMA.Ode3C13.incipit = "Ἰὸν τῆς αἱρέσεως πιών, εἰς…"
meMA.Ode3C13.text = "Ἰὸν τῆς αἱρέσεως πιών, εἰς κόρον ὁ παμπόνηρος, διδακτικῷ σου λόγῳ διήλεγκται· φωτὶ γὰρ σκότος ἐξαφανίζεται, καὶ γυμνοῦται πλάσματα, πονηροῦ βουλεύματος τῆς ἐνθέου σοφίας ταῖς λάμψεσι."

meMA.Ode3C1T.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C1T.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode3C1T.melody = he.h.m1_gr_GR_cog.heHE.OMonosEidos.name
meMA.Ode3C1T.incipit = "Λαμπρᾷ σε φωνῇ νῦν ἐκ…"
meMA.Ode3C1T.text = "Λαμπρᾷ σε φωνῇ νῦν ἐκ ψυχῆς, κυρίως Θεομήτορα, τὴν τὸν Θεὸν τεκοῦσαν κηρύττομεν, σεσαρκωμένον ἐκ σοῦ τὸν ἄσαρκον, ἐν ἡμῖν σκηνώσαντᾳ, οὐ τροπὴν δεξάμενον, οὐ φυρμῷ συγχυθέντα Πανάμωμε."

meMA.Ode3H.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3H.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode3H.greekmelody = he.h.m1_gr_GR_cog.heHE.OMonosEidos.greekname
