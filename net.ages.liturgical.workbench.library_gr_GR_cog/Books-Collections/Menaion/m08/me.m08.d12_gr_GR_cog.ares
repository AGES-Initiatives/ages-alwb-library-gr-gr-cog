meMA.Ode1C11.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode1C11.greekmelody = he.h.m4_gr_GR_cog.heHE.ThalassisToErythraion.greekname
meMA.Ode1C11.melody = he.h.m4_gr_GR_cog.heHE.ThalassisToErythraion.name
meMA.Ode1C11.incipit = "Τοῖς πόνοις τοῖς τῆς σαρκὸς…"
meMA.Ode1C11.text = "Τοῖς πόνοις τοῖς τῆς σαρκὸς πανεύφημοι, ἐγκαρτερήσαντες, εὐσθενεστάτῃ πίστει λογισμοῦ, πρὸς τὴν ἄπονον χαίροντες, ζωὴν μετεβιβάσθητε, ὑπερ ἡμῶν ἀεὶ πρεσβεύοντες."

meMA.Ode1C12.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode1C12.melody = he.h.m4_gr_GR_cog.heHE.ThalassisToErythraion.name
meMA.Ode1C12.incipit = "Ἐνθέοις ὀχυρωθεὶς Ἀνίκητε, ὅπλοις τῆς…"
meMA.Ode1C12.text = "Ἐνθέοις ὀχυρωθεὶς Ἀνίκητε, ὅπλοις τῆς πίστεως, πρὸς συμπλοκὴν ἀσάρκων δυσμενῶν, ῥωμαλέως ἐχώρησας, καὶ τὰ αὐτῶν στρατεύματα, θείᾳ δυνάμει ἐξηφάνισας."

meMA.Ode1C13.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode1C13.melody = he.h.m4_gr_GR_cog.heHE.ThalassisToErythraion.name
meMA.Ode1C13.incipit = "Ῥητόρων ἀδολεσχίαν ἔνδοξε, καὶ νοῦν…"
meMA.Ode1C13.text = "Ῥητόρων ἀδολεσχίαν ἔνδοξε, καὶ νοῦν ἀδόκιμον, Ἑλληνιστῶν φρονήσει θεϊκῇ, λαμπρυνόμενος ᾔσχυνας, καὶ σεαυτὸν Ἀνίκητε, ὅλον πρὸς ἄθλησιν ἐκδέδωκας."

meMA.Ode1C1T.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode1C1T.melody = he.h.m4_gr_GR_cog.heHE.ThalassisToErythraion.name
meMA.Ode1C1T.incipit = "Παρθένος καὶ πρὸ τόκου Ἄχραντε…"
meMA.Ode1C1T.text = "Παρθένος καὶ πρὸ τόκου Ἄχραντε, καὶ μετὰ γέννησιν, ὡς ἀληθῶς ἐδείχθης, τὸν Θεόν· ὑπὲρ λόγον γὰρ ἔτεκες, τὸν τοὺς Ἁγίους μάρτυρας, ἐνηθληκότας στεφανώσαντα."

meMA.Ode1C21.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C21.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C21.greekmelody = he.h.m8_gr_GR_cog.heHE.AsomenToKyrio.greekname
meMA.Ode1C21.melody = he.h.m8_gr_GR_cog.heHE.AsomenToKyrio.name
meMA.Ode1C21.incipit = "Ὅλην μοι τὴν θεόπνουν, ἐκ…"
meMA.Ode1C21.text = "Ὅλην μοι τὴν θεόπνουν, ἐκ τῆς μελιῤῥύτου καὶ ἡδείας σου, ἐναπόσταξον γλώττης, θεῖε Μάξιμε χάριν τοῦ Πνεύματος."

meMA.Ode1C22.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C22.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C22.melody = he.h.m8_gr_GR_cog.heHE.AsomenToKyrio.name
meMA.Ode1C22.incipit = "Πῦρ καιόμενον ὤφθης, κατὰ τῶν…"
meMA.Ode1C22.text = "Πῦρ καιόμενον ὤφθης, κατὰ τῶν αἱρέσεων μακάριε· ὡς καλάμην γὰρ ταύτας, κατηνάλωσας ζήλῳ τοῦ Πνεύματος."

meMA.Ode1C23.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C23.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C23.melody = he.h.m8_gr_GR_cog.heHE.AsomenToKyrio.name
meMA.Ode1C23.incipit = "Ἄλογον προετέθη, ἐκ δυσσεβεστάτης προαιρέσεως…"
meMA.Ode1C23.text = "Ἄλογον προετέθη, ἐκ δυσσεβεστάτης προαιρέσεως, μονοθέλητον δόγμα, τοῖς δὲ λόγοις σου Πάτερ διήλεγκται."

meMA.Ode1C2T.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C2T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C2T.melody = he.h.m8_gr_GR_cog.heHE.AsomenToKyrio.name
meMA.Ode1C2T.incipit = "Ἄχραντε Θεοτόκε, ἡ σεσαρκωμένον τὸν…"
meMA.Ode1C2T.text = "Ἄχραντε Θεοτόκε, ἡ σεσαρκωμένον τὸν ἀΐδιον, καὶ ὑπέρθεον Λόγον, ὑπὲρ φύσιν τεκοῦσα ὑμνοῦμέν σε."

meMA.Ode3C11.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C11.greekmelody = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.greekname
meMA.Ode3C11.melody = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.name
meMA.Ode3C11.incipit = "Νικήσας μετὰ Χριστοῦ, τὰς παρατάξεις…"
meMA.Ode3C11.text = "Νικήσας μετὰ Χριστοῦ, τὰς παρατάξεις τοῦ ἐχθροῦ στέφανον, νικητικὸν ἔλαβες, Μάρτυς ἀθλοφόρε Ἀνίκητε."

meMA.Ode3C12.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C12.melody = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.name
meMA.Ode3C12.incipit = "Ἠλόγησαν τῆς σαρκός, ὡς φθειρομένης…"
meMA.Ode3C12.text = "Ἠλόγησαν τῆς σαρκός, ὡς φθειρομένης οἱ Χριστοῦ μάρτυρες, καὶ χαλεπὰ βάσανα, γνώμῃ ῥωμαλέᾳ ὑπήνεγκαν."

meMA.Ode3C13.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C13.melody = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.name
meMA.Ode3C13.incipit = "Ναμάτων ζωοποιῶν, πεπληρωμένοι θολερὰ ῥεύματα…"
meMA.Ode3C13.text = "Ναμάτων ζωοποιῶν, πεπληρωμένοι θολερὰ ῥεύματα, πλάνης σοφοὶ μάρτυρες, ῥεύμασιν αἱμάτων ἐπαύσατε."

meMA.Ode3C1T.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C1T.melody = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.name
meMA.Ode3C1T.incipit = "Ἐνῴκησεν ἐπὶ σοί, ὁ κατοικῶν…"
meMA.Ode3C1T.text = "Ἐνῴκησεν ἐπὶ σοί, ὁ κατοικῶν τοὺς οὐρανοὺς Κύριος, καὶ ἐξ ἡμῶν ἅπασαν, πλάνην Θεοτόκε ἐξῴκισεν."

meMA.Ode3C21.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C21.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C21.greekmelody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.greekname
meMA.Ode3C21.melody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.name
meMA.Ode3C21.incipit = "Μέγιστος ὁ Μάξιμος, τῆς εὐσεβοῦς…"
meMA.Ode3C21.text = "Μέγιστος ὁ Μάξιμος, τῆς εὐσεβοῦς Χριστοῦ πίστεως, παναληθής, κήρυξ τε καὶ μάρτυς, ἀνεδείχθη διʼ αἵματος."

meMA.Ode3C22.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C22.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C22.melody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.name
meMA.Ode3C22.incipit = "Μάξιμε μακάριε, σὺ διʼ ἀσκήσεως…"
meMA.Ode3C22.text = "Μάξιμε μακάριε, σὺ διʼ ἀσκήσεως γέγονας, θεοπρεπές, τῆς φιλοσοφίας, καὶ σοφὸν ἐνδιαίτημα."

meMA.Ode3C23.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C23.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C23.melody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.name
meMA.Ode3C23.incipit = "Ἔβλυσεν ἡ γλῶσσά σου, ποταμηδὸν…"
meMA.Ode3C23.text = "Ἔβλυσεν ἡ γλῶσσά σου, ποταμηδὸν Χριστοῦ δόγματα, πανευσεβῆ, Μάξιμε παμμάκαρ, τῆς σοφίας ὁ τρόφιμος."

meMA.Ode3C2T.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C2T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C2T.melody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.name
meMA.Ode3C2T.incipit = "Σὲ πάντες κεκτήμεθα, καταφυγὴν καὶ…"
meMA.Ode3C2T.text = "Σὲ πάντες κεκτήμεθα, καταφυγὴν καὶ τεῖχος ἡμῶν, Χριστιανοί, σὲ δοξολογοῦμεν, ἀσιγήτως Πανύμνητε."

meMA.Ode3H.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3H.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3H.greekmelody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.greekname
meMA.Ode3H.melody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.melody
meMA.Ode3H.incipit = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.incipit
meMA.Ode3H.text = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.text

meMA.Kontakion1.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Kontakion1.greekmelody = he.a.m6_gr_GR_cog.heAU.TinYperImon.greekname
meMA.Kontakion1.melody = he.a.m6_gr_GR_cog.heAU.TinYperImon.name
meMA.Kontakion1.incipit = "Φῶς τὸ τριλαμπές, οἰκῆσαν ἐν…"
meMA.Kontakion1.text = "Φῶς τὸ τριλαμπές, οἰκῆσαν ἐν τῇ ψυχῇ σου, σκεῦος ἐκλεκτόν, ἀνέδειξέ σε παμμάκαρ· σαφηνίζεις καὶ γὰρ τὰ θεῖα τοῖς πέρασι, δυσεφίκτων νοημάτων τε, τὴν δήλωσιν μακάριε, τὴν Τριάδα πᾶσι Μάξιμε, ἀνακηρύττων τρανῶς, ὑπερούσιον ἄναρχον."

meMA.Oikos1.text = "Τῶν ἱερέων ἡ κρηπίς, ἡ βάσις τῶν δογμάτων, ἡ σάλπιγξ τῆς σοφίας, Μαρτύρων ἡ ἀκρότης, καὶ τῶν πιστῶν ὁ στηριγμός, κόσμῳ προανίσχει Μάξιμος ὁ θαυμαστός, διὰ τῆς θείας αὐτοῦ μνήμης σήμερον. Τούτου οὖν πρὸς τὴν λάμψιν ἀνάστητε πάντες, τὴν παῤῥησίαν καὶ τὸν ζῆλον εὐφημοῦντες, ὃν ἔδειξεν ὑπὲρ εὐσεβείας, διʼ ἣν ἀληθῶς, ὡς ποιμήν ὑπὲρ ποίμνης, παρετάξατο στεῤῥῶς κατὰ τῶν λύκων· οὓς καταβαλών, βραβείοις νίκης, ἐστέφθη, τὴν Τριάδα πᾶσιν ἀνακηρύττων τρανῶς, ὑπερούσιον ἄναρχον."

meMA.Ode3Kathisma1.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3Kathisma1.greekmelody = he.a.m4_gr_GR_cog.heAU.OYpsotheisEnToStavro.greekname
meMA.Ode3Kathisma1.melody = he.a.m4_gr_GR_cog.heAU.OYpsotheisEnToStavro.name
meMA.Ode3Kathisma1.incipit = "Καταβαλόντες τοῦ ἐχθροῦ τὰς ἐπάρσεις…"
meMA.Ode3Kathisma1.text = "Καταβαλόντες τοῦ ἐχθροῦ τὰς ἐπάρσεις, τῇ καρτερίᾳ τῶν δεινῶν ἀθλοφόροι, τὸν οὐρανὸν ᾠκήσατε γηθόμενοι, Φώτιε ἀοίδιμε, καὶ Ἀνίκητε μάκαρ· ὅθεν μακαρίζεσθε, εἰς αἰῶνας αἰῶνος, ὑπὲρ ἡμῶν πρεσβεύοντες Χριστῷ, τῶν ἐκτελούντων ὑμῶν τὰ μνημόσυνα."

meMA.Ode3Kathisma2.mode = miscellanea_gr_GR_cog.misc.Mode3
meMA.Ode3Kathisma2.greekmelody = he.a.m3_gr_GR_cog.heAU.TinOraiotita.greekname
meMA.Ode3Kathisma2.melody = he.a.m3_gr_GR_cog.heAU.TinOraiotita.name
meMA.Ode3Kathisma2.incipit = "Τὰ κατορθώματα τῆς πολιτείας σου…"
meMA.Ode3Kathisma2.text = "Τὰ κατορθώματα τῆς πολιτείας σου, πάντων φωτίζουσι τὰ διαβήματα, Μάξιμε Πάτερ ἀληθῶς, τῶν πίστει μιμουμένων σε· ὅθεν τοὺς ἐκ πόθου σε, μακαρίζοντας πάντοτε, σῷζε ταῖς πρεσβείαις σου, ἀπὸ πάσης στενώσεως, Χριστὸν τὸν ἀγαθὸν ἱκετεύων, μάκαρ ἀξιοθαύμαστε."

meMA.Ode3Kathisma3.mode = miscellanea_gr_GR_cog.misc.Mode3
meMA.Ode3Kathisma3.greekmelody = he.a.m3_gr_GR_cog.heAU.TinOraiotita.greekname
meMA.Ode3Kathisma3.melody = he.a.m3_gr_GR_cog.heAU.TinOraiotita.name
meMA.Ode3Kathisma3.incipit = me.m08.d08_gr_GR_cog.meMA.Ode3Kathisma2.incipit
meMA.Ode3Kathisma3.text = me.m08.d08_gr_GR_cog.meMA.Ode3Kathisma2.text

meMA.Ode4C11.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C11.greekmelody = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.greekname
meMA.Ode4C11.melody = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.name
meMA.Ode4C11.incipit = "Πυρακτούμενοι ἀγάπῃ Θεοῦ προθύμως, φλόγα…"
meMA.Ode4C11.text = "Πυρακτούμενοι ἀγάπῃ Θεοῦ προθύμως, φλόγα πυρὸς ὑπήλθετε, μένοντες τῇ δρόσῳ ἄφλεκτοι τοῦ Πνεύματος, ἀήττητοι μάρτυρες, θεῖοι πρεσβευταὶ τῶν ψυχῶν ἡμῶν."

meMA.Ode4C12.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C12.melody = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.name
meMA.Ode4C12.incipit = "Ἀπηρνήσαντο τὸ σῶμα ποικιλοτρόπως, οἱ…"
meMA.Ode4C12.text = "Ἀπηρνήσαντο τὸ σῶμα ποικιλοτρόπως, οἱ Ἀθληταὶ ξεόμενοι· ὅλῳ τῷ νοῒ γάρ, τῷ Θεῷ ἠτένιζον, τῷ τούτοις παρέχοντι τὴν ὑπομονὴν διὰ πίστεως."

meMA.Ode4C13.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C13.melody = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.name
meMA.Ode4C13.incipit = "Ἰσχυρότατοι ὡς πύργοι τῆς εὐσεβείας…"
meMA.Ode4C13.text = "Ἰσχυρότατοι ὡς πύργοι τῆς εὐσεβείας, τείχη ἐχθροῦ καθείλετε, καὶ πόλεως θείας, μάρτυρες κραυγάζοντες, πολῖται γεγόνατε· Δόξα τῇ δυνάμει σου Κύριε."

meMA.Ode4C14.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C14.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C14.melody = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.name
meMA.Ode4C14.incipit = "Νυσταγμὸν οὐκ ἐνυστάξατε δυσσεβείας, ἀλλ’…"
meMA.Ode4C14.text = "Νυσταγμὸν οὐκ ἐνυστάξατε δυσσεβείας, ἀλλʼ ἐγρηγόρσει μάρτυρες θείᾳ, τοὺς ὑπνοῦντας, πάντοτε εἰς θάνατον, πρὸς φῶς ἐξεγείρετε, τῆς θεογνωσίας ἐν χάριτι."

meMA.Ode4C1T.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C1T.melody = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.name
meMA.Ode4C1T.incipit = "Ὡς ὡραία ἀπεκύησας τὸν ὡραῖον…"
meMA.Ode4C1T.text = "Ὡς ὡραία ἀπεκύησας τὸν ὡραῖον, Παρθενομῆτορ ἄχραντε, τὸν ὡραιοτάτους, δείξαντα τοὺς μάρτυρας, στεῤῥῶς ἐναθλήσαντας, καὶ τὴν ἀθεΐαν μειώσαντας."

meMA.Ode4C21.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C21.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C21.greekmelody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.greekname
meMA.Ode4C21.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
meMA.Ode4C21.incipit = "Γηγενεῖς σε γεραίρουσι, καὶ τῶν…"
meMA.Ode4C21.text = "Γηγενεῖς σε γεραίρουσι, καὶ τῶν οὐρανίων τάξεις θαυμάζουσι· τῇ φιλίᾳ τῆς σοφίας γάρ, ἀνεδείχθης Πάτερ ὥσπερ ἄσαρκος."

meMA.Ode4C22.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C22.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C22.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
meMA.Ode4C22.incipit = "Ἰταμὸς μὲν ὁ τύραννος, ἀλλ’…"
meMA.Ode4C22.text = "Ἰταμὸς μὲν ὁ τύραννος, ἀλλʼ ἡ καρτερία σου ἀκατάπληκτος· ὅθεν σὺ μὲν μεμακάρισαι, ἐκεῖνος δὲ Πάτερ ἐξωστράκισται."

meMA.Ode4C23.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C23.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C23.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
meMA.Ode4C23.incipit = "Συναθλοῦσί σοι Μάξιμε, ἡ τῶν…"
meMA.Ode4C23.text = "Συναθλοῦσί σοι Μάξιμε, ἡ τῶν μαθητῶν δυὰς ἡ πανόλβιος, σοὶ τῶν ἄθλων, κοινωνήσαντες· διὸ καὶ ἐπάθλων ἴσων ἔτυχον."

meMA.Ode4C24.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C24.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C24.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
meMA.Ode4C24.incipit = "Τῇ ῥοῇ σου τοῦ αἵματος…"
meMA.Ode4C24.text = "Τῇ ῥοῇ σου τοῦ αἵματος, καταρδευομένη ἡ Χριστοῦ Ἐκκλησία, ἐξανθεῖ πατροπαράδοτον, τοῦ θείου σου σπόρου δόγμα Ὅσιε."

meMA.Ode4C2T.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C2T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C2T.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
meMA.Ode4C2T.incipit = "Ἡ τὸν στάχυν βλαστήσασα, τὸν…"
meMA.Ode4C2T.text = "Ἡ τὸν στάχυν βλαστήσασα, τὸν ζωοποιὸν ἀνήροτος ἄρουρα, τὸν παρέχοντα τῷ κόσμῳ ζωήν, Θεοτόκε σῷζε τοὺς ὑμνοῦντάς σε."

meMA.Ode5C11.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C11.greekmelody = he.h.m4_gr_GR_cog.heHE.SyKyrieMouFos.greekname
meMA.Ode5C11.melody = he.h.m4_gr_GR_cog.heHE.SyKyrieMouFos.name
meMA.Ode5C11.incipit = "Μυρίοις αἰκισμοῖς, νεκρωθὲν τὸ σαρκίον…"
meMA.Ode5C11.text = "Μυρίοις αἰκισμοῖς, νεκρωθὲν τὸ σαρκίον σου, Ἀνίκητε τὴν ἀγήρω, καὶ ἀθάνατον δόξαν, παμμάκαρ προεξένησεν."

meMA.Ode5C12.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C12.melody = he.h.m4_gr_GR_cog.heHE.SyKyrieMouFos.name
meMA.Ode5C12.incipit = "Ἀνάλωτος πυρί, κατακαίοντι γέγονας, ὦ…"
meMA.Ode5C12.text = "Ἀνάλωτος πυρί, κατακαίοντι γέγονας, ὦ Φώτιε θείου φέγγους, κοινωνὲ καὶ ἡμέρας, ἀδύτου τέκνον γνήσιον."

meMA.Ode5C13.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C13.melody = he.h.m4_gr_GR_cog.heHE.SyKyrieMouFos.name
meMA.Ode5C13.incipit = "Ῥεόντων τὴν φθοράν, παντελῶς ἀπεκρούσαντο…"
meMA.Ode5C13.text = "Ῥεόντων τὴν φθοράν, παντελῶς ἀπεκρούσαντο, ποθήσαντες τῶν μενόντων, τὸ τερπνὸν ὁλοψύχως, οἱ Μάρτυρες οἱ ἔνδοξοι."

meMA.Ode5C1T.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C1T.melody = he.h.m4_gr_GR_cog.heHE.SyKyrieMouFos.name
meMA.Ode5C1T.incipit = "Τίς δύναται τὸ σόν, ἑρμηνεῦσαι…"
meMA.Ode5C1T.text = "Τίς δύναται τὸ σόν, ἑρμηνεῦσαι μυστήριον, ὦ Δέσποινα Θεοτόκε; ὑπέρ νοῦν γὰρ καὶ λόγον, Θεὸν ἀφράστως τέτοκας."

meMA.Ode5C21.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C21.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C21.greekmelody = he.h.m8_gr_GR_cog.heHE.OrthrizontesVoomenSoi.greekname
meMA.Ode5C21.melody = he.h.m8_gr_GR_cog.heHE.OrthrizontesVoomenSoi.name
meMA.Ode5C21.incipit = "Ὁ γνῶσιν γηΐνων ἀθροίσας, καὶ…"
meMA.Ode5C21.text = "Ὁ γνῶσιν γηΐνων ἀθροίσας, καὶ οὐρανίων, ἐνδίκως φιλόσοφος, κατονομάζεται Μάξιμος."

meMA.Ode5C22.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C22.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C22.melody = he.h.m8_gr_GR_cog.heHE.OrthrizontesVoomenSoi.name
meMA.Ode5C22.incipit = "Σοφίας τῆς ἀμείνω τῷ ἔρωτι…"
meMA.Ode5C22.text = "Σοφίας τῆς ἀμείνω τῷ ἔρωτι, τοῦ Χριστοῦ σου, μιμητὴς πανάριστος, ὤφθης ἀοίδιμε Μάξιμε."

meMA.Ode5C23.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C23.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C23.melody = he.h.m8_gr_GR_cog.heHE.OrthrizontesVoomenSoi.name
meMA.Ode5C23.incipit = "Μανίᾳ ὑπερόριος γέγονας, τοῦ τυράννου…"
meMA.Ode5C23.text = "Μανίᾳ ὑπερόριος γέγονας, τοῦ τυράννου, εὗρες δὲ μακάριε, τὸν Ἰησοῦν παραμύθιον."

meMA.Ode5C2T.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C2T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C2T.melody = he.h.m8_gr_GR_cog.heHE.OrthrizontesVoomenSoi.name
meMA.Ode5C2T.incipit = "Κατεύνασον τὸν ἄστατον κλύδωνα, τῶν…"
meMA.Ode5C2T.text = "Κατεύνασον τὸν ἄστατον κλύδωνα, τῶν παθῶν μου, ἡ Θεὸν κυήσασα, τὸν κυβερνήτην καὶ Κύριον."

meMA.Ode6C11.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C11.greekmelody = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.greekname
meMA.Ode6C11.melody = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.name
meMA.Ode6C11.incipit = "Ὑψώθης, ἀνηλεῶς ἐν ξύλῳ ξεόμενος…"
meMA.Ode6C11.text = "Ὑψώθης, ἀνηλεῶς ἐν ξύλῳ ξεόμενος, τὸ τῆς νεκρώσεως πάθος, ἀποῤῥίπτων, θείαν καταστολήν τε, οὐρανόθεν, ἐνδυόμενος Μάρτυς Ἀνίκητε."

meMA.Ode6C12.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C12.melody = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.name
meMA.Ode6C12.incipit = "Ῥήξαντες, τοὺς δεσμοὺς τῆς ἀπάτης…"
meMA.Ode6C12.text = "Ῥήξαντες, τοὺς δεσμοὺς τῆς ἀπάτης οἱ Ἅγιοι, ἐν τῷ δεσμεῖσθαι εὐτόνως, τῆς ὑπομονῆς τε καὶ καρτερίας, θείοις βρόχοις, τὸν προστάτην τοῦ σκότους ἀπέπνιξαν."

meMA.Ode6C13.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C13.melody = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.name
meMA.Ode6C13.incipit = "Ὤφθητε, ὡς μεγάλοι φωστῆρες φωτίζοντες…"
meMA.Ode6C13.text = "Ὤφθητε, ὡς μεγάλοι φωστῆρες φωτίζοντες, μαρμαρυγαῖς ἰαμάτων, καὶ ἀκτῖσι θείων ἀγωνισμάτων, πᾶσαν κτίσιν, Ἀθλοφόροι Χριστοῦ γενναιότατοι."

meMA.Ode6C1T.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C1T.melody = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.name
meMA.Ode6C1T.incipit = "Νομίμων, ἀπεκύησας δίχα Πανάμωμε, τὸν…"
meMA.Ode6C1T.text = "Νομίμων, ἀπεκύησας δίχα Πανάμωμε, τὸν ἀληθῆ νομοθέτην, ὃν δυσώπει νόμῳ τῆς ἁμαρτίας, τὴν ψυχήν μου, τροπουμένην οἰκτεῖραι καὶ σῶσαί με."

meMA.Ode6C21.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C21.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C21.greekmelody = he.h.m8_gr_GR_cog.heHE.ChitonaMoiParaschou.greekname
meMA.Ode6C21.melody = he.h.m8_gr_GR_cog.heHE.ChitonaMoiParaschou.name
meMA.Ode6C21.incipit = "Ἀνένδοτον προσφέρων τῷ Θεῷ, θεοφόρε…"
meMA.Ode6C21.text = "Ἀνένδοτον προσφέρων τῷ Θεῷ, θεοφόρε δέησιν, ψυχῆς τε καὶ σώματος, τῶν παθῶν καὶ τῆς φθορᾶς ἡμᾶς λύτρωσαι."

meMA.Ode6C22.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C22.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C22.melody = he.h.m8_gr_GR_cog.heHE.ChitonaMoiParaschou.name
meMA.Ode6C22.incipit = "Ξηραίνεται αἱρέσεως πηγή, βορβορώδους ἅπασα…"
meMA.Ode6C22.text = "Ξηραίνεται αἱρέσεως πηγή, βορβορώδους ἅπασα, ἀοίδιμε Μάξιμε, φραττομένη τῇ ἡδύτητι τῆς γλώττης σου."

meMA.Ode6C23.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C23.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C23.melody = he.h.m8_gr_GR_cog.heHE.ChitonaMoiParaschou.name
meMA.Ode6C23.incipit = "Ἱλάσθητί μοι μόνε ἀγαθέ, καὶ…"
meMA.Ode6C23.text = "Ἱλάσθητί μοι μόνε ἀγαθέ, καὶ πηγὴν τῆς χάριτος Χριστὲ τῇ καρδίᾳ μου, ταῖς λιταῖς τοῦ σοῦ Ὁσίου ἀνάβλυσον."

meMA.Ode6C2T.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C2T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C2T.melody = he.h.m8_gr_GR_cog.heHE.ChitonaMoiParaschou.name
meMA.Ode6C2T.incipit = "Ἡ μόνη διὰ λόγου ἐν…"
meMA.Ode6C2T.text = "Ἡ μόνη διὰ λόγου ἐν σαρκί, τὸν Λόγον κυήσασα, ῥῦσαι δεόμεθα, τῶν παγίδων τοῦ ἐχθροῦ τὰς ψυχὰς ἡμῶν."

meMA.Ode6H.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6H.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6H.greekmelody = he.h.m8_gr_GR_cog.heHE.ChitonaMoiParaschou.greekname
meMA.Ode6H.melody = he.h.m8_gr_GR_cog.heHE.ChitonaMoiParaschou.melody
meMA.Ode6H.incipit = he.h.m8_gr_GR_cog.heHE.ChitonaMoiParaschou.incipit
meMA.Ode6H.text = he.h.m8_gr_GR_cog.heHE.ChitonaMoiParaschou.text

meMA.Kontakion2.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Kontakion2.greekmelody = he.a.m2_gr_GR_cog.heAU.TaAnoZiton.greekname
meMA.Kontakion2.melody = he.a.m2_gr_GR_cog.heAU.TaAnoZiton.name
meMA.Kontakion2.incipit = "Τοὺς θείους πιστοί, ὁπλίτας εὐφημήσωμεν…"
meMA.Kontakion2.text = "Τοὺς θείους πιστοί, ὁπλίτας εὐφημήσωμεν· τὸ ζεῦγος Χριστοῦ, τῆς δόξης ἐγκωμιάσωμεν· καὶ στεῤῥοὺς ὡς κήρυκας, εὐσεβείας ἅπαντες φίλαθλοι, καὶ Θεοῦ ὄντως ἐραστάς, ἐν ὕμνοις ᾀσμάτων καταστέψωμεν."

meMA.Oikos2.text = "Γῆ καὶ σποδός, σαπρία τε καὶ σκώληξ, διʼ αἰσχρῶν καὶ βεβήλων ἔργων καὶ λογισμῶν ὑπάρχων, μόνε φιλάνθρωπε, σὲ ἱκετεύω, καὶ σοὶ προσπίπτω, ἐκκαθᾶραὶ με τῷ σπόγγῳ τῶν οἰκτιρμῶν σου, Χριστέ, τοῦ ῥύπου, ὃν ἐν ἔργοις καὶ λόγοις συνήθροισα· φωτίσαι δὲ τὸ ζοφῶδες τοῦ νοός μου, τῇ αἴγλῃ τῆς σῆς χάριτος, ἵνα ἀξίως τὴν ἔνστασιν τῶν Μαρτύρων Σωτήρ μου ἐξείποιμι, ἐν ὕμνοις ᾀσμάτων καταστέφων αὐτούς."

meMA.Ode7C11.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C11.greekmelody = he.h.m4_gr_GR_cog.heHE.EnTiKaminoAvramiaioi.greekname
meMA.Ode7C11.melody = he.h.m4_gr_GR_cog.heHE.EnTiKaminoAvramiaioi.name
meMA.Ode7C11.incipit = "Ξένον ἀγῶνα, διηνυκότες ξένοι ὤφθητε…"
meMA.Ode7C11.text = "Ξένον ἀγῶνα, διηνυκότες ξένοι ὤφθητε, ὄντως φρονημάτων ἔνδοξοι τῆς σαρκός, τῶν Ἀγγέλων συμπολῖται δέ, κληρονόμοι τε τοῦ Χριστοῦ, μεγαλώνυμοι μάρτυρες."

meMA.Ode7C12.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C12.melody = he.h.m4_gr_GR_cog.heHE.EnTiKaminoAvramiaioi.name
meMA.Ode7C12.incipit = "Ὑπομονῇ τε, καὶ καρτερίᾳ ἑνικήσατε…"
meMA.Ode7C12.text = "Ὑπομονῇ τε, καὶ καρτερίᾳ ἑνικήσατε, πάντων δυσμενῶν ἀθέων ἐπιβουλάς, καὶ φαιδροὶ πόνοις γενόμενοι, πρὸς φέγγος ἄδυτον, μετεβιβάσθητε, παναοίδιμοι."

meMA.Ode7C13.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C13.melody = he.h.m4_gr_GR_cog.heHE.EnTiKaminoAvramiaioi.name
meMA.Ode7C13.incipit = "Ναὸς Τριάδος, τῆς ὑπερθέου γενόμενος…"
meMA.Ode7C13.text = "Ναὸς Τριάδος, τῆς ὑπερθέου γενόμενος, στήλας καὶ ναοὺς ἠδάφισαν ἀνδρικῶς, τῶν ἀθέων οἱ πανεύφημοι, καὶ πρὸς οὐράνιον, λαὸν μετεβιβάσθησαν ἀγαλλόμενοι."

meMA.Ode7C1T.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C1T.melody = he.h.m4_gr_GR_cog.heHE.EnTiKaminoAvramiaioi.name
meMA.Ode7C1T.incipit = "Ὡς πλατυτέρα, τῶν οὐρανῶν Ἁγνὴ…"
meMA.Ode7C1T.text = "Ὡς πλατυτέρα, τῶν οὐρανῶν Ἁγνὴ ὑποδέδεξαι, Λόγον μηδαμοῦ χωρούμενον ὑπὲρ νοῦν, στενωτάτην τοὺς ὁδεύοντας, ὁδὸν πανάμωμε, πρὸς πλατυσμόν, εἰσάγοντα θείας ζωῆς."

meMA.Ode7C21.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C21.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode7C21.greekmelody = he.h.m8_gr_GR_cog.heHE.OiEkTisIoudaias.greekname
meMA.Ode7C21.melody = he.h.m8_gr_GR_cog.heHE.OiEkTisIoudaias.name
meMA.Ode7C21.incipit = "Μίαν φύσιν Τριάδος, μίαν θέλησιν…"
meMA.Ode7C21.text = "Μίαν φύσιν Τριάδος, μίαν θέλησιν ἔφης, μίαν ἐνέργειαν, Θεοῦ δὲ σαρκωθέντος, δύο φύσεις, θελήσεις, ἐνεργείας ἐκήρυξας, ὁ τῶν Πατέρων βοῶν· Θεὸς εὐλογητός εἶ."

meMA.Ode7C22.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C22.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode7C22.melody = he.h.m8_gr_GR_cog.heHE.OiEkTisIoudaias.name
meMA.Ode7C22.incipit = "Οὐ θελήματα δύο, διαιρούμενα γνώμης…"
meMA.Ode7C22.text = "Οὐ θελήματα δύο, διαιρούμενα γνώμης ἐναντιότητι, ποιότητι δὲ μᾶλλον, ἀνεκήρυξας Πάτερ, φυσικῇ διαφέροντα, ὁ τῶν Πατέρων βοῶν· Θεὸς εὐλογητὸς εἶ."

meMA.Ode7C23.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C23.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode7C23.melody = he.h.m8_gr_GR_cog.heHE.OiEkTisIoudaias.name
meMA.Ode7C23.incipit = "Στήλην Ὀρθοδοξίας, τοὺς ἐνθέους σου…"
meMA.Ode7C23.text = "Στήλην Ὀρθοδοξίας, τοὺς ἐνθέους σου λόγους Πάτερ κατέχοντες, τὸν ἕνα τῆς Τριάδος, ἐν δυσὶ ταῖς οὐσίαις, καὶ θελήσεσι σέβομεν, τὸν τῶν Πατέρων ἡμῶν, Θεόν τὸν σαρκωθέντα."

meMA.Ode7C24.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C24.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode7C24.melody = he.h.m8_gr_GR_cog.heHE.OiEkTisIoudaias.name
meMA.Ode7C24.incipit = "Δύο Πάτερ εἰδότες, ἐνεργείας τοῦ…"
meMA.Ode7C24.text = "Δύο Πάτερ εἰδότες, ἐνεργείας τοῦ οἴκτῳ σεσαρκωμένου Θεοῦ, δισσὰς αὐτεξουσίως, θελήσεις διδαχθέντες, ὑπὸ σοῦ ἀναμέλπομεν· ὁ τῶν Πατέρων ἡμῶν, Θεὸς εὐλογητός εἶ."

meMA.Ode8C11.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C11.greekmelody = he.h.m4_gr_GR_cog.heHE.CheirasEkpetasasDaniil.greekname
meMA.Ode8C11.melody = he.h.m4_gr_GR_cog.heHE.CheirasEkpetasasDaniil.name
meMA.Ode8C11.incipit = "Ῥωμαλέον φρόνημα σοφοί, ἐπιδεικνύμενοι, θηρῶν…"
meMA.Ode8C11.text = "Ῥωμαλέον φρόνημα σοφοί, ἐπιδεικνύμενοι, θηρῶν ὁρμήματα, οὐκ ἐπτοήθητε βράσματα, τῶν λεβήτων οὐκ ἐπτήξατε, οὐ τῶν μελῶν τὰς ἐκτομάς, οὐ βασάνων ποινάς, ἐκβοῶντες· Πάντα τὰ ἔργα ὑμνεῖτε τὸν Κύριον."

meMA.Ode8C12.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C12.melody = he.h.m4_gr_GR_cog.heHE.CheirasEkpetasasDaniil.name
meMA.Ode8C12.incipit = "Ἰᾶσθε τὰ πάθη τῶν πιστῶν…"
meMA.Ode8C12.text = "Ἰᾶσθε τὰ πάθη τῶν πιστῶν, ἐξεικονίσαντες, πάθος μακάριον, ὑμῶν τοῖς πάθεσιν Ἅγιοι, καὶ καθαίρετε νοσήματα, καὶ φυγαδεύετε δεινὰ βοῶντες πνεύματα· Εὐλογεῖτε, πάντα τὰ ἔργα Κυρίου τὸν Κύριον."

meMA.Ode8C13.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C13.melody = he.h.m4_gr_GR_cog.heHE.CheirasEkpetasasDaniil.name
meMA.Ode8C13.incipit = "Δυὰς ἀθλητῶν περικαλλής, Τριάδα ἄκτιστον…"
meMA.Ode8C13.text = "Δυὰς ἀθλητῶν περικαλλής, Τριάδα ἄκτιστον, καθωμολόγησε, καὶ μυριόλεκτον ὤλεσε, πολεμίων ὄντως φάλαγγα, καὶ μυριάσι νοηταῖς συνήφθη μέλπουσα· Εὐλογεῖτε, πάντα τὰ ἔργα Κυρίου τὸν Κύριον."

meMA.Ode8C1T.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C1T.melody = he.h.m4_gr_GR_cog.heHE.CheirasEkpetasasDaniil.name
meMA.Ode8C1T.incipit = "Ἁγία Παρθένε ἡ Θεόν, τὸν…"
meMA.Ode8C1T.text = "Ἁγία Παρθένε ἡ Θεόν, τὸν ὑπεράγιον, ἀποκυήσασα, τὸν στεφανώσαντα χάριτι, τοὺς Ἁγίους τούτους μάρτυρας, σῷ σὸν ἁγίασον ἡμᾶς, προθύμως μέλποντας· Εὐλογεῖτε, πάντα τὰ ἔργα Κυρίου τὸν Κύριον."

meMA.Ode8C21.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C21.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C21.greekmelody = he.h.m8_gr_GR_cog.heHE.NikitaiTyrannou.greekname
meMA.Ode8C21.melody = he.h.m8_gr_GR_cog.heHE.NikitaiTyrannou.name
meMA.Ode8C21.incipit = "Ὁλικῶς ποθήσας, τὸν ὑπερποθήσαντα γένος…"
meMA.Ode8C21.text = "Ὁλικῶς ποθήσας, τὸν ὑπερποθήσαντα γένος ἀνθρώπων, τὸν σταυρόν σου ἦρας, καὶ αὐτῷ μακάριε συνεσταυρώθης· Εὐλογεῖτε κράζων, τὰ ἔργα τὸν Κύριον, καὶ ὑπερυψοῦτε εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8C22.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C22.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C22.melody = he.h.m8_gr_GR_cog.heHE.NikitaiTyrannou.name
meMA.Ode8C22.incipit = "Ξενωθεὶς ἁπάσης, ἡδονῆς μακάριε θανατηφόρου…"
meMA.Ode8C22.text = "Ξενωθεὶς ἁπάσης, ἡδονῆς μακάριε θανατηφόρου, σεαυτὸν εἰργάσω, ὅλον ἀκηλίδωτον ἔσοπτρον θεῖον· Εὐλογεῖτε μέλπων, τὰ ἔργα τὸν Κύριον, καὶ ὑπερυψοῦτε εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8C23.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C23.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C23.melody = he.h.m8_gr_GR_cog.heHE.NikitaiTyrannou.name
meMA.Ode8C23.incipit = "Ἀπηνοῦς τυράννου, δυσσεβοῦς ὠμότητα οὐ…"
meMA.Ode8C23.text = "Ἀπηνοῦς τυράννου, δυσσεβοῦς ὠμότητα οὐ κατεπλάγης, ἀλλʼ ὡς πύργος ἔστης, ἀκλινὴς ἀκλόνητος ὀρθοδοξίας· Εὐλογεῖτε μέλπων, τὰ ἔργα τὸν Κύριον, καὶ ὑπερυψοῦτε εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8C24.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C24.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C24.melody = he.h.m8_gr_GR_cog.heHE.NikitaiTyrannou.name
meMA.Ode8C24.incipit = "Ζωηφόρον αἴγλην, ἐκ μιᾶς Θεότητος…"
meMA.Ode8C24.text = "Ζωηφόρον αἴγλην, ἐκ μιᾶς Θεότητος τρισυποστάτου, δεδεγμένος ὤφθης, τοῖς ἐν σκότει ἥλιος καὶ πλανωμένοις· Εὐλογεῖτε μέλπων, τὰ ἔργα τὸν Κύριον, καὶ ὑπερυψοῦτε εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8C2T.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C2T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C2T.melody = he.h.m8_gr_GR_cog.heHE.NikitaiTyrannou.name
meMA.Ode8C2T.incipit = "Τοῦ Ἀδὰμ Παρθένε, τοῦ παραπεσόντος…"
meMA.Ode8C2T.text = "Τοῦ Ἀδὰμ Παρθένε, τοῦ παραπεσόντος μὲν ὤφθης θυγάτηρ, τοῦ Θεοῦ δὲ μήτηρ, τοῦ ἀνακαινίσαντός μου τὴν οὐσίαν, ὃν ὑμνοῦμεν πάντα τὰ ἔργα ὡς Κύριον, καὶ ὑπερυψοῦμεν εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8H.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8H.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8H.greekmelody = he.h.m8_gr_GR_cog.heHE.NikitaiTyrannou.greekname
meMA.Ode8H.melody = he.h.m8_gr_GR_cog.heHE.NikitaiTyrannou.melody
meMA.Ode8H.incipit = he.h.m8_gr_GR_cog.heHE.NikitaiTyrannou.incipit
meMA.Ode8H.text = he.h.m8_gr_GR_cog.heHE.NikitaiTyrannou.text

meMA.Ode9C11.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode9C11.greekmelody = he.h.m4_gr_GR_cog.heHE.LithosAcheirotmitos.greekname
meMA.Ode9C11.melody = he.h.m4_gr_GR_cog.heHE.LithosAcheirotmitos.name
meMA.Ode9C11.incipit = "Ἰδοὺ ἡ φαιδρὰ καὶ φωσφόρος…"
meMA.Ode9C11.text = "Ἰδοὺ ἡ φαιδρὰ καὶ φωσφόρος, καὶ πλήρης χάριτος ἡμέρα, ἔλαμψε Φωτίου τοῦ θείου, καὶ Ἀνικήτου πάντας φωτίζουσα, τοὺς ἐν αὐτῇ γεραίροντας, τὰ φωταυγῆ τούτων παθήματα."

meMA.Ode9C12.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode9C12.melody = he.h.m4_gr_GR_cog.heHE.LithosAcheirotmitos.name
meMA.Ode9C12.incipit = "Ὤφθητε ὡς ἄνθρακες πᾶσαν, ὕλην…"
meMA.Ode9C12.text = "Ὤφθητε ὡς ἄνθρακες πᾶσαν, ὕλην κακίας ἐμπιπρῶντες, ὤφθητε ὡς τίμιοι ἄρνες, ἐθελουσίως κατασφαττόμενοι, καὶ τῷ Ἀμνῷ τῷ αἴροντι, τὰς ἁμαρτίας προσαγόμενοι."

meMA.Ode9C13.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode9C13.melody = he.h.m4_gr_GR_cog.heHE.LithosAcheirotmitos.name
meMA.Ode9C13.incipit = "Στίγματα ὑμῶν Ἀθλοφόροι, καὶ τὰ…"
meMA.Ode9C13.text = "Στίγματα ὑμῶν Ἀθλοφόροι, καὶ τὰ παθήματα καὶ πόνους, καὶ τὴν ὑπὲρ νοῦν καρτερίαν, καὶ τοὺς ἀγῶνας, καὶ τὴν τελείωσιν, πανευκλεῶς δοξάζοντες, σήμερον πάντες εὐφραινόμεθα."

meMA.Ode9C14.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C14.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode9C14.melody = he.h.m4_gr_GR_cog.heHE.LithosAcheirotmitos.name
meMA.Ode9C14.incipit = "Ἤνθησαν κοιλάσιν ὁσίως, μαρτυρικαῖς καθάπερ…"
meMA.Ode9C14.text = "Ἤνθησαν κοιλάσιν ὁσίως, μαρτυρικαῖς καθάπερ ῥόδα, οἱ περικαλλεῖς, Ἀθλοφόροι, καὶ τὰς καρδίας εὐωδιάζουσι τῶν εὐσεβῶν ἐν πνεύματι, δυσώδη πλάνην ἐκμειώσαντες."

meMA.Ode9C1T.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode9C1T.melody = he.h.m4_gr_GR_cog.heHE.LithosAcheirotmitos.name
meMA.Ode9C1T.incipit = "Φώτισον ἡμᾶς ἡ τεκοῦσα, φῶς…"
meMA.Ode9C1T.text = "Φώτισον ἡμᾶς ἡ τεκοῦσα, φῶς τὸ ἀπρόσιτον Παρθένε, ἔμπλησον ἡμᾶς εὐφροσύνης, καὶ θυμηδίας καὶ θείας γνώσεως, τοὺς καθαρᾷ καρδίᾳ σε, εὐσεβοφρόνως μακαρίζοντας."

meMA.Ode9C21.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C21.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode9C21.greekmelody = he.h.m8_gr_GR_cog.heHE.TonProdilothenta.greekname
meMA.Ode9C21.melody = he.h.m8_gr_GR_cog.heHE.TonProdilothenta.name
meMA.Ode9C21.incipit = "Ἔτι σου ὡς Ἄβελ, τὸ…"
meMA.Ode9C21.text = "Ἔτι σου ὡς Ἄβελ, τὸ αἷμα καὶ εἰς αἰῶνας, τὰ θεόπνευστα δόγματα, φωνῇ διαπρυσίῳ, ἡ Χριστοῦ Ἐκκλησία κηρύττει, Μάξιμε παμμάκαρ καὶ παμμέγιστε."

meMA.Ode9C22.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C22.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode9C22.melody = he.h.m8_gr_GR_cog.heHE.TonProdilothenta.name
meMA.Ode9C22.incipit = "Τέτμηται ἡ χείρ σου, γράφει…"
meMA.Ode9C22.text = "Τέτμηται ἡ χείρ σου, γράφει δὲ θείῳ δακτύλῳ, ὡς καλάμῳ καὶ μέλανι, τετμημένῃ τῇ γλώσσῃ, τῷ σῷ αἵματι τῷ ὁσίῳ Παμμάκαρ, πίστιν ἐν καρδίαις τὴν ὀρθόδοξον."

meMA.Ode9C23.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C23.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode9C23.melody = he.h.m8_gr_GR_cog.heHE.TonProdilothenta.name
meMA.Ode9C23.incipit = "ᾌδεται ἐν κόσμῳ, ἡ ἔνθεος…"
meMA.Ode9C23.text = "ᾌδεται ἐν κόσμῳ, ἡ ἔνθεος παῤῥησία, καὶ τὸ πῦρ τὸ ἐγκάρδιον, τῆς ἐνθέου ἀγάπης, διʼ ἣν Ὅσιε τὴν τοῦ αἵματος χύσιν, Μάξιμε ὑπέστης προθυμότατα."

meMA.Ode9C24.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C24.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode9C24.melody = he.h.m8_gr_GR_cog.heHE.TonProdilothenta.name
meMA.Ode9C24.incipit = "Ἵστασο πρὸ βήματος, θείου μετὰ…"
meMA.Ode9C24.text = "Ἵστασο πρὸ βήματος, θείου μετὰ μαρτύρων, οἷς τοῦ ξύλου τῆς πίστεως, ἐκοινώνησας Πάτερ, καὶ ἡμᾶς οἰκειῶν τῷ Δεσπότῃ, καὶ σοῦ μιμητὰς ἀπεργαζόμενος."

meMA.Ode9C2T.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C2T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode9C2T.melody = he.h.m8_gr_GR_cog.heHE.TonProdilothenta.name
meMA.Ode9C2T.incipit = "Σὺ εἶ Θεοτόκε, τὰ ὅπλα…"
meMA.Ode9C2T.text = "Σὺ εἶ Θεοτόκε, τὰ ὅπλα ἡμῶν καὶ τεῖχος, σὺ εἶ ἡ ἀντίληψις, τῶν εἰς σὲ προστρεχόντων, σὲ καὶ νῦν εἰς πρεσβείαν κινοῦμεν, ἵνα λυτρωθῶμεν τῶν ἐχθρῶν ἡμῶν."

meMA.Ode9H.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9H.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode9H.greekmelody = he.h.m8_gr_GR_cog.heHE.TonProdilothenta.greekname
meMA.Ode9H.melody = he.h.m8_gr_GR_cog.heHE.TonProdilothenta.melody
meMA.Ode9H.incipit = he.h.m8_gr_GR_cog.heHE.TonProdilothenta.incipit
meMA.Ode9H.text = he.h.m8_gr_GR_cog.heHE.TonProdilothenta.text

meMA.Exaposteilarion1.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Exaposteilarion1.greekmelody = he.a.m2_gr_GR_cog.heAU.GynaikesAkoutisthite.greekname
meMA.Exaposteilarion1.melody = he.a.m2_gr_GR_cog.heAU.GynaikesAkoutisthite.name
meMA.Exaposteilarion1.incipit = "Τὰ βάθη τῶν κριμάτων σου…"
meMA.Exaposteilarion1.text = "Τὰ βάθη τῶν κριμάτων σου, ἠρεύνηται τῷ Πνεύματι, τὰ δὲ τοῦ Πνεύματος Σῶτερ, ἐν πνεύματι καὶ δυνάμει, Μάξιμος ἐξηρεύνησεν, ὡς ἔνδικος φιλόσοφος, θελήσεις ἐνεργείας τε, ἀνακηρύττων σου δύο· διὸ λαμπρῶς νῦν τιμᾶται."

meMA.Exaposteilarion2.mode = miscellanea_gr_GR_cog.misc.Mode3
meMA.Exaposteilarion2.greekmelody = he.a.m3_gr_GR_cog.heAU.FosAnalloioton.greekname
meMA.Exaposteilarion2.melody = he.a.m3_gr_GR_cog.heAU.FosAnalloioton.name
meMA.Exaposteilarion2.incipit = "Φῶς ἡ Φωτίου τοῖς ὄχλοις…"
meMA.Exaposteilarion2.text = "Φῶς ἡ Φωτίου τοῖς ὄχλοις, ἐγένετο μαρτυρία, τοῦ Ἀνικήτου νικῶντος, ἐν ταῖς βασάνοις τὴν πλάνην, Χριστοῦ τὰς δύο γεννήσεις, θεολογούντων· οὓς εὐφημήσωμεν ἅμα."

meMA.Exaposteilarion3.mode = miscellanea_gr_GR_cog.misc.Mode3
meMA.Exaposteilarion3.greekmelody = he.a.m3_gr_GR_cog.heAU.FosAnalloioton.greekname
meMA.Exaposteilarion3.melody = he.a.m3_gr_GR_cog.heAU.FosAnalloioton.melody
meMA.Exaposteilarion3.incipit = he.a.m3_gr_GR_cog.heAU.FosAnalloioton.incipit
meMA.Exaposteilarion3.text = he.a.m3_gr_GR_cog.heAU.FosAnalloioton.text

meMA.Aposticha1.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Aposticha1.greekmelody = he.a.m2_gr_GR_cog.heAU.OikosTouEfratha.greekname
meMA.Aposticha1.melody = he.a.m2_gr_GR_cog.heAU.OikosTouEfratha.name
meMA.Aposticha1.incipit = "Παρῆλθεν ἡ σκιά, ἐλήλυθεν ἡ…"
meMA.Aposticha1.text = "Παρῆλθεν ἡ σκιά, ἐλήλυθεν ἡ χάρις, ἐν τῷ Θαβὼρ τῷ ὄρει, Μωσῆς ἐβόα Σῶτερ, ἰδών σου τὴν Θεότητα."

meMA.Aposticha2.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Aposticha2.melody = he.a.m2_gr_GR_cog.heAU.OikosTouEfratha.name
meMA.Aposticha2.incipit = "Ἠλίας καὶ Μωσῆς, ἐν τῷ…"
meMA.Aposticha2.text = "Ἠλίας καὶ Μωσῆς, ἐν τῷ Θαβὼρ τῷ ὄρει, τὴν ἔξοδον ἐλάλουν, ἣν ἔμελλες πληρώσειν, ὑπὲρ ἡμῶν Φιλάνθρωπε."

meMA.Aposticha3.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Aposticha3.melody = he.a.m2_gr_GR_cog.heAU.OikosTouEfratha.name
meMA.Aposticha3.incipit = "Τῆς θείας σου μορφῆς, οἱ…"
meMA.Aposticha3.text = "Τῆς θείας σου μορφῆς, οἱ τρεῖς τῶν Μαθητῶν σου, ὁρᾶν μὴ δυνηθέντες, τὴν ἀστραπὴν ἐν φόβῳ, Σωτὴρ εἰς γῆν κατέπιπτον."

meMA.AposGlory.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.AposGlory.melody = he.a.m2_gr_GR_cog.heAU.OikosTouEfratha.name
meMA.AposGlory.incipit = "Ὕψωσον εὐσεβῶν, βασιλέων τὸ κέρας…"
meMA.AposGlory.text = "Ὕψωσον εὐσεβῶν, βασιλέων τὸ κέρας, Παμβασιλεῦ οἰκτίρμον, καὶ νίκας δίδου Σῶτερ, πρεσβείαις τῆς τεκούσης σε."


