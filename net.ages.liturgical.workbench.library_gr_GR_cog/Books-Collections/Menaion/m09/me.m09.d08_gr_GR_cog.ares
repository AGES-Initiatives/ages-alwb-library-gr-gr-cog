meMA.Ode8C1H.text = he.h.m2_gr_GR_cog.heHE.EnTiKamino.text

meMA.Ode8C11.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C11.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode8C11.greekmelody = he.h.m2_gr_GR_cog.heHE.EnTiKamino.greekname
meMA.Ode8C11.melody = he.h.m2_gr_GR_cog.heHE.EnTiKamino.name
meMA.Ode8C11.incipit = "Τῆς πρὸς Θεὸν ἡμῶν καταλλαγῆς…"
meMA.Ode8C11.text = "Τῆς πρὸς Θεὸν ἡμῶν καταλλαγῆς, ἡ προορισθεῖσα σκηνή, τοῦ εἶναι νῦν ἀπάρχεται, τεξομένη Λόγον ἡμῖν παχύτητι, σαρκὸς ἐμφανιζόμενον· ὃν ὑμνοῦμεν οἱ ἐκ μὴ ὄντων, διʼ αὐτοῦ τὸ εἶναι λαβόντες, καὶ ὑπερυψοῦμεν εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8C12.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C12.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode8C12.melody = he.h.m2_gr_GR_cog.heHE.EnTiKamino.name
meMA.Ode8C12.incipit = "Ἡ τῆς στειρώσεως μεταβολή, τὴν…"
meMA.Ode8C12.text = "Ἡ τῆς στειρώσεως μεταβολή, τὴν κοσμικὴν τῶν ἀγαθῶν, διέλυσε στείρωσιν, καὶ τρανῶς τὸ θαῦμα Χριστὸν ὑπέδειξε, βροτοῖς ἐπιδημήσαντα· ὃν ὑμνοῦμεν οἱ ἐκ μὴ ὄντων, διʼ αὐτοῦ τὸ εἶναι λαβόντες, καὶ ὑπερυψοῦμεν εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8C2H.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C2H.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C2H.greekmelody = he.h.m8_gr_GR_cog.heHE.OStegazonEnYdasi.greekname
meMA.Ode8C2H.melody = he.h.m8_gr_GR_cog.heHE.OStegazonEnYdasi.melody
meMA.Ode8C2H.incipit = he.h.m8_gr_GR_cog.heHE.OStegazonEnYdasi.incipit
meMA.Ode8C2H.text = he.h.m8_gr_GR_cog.heHE.OStegazonEnYdasi.text

meMA.Ode8C21.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C21.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C21.greekmelody = he.h.m8_gr_GR_cog.heHE.OStegazonEnYdasi.greekname
meMA.Ode8C21.melody = he.h.m8_gr_GR_cog.heHE.OStegazonEnYdasi.name
meMA.Ode8C21.incipit = "Ὁ ποιήσας παράδοξα, τῇ στειρωθείσῃ…"
meMA.Ode8C21.text = "Ὁ ποιήσας παράδοξα, τῇ στειρωθείσῃ γαστρί, ὁ ἀνοίξας Ἄννης ἄγονον μήτραν, καὶ καρπὸν αὐτῇ δούς, σὺ Θεὸς ἅγιος, σὺ Υἱὸς τῆς Παρθένου, σὺ ἐκ ταύτης σάρκα προσελάβου, τῆς ἀειθαλοῦς Παρθένου καὶ Θεοτόκου."

meMA.Ode8C22.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C22.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C22.melody = he.h.m8_gr_GR_cog.heHE.OStegazonEnYdasi.name
meMA.Ode8C22.incipit = "Ὁ σφραγίζων τὴν ἄβυσσον, καὶ…"
meMA.Ode8C22.text = "Ὁ σφραγίζων τὴν ἄβυσσον, καὶ ἐξανοίγων αὐτήν, ὁ ἀνάγων ὕδωρ ἐν ταῖς νεφέλαις, καὶ διδοὺς ὑετόν, σὺ ὁ δοὺς Κύριε, ῥίζης ἐκ τῆς ἀκάρπου ἐξανθῆσαι,, Ἄννης τῆς ἁγίας, ἄχραντον καρπόν, τὴν ῥάβδον τὴν Θεοτόκον."

meMA.Ode8C23.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C23.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C23.melody = he.h.m8_gr_GR_cog.heHE.OStegazonEnYdasi.name
meMA.Ode8C23.incipit = "Σὺ ὁ λύσας τὰ ἄλυτα…"
meMA.Ode8C23.text = "Σὺ ὁ λύσας τὰ ἄλυτα, τῆς ἀπαιδίας δεσμά· σὺ ὁ δοὺς τῇ στείρᾳ γόνιμον τόκον, καὶ καρπὸν εὐκλεῆ, ἧς Υἱὸς γέγονας καὶ βλαστὸς ἀνεφύης· ἣν Μητέρα ἔσχες κατὰ σάρκα, ἐν τῇ πρὸς ἡμᾶς Οἰκτίρμον ἐπιδημίᾳ."

meMA.Ode8C24.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C24.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C24.melody = he.h.m8_gr_GR_cog.heHE.OStegazonEnYdasi.name
meMA.Ode8C24.incipit = "Γεωργὲ τῶν φρενῶν ἡμῶν, καὶ…"
meMA.Ode8C24.text = "Γεωργὲ τῶν φρενῶν ἡμῶν, καὶ φυτουργὲ τῶν ψυχῶν, σὺ ὁ ἄκαρπον γῆν, εὔκαρπον δείξας· σὺ τὴν πάλαι ξηράν, γόνιμον εὔσταχυν, ἄρουραν καρποφόρον ἀπειργάσω, Ἄνναν τὴν ἁγίαν, ἄχραντον καρπὸν ἀνθῆσαι τὴν Θεοτόκον."

meMA.Ode8C2Tr.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C2Tr.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C2Tr.melody = he.h.m8_gr_GR_cog.heHE.OStegazonEnYdasi.name
meMA.Ode8C2Tr.incipit = "Ὢ Τριὰς ὑπερούσιε, μονὰς συνάναρχε…"
meMA.Ode8C2Tr.text = "Ὢ Τριὰς ὑπερούσιε, μονὰς συνάναρχε, σὲ ὑμνεῖ καὶ τρέμει, πληθὺς Ἀγγέλων, οὐρανὸς καὶ ἡ γῆ, ἄβυσσοι φρίττουσιν, ἄνθρωποι εὐλογοῦσι, πῦρ δουλεύει, πάντα ὑπακούει, σοὶ Τριὰς ἁγία, φόβῳ τὰ ἐν τῇ κτίσει."

meMA.Ode8C2T.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C2T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C2T.melody = he.h.m8_gr_GR_cog.heHE.OStegazonEnYdasi.name
meMA.Ode8C2T.incipit = "Ὢ καινότατον ἄκουσμα! Θεὸς Υἱὸς…"
meMA.Ode8C2T.text = "Ὢ καινότατον ἄκουσμα! Θεὸς Υἱὸς γυναικός, καὶ ἀσπόρου τόκου, ἄνανδρος Μήτηρ, καὶ Θεὸς τὸ τεχθέν, ὢ φρικτὸν θέαμα! ὢ συλλήψεως ξένης τῆς παρθένου! ὢ ἀφράστου τόκου! ὄντως ὑπὲρ νοῦν τὰ πάντα καὶ θεωρίαν."

meMA.Ode9C1H.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C1H.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode9C1H.greekmelody = he.h.m2_gr_GR_cog.heHE.ITonProIliouFostira.greekname
meMA.Ode9C1H.melody = he.h.m2_gr_GR_cog.heHE.ITonProIliouFostira.melody
meMA.Ode9C1H.incipit = he.h.m2_gr_GR_cog.heHE.ITonProIliouFostira.incipit
meMA.Ode9C1H.text = he.h.m2_gr_GR_cog.heHE.ITonProIliouFostira.text

meMA.Ode9C11.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C11.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode9C11.greekmelody = he.h.m2_gr_GR_cog.heHE.ITonProIliouFostira.greekname
meMA.Ode9C11.melody = he.h.m2_gr_GR_cog.heHE.ITonProIliouFostira.name
meMA.Ode9C11.incipit = "Ὁ τοῖς ἀπειθοῦσι λαοῖς, ἐξ…"
meMA.Ode9C11.text = "Ὁ τοῖς ἀπειθοῦσι λαοῖς, ἐξ ἀκροτόμου βλύσας νάματα, τοῖς εὐπειθοῦσιν ἔθνεσι χαρίζεται, ἐκ λαγόνων στειρωτικῶν, καρπὸν εἰς εὐφροσύνην ἡμῖν, σὲ Θεομῆτορ ἄχραντε, ἣν ἐπαξίως μεγαλύνομεν."

meMA.Ode9C12.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C12.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode9C12.melody = he.h.m2_gr_GR_cog.heHE.ITonProIliouFostira.name
meMA.Ode9C12.incipit = "Τὴν τῆς ἀποτόμου ἀρχαίας, ἀναιρέτιν…"
meMA.Ode9C12.text = "Τὴν τῆς ἀποτόμου ἀρχαίας, ἀναιρέτιν ἀποφάσεως, καὶ τῆς Προμήτορος τὴν ἐπανόρθωσιν, τὴν τοῦ γένους τῆς πρὸς Θεὸν αἰτίαν οἰκειώσεως, τὴν πρὸς τὸν Κτίστην γέφυραν, σὲ Θεοτόκε μεγαλύνομεν."

meMA.Ode9C2H.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C2H.mode = miscellanea_gr_GR_cog.misc.Mode8
