meVE.Stichera01.mode = miscellanea_gr_GR_cog.misc.Mode8
meVE.Stichera01.greekmelody = he.a.m8_gr_GR_cog.heAU.OiMartyresSouKyrie.greekname
meVE.Stichera01.melody = he.a.m8_gr_GR_cog.heAU.OiMartyresSouKyrie.name
meVE.Stichera01.incipit = "Πυξίον θείου Πνεύματος, θεομακάριστε ἐδείχθης…"
meVE.Stichera01.text = "Πυξίον θείου Πνεύματος, θεομακάριστε ἐδείχθης, Πάτερ τὰ θεῖα δόγματα ἐγγεγραμμένα, φέρων ἐν γνώσει θεϊκῇ, ὧν τῇ ἀναπτύξει κατεφώτισας, τοὺς τῇ ἀγνωσίᾳ κινδυνεύοντας· διὸ ταῖς σαῖς πρεσβείαις, αἴτησαι ἡμῖν πᾶσι τὸ μέγα ἔλεος."

meVE.Stichera02.mode = miscellanea_gr_GR_cog.misc.Mode8
meVE.Stichera02.melody = he.a.m8_gr_GR_cog.heAU.OiMartyresSouKyrie.name
meVE.Stichera02.incipit = "Αἱμάτων θείοις ῥεύμασι, προσεπιχρώσας σου…"
meVE.Stichera02.text = "Αἱμάτων θείοις ῥεύμασι, προσεπιχρώσας σου Ἱερωσύνης, στολὴν Δωρόθεε εἰσῆλθες, χαίρων εἰς τὸν οὐράνιον ναόν, νῦν ἐμφανισθῆναι τῷ Θεῷ ἡμῶν, νέμοντι στεφάνους ταῆς ἀθλήσεως, αὐτὸν καθικετεύων, ἅπασι δωρηθῆναι τὸ μέγα ἔλεος."

meVE.Stichera03.mode = miscellanea_gr_GR_cog.misc.Mode8
meVE.Stichera03.melody = he.a.m8_gr_GR_cog.heAU.OiMartyresSouKyrie.name
meVE.Stichera03.incipit = "Ὁ βίος σου Ἰσάγγελος, ἡ…"
meVE.Stichera03.text = "Ὁ βίος σου Ἰσάγγελος, ἡ μαρτυρία δὲ λελαμπρυσμένη, διʼ ἧς ἠξίωσαι μετὰ Ἀγγέλων, περιχορεύειν ἐν χαρᾷ, θαύμασι τὴν γῆν Πάτερ Δωρόθεε, θείοις καταυγάζων καὶ διδάγμασι· διὸ ταῖς σαῖς πρεσβείαις, αἴτησαι πᾶσι δοῦναι τὸ μέγα ἔλεος."

meVE.SticTheotokion.mode = miscellanea_gr_GR_cog.misc.Mode8
meVE.SticTheotokion.greekmelody = he.a.m8_gr_GR_cog.heAU.OiMartyresSouKyrie.greekname
meVE.SticTheotokion.melody = he.a.m8_gr_GR_cog.heAU.OiMartyresSouKyrie.name
meVE.SticTheotokion.incipit = "Ἐγὼ εἰμι Πανάμωμε, δένδρον τὸ…"
meVE.SticTheotokion.text = "Ἐγὼ εἰμι Πανάμωμε, δένδρον τὸ ἄκαρπον τοῦ θείου λόγου, καρπὸν σωτήριον μηδόλως φέρον, καὶ δειλιῶ τὴν ἐκκοπήν, μήπως εἰς τὸ πῦρ βληθῶ τὸ ἄσβεστον· ὅθεν δυσωπῶ σε. Τούτου ῥῦσαί με, δείξασα καρποφόρον, Ἀχραντε, τῷ Υἱῷ σου τῇ μεσιτείᾳ σου."

meVE.SticStavrotheotokion.mode = miscellanea_gr_GR_cog.misc.Mode8
meVE.SticStavrotheotokion.greekmelody = he.a.m8_gr_GR_cog.heAU.OiMartyresSouKyrie.greekname
meVE.SticStavrotheotokion.melody = he.a.m8_gr_GR_cog.heAU.OiMartyresSouKyrie.name
meVE.SticStavrotheotokion.incipit = me.m02.d19_gr_GR_cog.meVE.SticStavrotheotokion.incipit
meVE.SticStavrotheotokion.text = me.m02.d19_gr_GR_cog.meVE.SticStavrotheotokion.text

meVE.Apolytikion1ger.poet = poets_gr_GR_cog.po.Gerasimou
meVE.Apolytikion1ger.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.Apolytikion1ger.melody = he.a.m4_gr_GR_cog.heAU.TachyProkatalave.name
meVE.Apolytikion1ger.incipit = "Ὡς δώρημα τέλειον, ἐκ τοῦ…"
meVE.Apolytikion1ger.text = "Ὡς δώρημα τέλειον, ἐκ τοῦ τῶν φώτων Πατρός, σοφίας τὴν ἔλλαμψιν, ὡς Ἱεράρχης σοφός, ἐδέξω Δωρόθεε· ὅθεν καὶ πλεονάσας, τὸ σὸν τάλαντον μάκαρ ἤθλησας ὑπὲρ φύσιν, βαθυτάτῳ ἐν γήρᾳ, πρεσβεύων Ἱερομάρτυς, ὑπὲρ τῶν ψυχῶν ἡμῶν."

meMA.C1.acrostic.title = "Ὁ Κανὼν τοῦ Ἁγίου,"
meMA.C1.acrostic.notice1 = "οὗ ἡ Ἀκροστιχίς·"
meMA.C1.acrostic.text = "Δῶρον Θεοῦ σε παμμάκαρ Πάτερ σέβω. Ἰωσήφ."
meMA.C1.title = titles_gr_GR_cog.ti.CanonSaintM
meMA.C1.verse = prayers_gr_GR_cog.pr.verse_SaintM.text

meMA.Ode1C11.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C11.greekmelody = he.h.m8_gr_GR_cog.heHE.AsomenToKyrio.greekname
meMA.Ode1C11.melody = he.h.m8_gr_GR_cog.heHE.AsomenToKyrio.name
meMA.Ode1C11.incipit = "Δῶρον Θεῷ παμμάκαρ, σεαυτὸν προσῆξας…"
meMA.Ode1C11.text = "Δῶρον Θεῷ παμμάκαρ, σεαυτὸν προσῆξας καθαρώτατον, διὰ βίου τελείου, καὶ σεπτοῦ μαρτυρίου Δωρόθεε."

meMA.Ode1C12.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C12.melody = he.h.m8_gr_GR_cog.heHE.AsomenToKyrio.name
meMA.Ode1C12.incipit = "Ὥριμον ὥσπερ βότρυν, Πάτερ ἀποθλίψας…"
meMA.Ode1C12.text = "Ὥριμον ὥσπερ βότρυν, Πάτερ ἀποθλίψας τὴν διάνοιαν, εὐφροσύνης κρατῆρα, τοῖς πιστοῖς διδαγμάτων προέθηκας."

meMA.Ode1C13.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C13.melody = he.h.m8_gr_GR_cog.heHE.AsomenToKyrio.name
meMA.Ode1C13.incipit = "Ῥεύματα τῆς ἀπάτης, ἔστησας Δωρόθεε…"
meMA.Ode1C13.text = "Ῥεύματα τῆς ἀπάτης, ἔστησας Δωρόθεε τῷ ῥεύματι, τῆς πανσόφου σου γλώττης, καὶ πιστῶν διανοίας κατήρδευσας."

meMA.Ode1C1T.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C1T.melody = he.h.m8_gr_GR_cog.heHE.AsomenToKyrio.name
meMA.Ode1C1T.incipit = "Ὄμβρισὸν μοι Παρθένε, δέομαι σταγόνα…"
meMA.Ode1C1T.text = "Ὄμβρισὸν μοι Παρθένε, δέομαι σταγόνα κατανύξεως, τὸν ἐνόντα μοι ῥύπον, ἀποπλύνουσα ὅπως δοξάζω σε."

meMA.Ode3C11.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C11.greekmelody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.greekname
meMA.Ode3C11.melody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.name
meMA.Ode3C11.incipit = "Νύκτα ἀπεδίωξας, τῆς ματαιότητος ἅπασαν…"
meMA.Ode3C11.text = "Νύκτα ἀπεδίωξας, τῆς ματαιότητος ἅπασαν, ταῖς ἀστραπαῖς, Πάτερ Ἱεράρχα, τοῦ πανσόφου κηρύγματος."

meMA.Ode3C12.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C12.melody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.name
meMA.Ode3C12.incipit = "Θαύμασιν ἐστήριξας, καὶ ἱεροῖς Σοφὲ…"
meMA.Ode3C12.text = "Θαύμασιν ἐστήριξας, καὶ ἱεροῖς Σοφὲ δόγμασι, τὰς τῶν Πιστῶν, πάντων διανοίας, ὡς ποιμὴν ἱερώτατος."

meMA.Ode3C13.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C13.melody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.name
meMA.Ode3C13.incipit = "Ἔχρισέ σὲ Κύριος, τῷ ἱερῷ…"
meMA.Ode3C13.text = "Ἔχρισέ σὲ Κύριος, τῷ ἱερῷ σοφὲ χρίσματι, καὶ τῆς αὐτοῦ, θείας Ἐκκλησίας, ποιμενάρχην κατέστησε."

meMA.Ode3C1T.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C1T.melody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.name
meMA.Ode3C1T.incipit = "Ὅλην τὴν καρδίαν μου, τῷ…"
meMA.Ode3C1T.text = "Ὅλην τὴν καρδίαν μου, τῷ θεϊκῷ φωτὶ λάμπρυνον, πύλη φωτός, τὸν τῆς ἁμαρτίας, σκοτασμὸν ἐκδιώκουσα."

meMA.Ode3H.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3H.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3H.greekmelody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.greekname
meMA.Ode3H.melody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.melody
meMA.Ode3H.incipit = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.incipit
meMA.Ode3H.text = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.text

meMA.Ode3Kathisma1.mode = miscellanea_gr_GR_cog.misc.Mode3
meMA.Ode3Kathisma1.melody = he.a.m3_gr_GR_cog.heAU.TheiasPisteos.name
meMA.Ode3Kathisma1.incipit = "Θείοις δόγμασιν ἀεὶ ἐκλάμπων, δῶρον…"
meMA.Ode3Kathisma1.text = "Θείοις δόγμασιν ἀεὶ ἐκλάμπων, δῶρον ἅγιον σαυτὸν προσῆξας, τῷ ἀθανάτῳ Βασιλεῖ διʼ ἀθλήσεως, καὶ κατοικεῖς νῦν ἀεὶ εὐφραινόμενος, τῶν πρωτοτόκων τὴν ἄνω μητρόπολιν, Πάτερ Ὅσιε, Χριστὸν τὸν Θεὸν ἱκέτευε, δωρήσασθαι ἡμῖν τὸ μέγα ἔλεος."

meMA.Ode3KathismaT.mode = miscellanea_gr_GR_cog.misc.Mode3
meMA.Ode3KathismaT.melody = he.a.m3_gr_GR_cog.heAU.TheiasPisteos.name
meMA.Ode3KathismaT.incipit = me.m01.d25_gr_GR_cog.meMA.Kathisma1T.incipit
meMA.Ode3KathismaT.text = me.m01.d25_gr_GR_cog.meMA.Kathisma1T.text

meMA.Ode3KathismaSt.mode = miscellanea_gr_GR_cog.misc.Mode3
meMA.Ode3KathismaSt.melody = he.a.m3_gr_GR_cog.heAU.TheiasPisteos.name
meMA.Ode3KathismaSt.incipit = me.m01.d29_gr_GR_cog.meMA.Ode3KathismaSt.incipit
meMA.Ode3KathismaSt.text = me.m01.d29_gr_GR_cog.meMA.Ode3KathismaSt.text

meMA.Ode4C11.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C11.greekmelody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.greekname
meMA.Ode4C11.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
meMA.Ode4C11.incipit = "Ὑψουμένου κατέπτυσας, Ὅσιε χειμῶνος τῆς…"
meMA.Ode4C11.text = "Ὑψουμένου κατέπτυσας, Ὅσιε χειμῶνος τῆς ἀθεότητος, καὶ τῇ θέρμῃ θείου Πνεύματος, ἀβλαβεῖς καρδίας συνετήρησας."

meMA.Ode4C12.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C12.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
meMA.Ode4C12.incipit = "Συνετρίβη Δωρόθεε, μάκαρ πρὸ προσώπου…"
meMA.Ode4C12.text = "Συνετρίβη Δωρόθεε, μάκαρ πρὸ προσώπου σου ὄρη βίαια, τῶν δαιμόνων καὶ ἐτάκησαν, δυσμενῶν ἀνθρώπων τὰ φρονήματα."

meMA.Ode4C13.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C13.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
meMA.Ode4C13.incipit = "Ἐκαρτέρεις θλιβόμενος, πόλεως εἰς πόλιν…"
meMA.Ode4C13.text = "Ἐκαρτέρεις θλιβόμενος, πόλεως εἰς πόλιν ἐκδιωκόμενος· μακαρίας ὅθεν ἔτυχες, τῶν Μαρτύρων δόξης καὶ λαμπρότητος."

meMA.Ode4C1T.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C1T.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
meMA.Ode4C1T.incipit = "Πειρασμῶν με διάσωσον, καὶ τοῦ…"
meMA.Ode4C1T.text = "Πειρασμῶν με διάσωσον, καὶ τοῦ ψυχοφθόρου τῶν παθῶν κλύδωνος, ὅπως πίστει μακαρίζω σε, παναγία Κόρη Μητροπάρθενε."

meMA.Ode5C11.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C11.greekmelody = he.h.m8_gr_GR_cog.heHE.TonEkNyktosAgnosias.greekname
meMA.Ode5C11.melody = he.h.m8_gr_GR_cog.heHE.TonEkNyktosAgnosias.name
meMA.Ode5C11.incipit = "Ἀποσκοπῶν ἐμφρόνως, τὰς ἀντιδόσεις τὰς…"
meMA.Ode5C11.text = "Ἀποσκοπῶν ἐμφρόνως, τὰς ἀντιδόσεις τὰς θείας Δωρόθεε, τοὺς μεγίστους κινδύνους, καὶ τοὺς πειρασμοὺς ἐκαρτέρησας."

meMA.Ode5C12.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C12.melody = he.h.m8_gr_GR_cog.heHE.TonEkNyktosAgnosias.name
meMA.Ode5C12.incipit = "Μοναδικῶς τὴν θείαν, ἀνακηρύττων Τριάδα…"
meMA.Ode5C12.text = "Μοναδικῶς τὴν θείαν, ἀνακηρύττων Τριάδα Δωρόθεε, πολυθέου μανίας, μάταιον διέλυσας φρύαγμα."

meMA.Ode5C13.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C13.melody = he.h.m8_gr_GR_cog.heHE.TonEkNyktosAgnosias.name
meMA.Ode5C13.incipit = "Μὴ προσκυνήσας Μάρτυς, ἔργα χειρῶν…"
meMA.Ode5C13.text = "Μὴ προσκυνήσας Μάρτυς, ἔργα χειρῶν ἀνομούντων ὑπέμεινας, πειρασμοὺς καὶ κινδύνους, καὶ μαστίγων πόνους Δωρόθεε."

meMA.Ode5C1T.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C1T.melody = he.h.m8_gr_GR_cog.heHE.TonEkNyktosAgnosias.name
meMA.Ode5C1T.incipit = "Ἀνατολὴ ἡλίου, πρὸ ἑωσφόρου ἐκλάμψαντος…"
meMA.Ode5C1T.text = "Ἀνατολὴ ἡλίου, πρὸ ἑωσφόρου ἐκλάμψαντος γέγονας, διʼ οὗ φῶς οἱ ἐν σκότει, Ἄχραντε δεινῶν ἐθεάσαντο."

meMA.Ode6C11.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C11.greekmelody = he.h.m8_gr_GR_cog.heHE.ChitonaMoiParaschou.greekname
meMA.Ode6C11.melody = he.h.m8_gr_GR_cog.heHE.ChitonaMoiParaschou.name
meMA.Ode6C11.incipit = "Καρδίαν πυρωθεῖσαν ταῖς πνοαῖς, ἐκτήσω…"
meMA.Ode6C11.text = "Καρδίαν πυρωθεῖσαν ταῖς πνοαῖς, ἐκτήσω τοῦ Πνεύματος, πνευμάτων Δωρόθεε, ἐναντίων τὸν κρυμὸν διαλύουσαν."

meMA.Ode6C12.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C12.melody = he.h.m8_gr_GR_cog.heHE.ChitonaMoiParaschou.name
meMA.Ode6C12.incipit = "Ἁγίων πολιτείας ἱεραῖς, συγγραφαῖς ἐδήλωσας…"
meMA.Ode6C12.text = "Ἁγίων πολιτείας ἱεραῖς, συγγραφαῖς ἐδήλωσας, Πάτερ θεόπνευστε, θείᾳ γνώσει τὴν ψυχὴν φωτιζόμενος."

meMA.Ode6C13.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C13.melody = he.h.m8_gr_GR_cog.heHE.ChitonaMoiParaschou.name
meMA.Ode6C13.incipit = "Ῥημάτων σου ὁ φθόγγος καὶ…"
meMA.Ode6C13.text = "Ῥημάτων σου ὁ φθόγγος καὶ τερπνῶν, δογμάτων ἡ δύναμις, πᾶσαν ἐπέδραμε, Παμμακάριστε τὴν γῆν θείᾳ χάριτι."

meMA.Ode6C1T.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C1T.melody = he.h.m8_gr_GR_cog.heHE.ChitonaMoiParaschou.name
meMA.Ode6C1T.incipit = "Παρθένε ὃν ἐκύησας Θεόν, ἄνθρωπον…"
meMA.Ode6C1T.text = "Παρθένε ὃν ἐκύησας Θεόν, ἄνθρωπον γενόμενον, τοῦτον ἱκέτευε, λυτρωθῆναί με πυρὸς καὶ κολάσεως."

meMA.Ode6H.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6H.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6H.greekmelody = he.h.m8_gr_GR_cog.heHE.ChitonaMoiParaschou.greekname
meMA.Ode6H.melody = he.h.m8_gr_GR_cog.heHE.ChitonaMoiParaschou.melody
meMA.Ode6H.incipit = he.h.m8_gr_GR_cog.heHE.ChitonaMoiParaschou.incipit
