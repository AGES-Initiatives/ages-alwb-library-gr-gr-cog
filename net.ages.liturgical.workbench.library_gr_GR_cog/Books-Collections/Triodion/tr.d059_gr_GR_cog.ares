trVE.Stichera01.mode = miscellanea_gr_GR_cog.misc.Mode6
trVE.Stichera01.greekmelody = he.a.m6_gr_GR_cog.heAU.OlinApothemenoi.greekname
trVE.Stichera01.melody = he.a.m6_gr_GR_cog.heAU.OlinApothemenoi.name
trVE.Stichera01.incipit = "Πολλοῖς παραπτώμασι, καὶ ἡδοναῖς ταῖς…"
trVE.Stichera01.text = "Πολλοῖς παραπτώμασι, καὶ ἡδοναῖς ταῖς τοῦ βίου, ἀσθενοῦσαν κέκτημαι, τὴν ἰσχὺν ὁ δείλαιος, καὶ κατάκειμαι, ἐπὶ κλίνης πάντοτε, ῥαθυμίας Δέσποτα, καὶ βοῶ σοι· Συμπαθέστατε, ἐλθὼν ἐπίσκεψαι, ῥῶσίν μοι παρέχων καὶ ἔλεος, καὶ μὴ ἐγκαταλίπῃς με, μήποτε ὑπνώσω εἰς θάνατον, καὶ ἐπιχαρῇ μοι, ἐχθρὸς τῇ ἀπωλείᾳ, ὁ ζητῶν, εἰς ᾍδου βάραθρα πάνδεινα, Σῶτερ κατασπάσαι με."

trVE.Stichera02.mode = miscellanea_gr_GR_cog.misc.Mode6
trVE.Stichera02.greekmelody = he.a.m6_gr_GR_cog.heAU.OlinApothemenoi.greekname
trVE.Stichera02.melody = he.a.m6_gr_GR_cog.heAU.OlinApothemenoi.name
trVE.Stichera02.incipit = "Τὸν ἄσπλαγχνον Πλούσιον, ἐν ἀφροσύνῃ…"
trVE.Stichera02.text = "Τὸν ἄσπλαγχνον Πλούσιον, ἐν ἀφροσύνῃ ζηλώσας, ἡδέως εὐφραίνομαι, ἡδοναῖς καὶ πάθεσι βυθιζόμενος, καὶ ὁρῶν κείμενον, πρὸ πυλῶν πάντοτε, μετανοίας ὥσπερ Λάζαρον, τὸν νοῦν μου Κύριε, τοῦτον ἀναισθήτως παρέρχομαι λιμώττοντα νοσοῦντά τε, καὶ διηλκωμένον τοῖς πάθεσιν· ὅθεν τῆς γεέννης, ὑπόδικός εἰμι τῆς ἐν φλογί, ἐξ ἧς με λύτρωσαι Δέσποτα, μόνε πολυέλεε."

trVE.Stichera03.mode = miscellanea_gr_GR_cog.misc.Mode5
trVE.Stichera03.melody = "Κύριε ἐπὶ Μωϋσέως."
trVE.Stichera03.incipit = "Κύριε, πέραν Ἰορδάνου σαρκὶ διατρίβων…"
trVE.Stichera03.text = "Κύριε, πέραν Ἰορδάνου σαρκὶ διατρίβων, προαγορεύεις Λαζάρου τὴν νόσον, πρὸς θάνατον μὴ εἶναι, ἀλλʹ ὑπὲρ σοῦ τοῦ Θεοῦ ἡμῶν τῆς δόξης γεγενῆσθαι. Δόξα τῇ μεγαλουργίᾳ σου, καὶ παντοκρατορίᾳ σου, ὅτι καθεῖλες τὸν θάνατον, διὰ πλῆθος ἐλέους φιλάνθρωπε."

trVE.AposIdiomelon1.mode = miscellanea_gr_GR_cog.misc.Mode4
trVE.AposIdiomelon1.melody = miscellanea_gr_GR_cog.misc.Idiomelon
trVE.AposIdiomelon1.incipit = "Τῆς ψυχοβλαβοῦς πλεονεξίας, ἐλευθέρωσον Σωτήρ…"
trVE.AposIdiomelon1.text = "Τῆς ψυχοβλαβοῦς πλεονεξίας, ἐλευθέρωσον Σωτήρ, καὶ μετὰ Λαζάρου τοῦ πένητος, ἐν τοῖς κόλποις Ἀβραὰμ ἀρίθμησον ἡμᾶς· σὺ γὰρ ἐν ἐλέει πλούσιος ὤν, ἑκουσίως διʹ ἡμᾶς ἑπτώχευσας, καὶ ἐκ φθορᾶς εἰς ἀφθαρσίαν μετεπανήγαγες, ὡς συμπαθὴς Θεὸς καὶ φιλάνθρωπος."

trVE.AposMartyric.mode = miscellanea_gr_GR_cog.misc.Mode4
trVE.AposMartyric.melody = ""
trVE.AposMartyric.incipit = "Ὁ τῶν ἁγίων Μαρτύρων δεξάμενος…"
trVE.AposMartyric.text = "Ὁ τῶν ἁγίων Μαρτύρων δεξάμενος τὴν ὑπομονήν, καὶ παρʹ ἡμῶν δέχου τὴν ὑμνῳδίαν Φιλάνθρωπε, δωρούμενος ἡμῖν, ταῖς αὐτῶν ἱκεσίαις, τὸ μέγα ἔλεος."

trVE.AposTheotokion.mode = miscellanea_gr_GR_cog.misc.Mode4
trVE.AposTheotokion.melody = ""
trVE.AposTheotokion.incipit = "Ῥῦσαι ἡμᾶς ἐκ τῶν ἀναγκῶν…"
trVE.AposTheotokion.text = "Ῥῦσαι ἡμᾶς ἐκ τῶν ἀναγκῶν ἡμῶν Μήτηρ Χριστοῦ τοῦ Θεοῦ, ἡ τεκοῦσα τὸν τῶν ὅλων Ποιητήν, ἵνα πάντοτε κράζωμέν σοι· Χαῖρε ἡ προστασία τῶν ψυχῶν ἡμῶν."

trMA.Kathisma21.mode = miscellanea_gr_GR_cog.misc.Mode6
trMA.Kathisma21.greekmelody = he.a.m6_gr_GR_cog.heAU.AngelikaiDynameis.greekname
trMA.Kathisma21.melody = he.a.m6_gr_GR_cog.heAU.AngelikaiDynameis.name
trMA.Kathisma21.incipit = "Ἁμαρτιῶν τῇ νόσῳ κατατηκόμενος, τῆς…"
trMA.Kathisma21.text = "Ἁμαρτιῶν τῇ νόσῳ κατατηκόμενος, τῆς ἀπογνώσεως κλίνῃ κατάκειμαι· διό με Ἰατρὲ τῶν ἀσθενούντων, ἐπίσκεψαι σῇ φιλανθρωπίᾳ, καὶ μὴ παραχωρήσῃς, ἐναφυπνῶσαι δεινῶς, εἰς θάνατον πανοικτίρμον, ἵνα βοῶ σοι θερμῶς, ὁ τοῦ ἐλέους χορηγός, Κύριε δόξα σοι."

trMA.Kathisma22.mode = miscellanea_gr_GR_cog.misc.Mode6
trMA.Kathisma22.melody = ""
trMA.Kathisma22.incipit = "Ἀρχὴ σωτηρίας, ἡ τοῦ Γαβριὴλ…"
trMA.Kathisma22.text = "Ἀρχὴ σωτηρίας, ἡ τοῦ Γαβριὴλ προσηγορία, πρὸς τὴν Παρθένον γέγονεν· ἤκουσε γὰρ τὸ Χαῖρε, οὐκ ἀπέφυγε τὸν ἀσπασμόν, οὐκ ἐδίστασεν ὡς ἡ Σάῤῥα ἐν τῇ σκηνῇ, ἀλλʹ οὕτως ἔλεγεν· Ἰδοὺ ἡ δούλη Κυρίου, γένοιτό μοι κατὰ τὸ ῥῆμά σου."

trMA.Kathisma31.mode = miscellanea_gr_GR_cog.misc.Mode5
trMA.Kathisma31.melody = "Λάμπει σήμερον."
trMA.Kathisma31.incipit = "Χθὲς καὶ σήμερον, ἡ νόσος…"
trMA.Kathisma31.text = "Χθὲς καὶ σήμερον, ἡ νόσος ἡ τοῦ Λαζάρου· ταύτην γὰρ Χριστῷ δηλοῦσιν αἱ σύγγονοι, ἐν χαρᾷ ἡ Βηθανία προευτρεπίζου, τὸν Δεσπότην ξεναγωγῆσαι καὶ Βασιλέα, σὺν ἡμῖν βοῆσαι, Κύριε δόξα σοι."

trMA.Kathisma32.mode = miscellanea_gr_GR_cog.misc.Mode5
trMA.Kathisma32.melody = ""
trMA.Kathisma32.incipit = oc.m5.d3_gr_GR_cog.ocMA.Kathisma33.incipit
trMA.Kathisma32.text = oc.m5.d3_gr_GR_cog.ocMA.Kathisma33.text

