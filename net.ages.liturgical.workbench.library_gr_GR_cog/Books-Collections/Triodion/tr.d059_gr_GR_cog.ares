trMA.AposTheotokion.text = oc.m5.d7_gr_GR_cog.ocVE.AposTheotokion.text

trTE.Troparion.mode = miscellanea_gr_GR_cog.misc.Mode5
trTE.Troparion.melody = ""
trTE.Troparion.incipit = "Ἀκαταμάχητον τεῖχος ἔδωκας τῇ πόλει…"
trTE.Troparion.text = "Ἀκαταμάχητον τεῖχος ἔδωκας τῇ πόλει ἡμῶν, τὴν τεκοῦσάν σε Παρθένον, διʹ αὐτῆς Σῶτερ, ἀπὸ τῶν κύκλῳ συνεπιτιθεμένων κακῶν, ἐξελοῦ δεόμεθα τὰς ψυχὰς ἡμῶν."
trTE.Troparion2.incipit = "διʹ αὐτῆς Σῶτερ, ἀπὸ τῶν…"
trTE.Troparion2.text = "διʹ αὐτῆς Σῶτερ, ἀπὸ τῶν κύκλῳ συνεπιτιθεμένων κακῶν, ἐξελοῦ δεόμεθα τὰς ψυχὰς ἡμῶν."
