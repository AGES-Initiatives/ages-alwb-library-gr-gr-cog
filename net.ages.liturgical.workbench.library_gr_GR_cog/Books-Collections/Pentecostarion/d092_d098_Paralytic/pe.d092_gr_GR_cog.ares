peMA.Ode1C11.ode = miscellanea_gr_GR_cog.misc.Ode1
peMA.Ode1C11.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode1C11.greekmelody = he.h.m3_gr_GR_cog.heHE.ThavmastosEndoxos.greekname
peMA.Ode1C11.melody = he.h.m3_gr_GR_cog.heHE.ThavmastosEndoxos.name
peMA.Ode1C11.incipit = "Ἐνεργῶν σημεῖα, ποιῶν τέρατα, μόνε…"
peMA.Ode1C11.text = "Ἐνεργῶν σημεῖα, ποιῶν τέρατα, μόνε Βασιλεῦ, σταυρὸν ἐθελουσίως ὑπέστης, ὡς οἰκτίρμων, καὶ θάνατον θανάτῳ θανατώσας, ἐζωοποίησας ἡμᾶς."

peMA.Ode1C12.ode = miscellanea_gr_GR_cog.misc.Ode1
peMA.Ode1C12.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode1C12.melody = he.h.m3_gr_GR_cog.heHE.ThavmastosEndoxos.name
peMA.Ode1C12.incipit = "Τῇ Χριστοῦ Ἐγέρσει, λαοὶ σήμερον…"
peMA.Ode1C12.text = "Τῇ Χριστοῦ Ἐγέρσει, λαοὶ σήμερον, χορεύσωμεν πιστῶς, ἐσκύλευται ὁ ᾍδης, δεσμίους οὓς κατεῖχεν, ἀπέδωκε σπουδαίως, ἀνυμνοῦντας τὰ μεγαλεῖα τοῦ Θεοῦ."

peMA.Ode1C13.ode = miscellanea_gr_GR_cog.misc.Ode1
peMA.Ode1C13.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode1C13.melody = he.h.m3_gr_GR_cog.heHE.ThavmastosEndoxos.name
peMA.Ode1C13.incipit = "Ὁ δυνάμει θείᾳ ποτὲ Παράλυτον…"
peMA.Ode1C13.text = "Ὁ δυνάμει θείᾳ ποτὲ Παράλυτον, λόγῳ σου Χριστέ, συσφίγξας καὶ προστάξας, αὐτῷ τὴν κλίνην ἆραι, χρονίως ἀσθενοῦντι, τὴν ψυχήν μου νοσοῦσαν, ἴασαι δεινῶς."

peMA.Ode1C1F1.ode = miscellanea_gr_GR_cog.misc.Ode1
peMA.Ode1C1F1.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode1C1F1.greekmelody = he.h.m3_gr_GR_cog.heHE.ThavmastosEndoxos.greekname
peMA.Ode1C1F1.melody = he.h.m3_gr_GR_cog.heHE.ThavmastosEndoxos.name
peMA.Ode1C1F1.incipit = "Ἐν τῇ κολυμβήθρᾳ…"
peMA.Ode1C1F1.text = "Ἐν τῇ κολυμβήθρᾳ, ποτὲ Ἄγγελος, τῇ Προβατικῇ, κατήρχετο, καὶ ἕνα ἰᾶτο κατὰ χρόνον· Βαπτίσματι δὲ θείῳ νῦν καθαίρει, ἄπειρα πλήθη ὁ Χριστός."

peMA.Ode1C1F2.ode = miscellanea_gr_GR_cog.misc.Ode1
peMA.Ode1C1F2.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode1C1F2.melody = he.h.m3_gr_GR_cog.heHE.ThavmastosEndoxos.name
peMA.Ode1C1F2.incipit = "Ἀρχηγὲ Ἀγγέλων…"
peMA.Ode1C1F2.text = "Ἀρχηγὲ Ἀγγέλων, Ἀρχιστράτηγε τῶν ἄνω λειτουργῶν, τοὺς συναθροιζομένους, ἐν τῷ σεπτῷ ναῷ σου, καὶ τὸν Θεὸν ἀνυμνοῦντας, σκέπε, φρούρει, ἀπὸ παντοίων πειρασμῶν."

peMA.Ode1C1Tr.ode = miscellanea_gr_GR_cog.misc.Ode1
peMA.Ode1C1Tr.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode1C1Tr.melody = he.h.m3_gr_GR_cog.heHE.ThavmastosEndoxos.name
peMA.Ode1C1Tr.incipit = "Ἐν τρισὶ προσώποις…"
peMA.Ode1C1Tr.text = "Ἐν τρισὶ προσώποις, μιᾷ φύσει δέ, ἄναρχον Θεόν, ὑμνήσωμεν ἀπαύστως μετὰ τῶν Ἀσωμάτων, Πατέρα, Λόγον, Πνεῦμα, βασιλείαν ἔχουσαν, καὶ κράτος ἀμερές."

peMA.Ode1C1T.ode = miscellanea_gr_GR_cog.misc.Ode1
peMA.Ode1C1T.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode1C1T.melody = he.h.m3_gr_GR_cog.heHE.ThavmastosEndoxos.name
peMA.Ode1C1T.incipit = "Τὴν τιμῶσαν ταύτην…"
peMA.Ode1C1T.text = "Τὴν τιμῶσαν ταύτην, Ἁγνὴ πόλιν σου πάντοτε πιστῶς, περίσῳζε κινδύνων, ἁλώσεως βαρβάρων, πολέμου ἐμφυλίου, καὶ μαχαίρας, καὶ πάσης ἄλλης ἀπειλῆς."

peMA.Ode3C1H.ode = miscellanea_gr_GR_cog.misc.Ode3
peMA.Ode3C1H.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode3C1H.greekmelody = he.h.m3_gr_GR_cog.heHE.SteiraPsychi.greekname
peMA.Ode3C1H.melody = he.h.m3_gr_GR_cog.heHE.SteiraPsychi.melody
peMA.Ode3C1H.incipit = he.h.m3_gr_GR_cog.heHE.SteiraPsychi.incipit
peMA.Ode3C1H.text = he.h.m3_gr_GR_cog.heHE.SteiraPsychi.text

peMA.Ode3C11.ode = miscellanea_gr_GR_cog.misc.Ode3
peMA.Ode3C11.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode3C11.greekmelody = he.h.m3_gr_GR_cog.heHE.SteiraPsychi.greekname
peMA.Ode3C11.melody = he.h.m3_gr_GR_cog.heHE.SteiraPsychi.name
peMA.Ode3C11.incipit = "Ὁρῶν σε πάλαι ἥλιος…"
peMA.Ode3C11.text = "Ὁρῶν σε πάλαι ἥλιος, ξύλῳ κρεμάμενον, Λόγε τὸ φῶς συνέστειλε, καὶ ἐδονεῖτο ἡ γῆ ἅπασα, νεκροὶ ἐξανίσταντο, νεκροῦ γενομένου σου, παντοδύναμε."

peMA.Ode3C12.ode = miscellanea_gr_GR_cog.misc.Ode3
peMA.Ode3C12.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode3C12.melody = he.h.m3_gr_GR_cog.heHE.SteiraPsychi.name
peMA.Ode3C12.incipit = "Μετὰ ψυχῆς ἐλθόντος σου, ἐν…"
peMA.Ode3C12.text = "Μετὰ ψυχῆς ἐλθόντος σου, ἐν τῇ κοιλίᾳ τῆς γῆς, ψυχὰς ἅσπερ ἐκέκτητο, ἐναπεδίδου ᾍδης σπουδῇ, βοώσας τῷ κράτει σου, ᾠδὴν χαριστήριον, μόνε Κύριε."

peMA.Ode3C1F1.ode = miscellanea_gr_GR_cog.misc.Ode3
peMA.Ode3C1F1.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode3C1F1.greekmelody = he.h.m3_gr_GR_cog.heHE.SteiraPsychi.greekname
peMA.Ode3C1F1.melody = he.h.m3_gr_GR_cog.heHE.SteiraPsychi.name
peMA.Ode3C1F1.incipit = "Τὴν ἐπὶ χρόνους πλείονας…"
peMA.Ode3C1F1.text = "Τὴν ἐπὶ χρόνους πλείονας, ἐξασθενοῦσαν δεινῶς, ψυχήν μου ὑπεράγαθε, ὡς τὸν Παράλυτον πρίν, ὑγίωσον· ὡς ἂν βηματίζω σου, τὰς τρίβους, ἃς ἔδειξας τοῖς ποθοῦσί σε."

peMA.Ode3C1F2.ode = miscellanea_gr_GR_cog.misc.Ode3
peMA.Ode3C1F2.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode3C1F2.melody = he.h.m3_gr_GR_cog.heHE.SteiraPsychi.name
peMA.Ode3C1F2.incipit = "Μετὰ τῶν ἄνω τάξεων…"
peMA.Ode3C1F2.text = "Μετὰ τῶν ἄνω τάξεων, Θεοῦ Ἀρχάγγελε, ἱκετηρίαν ποίησον, ὑπὲρ τῶν πίστει ἀνυμνούντων σε φρουρῶν, συντηρῶν ἡμᾶς τοῦ βίου τοῖς πάθεσι περιπίπτοντας."

peMA.Ode3C1Tr.ode = miscellanea_gr_GR_cog.misc.Ode3
peMA.Ode3C1Tr.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode3C1Tr.melody = he.h.m3_gr_GR_cog.heHE.SteiraPsychi.name
peMA.Ode3C1Tr.incipit = "Δόξα, Πατρὶ βοήσωμεν…"
peMA.Ode3C1Tr.text = "Δόξα, Πατρὶ βοήσωμεν, Υἱῷ, καὶ Πνεύματι· εἷς γὰρ ὑπάρχει φύσει Θεός, ὃν αἱ δυνάμεις πᾶσαι τῶν οὐρανῶν, ἐν φόβῳ δοξάζουσι, τό, Ἅγιος, Ἅγιος, Ἅγιος, κράζουσι."

peMA.Ode3C1T.ode = miscellanea_gr_GR_cog.misc.Ode3
peMA.Ode3C1T.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode3C1T.melody = he.h.m3_gr_GR_cog.heHE.SteiraPsychi.name
peMA.Ode3C1T.incipit = "Ἄσπορον ἔσχες σύλληψιν…"
peMA.Ode3C1T.text = "Ἄσπορον ἔσχες σύλληψιν, καὶ ὑπὲρ νοῦν τοκετόν, Παρθενομῆτορ ἄχραντε· ἔργον φρικῶδες, θαῦμα μέγιστον, Ἀγγέλοις τιμώμενον, βροτοῖς δοξαζόμενον, Κόρη Δέσποινα."

peMA.Ode3Kathisma1.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode3Kathisma1.greekmelody = he.a.m3_gr_GR_cog.heAU.TinOraiotita.greekname
peMA.Ode3Kathisma1.melody = he.a.m3_gr_GR_cog.heAU.TinOraiotita.name
peMA.Ode3Kathisma1.incipit = "Ῥῆμα Παράλυτον…"
peMA.Ode3Kathisma1.text = "Ῥῆμα Παράλυτον, μόνον συνέσφιγξεν, ὡς ὁ παγκόσμιος, Λόγος ἐφθέγξατο, τοῦ διʼ ἡμᾶς ἐπὶ τῆς γῆς ὀφθέντος διʼ εὐσπλαγχνίαν· ὅθεν καὶ τὸν κράββατον, ἐπιφέρων διήρχετο, κἂν οἱ Γραμματεῖς ὁρᾷν, τὸ πραχθὲν οὐχ ὑπέφερον, κακίας κατεχόμενοι φθόνῳ, τῷ ψυχὰς παραλύοντι."

peMA.Ode3Kathisma2.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode3Kathisma2.greekmelody = he.a.m3_gr_GR_cog.heAU.TinOraiotita.greekname
peMA.Ode3Kathisma2.melody = he.a.m3_gr_GR_cog.heAU.TinOraiotita.melody
peMA.Ode3Kathisma2.incipit = he.a.m3_gr_GR_cog.heAU.TinOraiotita.incipit
peMA.Ode3Kathisma2.text = he.a.m3_gr_GR_cog.heAU.TinOraiotita.text

peMA.Ode4C1H.ode = miscellanea_gr_GR_cog.misc.Ode4
peMA.Ode4C1H.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode4C1H.greekmelody = he.h.m3_gr_GR_cog.heHE.ToKataskionOros.greekname
peMA.Ode4C1H.melody = he.h.m3_gr_GR_cog.heHE.ToKataskionOros.melody
peMA.Ode4C1H.incipit = he.h.m3_gr_GR_cog.heHE.ToKataskionOros.incipit
peMA.Ode4C1H.text = he.h.m3_gr_GR_cog.heHE.ToKataskionOros.text

peMA.Ode4C11.ode = miscellanea_gr_GR_cog.misc.Ode4
peMA.Ode4C11.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode4C11.greekmelody = he.h.m3_gr_GR_cog.heHE.ToKataskionOros.greekname
peMA.Ode4C11.melody = he.h.m3_gr_GR_cog.heHE.ToKataskionOros.name
peMA.Ode4C11.incipit = "Τῶν Ἑβραίων ὁ δῆμος, φθόνῳ…"
peMA.Ode4C11.text = "Τῶν Ἑβραίων ὁ δῆμος, φθόνῳ τηκόμενος, ξύλῳ ἐσταύρωσέ σε, Κύριε, καὶ λύσας τὸ κατάκριμα, τὸ τοῦ θανάτου ὡς κραταιὸς ἐξεγήγερσαι, τὸν κόσμον σεαυτῷ συνεγείρας."

peMA.Ode4C12.ode = miscellanea_gr_GR_cog.misc.Ode4
peMA.Ode4C12.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode4C12.melody = he.h.m3_gr_GR_cog.heHE.ToKataskionOros.name
peMA.Ode4C12.incipit = "Μετὰ μύρων Γυναῖκες, τί τὸ…"
peMA.Ode4C12.text = "Μετὰ μύρων Γυναῖκες, τί τὸ ἀκένωτον μύρον, ζητεῖτε; ἐξεγήγερται, ταῖς Μυροφόροις ἔφησεν, ὁ καθεζόμενος ἐν λευκοῖς, τὴν ὑφήλιον πληρώσας, νοητῆς εὐωδίας."

peMA.Ode4C13.ode = miscellanea_gr_GR_cog.misc.Ode4
peMA.Ode4C13.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode4C13.melody = he.h.m3_gr_GR_cog.heHE.ToKataskionOros.name
peMA.Ode4C13.incipit = "Δούλου φέρων εἰκόνα, ὑπερβολῇ εὐσπλαγχνίας…"
peMA.Ode4C13.text = "Δούλου φέρων εἰκόνα, ὑπερβολῇ εὐσπλαγχνίας, βαδίζων παραγέγονας, καὶ ὑγιῆ ἀπέδειξας, τὸν ἐπὶ χρόνοις Λόγε πολλοῖς, κατακείμενον, κελεύσας, καὶ τὸν κράββατον ἆραι."

peMA.Ode4C1F1.ode = miscellanea_gr_GR_cog.misc.Ode4
peMA.Ode4C1F1.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode4C1F1.greekmelody = he.h.m3_gr_GR_cog.heHE.ToKataskionOros.greekname
peMA.Ode4C1F1.melody = he.h.m3_gr_GR_cog.heHE.ToKataskionOros.name
peMA.Ode4C1F1.incipit = "Ἄγγελος μὲν Κυρίου…"
peMA.Ode4C1F1.text = "Ἄγγελος μὲν Κυρίου, κατὰ καιρὸν καταβαίνων, τὸ ὕδωρ ἐν τῇ Προβατικῇ ἐτάρασσε, τελῶν ὑγιῆ ἕνα καὶ μόνον· ὁ δὲ Χριστός, πλήθη ἄπειρα, τῷ θείῳ Βαπτισμῷ διασῴζει."

peMA.Ode4C1F2.ode = miscellanea_gr_GR_cog.misc.Ode4
peMA.Ode4C1F2.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode4C1F2.melody = he.h.m3_gr_GR_cog.heHE.ToKataskionOros.name
peMA.Ode4C1F2.incipit = "Ἀρχηγὲ τῶν Ἀγγέλων…"
peMA.Ode4C1F2.text = "Ἀρχηγὲ τῶν Ἀγγέλων, καὶ ὁδηγὲ πλανωμένων, Κυρίου Ἀρχιστράτηγε, τῇ ὥρᾳ ταύτῃ, μέσον ἡμῶν παραγενοῦ, καὶ τὰς προσευχὰς πάντων προσάγαγε, τῷ μόνῳ ποιητῇ καὶ Δεσπότῃ."

peMA.Ode4C1Tr.ode = miscellanea_gr_GR_cog.misc.Ode4
peMA.Ode4C1Tr.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode4C1Tr.melody = he.h.m3_gr_GR_cog.heHE.ToKataskionOros.name
peMA.Ode4C1Tr.incipit = "Ἑνουμένη τῇ φύσει…"
peMA.Ode4C1Tr.text = "Ἑνουμένη τῇ φύσει, διαιρουμένη προσώποις, ὑπάρχει ἡ ἁγία Τριάς· Πατὴρ ὁ ὑπερούσιος, ὁ συναΐδιός τε Υἱός, καὶ τὸ ἅγιον καὶ μόνον, παντοδύναμον Πνεῦμα."

peMA.Ode4C1T.ode = miscellanea_gr_GR_cog.misc.Ode4
peMA.Ode4C1T.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode4C1T.melody = he.h.m3_gr_GR_cog.heHE.ToKataskionOros.name
peMA.Ode4C1T.incipit = "Ἡ Ἁγνὴ πῶς θηλάζεις…"
peMA.Ode4C1T.text = "Ἡ Ἁγνὴ πῶς θηλάζεις; πῶς δὲ ἐγέννησας βρέφος, τοῦ Ἀδὰμ ἀρχαιότερον; πῶς ἐν ἀγκάλαις φέρεις Υἱόν, τὸν ἐπὶ ὤμων χερουβικῶν; ὡς ἐπίσταται, ὡς οἶδεν, ὁ τὸ πᾶν οὐσιώσας."

peMA.Ode5C1H.ode = miscellanea_gr_GR_cog.misc.Ode5
peMA.Ode5C1H.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode5C1H.greekmelody = he.h.m3_gr_GR_cog.heHE.FotiToAnespero.greekname
peMA.Ode5C1H.melody = he.h.m3_gr_GR_cog.heHE.FotiToAnespero.melody
peMA.Ode5C1H.incipit = he.h.m3_gr_GR_cog.heHE.FotiToAnespero.incipit
peMA.Ode5C1H.text = he.h.m3_gr_GR_cog.heHE.FotiToAnespero.text

peMA.Ode5C11.ode = miscellanea_gr_GR_cog.misc.Ode5
peMA.Ode5C11.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode5C11.greekmelody = he.h.m3_gr_GR_cog.heHE.FotiToAnespero.greekname
peMA.Ode5C11.melody = he.h.m3_gr_GR_cog.heHE.FotiToAnespero.name
peMA.Ode5C11.incipit = "Ὑψώθης ἐπὶ ξύλου τοῦ Σταυροῦ…"
peMA.Ode5C11.text = "Ὑψώθης ἐπὶ ξύλου τοῦ Σταυροῦ, καὶ κόσμον ἅπαντα συνανύψωσας, καὶ γενόμενος ἐν νεκροῖς ὁ Θεός, νεκροὺς ἐγείρεις τοὺς ἀπʼ αἰῶνος."

peMA.Ode5C12.ode = miscellanea_gr_GR_cog.misc.Ode5
peMA.Ode5C12.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode5C12.melody = he.h.m3_gr_GR_cog.heHE.FotiToAnespero.name
peMA.Ode5C12.incipit = "Ἀνέστη καθὼς εἶπεν ὁ Χριστός…"
peMA.Ode5C12.text = "Ἀνέστη καθὼς εἶπεν ὁ Χριστός, κενώσας ἅπαντα ᾍδου τὰ βασίλεια, καὶ ὀπτάνεται Ἀποστόλοις, χαρᾶς μεταδιδοὺς διαιωνιζούσης."

peMA.Ode5C13.ode = miscellanea_gr_GR_cog.misc.Ode5
peMA.Ode5C13.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode5C13.melody = he.h.m3_gr_GR_cog.heHE.FotiToAnespero.name
peMA.Ode5C13.incipit = "Λευκείμων καθωράθη γυναιξίν, ἀστράπτων Ἄγγελος…"
peMA.Ode5C13.text = "Λευκείμων καθωράθη γυναιξίν, ἀστράπτων Ἄγγελος, φάσκων· Μὴ κλαίετε, ἡ ζωὴ ἡμῶν ἐξεγήγερται, τοὺς ἐν τοῖς τάφοις νεκροὺς ζωώσας."

peMA.Ode5C1F1.ode = miscellanea_gr_GR_cog.misc.Ode5
peMA.Ode5C1F1.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode5C1F1.greekmelody = he.h.m3_gr_GR_cog.heHE.FotiToAnespero.greekname
peMA.Ode5C1F1.melody = he.h.m3_gr_GR_cog.heHE.FotiToAnespero.name
peMA.Ode5C1F1.incipit = "Παράλυτον ὡς ἤγειρας…"
peMA.Ode5C1F1.text = "Παράλυτον ὡς ἤγειρας Χριστέ, τὴν παρειμένην μου ψυχὴν ὑγίωσον, παραβάσεσι, καὶ βαδίζειν με τὰς σὰς ὀρθὰς εὐόδωσον τρίβους."

peMA.Ode5C1F2.ode = miscellanea_gr_GR_cog.misc.Ode5
peMA.Ode5C1F2.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode5C1F2.melody = he.h.m3_gr_GR_cog.heHE.FotiToAnespero.name
peMA.Ode5C1F2.incipit = "Ἀγγέλων ἀρχηγέτα…"
peMA.Ode5C1F2.text = "Ἀγγέλων ἀρχηγέτα Μιχαήλ, τὸν ἀθροιζόμενον ἐν τῷ τεμένει σου λαὸν σήμερον, καὶ κηρύττοντα τὰ τοῦ Θεοῦ, σῷζε μεγαλεῖα."

peMA.Ode5C1Tr.ode = miscellanea_gr_GR_cog.misc.Ode5
peMA.Ode5C1Tr.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode5C1Tr.melody = he.h.m3_gr_GR_cog.heHE.FotiToAnespero.name
peMA.Ode5C1Tr.incipit = "Ἰσότιμος τρισάριθμος…"
peMA.Ode5C1Tr.text = "Ἰσότιμος τρισάριθμος μονάς, διαιρουμένη μὲν πιστῶς ταῖς ὑποστάσεσιν, ἑνουμένη δὲ φύσει πέφυκε, Πατήρ, Υἱὸς καὶ τὸ θεῖον Πνεῦμα."

peMA.Ode5C1T.ode = miscellanea_gr_GR_cog.misc.Ode5
peMA.Ode5C1T.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode5C1T.melody = he.h.m3_gr_GR_cog.heHE.FotiToAnespero.name
peMA.Ode5C1T.incipit = "Τὴν ἄσπορον λοχείαν…"
peMA.Ode5C1T.text = "Τὴν ἄσπορον λοχείαν σου Ἁγνή, τὴν ἀκατάληπτον ὑμνοῦμεν γέννησιν, μακαρίζοντες ὡς Μητέρα σε, τοῦ ποιητοῦ πάντων καὶ Δεσπότου."

peMA.Ode6C1H.ode = miscellanea_gr_GR_cog.misc.Ode6
peMA.Ode6C1H.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode6C1H.greekmelody = he.h.m3_gr_GR_cog.heHE.VythosMoiTonPathon.greekname
peMA.Ode6C1H.melody = he.h.m3_gr_GR_cog.heHE.VythosMoiTonPathon.melody
peMA.Ode6C1H.incipit = he.h.m3_gr_GR_cog.heHE.VythosMoiTonPathon.incipit
peMA.Ode6C1H.text = he.h.m3_gr_GR_cog.heHE.VythosMoiTonPathon.text

peMA.Ode6C11.ode = miscellanea_gr_GR_cog.misc.Ode6
peMA.Ode6C11.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode6C11.greekmelody = he.h.m3_gr_GR_cog.heHE.VythosMoiTonPathon.greekname
peMA.Ode6C11.melody = he.h.m3_gr_GR_cog.heHE.VythosMoiTonPathon.name
peMA.Ode6C11.incipit = "Ὑψώθης ἑκουσίως ἐν ξύλῳ, ἐτέθης…"
peMA.Ode6C11.text = "Ὑψώθης ἑκουσίως ἐν ξύλῳ, ἐτέθης ὡς νεκρὸς ἐν μνημείῳ, καί τοὺς ἐν ᾍδῃ νεκρούς, πάντας ὁμοῦ ζωώσας Χριστέ, ἀνέστησας θεϊκῇ δυναστείᾳ."

peMA.Ode6C12.ode = miscellanea_gr_GR_cog.misc.Ode6
peMA.Ode6C12.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode6C12.melody = he.h.m3_gr_GR_cog.heHE.VythosMoiTonPathon.name
peMA.Ode6C12.incipit = "Ὁ ᾍδης συναντήσας σοι κάτω…"
peMA.Ode6C12.text = "Ὁ ᾍδης συναντήσας σοι κάτω, οἰκτίρμον ἐπικράνθη, δεσμώτας, ἀποδιδοὺς ἐν σπουδῇ, σοῦ τὴν φρικτὴν ὑμνοῦντας Σωτὴρ Ἀνάστασιν, ἐν φωναῖς ἀσιγήτοις."

peMA.Ode6C13.ode = miscellanea_gr_GR_cog.misc.Ode6
peMA.Ode6C13.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode6C13.melody = he.h.m3_gr_GR_cog.heHE.VythosMoiTonPathon.name
peMA.Ode6C13.incipit = "Οἱ θεῖοι Μαθηταὶ κατιδόντες ἐκ…"
peMA.Ode6C13.text = "Οἱ θεῖοι Μαθηταὶ κατιδόντες ἐκ τάφου, τὴν ζωὴν τῶν ἁπάντων, ἐγηγερμένον Χριστὸν πόθῳ πολλῷ, καὶ γνώμῃ εὐθεῖ, καὶ θυμηδίᾳ ψυχῆς προσεκύνουν."

peMA.Ode6C1F1.ode = miscellanea_gr_GR_cog.misc.Ode6
peMA.Ode6C1F1.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode6C1F1.greekmelody = he.h.m3_gr_GR_cog.heHE.VythosMoiTonPathon.greekname
peMA.Ode6C1F1.melody = he.h.m3_gr_GR_cog.heHE.VythosMoiTonPathon.name
peMA.Ode6C1F1.incipit = "Ὁ πάλαι ἐπὶ κλίνης…"
peMA.Ode6C1F1.text = "Ὁ πάλαι ἐπὶ κλίνης ὀδύνης, ἐν χρόνοις κατακείμενος πλείστοις, τῇ σῇ προστάξει Χριστέ, ἀρτιωθεὶς δοξάζει, ὑμνῶν τὴν εὐπλαγχνίαν, τὴν σὴν Ζωοδότα."

peMA.Ode6C1F2.ode = miscellanea_gr_GR_cog.misc.Ode6
peMA.Ode6C1F2.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode6C1F2.melody = he.h.m3_gr_GR_cog.heHE.VythosMoiTonPathon.name
peMA.Ode6C1F2.incipit = "Ἀγγέλων Μιχαὴλ ἀρχηγέτα…"
peMA.Ode6C1F2.text = "Ἀγγέλων Μιχαὴλ ἀρχηγέτα, τῷ θρόνῳ παρεστὼς τοῦ Δεσπότου, πάρεσο μέσον ἡμῶν, καθοδηγῶν πρὸς τρίβους ζωῆς, τοὺς σὲ προστάτην, θερμὸν κεκτημένους."

peMA.Ode6C1Tr.ode = miscellanea_gr_GR_cog.misc.Ode6
peMA.Ode6C1Tr.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode6C1Tr.melody = he.h.m3_gr_GR_cog.heHE.VythosMoiTonPathon.name
peMA.Ode6C1Tr.incipit = "Τριάδα τοῖς προσώποις…"
peMA.Ode6C1Tr.text = "Τριάδα τοῖς προσώποις σε σέβω, μονάδα τῇ οὐσίᾳ κηρύττω, ἄναρχε Πάτερ, Υἱέ, Πνεῦμα εὐθές, Θεὲ τοῦ παντός, μετὰ τῶν ἄνω φρικτῶν στρατευμάτων."

peMA.Ode6C1T.ode = miscellanea_gr_GR_cog.misc.Ode6
peMA.Ode6C1T.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode6C1T.melody = he.h.m3_gr_GR_cog.heHE.VythosMoiTonPathon.name
peMA.Ode6C1T.incipit = "Ὁ πάντα θείῳ νεύματι…"
peMA.Ode6C1T.text = "Ὁ πάντα θείῳ νεύματι φέρων, κρατεῖται Θεοτόκε Παρθένε, ἐν ταῖς ἀγκάλαις ταῖς σαῖς, πάντας ἡμᾶς ἁρπάζων χειρός, δουλείας τοῦ πονηροῦ, ὡς οἰκτίρμων."

peMA.Kontakion1.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Kontakion1.greekmelody = he.a.m3_gr_GR_cog.heAU.IParthenosSimeron.greekname
peMA.Kontakion1.melody = he.a.m3_gr_GR_cog.heAU.IParthenosSimeron.name
peMA.Kontakion1.incipit = "Τὴν ψυχήν μου Κύριε…"
peMA.Kontakion1.text = "Τὴν ψυχήν μου Κύριε, ἐν ἁμαρτίαις παντοίαις, καὶ ἀτόποις πράξεσι, δεινῶς παραλελυμένην, ἔγειρον τῇ θεϊκῇ σου ἐπιστασίᾳ, ὥς περ καὶ τὸν Παράλυτον, ἤγειρας πάλαι, ἵνα κράζω σεσωσμένος· Οἰκτίρμον δόξα, Χριστέ, τῷ κράτει σου."

peMA.Oikos1.text = "Ὁ χειρί σου δρακὶ περικρατῶν τὰ πέρατα, Ἰησοῦ ὁ Θεός, ὁ τῷ Πατρὶ συνάναρχος, καὶ Πνεύματι ἁγίῳ συνδεσπόζων ἁπάντων, σαρκὶ ἐφάνης, νόσους ἰώμενος, καὶ πάθη ἀπήλασας, τυφλοὺς ἐφώτισας, καὶ τὸν Παράλυτον λόγῳ θεϊκῷ σὺ ἐξανέστησας, περιπατεῖν ἀθρόως προστάξας, καὶ τὴν βαστάσασαν αὐτὸν κλίνην ἐπὶ τῶν ὤμων ἆραι· ὅθεν πάντες σὺν τούτῳ ἀνυμνοῦμεν καὶ ἐκβοῶμεν· Οἰκτίρμον δόξα, Χριστὲ τῷ κράτει σου."

peSY.commemoration.text = "Τῇ αὐτῇ ἡμέρᾳ, Κυριακῇ τετάρτῃ ἀπὸ τοῦ Πάσχα, τοῦ Παραλύτου μνείαν ποιούμεθα καὶ ὡς εἰκὸς τὸ τοιοῦτον ἑορτάζομεν θαῦμα."
peSY.commemoration.couplet1.title = titles_gr_GR_cog.ti.Verses
peSY.commemoration.couplet1.verse1 = "Τὸ ρῆμα Χριστοῦ σφίγμα τῷ παρειμένῳ."
peSY.commemoration.couplet1.verse2 = "Οὕτως ἴαμα τοῦτο ρῆμα καὶ μόνον."
peSY.commemoration.narration.p01 = "[paragraph 01]"
peSY.commemoration.narration.p02 = "[paragraph 02]"
peSY.commemoration.narration.p03 = "[paragraph 03]"
peSY.commemoration.narration.p04 = "[paragraph 04]"
peSY.commemoration.narration.p05 = "[paragraph 05]"
peSY.commemoration.narration.p06 = "[paragraph 06]"
peSY.commemoration.narration.p07 = "[paragraph 07]"
peSY.lastline = "Τῷ ἀπείρῳ ἐλέει σου, Χριστὲ ὁ Θεός, ἐλέησον ἡμᾶς. Ἀμήν."

peMA.Ode7C1H.ode = miscellanea_gr_GR_cog.misc.Ode7
peMA.Ode7C1H.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode7C1H.greekmelody = he.h.m3_gr_GR_cog.heHE.OTinFloga.greekname
