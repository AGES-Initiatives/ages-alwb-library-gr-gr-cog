meVE.Stichera02.mode = miscellanea_gr_GR_cog.misc.Mode5
meVE.Stichera02.melody = he.a.m5_gr_GR_cog.heAU.OsiePater.name
meVE.Stichera02.incipit = "Ὅσιε Πάτερ, Θεοφόρε Θεοδόσιε, εὑροῦσα…"
meVE.Stichera02.text = "Ὅσιε Πάτερ, Θεοφόρε Θεοδόσιε, εὑροῦσα ὡς ἐπόθει, τὴν καθαράν σου ψυχήν, τοῦ Πνεύματος ἡ χάρις τοῦ Παναγίου, σοὶ ἐνεσκήνωσεν ὡς ἄχραντον φῶς, οὗ τῇ ἐνεργείᾳ φαιδρῶς ἠγλαϊσμένος, Χριστὸν ἀπαύστως δοξολογεῖς, τὸν ἐν δυσὶ ταῖς οὐσίαις ἕνα Υἱόν, τὸν βαπτιζόμενον χειρὶ τῇ τοῦ Προδρόμου, καὶ μαρτυρούμενον τῇ φωνῇ τῇ πατρῴᾳ. Αὐτὸν ἱκέτευε, αὐτὸν δυσώπει Ὅσιε, δωρηθῆναι τῇ οἰκουμένῃ, ὁμόνοιαν, εἰρήνην, καὶ μέγα ἔλεος."

meVE.Stichera03.mode = miscellanea_gr_GR_cog.misc.Mode5
meVE.Stichera03.melody = he.a.m5_gr_GR_cog.heAU.OsiePater.name
meVE.Stichera03.incipit = "Ὅσιε Πάτερ, Θεοφόρε Θεοδόσιε, εἰκότως…"
meVE.Stichera03.text = "Ὅσιε Πάτερ, Θεοφόρε Θεοδόσιε, εἰκότως ἠξιώθης τῆς μακαρίας ζωῆς, ἁγνείᾳ καὶ ἀσκήσει ταύτην εὑρών· ζῶν γὰρ μετέστης πρὸς τὴν ἄνω ζωήν, πᾶσι χαίρειν εἰπών, καὶ σὺν τοῖς Ἀσωμάτοις, Χριστὸν ἀπαύστως δοξολογῶν, τὸν ἐκ Παρθένου ἀφράστως σάρκα λαβόντα, καὶ καταδύσαντα τὰς ἡμῶν ἁμαρτίας, ἐν τῷ Βαπτίσματι, ἐν ῥείθροις Ἰορδάνου. Αὐτὸν ἱκέτευε, αὐτὸν δυσώπει Ὅσιε, δωρηθῆναι τῇ οἰκουμένῃ, ὁμόνοιαν, εἰρήνην, καὶ μέγα ἔλεος."

meVE.Stichera8910.text = properties_gr_GR_cog.media.key

meVE.SticGlory.poet = poets_gr_GR_cog.po.Anatoliou
meVE.SticGlory.mode = miscellanea_gr_GR_cog.misc.Mode2
meVE.SticGlory.melody = ""
meVE.SticGlory.incipit = "Ὅσιε Πάτερ, ὡς πόλιν ζῶντος…"
meVE.SticGlory.text = "Ὅσιε Πάτερ, ὡς πόλιν ζῶντος Θεοῦ, εὔφραναν τὴν ἱεράν σου ψυχήν, τοῦ ποταμοῦ τὰ ὁρμήματα, τοῦ ὕδατος τῆς ἀφέσεως, τοῦ διελθόντος τὸν Ἰορδάνην, καὶ πᾶσι τοῖς πέρασι, τῆς εὐσεβείας τὸν λόγον πηγάσαντος, Χριστοῦ τοῦ Θεοῦ ἡμῶν, ὃν ἱκέτευε παμμάκαρ Θεοδόσιε, τοῦ σωθῆναι τὰς ψυχὰς ἡμῶν."

meVE.SticBoth.poet = poets_gr_GR_cog.po.Vyzantiou
meVE.SticBoth.mode = miscellanea_gr_GR_cog.misc.Mode2
meVE.SticBoth.melody = ""
