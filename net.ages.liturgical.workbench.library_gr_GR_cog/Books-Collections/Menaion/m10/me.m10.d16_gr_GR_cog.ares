meVE.Apolytikion1ger.poet = poets_gr_GR_cog.po.Gerasimou
meVE.Apolytikion1ger.mode = miscellanea_gr_GR_cog.misc.Mode1
meVE.Apolytikion1ger.melody = he.a.m1_gr_GR_cog.heAU.TouLithouSfragisthentos.name
meVE.Apolytikion1ger.incipit = "Τὸν Ἥλιον τῆς δόξης Σταυρῷ…"
meVE.Apolytikion1ger.text = "Τὸν Ἥλιον τῆς δόξης Σταυρῷ προσηλωθέντα, καὶ τοῖς ἐν σκιᾷ τοῦ θανάτου ἐκλάμποντα ὡς εἶδες, ηὐγάσθης αὐτοῦ ταῖς ἀστραπαῖς, καὶ ἤθλησας Λογγῖνε εὐσεβῶς· διὰ τοῦτο νοσημάτων παντοδαπῶν, λυτροῦσαι τοὺς ἐκβοῶντας· δόξα τῷ δεδωκότι σοι ἰσχύν, δόξα τῷ σὲ στεφανώσαντι, δόξα τῷ ἐνεργοῦντι διὰ σοῦ, πᾶσιν ἰάματα."

meMA.C1.acrostic.title = "Κανὼν τοῦ Ἁγίου,"
meMA.C1.acrostic.notice1 = "οὗ ἡ Ἀκροστιχίς·"
meMA.C1.acrostic.text = "Τὸ Λογγίνου μέγιστον ὑμνήσω κλέος. Ἰωσήφ."
meMA.C1.title = titles_gr_GR_cog.ti.CanonSaintM
meMA.C1.verse = prayers_gr_GR_cog.pr.verse_SaintM.text

meMA.Ode1C11.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode1C11.greekmelody = he.h.m4_gr_GR_cog.heHE.ThalassisToErythraion.greekname
meMA.Ode1C11.melody = he.h.m4_gr_GR_cog.heHE.ThalassisToErythraion.name
meMA.Ode1C11.incipit = "Ταῖς θείαις, φωτοχυσίαις Ἔνδοξε, περιλαμπόμενος…"
meMA.Ode1C11.text = "Ταῖς θείαις, φωτοχυσίαις Ἔνδοξε, περιλαμπόμενος, καὶ σὺν Ἀγγέλων τάξεσι Χριστῷ, τῷ Θεῷ παριστάμενος, τὴν φωτοφόρον μνήμην σου, τοὺς ἐκτελοῦντας καταφαίδρυνον."

meMA.Ode1C12.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode1C12.melody = he.h.m4_gr_GR_cog.heHE.ThalassisToErythraion.name
meMA.Ode1C12.incipit = "Οὐδέν σε, τῆς πρὸς Θεὸν…"
meMA.Ode1C12.text = "Οὐδέν σε, τῆς πρὸς Θεὸν ἑνώσεως, Μάρτυς ἐχώρισε, κεχωρισμένον πάσης σαρκικῆς, ἡδονῆς διὰ πίστεως, καὶ καθαρῶς τῷ ἔρωτι, τῷ ἀκροτάτῳ ἐνηδόμενον."

meMA.Ode1C13.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode1C13.melody = he.h.m4_gr_GR_cog.heHE.ThalassisToErythraion.name
meMA.Ode1C13.incipit = "Λατρεύων, Θεῷ τῷ ζῶντι ἔνδοξε…"
meMA.Ode1C13.text = "Λατρεύων, Θεῷ τῷ ζῶντι ἔνδοξε, Λογγῖνε πάνσοφε, νεκροῖς ξοάνοις μάκαρ οὐδαμῶς, σὺ λατρείαν προσένειμας· διὸ πρὸς ἀτελεύτητον, θανατωθεὶς ζωὴν κατήντησας."

meMA.Ode1C1T.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode1C1T.melody = he.h.m4_gr_GR_cog.heHE.ThalassisToErythraion.name
meMA.Ode1C1T.incipit = "Ὁ Λόγος, ὁ τοῦ Θεοῦ…"
meMA.Ode1C1T.text = "Ὁ Λόγος, ὁ τοῦ Θεοῦ ἐν μήτρᾳ σου, λόγῳ συνείληπται, τῆς ἀλογίας ῥύσασθαι βροτούς, βουληθεὶς ἀγαθότητι, Παρθενομῆτορ ἄχραντε, εὐλογημένη παναμώμητε."

meMA.Ode3C11.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C11.greekmelody = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.greekname
meMA.Ode3C11.melody = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.name
meMA.Ode3C11.incipit = "Γνωρίσας ἐπὶ Σταυροῦ, Υἱὸν Θεοῦ…"
meMA.Ode3C11.text = "Γνωρίσας ἐπὶ Σταυροῦ, Υἱὸν Θεοῦ τὸν ἀληθῆ Κύριον, ὑπὲρ ἡμῶν πάσχοντα, τούτῳ ὁλοψύχως ἐπίστευσας."

meMA.Ode3C12.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C12.melody = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.name
meMA.Ode3C12.incipit = "Γῆ ὥσπερ πίων ἡ σή…"
meMA.Ode3C12.text = "Γῆ ὥσπερ πίων ἡ σή, ψυχὴ λαβοῦσα λογικὰ σπέρματα, στάχυν Σοφὲ ὥριμον, θείας εὐσεβείας ἐβλάστησεν."

meMA.Ode3C13.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C13.melody = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.name
meMA.Ode3C13.incipit = "Ἱστίῳ τῷ τοῦ Σταυροῦ, τὸ…"
meMA.Ode3C13.text = "Ἱστίῳ τῷ τοῦ Σταυροῦ, τὸ τῶν βασάνων διελθὼν πέλαγος, τοὺς γαληνοὺς ἔφθασας, ἔνδοξε λιμένας γηθόμενος."

meMA.Ode3C1T.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C1T.melody = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.name
meMA.Ode3C1T.incipit = "Νοσοῦσάν μου τὴν ψυχήν, ταῖς…"
meMA.Ode3C1T.text = "Νοσοῦσάν μου τὴν ψυχήν, ταῖς τῶν παθῶν ἐπαγωγαῖς ἴασαι, Μήτηρ Θεοῦ Πάναγνε, καὶ πρὸς σωτηρίαν κατεύθυνον."

meMA.Ode3H.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3H.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3H.greekmelody = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.greekname
meMA.Ode3H.melody = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.melody
meMA.Ode3H.incipit = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.incipit
meMA.Ode3H.text = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.text

meMA.Ode3Kathisma1.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3Kathisma1.greekmelody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.greekname
meMA.Ode3Kathisma1.melody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.name
meMA.Ode3Kathisma1.incipit = "Σταυρωθέντα κατεῖδες τὸν Λυτρωτήν, καὶ…"
meMA.Ode3Kathisma1.text = "Σταυρωθέντα κατεῖδες τὸν Λυτρωτήν, καὶ αὐτοῦ ταῖς ἀκτῖσι καταυγασθείς, σκότος τὸ βαθύτατον, τῆς ἀγνοίας ἀπέφυγες, καὶ πρὸς θείαν γνῶσιν, σαυτὸν μετεῤῥύθμισας, πιστωθεὶς ὡς ἔστι, Θεὸς ὁ ὁρώμενος· ὅθεν καὶ ἀθλήσας, τοῖς χοροῖς ἠριθμήθης, Λογγῖνε μακάριε, τῶν Μαρτύρων γηθόμενος, μεθʼ ὧν πάντοτε ἔνδοξε, πρέσβευε Χριστῷ τῷ Θεῷ, τῶν πταισμάτων ἄφεσιν δωρήσασθαι, τοῖς ἑορτάζουσι πόθῳ, τὴν ἁγίαν μνήμην σου."

meMA.Ode3KathismaT.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3KathismaT.melody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.name
meMA.Ode3KathismaT.incipit = "Τὴν ψυχήν μου Παρθένε, τὴν…"
meMA.Ode3KathismaT.text = "Τὴν ψυχήν μου Παρθένε, τὴν ταπεινήν, τὴν ἐν ζάλῃ τοῦ βίου τῶν πειρασμῶν, νῦν ὡς ἀκυβέρνητον, ποντουμένην Πανάμωμε, ἁμαρτιῶν τῷ φόρτῳ, φανεῖσαν ὑπέραντλον, καὶ εἰς πυθμένα ᾍδου, πεσεῖν κινδυνεύουσαν· φθάσον Θεοτόκε, τῇ θερμῇ σου πρεσβείᾳ, καὶ σῶσον παρέχουσα, σὸν λιμένα τὸν εὔδιον, ἵνα πίστει κραυγάζω σοι· Πρέσβευε τῷ σῷ Υἱῷ καὶ Θεῷ, τῶν πταισμάτων δοῦναί μοι τὴν ἄφεσιν· σὲ γὰρ ἔχω ἐλπίδα, ὁ δοῦλός σου Δέσποινα."

meMA.Ode3KathismaSt.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3KathismaSt.melody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.name
meMA.Ode3KathismaSt.incipit = oc.m8.d4_gr_GR_cog.ocMA.Kathisma13.incipit
meMA.Ode3KathismaSt.text = oc.m8.d4_gr_GR_cog.ocMA.Kathisma13.text

meMA.Ode4C11.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C11.greekmelody = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.greekname
meMA.Ode4C11.melody = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.name
meMA.Ode4C11.incipit = "Ὀχυρώτατον κτησάμενος ἐν τῇ πίστει…"
meMA.Ode4C11.text = "Ὀχυρώτατον κτησάμενος ἐν τῇ πίστει, τὸν λογισμὸν μακάριε, ἀνίσχυρον πλάνης, ἔλυσας ὀχύρωμα, Λογγῖνε καὶ ἔκραζες· Δόξα τῇ δυνάμει σου Κύριε."

meMA.Ode4C12.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C12.melody = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.name
meMA.Ode4C12.incipit = "Ὑψωθέντα καὶ τῷ ξύλῳ προσηλωθέντα…"
meMA.Ode4C12.text = "Ὑψωθέντα καὶ τῷ ξύλῳ προσηλωθέντα, τὸν ἀπαθῆ θεώμενος, Κύριον τῆς δόξης, τοῦτον ὡμολόγησας, Υἱὸν Θεοῦ ἔνδοξε, πάσχοντα οἰκείῳ θελήματι."

meMA.Ode4C13.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C13.melody = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.name
meMA.Ode4C13.incipit = "Μαραινόμενον τὸν θάνατον τῷ θανάτῳ…"
meMA.Ode4C13.text = "Μαραινόμενον τὸν θάνατον τῷ θανάτῳ, τοῦ ἀπαθοῦς θεώμενος, ἔσπευσας Λογγῖνε, διʼ αὐτὸν τεθνήξεσθαι, καὶ ζῆν μετὰ θάνατον, σὺν τοῖς εὐκλεῶς ἐναθλήσασιν."

meMA.Ode4C1T.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C1T.melody = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.name
meMA.Ode4C1T.incipit = "Ἑνωθέντα καθʼ ὑπόστασιν ἐν γαστρί…"
meMA.Ode4C1T.text = "Ἑνωθέντα καθʼ ὑπόστασιν ἐν γαστρί σου, καὶ γεγονότα ἄνθρωπον, Ἄχραντε τὸν Λόγον, ὑπὲρ λόγον τέτοκας, διπλοῦν ταῖς θελήσεσι, καὶ ταῖς ἐνεργείαις δεικνύμενον."

meMA.Ode5C11.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C11.greekmelody = he.h.m4_gr_GR_cog.heHE.SyKyrieMouFos.greekname
meMA.Ode5C11.melody = he.h.m4_gr_GR_cog.heHE.SyKyrieMouFos.name
meMA.Ode5C11.incipit = "Γῆς πάσης ἀληθῶς, γεγονὼς παρεπίδημος…"
meMA.Ode5C11.text = "Γῆς πάσης ἀληθῶς, γεγονὼς παρεπίδημος, ἐν στίγμασιν Ἀθλοφόρων, ὡραιώθης Λογγῖνε, καὶ γῆν πραέων ᾤκησας."

meMA.Ode5C12.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C12.melody = he.h.m4_gr_GR_cog.heHE.SyKyrieMouFos.name
meMA.Ode5C12.incipit = "Ἴσχυσας τοῦ Σταυροῦ, τῇ δυνάμει…"
meMA.Ode5C12.text = "Ἴσχυσας τοῦ Σταυροῦ, τῇ δυνάμει φραξάμενος, ἀνίσχυρον τῶν τυράννων, Ἀθλοφόρε Λογγῖνε, τροπώσασθαι ἀπόνοιαν."

meMA.Ode5C13.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C13.melody = he.h.m4_gr_GR_cog.heHE.SyKyrieMouFos.name
meMA.Ode5C13.incipit = "Στήσας ἐν ἀσφαλεῖ, θεμελίῳ καρδίας…"
meMA.Ode5C13.text = "Στήσας ἐν ἀσφαλεῖ, θεμελίῳ καρδίας σου, Ἀήττητε τὰς κινήσεις, πειρασμῶν ἐπικλύσει, ἀσάλευτος διέμεινας."

meMA.Ode5C1T.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C1T.melody = he.h.m4_gr_GR_cog.heHE.SyKyrieMouFos.name
meMA.Ode5C1T.incipit = "Τὴν μόνην τὸν Θεόν, ἀπειράνδρως…"
meMA.Ode5C1T.text = "Τὴν μόνην τὸν Θεόν, ἀπειράνδρως κυήσασαν, ὑμνήσωμεν Θεοτόκον, διʼ αὐτῆς οἱ σωθέντες, καὶ πόθῳ μακαρίσωμεν."

meMA.Ode6C11.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C11.greekmelody = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.greekname
meMA.Ode6C11.melody = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.name
meMA.Ode6C11.incipit = "Ὄρη τε, καὶ τὴν γῆν…"
meMA.Ode6C11.text = "Ὄρη τε, καὶ τὴν γῆν σαλευθεῖσαν θεώμενος, ἐν τῇ τοῦ πάθους ἡμέρᾳ, ἀπιστίας στάσεως ἐσαλεύθης, καὶ τῇ πίστει, ἐνιδρύθης Λογγῖνε τοῦ πάσχοντος."

meMA.Ode6C12.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C12.melody = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.name
meMA.Ode6C12.incipit = "Νόησιν, ἀληθῆ καὶ τελείαν καὶ…"
meMA.Ode6C12.text = "Νόησιν, ἀληθῆ καὶ τελείαν καὶ ἔμφρονα, ἀναλαβεῖν ἠδυνήθης, καθορῶν τὴν κτίσιν ἀλλοιουμένην, καὶ πρὸς θεῖαν, ἠλλοιώθης Λογγῖνε ἐπίγνωσιν."

meMA.Ode6C13.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C13.melody = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.name
meMA.Ode6C13.incipit = "Ὑπέστης, χαλεπὴν τιμωρίαν γηθόμενος, καὶ…"
meMA.Ode6C13.text = "Ὑπέστης, χαλεπὴν τιμωρίαν γηθόμενος, καὶ τὸν αὐχένα τῷ ξίφει, ἀδιστάκτῳ γνώμῃ τμηθεὶς Λογγῖνε, διακόπτεις, κεφαλὰς δυσμενῶν διὰ πίστεως."

meMA.Ode6C1T.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C1T.melody = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.name
meMA.Ode6C1T.incipit = "Μάρανον, καὶ προῤῥίζους Πανάμωμε ἔκτιλον…"
meMA.Ode6C1T.text = "Μάρανον, καὶ προῤῥίζους Πανάμωμε ἔκτιλον, τὰς ἀκανθώδεις ἐννοίας, τῆς ἐμῆς καρδίας, καὶ καρποφόρον, ταύτην δεῖξον, μεσιτείᾳ σου Κόρη πανάμωμε."

meMA.Ode6H.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6H.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6H.greekmelody = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.greekname
meMA.Ode6H.melody = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.melody
meMA.Ode6H.incipit = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.incipit
meMA.Ode6H.text = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.text

meMA.Kontakion1.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Kontakion1.melody = he.a.m4_gr_GR_cog.heAU.EpefanisSimeron.name
meMA.Kontakion1.incipit = "Εὐφροσύνως γέγηθεν ἡ Ἐκκλησία, ἐν…"
meMA.Kontakion1.text = "Εὐφροσύνως γέγηθεν ἡ Ἐκκλησία, ἐν τῇ μνήμῃ σήμερον, τοῦ ἀοιδίμου ἀθλητοῦ, Λογγίνου ἀνακραυγάζουσα· Σύ μου τὸ κράτος, Χριστὲ καὶ στερέωμα."

meMA.Oikos1.text = "Τὸν οὐρανὸν σκότει πολλῷ, τὴν γῆν τε σειομένην, καὶ πέτρας ῥηγνυμένας, ναοῦ τε τὸ καταπέτασμα σχισθὲν εἰς δύο θεωρῶν ἐν τῷ θείῳ πάθει τοῦ Χριστοῦ ὁ ἀθλητής, Θεοῦ Υἱὸν αὐτὸν ἐγνώρισε, πάσχοντα τῇ οἰκείᾳ εὐσπλαγχνίᾳ, ἀπαθῆ δὲ ὄντα τῇ Θεότητι καὶ δόξῃ, σὺν τῷ Πατρὶ καὶ Πνεύματι Ἁγίῳ συνέχοντα τὸ πᾶν καὶ διακρατοῦντα, ὡς Θεὸν ἀληθινὸν καὶ Βασιλέα· ὅθεν ἐν χαρᾷ Λογγῖνος ἀνακραυγάζει· Σύ μου τὸ κράτος Χριστὲ καὶ στερέωμα."

meMA.Ode7C11.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C11.greekmelody = he.h.m4_gr_GR_cog.heHE.EnTiKaminoAvramiaioi.greekname
meMA.Ode7C11.melody = he.h.m4_gr_GR_cog.heHE.EnTiKaminoAvramiaioi.name
meMA.Ode7C11.incipit = "Νομίμως Μάρτυς, ἐναθλήσας ἀνδρείᾳ γνώμῃ…"
meMA.Ode7C11.text = "Νομίμως Μάρτυς, ἐναθλήσας ἀνδρείᾳ γνώμῃ ψυχῆς, πάσας ἀνομούντων ᾔσχυνας τὰς βουλάς, καὶ νομίμως ἐστεφάνωσαι· Εὐλογημένος εἶ ἀναβοῶν, τῆς δόξης ὁ Κύριος."

meMA.Ode7C12.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C12.melody = he.h.m4_gr_GR_cog.heHE.EnTiKaminoAvramiaioi.name
meMA.Ode7C12.incipit = "Ἡ θεία χάρις, ἐπιφοιτῶσα τοῖς…"
meMA.Ode7C12.text = "Ἡ θεία χάρις, ἐπιφοιτῶσα τοῖς λειψάνοις σου, πᾶσαν ἀλγηδόνα λύει τῶν εὐσεβῶς, προσιόντων σοι Μακάριε, καὶ κηρυττόντων σε, ἀληθείας ἀήττητον Μάρτυρα."

meMA.Ode7C13.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C13.melody = he.h.m4_gr_GR_cog.heHE.EnTiKaminoAvramiaioi.name
meMA.Ode7C13.incipit = "Σαρκὸς θνῃσκούσης, οὐκ ἐφείσω Λογγῖνε…"
meMA.Ode7C13.text = "Σαρκὸς θνῃσκούσης, οὐκ ἐφείσω Λογγῖνε διὰ Χριστόν, θύειν ἀπηρνήσω δαίμοσι καὶ σαυτόν, τῷ Θεῷ θυσίαν ἄμωμον, χαίρων προσήγαγες, διὰ ξίφους τμηθεὶς ἀξιάγαστε."

meMA.Ode7C1T.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C1T.melody = he.h.m4_gr_GR_cog.heHE.EnTiKaminoAvramiaioi.name
meMA.Ode7C1T.incipit = "Ὡς καθαρὰν σε, ὁ καθαρὸς…"
meMA.Ode7C1T.text = "Ὡς καθαρὰν σε, ὁ καθαρὸς Λόγος κατῴκησε, Κόρη διὰ σοῦ καθάραι τοὺς μολυσμοὺς τῶν ἀνθρώπων προελόμενος· εὐλογημένος γὰρ ὁ καρπός, τῆς κοιλίας σου πέφυκεν."

meMA.Ode8C11.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C11.greekmelody = he.h.m4_gr_GR_cog.heHE.CheirasEkpetasasDaniil.greekname
meMA.Ode8C11.melody = he.h.m4_gr_GR_cog.heHE.CheirasEkpetasasDaniil.name
meMA.Ode8C11.incipit = "Κοιμίσας τὸ πρὶν τὰς τῆς…"
meMA.Ode8C11.text = "Κοιμίσας τὸ πρὶν τὰς τῆς σαρκός, κινήσεις Ἔνδοξε, ταῖς ἐγρηγόρσεσι, ταῖς πρὸς τὸν Κύριον ὕπνωσας, τὸν γλυκὺν ὕπνον ἀοίδιμε, θανατωθεὶς διὰ Χριστόν, τὸν ἐπὶ ξύλου Σταυροῦ, ἀφυπνώσει θείᾳ, νεκροῖς τὴν ζωὴν ἀναβλύσαντα."

meMA.Ode8C12.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C12.melody = he.h.m4_gr_GR_cog.heHE.CheirasEkpetasasDaniil.name
meMA.Ode8C12.incipit = "Λόγχῃ κεντηθέντα τὴν πλευράν, Μακαριώτατε…"
meMA.Ode8C12.text = "Λόγχῃ κεντηθέντα τὴν πλευράν, Μακαριώτατε, καὶ αἷμα βλύσαντα, Θεὸν ἑώρακας ἄνθρωπον, διὰ σπλάγχνα χρηματίσαντα, καὶ κατετρώθης τῷ αὐτοῦ ἡδίστῳ ἔρωτι, ἀναμέλπων· Πάντα τὰ ἔργα ὑμνεῖτε τὸν Κύριον."

meMA.Ode8C13.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C13.melody = he.h.m4_gr_GR_cog.heHE.CheirasEkpetasasDaniil.name
meMA.Ode8C13.incipit = "Ἔσβεσας πυρὰν εἰδωλικήν, αἱμάτων ῥεύμασι…"
meMA.Ode8C13.text = "Ἔσβεσας πυρὰν εἰδωλικήν, αἱμάτων ῥεύμασι, Λογγῖνε πάνσοφε, καὶ κατενέπρησας ξόανα, καὶ ἀνίδρυτα σεβάσματα, τῷ θείῳ ζήλῳ τὴν ψυχὴν ἀναφλεγόμενος, καὶ κραυγάζων· Πάντα τὰ ἔργα ὑμνεῖτε τὸν Κύριον."

meMA.Ode8C14.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C14.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C14.melody = he.h.m4_gr_GR_cog.heHE.CheirasEkpetasasDaniil.name
meMA.Ode8C14.incipit = "Ὄμματα καρδίας φωτισθείς, ταῖς θείαις…"
meMA.Ode8C14.text = "Ὄμματα καρδίας φωτισθείς, ταῖς θείαις λάμψεσι, τοῦ ἀμαυρώσαντος ἡλίου πάνσοφε ἔλλαμψιν, τὴν ἀχλὺν Λογγῖνε ἔλιπες, τῆς δυσσεβείας καὶ φωστήρ, ὤφθης φωτίζων ἡμᾶς, τοὺς βοῶντας· Πάντα τὰ ἔργα ὑμνεῖτε τὸν Κύριον."

meMA.Ode8C1T.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C1T.melody = he.h.m4_gr_GR_cog.heHE.CheirasEkpetasasDaniil.name
meMA.Ode8C1T.incipit = "Στόματι καὶ γλώσσῃ καὶ ψυχῇ…"
meMA.Ode8C1T.text = "Στόματι καὶ γλώσσῃ καὶ ψυχῇ, ἀνακηρύττω σε θεογεννήτριαν, καὶ ἱκετεύω σε Ἄχραντε· Τὴν καρδίαν μου κατάγαυσον, τῷ φωτισμῷ σου, καὶ δεινοῦ σκότους με λύτρωσαι, ἐκβοῶντα· Πάντα τὰ ἔργα ὑμνεῖτε τὸν Κύριον."

meMA.Ode8H.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8H.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8H.greekmelody = he.h.m4_gr_GR_cog.heHE.CheirasEkpetasasDaniil.greekname
meMA.Ode8H.melody = he.h.m4_gr_GR_cog.heHE.CheirasEkpetasasDaniil.melody
meMA.Ode8H.incipit = he.h.m4_gr_GR_cog.heHE.CheirasEkpetasasDaniil.incipit
meMA.Ode8H.text = he.h.m4_gr_GR_cog.heHE.CheirasEkpetasasDaniil.text

meMA.Ode9C11.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode9C11.greekmelody = he.h.m4_gr_GR_cog.heHE.LithosAcheirotmitos.greekname
meMA.Ode9C11.melody = he.h.m4_gr_GR_cog.heHE.LithosAcheirotmitos.name
meMA.Ode9C11.incipit = "Ἰσχύς μου καὶ ὕμνησις Λόγος…"
meMA.Ode9C11.text = "Ἰσχύς μου καὶ ὕμνησις Λόγος, ὁ ἐν Σταυρῷ χεῖρας ἁπλώσας, Μάρτυς τοῖς ἀνόμοις ἐβόας, αὐτοῦ τὸ πάθος τὸ παμμακάριστον, ἐξεικονίζων θάνατον, ἐθελουσίως καθυφίσταμαι."

meMA.Ode9C12.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode9C12.melody = he.h.m4_gr_GR_cog.heHE.LithosAcheirotmitos.name
meMA.Ode9C12.incipit = "Ὡς περικαλὴς ἀθλοφόρος, χαριτωθεὶς Μάρτυς…"
meMA.Ode9C12.text = "Ὡς περικαλὴς ἀθλοφόρος, χαριτωθεὶς Μάρτυς Λογγῖνε, ὅλος ὡραιότατος ὤφθης, καὶ τῷ ὡραίῳ Λόγῳ παρίστασαι, τὰς ἀμοιβὰς τῶν πόνων σου, πλουσιωτάτως κομιζόμενος."

meMA.Ode9C13.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode9C13.melody = he.h.m4_gr_GR_cog.heHE.LithosAcheirotmitos.name
meMA.Ode9C13.incipit = "Σὲ τὸν διαυγίᾳ φανέντα, ἥλιον…"
meMA.Ode9C13.text = "Σὲ τὸν διαυγίᾳ φανέντα, ἥλιον ἄδυτον Λογγῖνε, καὶ μαρμαρυγαῖς ἰαμάτων, τὴν οἰκουμένην πᾶσαν φωτίζοντα, καὶ τὴν ἀχλὺν σκεδάζοντα, τῆς ἀγνωσίας μακαρίζομεν."

meMA.Ode9C14.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C14.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode9C14.melody = he.h.m4_gr_GR_cog.heHE.LithosAcheirotmitos.name
meMA.Ode9C14.incipit = "Ἤρθης πρὸς μονὰς οὐρανίους, καὶ…"
meMA.Ode9C14.text = "Ἤρθης πρὸς μονὰς οὐρανίους, καὶ τοῖς χοροῖς συνηριθμήθης, πάντων τῶν ἁγίων Μαρτύρων, μεθʼ ὧν Λογγῖνε Μάρτυς μνημόνευε, τῶν εὐσεβῶς τὴν μνήμην σου, ἑορταζόντων παμμακάριστε."

meMA.Ode9C1T.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode9C1T.melody = he.h.m4_gr_GR_cog.heHE.LithosAcheirotmitos.name
meMA.Ode9C1T.incipit = "Φωτὸς οἰκητήριον ὤφθης, τοῦ ἀνεσπέρου…"
meMA.Ode9C1T.text = "Φωτὸς οἰκητήριον ὤφθης, τοῦ ἀνεσπέρου Θεοτόκε, διʼ οὗ οἱ ἐν σκότει καὶ σκιᾷ, τῆς ἁμαρτίας φῶς ἐθεάσαντο· διό μου φωταγώγησον, τὰ τῆς καρδίας αἰσθητήρια."

meMA.Ode9H.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9H.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode9H.greekmelody = he.h.m4_gr_GR_cog.heHE.LithosAcheirotmitos.greekname
meMA.Ode9H.melody = he.h.m4_gr_GR_cog.heHE.LithosAcheirotmitos.melody
meMA.Ode9H.incipit = he.h.m4_gr_GR_cog.heHE.LithosAcheirotmitos.incipit
meMA.Ode9H.text = he.h.m4_gr_GR_cog.heHE.LithosAcheirotmitos.text

meMA.Exaposteilarion1.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Exaposteilarion1.greekmelody = he.a.m2_gr_GR_cog.heAU.ToisMathitaisSynelthomen.greekname
meMA.Exaposteilarion1.melody = he.a.m2_gr_GR_cog.heAU.ToisMathitaisSynelthomen.name
meMA.Exaposteilarion1.incipit = "Ὁρῶν τὸν πάντων Κύριον, καὶ…"
meMA.Exaposteilarion1.text = "Ὁρῶν τὸν πάντων Κύριον, καὶ Θεὸν ἀφυπνοῦντα, ἐπὶ Σταυροῦ θελήματι, κλονουμένην τε πᾶσαν, καιρῷ τοῦ πάθους τὴν κτίσιν, ἀθλοφόρε Λογγῖνε, σὺν τῷ ληστῇ εὐγνώμονι, ὡμολόγησας τοῦτον, Υἱὸν Θεοῦ, ὑπὲρ οὗ καὶ χαίρων ἐσφαγιάσθης· καὶ νῦν ὡς Μάρτυς ἄριστος, ὑπὲρ πάντων πρεσβεύεις."

