meMA.Ode1C11.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C11.greekmelody = he.h.m8_gr_GR_cog.heHE.AsomenToKyrio.greekname
meMA.Ode1C11.melody = he.h.m8_gr_GR_cog.heHE.AsomenToKyrio.name
meMA.Ode1C11.incipit = "Ταύτην τὴν φωτοφόρον…"
meMA.Ode1C11.text = "Ταύτην τὴν φωτοφόρον, μνήμην σου γεραίροντι Πορφύριε, φωτοφόρον μοι αἴγλην, οὐρανόθεν δοθῆναι ἱκέτευε."

meMA.Ode1C12.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C12.melody = he.h.m8_gr_GR_cog.heHE.AsomenToKyrio.name
meMA.Ode1C12.incipit = "Ὅλον ἀπὸ σπαργάνων…"
meMA.Ode1C12.text = "Ὅλον ἀπὸ σπαργάνων, σεαυτὸν Κυρίῳ προσανέθηκας, καὶ αὐτῷ ἐκολλήθης, καὶ ψυχῇ καὶ καρδίᾳ Πορφύριε."

meMA.Ode1C13.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C13.melody = he.h.m8_gr_GR_cog.heHE.AsomenToKyrio.name
meMA.Ode1C13.incipit = "Πάθη προθανατώσας…"
meMA.Ode1C13.text = "Πάθη προθανατώσας, πάντα διʼ ἀσκήσεως Πορφύριε, Ἱερεὺς ἀνεδείχθης, τοῦ ζωῆς καὶ θανάτου δεσπόζοντος."

meMA.Ode1C1T.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C1T.melody = he.h.m8_gr_GR_cog.heHE.AsomenToKyrio.name
meMA.Ode1C1T.incipit = "Ὄμβρον εἰσδεξαμένη…"
meMA.Ode1C1T.text = "Ὄμβρον εἰσδεξαμένη, σοῦ ἐν τῇ νηδύϊ τὸν οὐράνιον, Θεοτόκε Παρθένε, ἁμαρτίας τοὺς ὄμβρους ἐξήρανας."

meMA.Ode3C1H.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C1H.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C1H.greekmelody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.greekname
meMA.Ode3C1H.melody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.melody
meMA.Ode3C1H.incipit = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.incipit
meMA.Ode3C1H.text = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.text

meMA.Ode3C11.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C11.greekmelody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.greekname
meMA.Ode3C11.melody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.name
meMA.Ode3C11.incipit = "Ῥάβδῳ τῶν δογμάτων…"
meMA.Ode3C11.text = "Ῥάβδῳ τῶν δογμάτων σου, ἐπὶ νομὰς ζωῆς Ὅσιε, τὴν ἱεράν, Ποίμνην σου ποιμάνας, τῷ Θεῷ εὐηρέστησας."

meMA.Ode3C12.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C12.melody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.name
meMA.Ode3C12.incipit = "Φέγγει θείας γνώσεως…"
meMA.Ode3C12.text = "Φέγγει θείας γνώσεως, τοὺς ἐν νυκτὶ δεινῶν Ὅσιε, φωταγωγῶν, ἔδειξας ἡμέρας, κοινωνοὺς διὰ πίστεως."

meMA.Ode3C13.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C13.melody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.name
meMA.Ode3C13.incipit = "Ὕψει διαπρέποντα…"
meMA.Ode3C13.text = "Ὕψει διαπρέποντα, τῶν ἀρετῶν Θεὸς τέθεικεν, ἐν ὑψηλῇ, πάνσοφε λυχνία, τοὺς Πιστοὺς καταυγάζοντα."

meMA.Ode3C1T.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C1T.melody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.name
meMA.Ode3C1T.incipit = "Ῥῦσαι με πρεσβείαις…"
meMA.Ode3C1T.text = "Ῥῦσαί με πρεσβείαις σου, τῶν τοῦ ἐχθροῦ βελῶν Ἄχραντε, καὶ λογισμῶν, τῶν ἐπεμβαινόντων, ἀφειδῶς τῇ καρδίᾳ μου."

meMA.Ode3Kathisma1.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3Kathisma1.greekmelody = he.a.m4_gr_GR_cog.heAU.TachyProkatalave.greekname
meMA.Ode3Kathisma1.melody = he.a.m4_gr_GR_cog.heAU.TachyProkatalave.name
meMA.Ode3Kathisma1.incipit = "Ἐν πέτρᾳ τῆς πίστεως…"
meMA.Ode3Kathisma1.text = "Ἐν πέτρᾳ τῆς πίστεως, ἐρηρεισμένος σοφέ, τὴν πέτραν ἐπόθησας, ἣν εἶδε πρὶν Δανιήλ, Χριστὸν τὸν Θεὸν ἡμῶν· ὕψωσας τὴν φωνήν σου, καὶ τὸν νοῦν πρὸς τὸν Κτίστην· ἔβαλες τοὺς ὀχλοῦντας, τῇ σαρκὶ πολεμίους, τῇ σῇ σφενδόνῃ Γάζης ὁ σεπτός, πρόβολος Πάτερ Πορφύριε."

meMA.Ode3KathismaT.mode = miscellanea_gr_GR_cog.misc.Mode4
