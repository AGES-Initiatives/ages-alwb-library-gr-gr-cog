meMA.Ode1C1H.incipit = he.h.m4_gr_GR_cog.heHE.ChoroiIsrail.incipit
meMA.Ode1C1H.text = he.h.m4_gr_GR_cog.heHE.ChoroiIsrail.text

meMA.Ode1C11.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode1C11.greekmelody = he.h.m4_gr_GR_cog.heHE.ChoroiIsrail.greekname
meMA.Ode1C11.melody = he.h.m4_gr_GR_cog.heHE.ChoroiIsrail.name
meMA.Ode1C11.incipit = "Ῥήματα ζωῆς…"
meMA.Ode1C11.text = "Ῥήματα ζωῆς τοῖς φίλοις Χριστός, καὶ περὶ τῆς θείας δημηγορῶν βασιλείας ἔφη· Ἐν ἐμοὶ τὸν Πατέρα ἐπιγνώσεσθε, φωτὶ ὡς ἐξαστράψω ἀπροσίτῳ, ἐν ἀγαλλιάσει μέλποντες· ᾌσωμεν τῷ Θεῷ ἡμῶν, ὅτι δεδόξασται."

meMA.Ode1C12.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode1C12.melody = he.h.m4_gr_GR_cog.heHE.ChoroiIsrail.name
meMA.Ode1C12.incipit = "Ἰσχὺν τῶν ἐθνῶν κατέδεσθε…"
meMA.Ode1C12.text = "Ἰσχὺν τῶν ἐθνῶν κατέδεσθε, φίλοι Μαθηταί, θαυμασθήσεσθε δὲ τῷ πλούτῳ αὐτῶν, ὅτι δόξης πληροῦσθε, ὡς ὀφθήσομαι, λαμπρότερος ἡλίου ἐξαστράπτων, ἐν ἀγαλλιάσει μέλποντες· ᾌσωμεν τῷ Θεῷ ἡμῶν, ὅτι δεδόξασται."

meMA.Ode1C13.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode1C13.melody = he.h.m4_gr_GR_cog.heHE.ChoroiIsrail.name
meMA.Ode1C13.incipit = "Σήμερον Χριστὸς ἐν ὄρει Θαβώρ…"
meMA.Ode1C13.text = "Σήμερον Χριστὸς ἐν ὄρει Θαβώρ, λάμψας ἀμυδρῶς, θεϊκῆς αὐγῆς ὡς ὑπέσχετο, Μαθηταῖς παρεγύμνου χαρακτῆρα· σελασφόρου δὲ πλησθέντες θείας αἴγλης, ἐν ἀγαλλιάσει ἔμελπον· ᾌσωμεν τῷ Θεῷ ἡμῶν, ὅτι δεδόξασται."

meMA.Ode1C21.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C21.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C21.greekmelody = he.h.m8_gr_GR_cog.heHE.YgranDiodefsas.greekname
meMA.Ode1C21.melody = he.h.m8_gr_GR_cog.heHE.YgranDiodefsas.name
meMA.Ode1C21.incipit = "Μωσῆς ἐν θαλάσσῃ…"
meMA.Ode1C21.text = "Μωσῆς ἐν θαλάσσῃ προφητικῶς, ἰδὼν ἐν νεφέλῃ καὶ ἐν στύλῳ πάλαι πυρός, τὴν δόξαν Κυρίου ἀνεβόα· τῷ Λυτρωτῇ καὶ Θεῷ ἡμῶν ᾄσωμεν."

meMA.Ode1C22.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C22.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C22.melody = he.h.m8_gr_GR_cog.heHE.YgranDiodefsas.name
meMA.Ode1C22.incipit = "Ὡς πέτρα τῷ σώματι σκεπασθείς…"
meMA.Ode1C22.text = "Ὡς πέτρα τῷ σώματι σκεπασθείς, τῷ τεθεωμένῳ, τὸν ἀόρατον καθορῶν, Μωσῆς ὁ θεόπτης ἀνεβόα· τῷ Λυτρωτῇ καὶ Θεῷ ἡμῶν ᾄσωμεν."

meMA.Ode1C23.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C23.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C23.melody = he.h.m8_gr_GR_cog.heHE.YgranDiodefsas.name
meMA.Ode1C23.incipit = "Σὺ ἐπὶ τοῦ ὄρους τοῦ…"
meMA.Ode1C23.text = "Σὺ ἐπὶ τοῦ ὄρους τοῦ νομικοῦ, καὶ ἐν Θαβωρίῳ, καθωράθης τῷ Μωϋσῆ, ἐν γνόφῳ τὸ πάλαι, ἐν φωτὶ δέ, νῦν ἀπροσίτῳ τῆς Θεότητος."

meMA.Ode3C1H.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C1H.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C1H.greekmelody = he.h.m4_gr_GR_cog.heHE.ToxonDynatonIsthenise.greekname
meMA.Ode3C1H.melody = he.h.m4_gr_GR_cog.heHE.ToxonDynatonIsthenise.melody
meMA.Ode3C1H.incipit = he.h.m4_gr_GR_cog.heHE.ToxonDynatonIsthenise.incipit
meMA.Ode3C1H.text = he.h.m4_gr_GR_cog.heHE.ToxonDynatonIsthenise.text

meMA.Ode3C11.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C11.greekmelody = he.h.m4_gr_GR_cog.heHE.ToxonDynatonIsthenise.greekname
meMA.Ode3C11.melody = he.h.m4_gr_GR_cog.heHE.ToxonDynatonIsthenise.name
meMA.Ode3C11.incipit = "Ὅλον τὸν Ἀδὰμ φορέσας Χριστέ…"
meMA.Ode3C11.text = "Ὅλον τὸν Ἀδὰμ φορέσας Χριστέ, τὴν ἀμαυρωθεῖσαν ἀμείψας ἐλάμπρυνας πάλαι φύσιν, καὶ ἀλλοιώσει τῆς μορφῆς σου ἐθεούργησας."

meMA.Ode3C12.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C12.melody = he.h.m4_gr_GR_cog.heHE.ToxonDynatonIsthenise.name
meMA.Ode3C12.incipit = "Στύλῳ πυριμόρφῳ καὶ νεφέλῃ πάλαι…"
meMA.Ode3C12.text = "Στύλῳ πυριμόρφῳ καὶ νεφέλῃ πάλαι, ὁ ἐν τῇ ἐρήμῳ τὸν Ἰσραὴλ ἄγων, σήμερον ἐν τῷ ὄρει Θαβὼρ ἀῤῥήτως, ἐν φωτὶ Χριστὸς ἐξέλαμψεν."

meMA.Ode3C21.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C21.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C21.greekmelody = he.h.m8_gr_GR_cog.heHE.OuraniasApsidos.greekname
meMA.Ode3C21.melody = he.h.m8_gr_GR_cog.heHE.OuraniasApsidos.name
meMA.Ode3C21.incipit = "Ἡ σκιάζουσα δόξα, ἐν τῇ…"
meMA.Ode3C21.text = "Ἡ σκιάζουσα δόξα, ἐν τῇ σκηνῇ πρότερον, καὶ τῷ Μωϋσῇ ὁμιλοῦσα, τῷ σῷ θεράποντι, τύπος γεγένηται, τῆς ἀστραψάσης ἀῤῥήτως, ἐν Θαβώρ σου Δέσποτα Μεταμορφώσεως."

meMA.Ode3C22.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C22.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C22.melody = he.h.m8_gr_GR_cog.heHE.OuraniasApsidos.name
meMA.Ode3C22.incipit = "Συνανῆλθέ σοι Λόγε, μονογενὲς ὕψιστε…"
meMA.Ode3C22.text = "Συνανῆλθέ σοι Λόγε, μονογενὲς ὕψιστε, ἡ τῶν Ἀποστόλων ἀκρότης, ἐπὶ τοῦ ὄρους Θαβώρ, καὶ συμπαρέστησαν ὅ,τε Μωσῆς καὶ Ἠλίας, ὡς Θεοῦ θεράποντες, μόνε φιλάνθρωπε."

meMA.Ode3C23.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C23.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C23.melody = he.h.m8_gr_GR_cog.heHE.OuraniasApsidos.name
meMA.Ode3C23.incipit = "Θεὸς ὅλος ὑπάρχων, ὅλος βροτὸς…"
meMA.Ode3C23.text = "Θεὸς ὅλος ὑπάρχων, ὅλος βροτὸς γέγονας, ὅλη τῇ Θεότητι μίξας τὴν ἀνθρωπότητα, ἐν ὑποστάσει σου, ἣν ἐν δυσὶ ταῖς οὐσίαις, Μωϋσῆς Ἠλίας τε, εἶδον ἐν ὄρει Θαβώρ."

meMA.Ode3Kathisma1.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3Kathisma1.greekmelody = he.a.m4_gr_GR_cog.heAU.KateplagiIosif.greekname
meMA.Ode3Kathisma1.melody = he.a.m4_gr_GR_cog.heAU.KateplagiIosif.name
meMA.Ode3Kathisma1.incipit = "Ἐπὶ τὸ ὄρος τὸ Θαβώρ…"
meMA.Ode3Kathisma1.text = "Ἐπὶ τὸ ὄρος τὸ Θαβώρ, μετεμορφώθης ὁ Θεός, ἀναμέσον Ἠλιού, καὶ Μωϋσέως τῶν σοφῶν, σὺν Ἰακώβῳ καὶ Πέτρῳ καὶ Ἰωάννῃ, ὁ Πέτρος δὲ συνών, ταῦτά σοι ἔλεγε· Καλόν ὧδέ ἐστι, ποιῆσαι τρεῖς σκηνάς, μίαν Μωσεῖ, καὶ μίαν Ἠλίᾳ, καὶ μίαν σοὶ τῷ Δεσπότῃ Χριστῷ, ὁ τότε τούτοις, τὸ φῶς σου λάμψας, φώτισον τὰς ψυχὰς ἡμῶν."

meMA.Ode4C1H.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C1H.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C1H.greekmelody = he.h.m4_gr_GR_cog.heHE.EisakikoaTinEndoxon.greekname
meMA.Ode4C1H.melody = he.h.m4_gr_GR_cog.heHE.EisakikoaTinEndoxon.melody
meMA.Ode4C1H.incipit = he.h.m4_gr_GR_cog.heHE.EisakikoaTinEndoxon.incipit
meMA.Ode4C1H.text = he.h.m4_gr_GR_cog.heHE.EisakikoaTinEndoxon.text

meMA.Ode4C11.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C11.greekmelody = he.h.m4_gr_GR_cog.heHE.EisakikoaTinEndoxon.greekname
meMA.Ode4C11.melody = he.h.m4_gr_GR_cog.heHE.EisakikoaTinEndoxon.name
meMA.Ode4C11.incipit = "Νόμον ἐν Σινᾷ τῷ γράμματι…"
meMA.Ode4C11.text = "Νόμον ἐν Σινᾷ τῷ γράμματι, διατυπούμενος, Χριστὲ ὁ Θεός, ἐν τῇ νεφέλη πυρί, καὶ γνόφῳ καὶ ἐν θυέλλῃ, ὤφθης ἐποχούμενος· Δόξα τῇ δυνάμει σου Κύριε."

meMA.Ode4C12.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C12.melody = he.h.m4_gr_GR_cog.heHE.EisakikoaTinEndoxon.name
meMA.Ode4C12.incipit = "Ἴνα πιστώσῃ τὴν ἔνδοξον, οἰκονομίαν…"
meMA.Ode4C12.text = "Ἴνα πιστώσῃ τὴν ἔνδοξον, οἰκονομίαν σου, Χριστὲ ὁ Θεός, ὡς προϋπάρχων πρὸ τῶν αἰώνων, καὶ ὁ αὐτὸς ἐν νέφει τὴν ἐπίβασιν θείς, ἐν τῷ Θαβὼρ ἀῤῥήτως ἐξέλαμψας."

meMA.Ode4C13.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C13.melody = he.h.m4_gr_GR_cog.heHE.EisakikoaTinEndoxon.name
meMA.Ode4C13.incipit = "Συλλαλοῦντες παρειστήκεισαν δουλοπρεπῶς, σοὶ τῷ…"
meMA.Ode4C13.text = "Συλλαλοῦντες παρειστήκεισαν δουλοπρεπῶς, σοὶ τῷ Δεσπότῃ Χριστῷ, οἷς ἐν πυρὸς ἀτμίδι καὶ γνόφῳ, καὶ λεπτοτάτῃ αὔρᾳ προσωμίλησας· Δόξα τῇ δυνάμει σου Κύριε."

meMA.Ode4C14.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C14.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C14.melody = he.h.m4_gr_GR_cog.heHE.EisakikoaTinEndoxon.name
meMA.Ode4C14.incipit = "Κατεμήνυον τὴν ἔξοδον, τὴν ἐν…"
meMA.Ode4C14.text = "Κατεμήνυον τὴν ἔξοδον, τὴν ἐν Σταυρῷ σου ἐν Θαβὼρ παρόντες, ὁ ἐν πυρί σε καὶ βάτῳ πάλαι, προκατιδὼν Μωσῆς, καὶ ὁ μετάρσιος δίφρῳ, ἐν πυρίνῳ Ἠλίας Χριστέ."

meMA.Ode4C21.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C21.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C21.greekmelody = he.h.m8_gr_GR_cog.heHE.EkSarkosSouVolides.greekname
meMA.Ode4C21.melody = he.h.m8_gr_GR_cog.heHE.EkSarkosSouVolides.name
meMA.Ode4C21.incipit = he.h.m8_gr_GR_cog.heHE.EkSarkosSouVolides.incipit
meMA.Ode4C21.text = he.h.m8_gr_GR_cog.heHE.EkSarkosSouVolides.text

meMA.Ode4C22.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C22.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C22.melody = he.h.m8_gr_GR_cog.heHE.EkSarkosSouVolides.name
meMA.Ode4C22.incipit = "Ὁ τὴν βάτον φυλάξας ἀλώβητον…"
meMA.Ode4C22.text = "Ὁ τὴν βάτον φυλάξας ἀλώβητον, προσομιλοῦσαν πυρί, τῷ Μωσεῖ θεολαμποῦσαν σάρκα ὑπέδειξας, Δέσποτα μελῳδοῦντι· Δόξα τῇ δυνάμει σου Κύριε."

meMA.Ode4C23.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C23.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C23.melody = he.h.m8_gr_GR_cog.heHE.EkSarkosSouVolides.name
meMA.Ode4C23.incipit = "Ὑπεκρύβη ἀκτῖσι Θεότητος, αἰσθητὸς ἥλιος…"
meMA.Ode4C23.text = "Ὑπεκρύβη ἀκτῖσι Θεότητος, αἰσθητὸς ἥλιος, ὡς ἐν ὄρει Θαβωρίῳ, εἶδέ σε μεταμορφούμενον Ἰησοῦ μου· Δόξα τῇ δυνάμει σου Κύριε."

meMA.Ode4C24.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C24.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C24.melody = he.h.m8_gr_GR_cog.heHE.EkSarkosSouVolides.name
meMA.Ode4C24.incipit = "Πῦρ μὴ φλέγον τὴν ὕλην…"
meMA.Ode4C24.text = "Πῦρ μὴ φλέγον τὴν ὕλην τοῦ σώματος, ὡράθης ἄϋλον, ὡς Μωσεῖ καὶ Ἀποστόλοις, ὤφθης Ἠλίᾳ τε, Δέσποτα εἷς ἐκ δύο, ἐν δυσὶ τελείαις ταῖς φύσεσιν."

meMA.Ode5C1H.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C1H.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C1H.greekmelody = he.h.m4_gr_GR_cog.heHE.OTouFotosDiatmixas.greekname
meMA.Ode5C1H.melody = he.h.m4_gr_GR_cog.heHE.OTouFotosDiatmixas.melody
meMA.Ode5C1H.incipit = he.h.m4_gr_GR_cog.heHE.OTouFotosDiatmixas.incipit
meMA.Ode5C1H.text = he.h.m4_gr_GR_cog.heHE.OTouFotosDiatmixas.text

meMA.Ode5C11.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C11.greekmelody = he.h.m4_gr_GR_cog.heHE.OTouFotosDiatmixas.greekname
meMA.Ode5C11.melody = he.h.m4_gr_GR_cog.heHE.OTouFotosDiatmixas.name
meMA.Ode5C11.incipit = "Προσενωπίῳ σοι ὧραι ὑπεκλίθησαν· φῶς…"
meMA.Ode5C11.text = "Προσενωπίῳ σοι ὧραι ὑπεκλίθησαν· φῶς γὰρ καὶ πρὸ ποδῶν, ὑψίδρομον σέλας Χριστέ, ἥλιος εἶκε μορφὴν βροτείαν, ὡς ἀμεῖψαι εὐδόκησας."

meMA.Ode5C12.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C12.melody = he.h.m4_gr_GR_cog.heHE.OTouFotosDiatmixas.name
meMA.Ode5C12.incipit = "Ἰδοὺ Σωτὴρ ἀνεβόων, Μωϋσῆς καὶ…"
meMA.Ode5C12.text = "Ἰδοὺ Σωτὴρ ἀνεβόων, Μωϋσῆς καὶ Ἠλίας τῶν Μαθητῶν, ἐν ὄρει ἁγίῳ Θαβὼρ ἐνηχουμένων, Χριστὸς ὃν πάλαι, προηγγείλαμεν ὄντα Θεόν."

meMA.Ode5C13.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C13.melody = he.h.m4_gr_GR_cog.heHE.OTouFotosDiatmixas.name
meMA.Ode5C13.incipit = "Ἡ ἀναλλοίωτος φύσις, τῇ βροτείᾳ…"
meMA.Ode5C13.text = "Ἡ ἀναλλοίωτος φύσις, τῇ βροτείᾳ μιχθεῖσα, τῆς ἐμφεροῦς ἀΰλου Θεότητος, φῶς παραγυμνοῦσα τοῖς Ἀποστόλοις, ἀποῤῥήτως ἐξέλαμψε."

meMA.Ode5C14.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C14.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C14.melody = he.h.m4_gr_GR_cog.heHE.OTouFotosDiatmixas.name
meMA.Ode5C14.incipit = "Σὲ τὸ ἀΐδιον φέγγος, ἐν…"
meMA.Ode5C14.text = "Σὲ τὸ ἀΐδιον φέγγος, ἐν πατρῴᾳ τῇ δόξῃ, οἱ Μαθηταί, ὡς εἶδον ἐκλάμψαν Χριστέ, σοὶ ἀνεβόων· Ἐν τῷ φωτί σου, τὰς ὁδοὺς ἡμῶν εὔθυνον."

meMA.Ode5C21.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C21.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C21.greekmelody = he.h.m8_gr_GR_cog.heHE.InaTiMeAposo.greekname
meMA.Ode5C21.melody = he.h.m8_gr_GR_cog.heHE.InaTiMeAposo.name
meMA.Ode5C21.incipit = "Ῥητορεύουσα γλῶσσα, σοῦ τὰ μεγαλεῖα…"
meMA.Ode5C21.text = "Ῥητορεύουσα γλῶσσα, σοῦ τὰ μεγαλεῖα οὐ δύναται φθέγξασθαι· ὁ κρατῶν ζωῆς γάρ, καὶ θανάτου δεσπόζων παρέστησας, ἐν Θαβὼρ τῷ ὄρει, τὸν Μωϋσῆν καὶ τόν, Ἠλίαν, μαρτυρήσοντάς σου τὴν Θεότητα."

meMA.Ode5C22.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C22.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C22.melody = he.h.m8_gr_GR_cog.heHE.InaTiMeAposo.name
meMA.Ode5C22.incipit = "Ὁ χερσὶν ἀοράτοις, πλάσας κατ’…"
meMA.Ode5C22.text = "Ὁ χερσὶν ἀοράτοις, πλάσας κατʼ εἰκόνα σου Χριστὲ τὸν ἄνθρωπον, τὸ ἀρχέτυπον σου, ἐν τῷ πλάσματι κάλλος ὑπέδειξας, οὐχ ὡς ἐν εἰκόνι, ἀλλʼ ὡς αὐτὸς εἶ κατʼ οὐσίαν, ὁ Θεὸς χρηματίσας καὶ ἄνθρωπος."

meMA.Ode5C23.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C23.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C23.melody = he.h.m8_gr_GR_cog.heHE.InaTiMeAposo.name
meMA.Ode5C23.incipit = "Συγκραθεὶς ἀσυγχύτως, ἄνθρακα ὑπέδειξας ἡμῖν…"
meMA.Ode5C23.text = "Συγκραθεὶς ἀσυγχύτως, ἄνθρακα ὑπέδειξας ἡμῖν Θεότητος, καταφλέγοντα μὲν ἁμαρτίας, ψυχὰς δὲ φωτίζοντα, ἐν Θαβὼρ τῷ ὄρει, ᾧ Μωϋσῆν καὶ τὸν Ἠλίαν, Μαθητῶν τε ἐξάρχους ἐξέστησας."

meMA.Ode6C1H.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C1H.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C1H.greekmelody = he.h.m4_gr_GR_cog.heHE.EnToThlivesthaiMe.greekname
meMA.Ode6C1H.melody = he.h.m4_gr_GR_cog.heHE.EnToThlivesthaiMe.melody
meMA.Ode6C1H.incipit = he.h.m4_gr_GR_cog.heHE.EnToThlivesthaiMe.incipit
meMA.Ode6C1H.text = he.h.m4_gr_GR_cog.heHE.EnToThlivesthaiMe.text

meMA.Ode6C11.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C11.greekmelody = he.h.m4_gr_GR_cog.heHE.EnToThlivesthaiMe.greekname
meMA.Ode6C11.melody = he.h.m4_gr_GR_cog.heHE.EnToThlivesthaiMe.name
meMA.Ode6C11.incipit = "Λαμπηδόνος πλέον, ἡλίου φῶς τρανότερον…"
meMA.Ode6C11.text = "Λαμπηδόνος πλέον, ἡλίου φῶς τρανότερον, ἐν Θαβὼρ ἐκλάμψας, ὁ Χριστὸς ἡμᾶς ἐφώτισεν."

meMA.Ode6C12.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C12.melody = he.h.m4_gr_GR_cog.heHE.EnToThlivesthaiMe.name
meMA.Ode6C12.incipit = "Ἀνελθὼν ἐν ὄρει, Θαβὼρ μετεμορφώθης…"
meMA.Ode6C12.text = "Ἀνελθὼν ἐν ὄρει, Θαβὼρ μετεμορφώθης Χριστέ, καὶ τὴν πλάνην πᾶσαν, ἀμαυρώσας φῶς ἐξέλαμψας."

meMA.Ode6C13.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C13.melody = he.h.m4_gr_GR_cog.heHE.EnToThlivesthaiMe.name
meMA.Ode6C13.incipit = "Σὲ Θεὸν ἐπέγνων, οἱ ἔνδοξοι…"
meMA.Ode6C13.text = "Σὲ Θεὸν ἐπέγνων, οἱ ἔνδοξοι Ἀπόστολοι, ἐν Θαβὼρ Χριστὲ δέ, ἐκπλαγέντες γόνυ ἔκλιναν."

meMA.Ode6C21.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C21.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C21.greekmelody = he.h.m8_gr_GR_cog.heHE.IlasthitiMoiSotir.greekname
meMA.Ode6C21.melody = he.h.m8_gr_GR_cog.heHE.IlasthitiMoiSotir.name
meMA.Ode6C21.incipit = "Ὡς μέγα καὶ φοβερον, ὡράθη…"
meMA.Ode6C21.text = "Ὡς μέγα καὶ φοβερον, ὡράθη θέαμα σήμερον! ἐξ οὐρανοῦ αἰσθητός, ἐκ γῆς δὲ ἀσύγκριτος, ἐξήστραψεν ἥλιος, τῆς δικαιοσύνης, νοητὸς ἐπὶ τοῦ ὄρους Θαβώρ."

meMA.Ode6C22.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C22.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C22.melody = he.h.m8_gr_GR_cog.heHE.IlasthitiMoiSotir.name
meMA.Ode6C22.incipit = "Παρῆλθε μὲν ἡ σκιά, τοῦ…"
meMA.Ode6C22.text = "Παρῆλθε μὲν ἡ σκιά, τοῦ νόμου ἐξασθενήσασα, ἐλήλυθε δὲ σαφῶς, Χριστὸς ἡ ἀλήθεια, Μωσῆς ἀνεβόησεν ἐν τῷ Θαβωρίῳ, κατιδών σου τὴν Θεότητα."

meMA.Ode6C23.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C23.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C23.melody = he.h.m8_gr_GR_cog.heHE.IlasthitiMoiSotir.name
meMA.Ode6C23.incipit = "Ὁ στῦλος τῷ Μωϋσεῖ, Χριστὸν…"
meMA.Ode6C23.text = "Ὁ στῦλος τῷ Μωϋσεῖ, Χριστὸν τὸν μεταμορφουμενον, ἡ δὲ νεφέλη σαφῶς, τὴν χάριν τοῦ Πνεύματος, τὴν ἐπισκιάσασαν, ἐν τῷ Θαβωρίῳ, παρεδήλου ἐμφανέστατα."

meMA.Kontakion1.mode = miscellanea_gr_GR_cog.misc.Mode7
meMA.Kontakion1.melody = ""
meMA.Kontakion1.incipit = "Ἐπὶ τοῦ ὄρους μετεμορφώθης…"
meMA.Kontakion1.text = "Ἐπὶ τοῦ ὄρους μετεμορφώθης, καὶ ὡς ἐχώρουν οἱ Μαθηταί σου τὴν δόξαν σου, Χριστὲ ὁ Θεὸς ἐθεάσαντο, ἵνα ὅταν σε ἴδωσι σταυρούμενον, τὸ μὲν πάθος νοήσωσιν ἑκούσιον, τῷ δὲ κόσμῳ κηρύξωσιν, ὅτι σὺ ὑπάρχεις ἀληθῶς, τοῦ Πατρὸς τὸ ἀπαύγασμα."

meMA.Oikos1.text = "Ἐγέρθητε οἱ νωθεῖς, μὴ πάντοτε χαμερπεῖς, οἱ συγκάμπτοντες εἰς γῆν τὴν ψυχήν μου λογισμοί, ἐπάρθητε καὶ ἄρθητε εἰς ὕψος θείας ἀναβάσεως, προσδράμωμεν Πέτρῳ καὶ τοῖς Ζεβεδαίου, καὶ ἅμα ἐκείνοις τὸ Θαβώριον ὄρος προφθάσωμεν, ἵνα ἴδωμεν σὺν αὐτοῖς τὴν δόξαν τοῦ Θεοῦ ἡμῶν, φωνῆς δὲ ἀκούσωμεν, ἧς περ ἄνωθεν ἤκουσαν, καὶ ἐκήρυξαν, τοῦ Πατρὸς τὸ ἀπαύγασμα."

meMA.Ode7C1H.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C1H.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C1H.greekmelody = he.h.m4_gr_GR_cog.heHE.AvramiaioiPote.greekname
meMA.Ode7C1H.melody = he.h.m4_gr_GR_cog.heHE.AvramiaioiPote.melody
meMA.Ode7C1H.incipit = he.h.m4_gr_GR_cog.heHE.AvramiaioiPote.incipit
meMA.Ode7C1H.text = he.h.m4_gr_GR_cog.heHE.AvramiaioiPote.text

meMA.Ode7C11.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C11.greekmelody = he.h.m4_gr_GR_cog.heHE.AvramiaioiPote.greekname
meMA.Ode7C11.melody = he.h.m4_gr_GR_cog.heHE.AvramiaioiPote.name
meMA.Ode7C11.incipit = "Περιχυθέντες φωτί, τῆς ἀπροσίτου δόξης…"
meMA.Ode7C11.text = "Περιχυθέντες φωτί, τῆς ἀπροσίτου δόξης, Θαβὼρ ἐν ὄρει οἱ Ἀπόστολοι, Χριστὲ ἀνεβόησαν· ὁ τῶν Πατέρων Θεὸς εὐλογητὸς εἶ."

meMA.Ode7C12.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C12.melody = he.h.m4_gr_GR_cog.heHE.AvramiaioiPote.name
meMA.Ode7C12.incipit = "Λαίλαπι θείας ἠχῆς, καὶ δροσοβόλῳ…"
meMA.Ode7C12.text = "Λαίλαπι θείας ἠχῆς, καὶ δροσοβόλῳ νεφέλῃ, Χριστὲ καὶ αἴγλῃ σου ἡδόμενοι, οἱ Ἀπόστολοι ἔψαλλον· ὁ τῶν Πατέρων Θεὸς εὐλογητὸς εἶ."

meMA.Ode7C13.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C13.melody = he.h.m4_gr_GR_cog.heHE.AvramiaioiPote.name
meMA.Ode7C13.incipit = "Ἐν ἀπροσίτῳ φωτί, ὡς σὲ…"
meMA.Ode7C13.text = "Ἐν ἀπροσίτῳ φωτί, ὡς σὲ κατεῖδε Πέτρος, Θαβὼρ ἐν ὄρει ἐξαστράψαντα, Χριστὲ ἀνεβόησεν· ὁ τῶν Πατέρων Θεὸς εὐλογητὸς εἶ."

meMA.Ode7C14.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C14.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C14.melody = he.h.m4_gr_GR_cog.heHE.AvramiaioiPote.name
meMA.Ode7C14.incipit = "Τῷ ἀρχηγῷ τῆς γῆς, Χριστῷ…"
meMA.Ode7C14.text = "Τῷ ἀρχηγῷ τῆς γῆς, Χριστῷ συνόντες παῖδες, οἱ Ζεβεδαίου ὡς ἀπέπεμψε, μορφῆς φῶς ἑβρόντησαν· ὁ τῶν Πατέρων Θεὸς εὐλογητὸς εἶ."

meMA.Ode7C21.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C21.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode7C21.greekmelody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.greekname
meMA.Ode7C21.melody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.name
meMA.Ode7C21.incipit = "Νῦν καθωράθη Ἀποστόλοις, τὰ ἀθέατα…"
meMA.Ode7C21.text = "Νῦν καθωράθη Ἀποστόλοις, τὰ ἀθέατα, Θεότης ἐν σαρκίῳ, ἐν τῷ ὄρει Θαβώρ, ἀστράπτουσα βοῶσιν· Εὐλογητὸς εἶ Κύριε, ὁ Θεὸς εἰς τοὺς αἰῶνας."

meMA.Ode7C22.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C22.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode7C22.melody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.name
meMA.Ode7C22.incipit = "Ἔφριξαν φόβῳ ἐκπλαγέντες, τὴν εὐπρέπειαν…"
meMA.Ode7C22.text = "Ἔφριξαν φόβῳ ἐκπλαγέντες, τὴν εὐπρέπειαν τῆς θείας βασιλείας, ἐν τῷ ὄρει Θαβώρ, Ἀπόστολοι βοῶντες· Εὐλογητὸς εἶ Κύριε, ὁ Θεὸς εἰς τοὺς αἰῶνας."

meMA.Ode7C23.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C23.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode7C23.melody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.name
meMA.Ode7C23.incipit = "Νῦν τὰ ἀνήκουστα ἠκούσθη…"
meMA.Ode7C23.text = "Νῦν τὰ ἀνήκουστα ἠκούσθη· ὁ ἀπάτωρ γὰρ Υἱὸς ἐκ τῆς Παρθένου, τῇ πατρῴᾳ φωνῇ, ἐνδόξως μαρτυρεῖται, οἷα Θεὸς καὶ ἄνθρωπος, ὁ αὐτὸς εἰς τοὺς αἰῶνας."

meMA.Ode7C24.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C24.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode7C24.melody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.name
meMA.Ode7C24.incipit = "Θέσει οὐ γέγονας Ὑψίστου, τῇ…"
meMA.Ode7C24.text = "Θέσει οὐ γέγονας Ὑψίστου, τῇ οὐσίᾳ δὲ Υἱὸς ἠγαπημένος, προϋπάρχων ἡμῖν, ὡμίλησας ἀτρέπτως· Εὐλογητὸς εἶ κράζουσιν, ὁ Θεὸς εἰς τοὺς αἰῶνας."

meMA.Ode8C1H.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C1H.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C1H.greekmelody = he.h.m4_gr_GR_cog.heHE.OiEnVavyloniPaides.greekname
meMA.Ode8C1H.melody = he.h.m4_gr_GR_cog.heHE.OiEnVavyloniPaides.melody
meMA.Ode8C1H.incipit = he.h.m4_gr_GR_cog.heHE.OiEnVavyloniPaides.incipit
meMA.Ode8C1H.text = he.h.m4_gr_GR_cog.heHE.OiEnVavyloniPaides.text

meMA.Ode8C11.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C11.greekmelody = he.h.m4_gr_GR_cog.heHE.OiEnVavyloniPaides.greekname
meMA.Ode8C11.melody = he.h.m4_gr_GR_cog.heHE.OiEnVavyloniPaides.name
meMA.Ode8C11.incipit = "Νεύματι τὸ πᾶν ὁ φέρων…"
meMA.Ode8C11.text = "Νεύματι τὸ πᾶν ὁ φέρων, ποσὶν ἀχράντοις ὄρους ἐπέβη Θαβώρ, ἐν ᾧ ἡλίου μᾶλλον αὐγῆς, ἐξαστράψας τὸ πρόσωπον, νόμου προκρίτους, καὶ τῆς χάριτος ἔδειξε μέλποντας· Εὐλογεῖτε πάντα τὰ ἔργα Κυρίου τὸν Κύριον."

meMA.Ode8C12.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C12.melody = he.h.m4_gr_GR_cog.heHE.OiEnVavyloniPaides.name
meMA.Ode8C12.incipit = "Ἐν τῇ ἀπροσίτῳ δόξῃ, κατ’…"
meMA.Ode8C12.text = "Ἐν τῇ ἀπροσίτῳ δόξῃ, κατʼ ὄρος ἐκφανθὲν ἀποῤῥήτως Θαβώρ, τὸ ἄσχετον καὶ ἄδυτον φῶς, τοῦ Πατρὸς τὸ ἀπαύγασμα, τὴν κτίσιν φαιδρῦναν, τοὺς ἀνθρώπους ἐθέωσε μέλποντας· Εὐλογεῖτε πάντα τὰ ἔργα Κυρίου τὸν Κύριον."

meMA.Ode8C13.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C13.melody = he.h.m4_gr_GR_cog.heHE.OiEnVavyloniPaides.name
meMA.Ode8C13.incipit = "Ἱεροπρεπῶς ἑστῶτες, Μωσῆς τε καὶ…"
meMA.Ode8C13.text = "Ἱεροπρεπῶς ἑστῶτες, Μωσῆς τε καὶ Ἠλίας ἐν ὄρει Θαβώρ, τῆς θείας χαρακτῆρα, τρανῶς ὑποστάσεως βλέποντες, Χριστὸν ἐν πατρῴᾳ, ἐξαστράψαντα δόξῃ, ἀνέμελπον· Εὐλογεῖτε πάντα τὰ ἔργα Κυρίου τὸν Κύριον."

meMA.Ode8C14.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C14.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C14.melody = he.h.m4_gr_GR_cog.heHE.OiEnVavyloniPaides.name
meMA.Ode8C14.incipit = "Διὰ τῆς ἐν γνόφῳ θείας…"
meMA.Ode8C14.text = "Διὰ τῆς ἐν γνόφῳ θείας ὀμφῆς, τὸ πρόσωπον ποτὲ ἐδοξάσθη Μωσῆς, Χριστὸς δὲ ὡς ἱμάτιον, φῶς καὶ δόξαν ἀναβάλλεται· φωτὸς αὐτουργὸς γὰρ πεφυκὼς καταυγάζει τοὺς μέλποντας· Εὐλογεῖτε πάντα τὰ ἔργα Κυρίου τὸν Κύριον."

meMA.Ode8C15.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C15.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C15.melody = he.h.m4_gr_GR_cog.heHE.OiEnVavyloniPaides.name
meMA.Ode8C15.incipit = "Ἐκ φωτογενοῦς νεφέλης, Χριστὸν οἱ…"
meMA.Ode8C15.text = "Ἐκ φωτογενοῦς νεφέλης, Χριστὸν οἱ μαθηταὶ ἀμπεχόμενον, ὁρῶντες ἐν Θαβώρ, καὶ πρηνεῖς ἐπὶ γῆς κατανεύσαντες, τὸν νοῦν ἐλλαμφθέντες, σὺν Πατρὶ τοῦτον ὕμνουν καὶ Πνεύματι· Εὐλογεῖτε πάντα τὰ ἔργα Κυρίου τὸν Κύριον."

meMA.Ode8C21.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C21.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C21.greekmelody = he.h.m8_gr_GR_cog.heHE.EptaplasiosKaminon.greekname
meMA.Ode8C21.melody = he.h.m8_gr_GR_cog.heHE.EptaplasiosKaminon.name
meMA.Ode8C21.incipit = "Ἀκηκοότες Δέσποτα, ἐκ Πατρὸς μαρτυρούμενον…"
meMA.Ode8C21.text = "Ἀκηκοότες Δέσποτα, ἐκ Πατρὸς μαρτυρούμενον, καὶ ὡς ἀνθρωπίνης, στεῤῥοτέραν ὄψεως, ὁρᾶν τοῦ προσώπου σου τὴν ἀστραπὴν μὴ φέροντες, σοῦ οἱ μαθηταί, ἐπὶ τὴν γῆν κατέπιπτον, ἐν φόβῳ μελῳδοῦντες· Ἱερεῖς εὐλογεῖτε, λαὸς ὑπερυψοῦτε, Χριστὸν εἰς τοὺς αἰῶνας."

meMA.Ode8C22.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C22.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C22.melody = he.h.m8_gr_GR_cog.heHE.EptaplasiosKaminon.name
meMA.Ode8C22.incipit = "Βασιλευόντων πέφυκας, Βασιλεὺς ὡραιότατος, καὶ…"
meMA.Ode8C22.text = "Βασιλευόντων πέφυκας, Βασιλεὺς ὡραιότατος, καὶ τῶν πανταχοῦ κυριευόντων Κύριος, δυνάστης μακάριος, καὶ φῶς οἰκῶν ἀπρόσιτον, ᾧ οἱ Μαθηταί, καταπλαγέντες ἐβόων· οἱ Παῖδες εὐλογεῖτε, Ἱερεῖς ἀνυμνεῖτε, λαὸς ὑπερυψοῦτε Χριστὸν εἰς τοὺς αἰῶνας."

meMA.Ode8C23.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C23.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C23.melody = he.h.m8_gr_GR_cog.heHE.EptaplasiosKaminon.name
meMA.Ode8C23.incipit = "Ὡς οὐρανοῦ δεσπόζοντι, καὶ τῆς…"
meMA.Ode8C23.text = "Ὡς οὐρανοῦ δεσπόζοντι, καὶ τῆς γῆς βασιλεύοντι, καὶ καταχθονίων, τὴν κυρείαν ἔχοντι, Χριστέ σοι παρέστησαν, ἐκ μὲν τῆς γῆς Ἀπόστολοι, ὡς ἐξ οὐρανοῦ δὲ ὁ Θεσβίτης Ἠλίας, Μωϋσῆς δὲ ἐκ νεκάδων, μελῳδοῦντες συμφώνως· Λαὸς ὑπερυψοῦτε Χριστὸν εἰς τοὺς αἰῶνας."

meMA.Ode8C24.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C24.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C24.melody = he.h.m8_gr_GR_cog.heHE.EptaplasiosKaminon.name
meMA.Ode8C24.incipit = "Ῥαθυμοτόκοι μέριμναι, ἐπὶ γῆς κατελείφθησαν…"
meMA.Ode8C24.text = "Ῥαθυμοτόκοι μέριμναι, ἐπὶ γῆς κατελείφθησαν, τῇ τῶν Ἀποστόλων, ἐκλογῇ φιλάνθρωπε, ὡς σοὶ ἠκολούθησαν, πρὸς τὴν ἐκ γῆς μετάρσιον, θείαν πολιτείαν· ὅθεν καὶ ἐπαξίως, τῆς σῆς θεοφανείας, ἐμελῴδουν τυχόντες· Λαὸς ὑπερυψοῦτε, Χριστὸν εἰς τοὺς αἰῶνας."

meMA.Ode9C1H.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C1H.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode9C1H.greekmelody = he.h.m4_gr_GR_cog.heHE.OTokosSouAfthoros.greekname
meMA.Ode9C1H.melody = he.h.m4_gr_GR_cog.heHE.OTokosSouAfthoros.melody
meMA.Ode9C1H.incipit = he.h.m4_gr_GR_cog.heHE.OTokosSouAfthoros.incipit
meMA.Ode9C1H.text = he.h.m4_gr_GR_cog.heHE.OTokosSouAfthoros.text

meMA.Ode9C11.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode9C11.greekmelody = he.h.m4_gr_GR_cog.heHE.OTokosSouAfthoros.greekname
meMA.Ode9C11.melody = he.h.m4_gr_GR_cog.heHE.OTokosSouAfthoros.name
meMA.Ode9C11.incipit = "Σύντρομοι καινῇ φωτοχυσίᾳ…"
meMA.Ode9C11.text = "Σύντρομοι καινῇ φωτοχυσίᾳ, ἀθρόως οἱ Μαθηταὶ ἐλλαμφθέντες, πρὸς ἀλλήλους ἑώρων, καὶ πρηνεῖς εἰς γῆν καταπεσόντες, σοὶ τῷ Δεσπότῃ τῶν ἁπάντων προσεκύνησαν."

meMA.Ode9C12.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode9C12.melody = he.h.m4_gr_GR_cog.heHE.OTokosSouAfthoros.name
meMA.Ode9C12.incipit = "Ἦχος ἐκ νεφέλης ἀνεπέμπετο, θεόκτυπος…"
meMA.Ode9C12.text = "Ἦχος ἐκ νεφέλης ἀνεπέμπετο, θεόκτυπος βεβαιῶν τὸ θαῦμα· ὁ Πατὴρ γὰρ τῶν φώτων. Οὗτός ἐστιν Υἱὸς ὁ ἀγαπητός μου, τοῖς Ἀποστόλοις ἀνεβόα, οὗ ἀκούετε."

meMA.Ode9C13.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode9C13.melody = he.h.m4_gr_GR_cog.heHE.OTokosSouAfthoros.name
meMA.Ode9C13.incipit = "Καινὰ κατιδόντες καὶ παράδοξα, φωνῆς…"
meMA.Ode9C13.text = "Καινὰ κατιδόντες καὶ παράδοξα, φωνῆς πατρικῆς ἐνωτισθέντες, ἐν Θαβὼρ οἱ τοῦ Λόγου ὑπηρέται, ἐκμαγεῖον τοῦ ἀρχετύπου. Οὗτος ὑπάρχει ἀνεβόων ὁ Σωτὴρ ἡμῶν."

meMA.Ode9C14.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C14.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode9C14.melody = he.h.m4_gr_GR_cog.heHE.OTokosSouAfthoros.name
meMA.Ode9C14.incipit = "Εἰκὼν ἀπαράλλακτε τοῦ Ὄντος, ἀκίνητε…"
meMA.Ode9C14.text = "Εἰκὼν ἀπαράλλακτε τοῦ Ὄντος, ἀκίνητε σφραγὶς ἀναλλοίωτε, Υἱὲ Λόγε σοφία καὶ βραχίων, δεξιὰ Ὑψίστου σθένος, σὲ ἀνυμνοῦμεν, σὺν Πατρί τε καὶ τῷ Πνεύματι."

meMA.Ode9C2H.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C2H.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode9C2H.greekmelody = he.h.m8_gr_GR_cog.heHE.EfrixePasaAkoi.greekname
meMA.Ode9C2H.melody = he.h.m8_gr_GR_cog.heHE.EfrixePasaAkoi.melody
meMA.Ode9C2H.incipit = he.h.m8_gr_GR_cog.heHE.EfrixePasaAkoi.incipit
meMA.Ode9C2H.text = he.h.m8_gr_GR_cog.heHE.EfrixePasaAkoi.text

meMA.Ode9C21.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C21.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode9C21.greekmelody = he.h.m8_gr_GR_cog.heHE.EfrixePasaAkoi.greekname
meMA.Ode9C21.melody = he.h.m8_gr_GR_cog.heHE.EfrixePasaAkoi.name
meMA.Ode9C21.incipit = "Ἵνα σου δείξῃς ἐμφανῶς…"
meMA.Ode9C21.text = "Ἵνα σου δείξῃς ἐμφανῶς, τὴν ἀπόῤῥητον δευτέραν κατάβασιν, ὅπως ὁ Ὑψιστος Θεός, ὀφθήσῃ ἑστὼς ἐν μέσῳ θεῶν, τοῖς Ἀποστόλοις ἐν Θαβώρ, Μωσεῖ σὺν Ἠλίᾳ τε, ἀῤῥήτως ἔλαμψας· διὸ πάντες σε Χριστὲ μεγαλύνομεν."

meMA.Ode9C22.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C22.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode9C22.melody = he.h.m8_gr_GR_cog.heHE.EfrixePasaAkoi.name
meMA.Ode9C22.incipit = "Δεῦτέ μοι πείθεσθε λαοί, ἀναβάντες…"
meMA.Ode9C22.text = "Δεῦτέ μοι πείθεσθε λαοί, ἀναβάντες εἰς τὸ ὄρος τὸ Ἅγιον, τὸ ἐπουράνιον ἀΰλως στῶμεν ἐν πόλει ζῶντος Θεοῦ, καὶ ἐποπτεύσωμεν νοῒ Θεότητα ἄϋλον, Πατρὸς καὶ Πνεύματος, ἐν Υἱῷ μονογενεῖ ἀπαστράπτουσαν."

meMA.Ode9C23.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C23.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode9C23.melody = he.h.m8_gr_GR_cog.heHE.EfrixePasaAkoi.name
meMA.Ode9C23.incipit = "Ἔθελξας πόθῳ με Χριστέ…"
meMA.Ode9C23.text = "Ἔθελξας πόθῳ με, Χριστέ, καὶ ἠλλοίωσας τῷ θείῳ σου ἔρωτι, ἀλλὰ κατάφλεξον πυρὶ ἀΰλω τὰς ἁμαρτίας μου, καὶ ἐμπλησθῆναι τῆς ἐν σοὶ τρυφῆς καταξίωσον, ἵνα τὰς δύο σκιρτῶν μεγαλύνω, ἀγαθέ, παρουσίας σου."

meMA.Exaposteilarion1.mode = miscellanea_gr_GR_cog.misc.Mode3
meMA.Exaposteilarion1.greekmelody = he.a.m3_gr_GR_cog.heAU.FosAnalloioton.greekname
meMA.Exaposteilarion1.melody = he.a.m3_gr_GR_cog.heAU.FosAnalloioton.melody
meMA.Exaposteilarion1.incipit = he.a.m3_gr_GR_cog.heAU.FosAnalloioton.incipit
meMA.Exaposteilarion1.text = he.a.m3_gr_GR_cog.heAU.FosAnalloioton.text

meMA.Lauds1.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Lauds1.greekmelody = he.a.m4_gr_GR_cog.heAU.OExYpsistouKlitheis.greekname
meMA.Lauds1.melody = he.a.m4_gr_GR_cog.heAU.OExYpsistouKlitheis.name
meMA.Lauds1.incipit = "Πρὸ τοῦ τιμίου Σταυροῦ σου…"
meMA.Lauds1.text = "Πρὸ τοῦ τιμίου Σταυροῦ σου καὶ τοῦ πάθους, λαβὼν οὓς προέκρινας τῶν Ἱερῶν Μαθητῶν, πρὸς τὸ Θαβώριον Δέσποτα, ἀνῆλθες ὄρος, δεῖξαι θελήσας τούτοις τὴν δόξαν σου, οἳ καὶ κατιδόντες σε μεταμορφούμενον, καὶ ὑπὲρ ἥλιον λάμψαντα, πρηνεῖς πεσόντες, τὴν δυναστείαν σου κατεπλάγησαν, ἀναβοῶντες· Σὺ τὸ ἄχρονον, φῶς ὑπάρχεις Χριστὲ καὶ ἀπαύγασμα, τοῦ Πατρός, εἰ καὶ θέλων, σάρξ ὡράθης ἀναλλοίωτος."

meMA.Lauds2.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Lauds2.melody = he.a.m4_gr_GR_cog.heAU.OExYpsistouKlitheis.name
meMA.Lauds2.incipit = "Ὁ πρὸ αἰώνων ὑπάρχων Θεὸς Λόγος…"
meMA.Lauds2.text = "Ὁ πρὸ αἰώνων ὑπάρχων Θεὸς Λόγος ὁ φῶς ὡς ἱμάτιον περιβαλλόμενος, μεταμορφούμενος ἔμπροσθεν, τῶν μαθητῶν σου, ὑπὲρ τὸν ἥλιον Λόγε ἔλαμψας, Μωσῆς καὶ Ἠλίας δέ, σοὶ παρειστήκεισαν, νεκρῶν καὶ ζώντων σε Κύριον, δηλοποιοῦντες, καὶ σοῦ δοξάζοντες τὴν ἀπόῤῥητον, οἰκονομίαν καὶ τὸ ἔλεος, καὶ τὴν πολλὴν συγκατάβασιν, διʼ ἧς ἔσωσας κόσμον, ἁμαρτίαις ἀπολλύμενον."

meMA.Lauds3.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Lauds3.melody = he.a.m4_gr_GR_cog.heAU.OExYpsistouKlitheis.name
meMA.Lauds3.incipit = "Παρθενικῆς ἐκ νεφέλης…"
meMA.Lauds3.text = "Παρθενικῆς ἐκ νεφέλης σε τεχθέντα, καὶ σάρκα γενόμενον, καὶ πρὸς τὸ ὄρος Θαβώρ, μεταμορφούμενον Κύριε, καὶ τῇ νεφέλῃ, τῇ φωτεινῇ σε περικυκλούμενον, φωνὴ τοῦ Γεννήτορος, ἀγαπητὸν σὲ Υἱόν, τῶν μαθητῶν συμπαρόντων σοι, σαφῶς ἐδήλου, ὡς ὁμοούσιον καὶ ὁμόθρονον· ὅθεν ὁ Πέτρος ἐκπληττόμενος, Καλὸν ὧδέ ἐστιν εἶναι ἔλεγε, μὴ εἰδὼς ὃ ἐλάλει, Εὐεργέτα πολυέλεε."

meMA.LaudsGlory.poet = poets_gr_GR_cog.po.Vyzantos
meMA.LaudsGlory.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.LaudsGlory.melody = ""
meMA.LaudsGlory.incipit = "Παρέλαβεν ὁ Χριστός…"
meMA.LaudsGlory.text = "Παρέλαβεν ὁ Χριστός, τὸν Πέτρον καὶ Ἰάκωβον καὶ Ἰωάννην, εἰς ὄρος ὑψηλὸν κατʼ ἰδίαν, καὶ μετεμορφώθη ἔμπροσθεν αὐτῶν, καὶ ἔλαμψε τὸ πρόσωπον αὐτοῦ ὡς ὁ Ἥλιος, τὰ δὲ ἱμάτια αὐτοῦ, ἐγένετο λευκὰ ὡς τὸ φῶς. Καὶ ὤφθησαν Μωϋσῆς καὶ Ἠλίας μετʼ αὐτοῦ συλλαλοῦντες, καὶ νεφέλη φωτεινὴ ἐπεσκίασεν αὐτούς, καὶ ἰδοὺ φωνὴ ἐκ τῆς νεφέλης λέγουσα· Οὗτός ἐστιν ὁ Υἱός μου ὁ ἀγαπητός, ἐν ᾧ ηὐδόκησᾳ, αὐτοῦ ἀκούετε."

