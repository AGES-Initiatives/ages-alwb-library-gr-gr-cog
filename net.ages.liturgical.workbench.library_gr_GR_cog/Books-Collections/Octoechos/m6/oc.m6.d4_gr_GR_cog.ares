A_Resource_Whose_Name = oc.m6.d4_gr_GR_cog

ocBK.ModeNaked = miscellanea_gr_GR_cog.misc.Mode6.naked

ocVE.Stichera1.mode = miscellanea_gr_GR_cog.misc.Mode6
ocVE.Stichera1.greekmelody = he.a.m6_gr_GR_cog.heAU.IApegnosmeni.greekname
ocVE.Stichera1.melody = he.a.m6_gr_GR_cog.heAU.IApegnosmeni.name
ocVE.Stichera1.incipit = "Ὁ Θεὸς ὁ θέλων σωθῆναι…"
ocVE.Stichera1.text = "Ὁ Θεὸς ὁ θέλων σωθῆναι πάντας, ἐπίβλεψον, ἴδε τὴν προσευχήν μου, καὶ μή μου τὰ δάκρυα, ὡς μάταια ἀπώσῃ· τίς γὰρ προσῆλθέ σοι προσπίπτων, καὶ εὐθὺς οὐκ ἐσώθη; τίς δὲ ἐβόησε θερμῶς σοι, καὶ εὐθὺς οὐκ ἠκούσθη; καὶ γὰρ Δέσποτα, ταχὺς εὑρίσκῃ, εἰς σωτηρίαν πᾶσι τοῖς αἰτοῦσί σε Κύριε, διὰ τὸ μέγα σου ἔλεος."

ocVE.Stichera2.mode = miscellanea_gr_GR_cog.misc.Mode6
ocVE.Stichera2.melody = he.a.m6_gr_GR_cog.heAU.IApegnosmeni.name
ocVE.Stichera2.incipit = "Τὸν ταῖς ἁμαρτίαις καταποθέντα, καὶ…"
ocVE.Stichera2.text = "Τὸν ταῖς ἁμαρτίαις καταποθέντα, καὶ φιληδονίαις καταῤῥαγέντα, ἀνάστησον εὔσπλαγχνε, τῇ σῇ φιλανθρωπίᾳ, μή με ἐπίχαρμα δαιμόνων, συγχωρήσῃς γενέσθαι, μηδὲ ἀνάξιον ἐάσῃς, τῶν φρικτῶν μυστηρίων, ἀλλὰ πρόσδεξαι ὡς ἐλεήμων, πρὸ τῆς ἐξόδου στεναγμούς μου καὶ δάκρυα, καὶ τῶν παθῶν με ἐλευθέρωσον."

ocVE.Stichera3.mode = miscellanea_gr_GR_cog.misc.Mode6
ocVE.Stichera3.melody = he.a.m6_gr_GR_cog.heAU.IApegnosmeni.name
ocVE.Stichera3.incipit = "Θρήνησον ψυχή μου πρὸ τῆς…"
ocVE.Stichera3.text = "Θρήνησον ψυχή μου πρὸ τῆς ἐξόδου, πρόσπεσον τῷ Κτίστῃ ἐν μετανοίᾳ, δάκρυα προσάγαγε, ἐξ ὅλης τῆς καρδίας, κρᾶξον Χριστῷ ἐν κατανύξει. Ἡμάρτηκά σοι Λόγε, δέξαι πολλὰ ἡμαρτηκότα, μή με ἀποβδελύξῃ, μὴ παρόψῃ με Δικαιοκρῖτα, μηδὲ εἰς πῦρ με ἀποπέμψῃς μακρόθυμε, διὰ τὸ μέγα σου ἔλεος."

ocVE.Stichera4.mode = miscellanea_gr_GR_cog.misc.Mode6
ocVE.Stichera4.greekmelody = he.a.m6_gr_GR_cog.heAU.OlinApothemenoi.greekname
ocVE.Stichera4.melody = he.a.m6_gr_GR_cog.heAU.OlinApothemenoi.name
ocVE.Stichera4.incipit = "Ὁ γάμος ηὐτρέπισται, τοῦ οὐρανίου…"
ocVE.Stichera4.text = "Ὁ γάμος ηὐτρέπισται, τοῦ οὐρανίου Νυμφίου, οἱ κήρυκες κράζουσι, τί βραδύνεις μέλαινα, ἑκουσίως ψυχὴ; ῥυπαρὰ πάντα σου, ἀληθῶς ὑπάρχει, ἀσελγείαις τὰ ἱμάτια, καὶ πῶς ταλαίπωρε, μέλλεις εἰσελθεῖν, δεῦρο, πρόσπεσον, πρὸ τέλους ἐκ καρδίας σου, τῇ Ἀειπαρθένῳ κραυγάζουσα. Ὑσσώπῳ πρεσβείας, ῥαντίσασα καθάρισον Ἁγνή, καὶ τοῦ νυμφῶνος ἀξίωσον, ὅπως μεγαλύνω σε."

ocVE.Stichera5.mode = miscellanea_gr_GR_cog.misc.Mode6
ocVE.Stichera5.melody = he.a.m6_gr_GR_cog.heAU.OlinApothemenoi.name
ocVE.Stichera5.incipit = oc.m6.d1_gr_GR_cog.ocSV.Aposticha2.incipit
ocVE.Stichera5.text = oc.m6.d1_gr_GR_cog.ocSV.Aposticha2.text

ocVE.Stichera6.mode = miscellanea_gr_GR_cog.misc.Mode6
ocVE.Stichera6.melody = he.a.m6_gr_GR_cog.heAU.OlinApothemenoi.name
ocVE.Stichera6.incipit = "Ἡ πόρνη κενώσασα, μύρου ἀλάβαστρον…"
ocVE.Stichera6.text = "Ἡ πόρνη κενώσασα, μύρου ἀλάβαστρον πάλαι, τοῖς ποσί σου Δέσποτα, πάντων ἠλευθέρωται, ὧν περ ἔπραξεν, ὁ Λῃστὴς μόνον σου, μνημονεύσας ὤφθη, καὶ εὐθέως εἰς Παράδεισον, αὐτὸν εἰσήγαγες, ἐγὼ δὲ τί πράξω ὁ ἄθλιος; οὐ μύρον, οὐ κατάνυξιν, ἔχω, δωρεάν με ἐλέησον, οἴκτειρόν με Λόγε, ἡ ἄχραντός σου Μήτηρ δυσωπεῖ, ἣν ἐδωρήσω τῷ κόσμῳ σου, μέγα καταφύγιον."

ocVE.SticTheotokion.mode = miscellanea_gr_GR_cog.misc.Mode6
ocVE.SticTheotokion.greekmelody = he.a.m6_gr_GR_cog.heAU.OlinApothemenoi.greekname
ocVE.SticTheotokion.melody = he.a.m6_gr_GR_cog.heAU.OlinApothemenoi.name
ocVE.SticTheotokion.incipit = "Κρίσιν Ἰσραὴλ κριταί…"
ocVE.SticTheotokion.text = "Κρίσιν Ἰσραὴλ κριταί, θανατηφόρον κριθῆναι, σὲ Υἱὲ κατέκριναν, ὡς κριτόν σε στήσαντες, ἐπὶ βήματος, τὸν νεκροὺς κρίνοντα, καὶ τοὺς ζῶντας Σῶτερ, καὶ Πιλάτῳ παριστῶσί σε, καὶ κατακρίνουσι, πρὸ τῆς δίκης φεῦ! οἱ παράνομοι· καὶ βλέπουσα τιτρώσκομαι, καὶ συγκατακρίνομαι Κύριε· ὅθεν καὶ προκρίνω, θανεῖν ὑπὲρ τὸ ζῆν ἐν στεναγμοῖς, ἡ Θεοτόκος ἐκραύγαζε, μόνε Πολυέλεε."

ocVE.Aposticha1.mode = miscellanea_gr_GR_cog.misc.Mode6
ocVE.Aposticha1.melody = ""
ocVE.Aposticha1.incipit = "Ὁ Σταυρός σου Κύριε, ζωὴ…"
ocVE.Aposticha1.text = "Ὁ Σταυρός σου Κύριε, ζωὴ καὶ ἀντίληψις ὑπάρχει τῷ λαῷ σου· καὶ ἐπʼ αὐτῷ πεποιθότες, σὲ τὸν σταυρωθέντα, σαρκὶ Θεὸν ὑμνοῦμεν· ἐλέησον ἡμᾶς."

ocVE.Aposticha2.mode = miscellanea_gr_GR_cog.misc.Mode6
ocVE.Aposticha2.melody = ""
ocVE.Aposticha2.incipit = "Ὁ Σταυρός σου Κύριε…"
ocVE.Aposticha2.text = "Ὁ Σταυρός σου Κύριε, Παράδεισον ἤνοιξε, τῷ γένει τῶν ἀνθρώπων· καὶ ἐκ φθορᾶς λυτρωθέντες, σὲ τὸν σταυρωθέντα, σαρκὶ Θεὸν ὑμνοῦμεν· ἐλέησον ἡμᾶς."
ocVE.Aposticha2.similarto = oc.m6.d1_gr_GR_cog.ocMA.Lauds2.text

ocVE.Aposticha3.mode = miscellanea_gr_GR_cog.misc.Mode6
ocVE.Aposticha3.melody = ""
ocVE.Aposticha3.incipit = "Οἱ ἀθλοφόροι Μάρτυρες…"
ocVE.Aposticha3.text = "Οἱ ἀθλοφόροι Μάρτυρες, καὶ οὐρανοπολῖται, ἐπὶ γῆς ἀθλήσαντες, πολλὰς βασάνους ὑπέμειναν· πρεσβείαις Κύριε, καὶ εὐχαῖς αὐτῶν, πάντας ἡμᾶς διαφύλαξον."

ocVE.AposTheotokion.mode = miscellanea_gr_GR_cog.misc.Mode6
ocVE.AposTheotokion.greekmelody = he.a.m6_gr_GR_cog.heAU.TriimerosAnestis.greekname
ocVE.AposTheotokion.melody = he.a.m6_gr_GR_cog.heAU.TriimerosAnestis.name
ocVE.AposTheotokion.incipit = "Ὁρῶσά σε σταυρούμενον…"
ocVE.AposTheotokion.text = "Ὁρῶσά σε σταυρούμενον, Χριστὲ ἡ σὲ κυήσασα, ἀνεβόα· Τί τὸ ξένον, ὃ ὁρῶ, μυστήριον Υἱέ μου, πῶς ἐπὶ ξύλου θνῄσκεις, σαρκὶ κρεμάμενος, ζωῆς χορηγέ;"

ocVE.ApolTheotokionVM.mode = miscellanea_gr_GR_cog.misc.Mode6
ocVE.ApolTheotokionVM.melody = ""
ocVE.ApolTheotokionVM.incipit = oc.m6.d4_gr_GR_cog.ocMA.Kathisma33.incipit
ocVE.ApolTheotokionVM.text = oc.m6.d4_gr_GR_cog.ocMA.Kathisma33.text

ocMA.Kathisma11.mode = miscellanea_gr_GR_cog.misc.Mode6
ocMA.Kathisma11.melody = ""
ocMA.Kathisma11.incipit = "Μόνον ἐπάγη τὸ ξύλον…"
ocMA.Kathisma11.text = "Μόνον ἐπάγη τὸ ξύλον, Χριστὲ τοῦ Σταυροῦ σου, τὰ θεμέλια ἐσαλεύθησαν τοῦ θανάτου Κύριε· ὃν γὰρ κατέπιε πόθῳ ὁ ᾅδης, ἀπέλυσε τρόμῳ. Ἐδειξας ἡμῖν τὸ σωτήριόν σου ἅγιε, καὶ δοξολογοῦμέν σε Υἱὲ Θεοῦ· ἐλέησον ἡμᾶς."

ocMA.Kathisma12.mode = miscellanea_gr_GR_cog.misc.Mode6
ocMA.Kathisma12.melody = ""
ocMA.Kathisma12.incipit = "Κύριε, κατέκρινάν σε…"
ocMA.Kathisma12.text = "Κύριε, κατέκρινάν σε Ἰουδαῖοι θανάτῳ, τὴν ζωὴν τῶν ἁπάντων· οἱ τὴν Ἐρυθρὰν ῥάβδῳ πεζεύσαντες, Σταυρῷ σε προσήλωσαν· καὶ οἱ ἐκ πέτρας μέλι θηλάσαντες, χολήν σοι προσήνεγκαν· ἀλλʼ ἑκὼν ὑπέμεινας, ἵνα ἡμᾶς ἐλευθερώσῃς, τῆς δουλείας τοῦ ἐχθροῦ· Χριστὲ ὁ Θεὸς δόξα σοι."

ocMA.Kathisma13.mode = miscellanea_gr_GR_cog.misc.Mode6
ocMA.Kathisma13.greekmelody = he.a.m6_gr_GR_cog.heAU.ElpisTouKosmouAgathi.greekname
ocMA.Kathisma13.melody = he.a.m6_gr_GR_cog.heAU.ElpisTouKosmouAgathi.name
ocMA.Kathisma13.incipit = "Ἡ σὲ κυήσασα Χριστέ…"
ocMA.Kathisma13.text = "Ἡ σὲ κυήσασα Χριστέ, ἀειπάρθενος Κόρη, ἐν τῷ Σταυρῷ σὲ διʼ ἡμᾶς ὑψωθέντα ὁρῶσα, ἐτέτρωτο λύπης ῥομφαίᾳ τὴν ψυχήν, καὶ ἔκλαιε θρηνῳδοῦσα μητρικῶς. Αὐτῆς ταῖς παρακλήσεσιν, ἐλέησον ἡμᾶς, Κύριε τοῦ ἐλέους."

ocMA.Kathisma21.mode = miscellanea_gr_GR_cog.misc.Mode6
ocMA.Kathisma21.melody = ""
ocMA.Kathisma21.incipit = "Σήμερον τὸ προφητικὸν…"
ocMA.Kathisma21.text = "Σήμερον τὸ προφητικὸν πεπλήρωται λόγιον· ἰδοὺ γὰρ προσκυνοῦμεν εἰς τὸν τόπον, οὗ ἔστησαν οἱ πόδες σου Κύριε· καὶ ξύλου σωτηρίας γευσάμενοι, τῶν ἐξ ἁμαρτίας παθῶν ἐλευθερίας ἐτύχομεν, πρεσβείαις τῆς Θεοτόκου μόνε Φιλάνθρωπε."

ocMA.Kathisma22.mode = miscellanea_gr_GR_cog.misc.Mode6
ocMA.Kathisma22.melody = ""
ocMA.Kathisma22.incipit = "Ὁ Σταυρός σου Κύριε…"
ocMA.Kathisma22.text = "Ὁ Σταυρός σου Κύριε ἡγίασται· ἐν αὐτῷ γὰρ γίνονται ἰάματα, τοῖς ἀσθενοῦσιν ἐν ἁμαρτίαις· διʼ αὐτοῦ σοι προσπίπτομεν· Ἐλέησον ἡμᾶς."

ocMA.Kathisma23.mode = miscellanea_gr_GR_cog.misc.Mode6
ocMA.Kathisma23.greekmelody = he.a.m6_gr_GR_cog.heAU.ElpisTouKosmouAgathi.greekname
ocMA.Kathisma23.melody = he.a.m6_gr_GR_cog.heAU.ElpisTouKosmouAgathi.name
ocMA.Kathisma23.incipit = "Ἐν ξύλῳ βλέπουσα νεκρόν…"
ocMA.Kathisma23.text = "Ἐν ξύλῳ βλέπουσα νεκρόν, κρεμάμενον ἡ Παρθένος, Θεὸν ὃν ἔτεκε σαρκί, θρηνῳδοῦσα ἐβόα· Τὴν ἄφατον μακροθυμίαν σου Υἱέ, ἐκπλήττομαι· πῶς ὡς κατάκριτον Σταυρῷ, προσήλωσαν οἱ ἄνομοι, τὸν δίκαιον Κριτήν, καὶ μόνον σε Δεσπότην."

ocMA.Kathisma31.mode = miscellanea_gr_GR_cog.misc.Mode6
ocMA.Kathisma31.greekmelody = he.a.m6_gr_GR_cog.heAU.ElpisTouKosmouAgathi.greekname
ocMA.Kathisma31.melody = he.a.m6_gr_GR_cog.heAU.ElpisTouKosmouAgathi.name
ocMA.Kathisma31.incipit = "Τὸ ξύλον ἤνεγκε φθοράν…"
ocMA.Kathisma31.text = "Τὸ ξύλον ἤνεγκε φθοράν, ἐν Ἐδὲμ τῷ γενάρχῃ· Σταυροῦ δὲ ξύλον τὴν ζωήν, ἤνθησεν ἐν Κρανίῳ· πεπάτηται καὶ γὰρ κακίᾳ τοῦ ἐχθροῦ· ἠλέηται τῇ προσηλώσει τοῦ Χριστοῦ, καὶ εὗρε τὸν Παράδεισον, κραυγάζων ὁ Ἀδάμ· Ὦ ξύλον εὐλογημένον!"

ocMA.Kathisma32.mode = miscellanea_gr_GR_cog.misc.Mode6
ocMA.Kathisma32.melody = ""
ocMA.Kathisma32.incipit = "Ἀθλητικὸν ἀγῶνα ὑπομείναντες…"
ocMA.Kathisma32.text = "Ἀθλητικὸν ἀγῶνα, ὑπομείναντες οἱ Ἅγιοι, καὶ τὰ βραβεῖα τῆς νίκης, παρὰ σοῦ κομισάμενοι, κατήργησαν τὰς ἐπινοίας τῶν παρανόμων, ἐδέξαντο στεφάνους τῆς ἀφθαρσίας. Διʼ αὐτῶν ὁ Θεὸς δυσωπούμενος, δώρησαι ἡμῖν τὸ μέγα ἔλεος."

ocMA.Kathisma33.mode = miscellanea_gr_GR_cog.misc.Mode6
ocMA.Kathisma33.melody = ""
ocMA.Kathisma33.incipit = "Θεοτόκε Παρθένε, ἱκέτευε…"
ocMA.Kathisma33.text = "Θεοτόκε Παρθένε, ἱκέτευε τὸν Υἱόν σου, τὸν ἑκουσίως προσπαγέντα ἐν Σταυρῷ, καὶ τὸν κόσμον ἐκ πλάνης ἐλευθερώσαντα, Χριστὸν τὸν Θεὸν ἡμῶν, τοῦ ἐλεῆσαι τὰς ψυχὰς ἡμῶν."

ocMA.C1.acrostic.title = "Κανὼν Σταυρώσιμος,"
ocMA.C1.acrostic.notice1 = "οὗ ἡ ἀκροστιχὶς μετὰ τῶν Σταυροθεοτοκίων ἢ Θεοτοκίων·"
ocMA.C1.acrostic.text = "Σῴζεις με, Σῶτερ, προσπαγεὶς ἐπὶ ξύλου, τὸν Ἰωσήφ."

ocMA.C2.acrostic.title = "Κανὼν τῆς Θεοτόκου,"
ocMA.C2.acrostic.notice1 = "οὗ ἡ ἀκροστιχίς·"
ocMA.C2.acrostic.text = "Ἕκτον προσάξω τῇ Θεοτόκῳ μέλος."

ocMA.CA.notice = rubrical_gr_GR_cog.ru.unavailable.octoechos

ocMA.OdeW1C1H.mode = miscellanea_gr_GR_cog.misc.Mode6
ocMA.OdeW1C1H.ode = miscellanea_gr_GR_cog.misc.Ode1
ocMA.OdeW1C1H.greekmelody = he.h.m6_gr_GR_cog.heHE.OsEnIpeiroPezefsas.greekname
