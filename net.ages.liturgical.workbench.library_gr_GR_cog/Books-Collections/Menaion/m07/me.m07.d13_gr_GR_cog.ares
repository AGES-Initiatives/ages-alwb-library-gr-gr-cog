meMA.Ode3C21.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C21.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode3C21.greekmelody = he.h.m2_gr_GR_cog.heHE.ExinthisenIErimos.greekname
meMA.Ode3C21.melody = he.h.m2_gr_GR_cog.heHE.ExinthisenIErimos.name
meMA.Ode3C21.incipit = "Ἐξέλαμψεν ὁ βίος σου, θεοφόρε…"
meMA.Ode3C21.text = "Ἐξέλαμψεν ὁ βίος σου, θεοφόρε Στέφανε, ὁλολαμπὴς ὡς ἥλιος, ἐν τῷ κόσμῳ καὶ κατεφώτισε, τοὺς σοὶ πίστει καὶ πόθῳ προσανέχοντας."

meMA.Ode3C22.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C22.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode3C22.melody = he.h.m2_gr_GR_cog.heHE.ExinthisenIErimos.name
meMA.Ode3C22.incipit = "Φωτίζονται τὰ πέρατα, διδαχαῖς σου…"
meMA.Ode3C22.text = "Φωτίζονται τὰ πέρατα, διδαχαῖς σου Ὅσιε· ὡς γὰρ φωστὴρ ἐξέλαμψας, θεοφόρε τῇ Ἐκκλησίᾳ Χριστοῦ, ἐν ᾗ ἐστερεώθη ἡ καρδία σου."

meMA.Ode3C23.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C23.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode3C23.melody = he.h.m2_gr_GR_cog.heHE.ExinthisenIErimos.name
meMA.Ode3C23.incipit = "Ἀδούλωτον ἐτήρησας, τὴν ψυχήν σου…"
meMA.Ode3C23.text = "Ἀδούλωτον ἐτήρησας, τὴν ψυχήν σου πάθεσι, καὶ ἡδοναῖς τοῦ σώματος, θεοφόρε Στέφανε τίμιε· διὸ νῦν ἐπαξίως εὐφημοῦμέν σε."

meMA.Ode3C2T.ode = miscellanea_gr_GR_cog.misc.Ode3
