meVE.SticTheotokion.text = me.m10.d02_gr_GR_cog.meMA.AposTheotokion.text

meVE.SticStavrotheotokion.mode = miscellanea_gr_GR_cog.misc.Mode8
meVE.SticStavrotheotokion.greekmelody = he.a.m8_gr_GR_cog.heAU.OTouParadoxou.greekname
meVE.SticStavrotheotokion.melody = he.a.m8_gr_GR_cog.heAU.OTouParadoxou.name
meVE.SticStavrotheotokion.incipit = me.m10.d07_gr_GR_cog.meVE.AposStavrotheotokion.incipit
meVE.SticStavrotheotokion.text = me.m10.d07_gr_GR_cog.meVE.AposStavrotheotokion.text

meVE.Apolytikion1ger.poet = poets_gr_GR_cog.po.Gerasimou
meVE.Apolytikion1ger.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.Apolytikion1ger.greekmelody = he.a.m4_gr_GR_cog.heAU.TachyProkatalave.greekname
meVE.Apolytikion1ger.melody = he.a.m4_gr_GR_cog.heAU.TachyProkatalave.name
meVE.Apolytikion1ger.incipit = "Ἀσκήσει ἐκλάμψασα…"
meVE.Apolytikion1ger.text = "Ἀσκήσει ἐκλάμψασα, ὥσπερ παρθένος σεμνή, ἀθλήσεως αἵμασι, τὴν τῆς ἁγνείας στολήν, ἐνθέως ἐφοίνιξας· ὅθεν Ἀναστασία, ὡς Ὁσία καὶ Μάρτυς, χάριτας ἰαμάτων, ἀπαστράπτεις τῷ κόσμῳ, πρεσβεύοθσα τῷ Σωτῆρι, ὑπὲρ τῶν ψυχῶν ἡμῶν."

meVE.Apolytikion2ger.poet = poets_gr_GR_cog.po.Gerasimou
meVE.Apolytikion2ger.mode = miscellanea_gr_GR_cog.misc.Mode1
meVE.Apolytikion2ger.greekmelody = he.a.m1_gr_GR_cog.heAU.TouLithouSfragisthentos.greekname
meVE.Apolytikion2ger.melody = he.a.m1_gr_GR_cog.heAU.TouLithouSfragisthentos.name
meVE.Apolytikion2ger.incipit = "Ζωῆς τῆς φθειρομένης…"
meVE.Apolytikion2ger.text = "Ζωῆς τῆς φθειρομένης λιπὼν τὰς ἀπολαύσεις, ἐν τῇ τῶν μελλόντων ἐλπίδι, Ἀβράμε θεοφόρε, ὁσίως ἐβίωσας ἐν γῇ, καὶ χρῖσμα ὑπεδέξω ἱερόν· διὰ τοῦτο ὡς τοῦ Λόγου μυσταγωγός, κατηύγασας τοὺς βοῶντας· δόξα τῷ δεδωκότι σοι ἰσχύν, δόξα τῷ σὲ στεφανώσαντι, δόξα τῷ ἐνεργοῦντι διὰ σοῦ, πᾶσιν ἰάματα."

meMA.C1.acrostic.title = "Ὁ Κανὼν τῆς Μάρτυρος,"
meMA.C1.acrostic.notice1 = "οὗ ἡ Ἀκροστιχίς·"
meMA.C1.acrostic.text = "Τοὺς ἀνδρικούς σου, Μάρτυς, εὐφημῶ πόνους. Ἰωσήφ."
meMA.C1.title = titles_gr_GR_cog.ti.CanonSaintW
meMA.C1.verse = prayers_gr_GR_cog.pr.verse_SaintW.text

meMA.C2.acrostic.title = "Ὁ Κανὼν τοῦ Ὁσίου,"
meMA.C2.acrostic.notice1 = "οὗ ἡ Ἀκροστιχίς·"
meMA.C2.acrostic.text = "Τὸν σὸν γεραίρω παμφαῆ βίον μάκαρ. Ἰωσήφ."
meMA.C2.title = titles_gr_GR_cog.ti.CanonDevoutM
meMA.C2.verse = prayers_gr_GR_cog.pr.verse_DevoutMan.text

meMA.Ode1C11.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C11.greekmelody = he.h.m8_gr_GR_cog.heHE.AsomenToKyrio.greekname
meMA.Ode1C11.melody = he.h.m8_gr_GR_cog.heHE.AsomenToKyrio.name
meMA.Ode1C11.incipit = "Ταύτην τὴν φωτοφορον, μνήμην σου…"
meMA.Ode1C11.text = "Ταύτην τὴν φωτοφορον, μνήμην σου γεραίροντι Θεόνυμφε, φωτισμόν μοι παράσχου, τῆς ψυχῆς μου τὸ σκότος διώκουσα."

meMA.Ode1C12.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C12.melody = he.h.m8_gr_GR_cog.heHE.AsomenToKyrio.name
meMA.Ode1C12.incipit = "Ὅλην ἀπὸ σπαργάνων, σεαυτὴν ἀνέθηκας…"
meMA.Ode1C12.text = "Ὅλην ἀπὸ σπαργάνων, σεαυτὴν ἀνέθηκας τῷ Κτίστῃ σου, καὶ πυρὶ ἐγκρατείας, τὰ τοῦ σώματος πάθη κατέφλεξας."

meMA.Ode1C13.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C13.melody = he.h.m8_gr_GR_cog.heHE.AsomenToKyrio.name
meMA.Ode1C13.incipit = "Ὕψος πρὸς μαρτυρίου, Μάρτυς ἀνηνέχθης…"
meMA.Ode1C13.text = "Ὕψος πρὸς μαρτυρίου, Μάρτυς ἀνηνέχθης ἀφειδήσασα, τῆς σαρκὸς καὶ θαλάμων, νοητῶν ὡς παρθένος ἠξίωσαι."

meMA.Ode1C1T.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C1T.melody = he.h.m8_gr_GR_cog.heHE.AsomenToKyrio.name
meMA.Ode1C1T.incipit = "Σὲ τὴν οὐρανομήκη, κλίμακα ἐν…"
meMA.Ode1C1T.text = "Σὲ τὴν οὐρανομήκη, κλίμακα ἐν ᾗ Θεὸς ἐπεστήρικτο, τοὺς βροτοὺς οὐρανίους, ἐργαζόμενος Κόρη γεραίρομεν."

meMA.Ode1C21.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C21.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C21.greekmelody = he.h.m8_gr_GR_cog.heHE.YgranDiodefsas.greekname
meMA.Ode1C21.melody = he.h.m8_gr_GR_cog.heHE.YgranDiodefsas.name
meMA.Ode1C21.incipit = "Ταῖς θείαις λαμπόμενος ἀστραπαῖς, τοὺς…"
meMA.Ode1C21.text = "Ταῖς θείαις λαμπόμενος ἀστραπαῖς, τοὺς τὴν φωτοφόρον, ἐκτελοῦντάς σου ἑορτήν, παθῶν ἀμαυρότητος παμμάκαρ, ταῖς προσευχαῖς σου Ἀβράμιε λύτρωσαι."

meMA.Ode1C22.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C22.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C22.melody = he.h.m8_gr_GR_cog.heHE.YgranDiodefsas.name
meMA.Ode1C22.incipit = "Ὁ θεῖος ἀνάψας σου τὴν…"
meMA.Ode1C22.text = "Ὁ θεῖος ἀνάψας σου τὴν ψυχήν, Ἀβράμιε πόθος, ἐναπέσβεσε τῆς σαρκός, τοὺς ἔρωτας Πάτερ καὶ ἀΰλως, ἐπὶ τῆς γῆς σε βιῶναι ἐποίησεν."

meMA.Ode1C23.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C23.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C23.melody = he.h.m8_gr_GR_cog.heHE.YgranDiodefsas.name
meMA.Ode1C23.incipit = "Νεκρώσας τὰ μέλη τὰ ἐπὶ…"
meMA.Ode1C23.text = "Νεκρώσας τὰ μέλη τὰ ἐπὶ γῆς, νηστείᾳ καὶ πάσαις, κακουχίαις Πάτερ σοφέ, ζωῆς ἠξιώθης τῆς ἀμείνω, ἐν οὐρανοῖς θεοφόρε Ἀβράμιε."

meMA.Ode1C2T.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C2T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C2T.melody = he.h.m8_gr_GR_cog.heHE.YgranDiodefsas.name
meMA.Ode1C2T.incipit = "Σαρκὸς ἐξ αἱμάτων σου ἱερῶν…"
meMA.Ode1C2T.text = "Σαρκὸς ἐξ αἱμάτων σου ἱερῶν, Θεοῦ Πατρὸς Λόγος, ἐσαρκώθη ὑπερφυῶς, παρθένε Μαρία, ὃν δυσώπει, τὰ τῆς σαρκός μου νεκρῶσαι φρονήματα."

meMA.Ode3C11.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C11.greekmelody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.greekname
meMA.Ode3C11.melody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.name
meMA.Ode3C11.incipit = "Ἵστασο πρὸ βήματος, τυραννικοῦ Χριστὸν…"
meMA.Ode3C11.text = "Ἵστασο πρὸ βήματος, τυραννικοῦ Χριστὸν ἔνδοξε, τὸν ποιητήν, πάντων καὶ Δεσπότην, Θεὸν Λόγον κηρύττουσα."

meMA.Ode3C12.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C12.melody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.name
meMA.Ode3C12.incipit = "Κάλλος τὸ ἐγκάρδιον, τῇ ὁρατῇ…"
meMA.Ode3C12.text = "Κάλλος τὸ ἐγκάρδιον, τῇ ὁρατῇ μορφῇ Ἔνδοξε, διαδοθέν, σὲ ὡραιοτάτην, τοῖς ὁρῶσιν ὑπέφαινεν."

meMA.Ode3C13.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C13.melody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.name
meMA.Ode3C13.incipit = "Ὄλβον ἀναφαίρετον, τῶν ἰαμάτων σοι…"
meMA.Ode3C13.text = "Ὄλβον ἀναφαίρετον, τῶν ἰαμάτων σοι δίδωσι, Κόρη Χριστός, οὗ Ἀναστασία, τὴν πτωχείαν ἠγάπησας."

meMA.Ode3C1T.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C1T.melody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.name
meMA.Ode3C1T.incipit = "Ὕλην τῶν πταισμάτων μου, σῶν…"
meMA.Ode3C1T.text = "Ὕλην τῶν πταισμάτων μου, σῶν πρεσβειῶν πυρὶ σύμφλεξον, Μήτηρ Θεοῦ, δρόσον μοι τὴν θείαν, τῆς ἀφέσεως φέρουσα."

meMA.Ode3C21.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C21.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C21.greekmelody = he.h.m8_gr_GR_cog.heHE.OuraniasApsidos.greekname
meMA.Ode3C21.melody = he.h.m8_gr_GR_cog.heHE.OuraniasApsidos.name
meMA.Ode3C21.incipit = "Ὁλοτρόπως ποθήσας, τῶν ὀρεκτῶν ἔσχατον…"
meMA.Ode3C21.text = "Ὁλοτρόπως ποθήσας, τῶν ὀρεκτῶν ἔσχατον, κόσμου καὶ σαρκὸς θεοφόρε, ἔξω γεγένησαι· ὅθεν ἀπείληφας, τὴν ὑπερκόσμιον δόξαν, καὶ θείαν ἀπόλαυσιν, Πάτερ Ἀβράμιε."

meMA.Ode3C22.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C22.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C22.melody = he.h.m8_gr_GR_cog.heHE.OuraniasApsidos.name
meMA.Ode3C22.incipit = "Ναὸς ζῶν ἀνεδείχθης, τοῦ παντουργοῦ…"
meMA.Ode3C22.text = "Ναὸς ζῶν ἀνεδείχθης, τοῦ παντουργοῦ Πνεύματος, πάτερ ἐν οἰκίσκῳ τὸ σῶμα, συγκλείσας Ὅσιε· ὅθεν ἀπήστραψας, τῶν ἀρετῶν λαμπηδόνας, ἱερῶς κοσμούμενος, Πάτερ, Ἀβράμιε."

meMA.Ode3C23.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C23.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C23.melody = he.h.m8_gr_GR_cog.heHE.OuraniasApsidos.name
meMA.Ode3C23.incipit = "Γεωργίᾳ τῶν πόνων, τῶν ἀρετῶν…"
meMA.Ode3C23.text = "Γεωργίᾳ τῶν πόνων, τῶν ἀρετῶν ὥριμον, Ὅσιε ἐξήνθησας στάχυν, Πάτερ Ἀβράμιε, ᾧ διατρέφονται, μιμητικῶς οἱ τὴν θείαν, καὶ σεπτήν σου κοίμησιν πανηγυρίζοντες."

meMA.Ode3C2T.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C2T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C2T.melody = he.h.m8_gr_GR_cog.heHE.OuraniasApsidos.name
meMA.Ode3C2T.incipit = "Ἐπὶ σοὶ Παναγία, ὁ τοῦ…"
meMA.Ode3C2T.text = "Ἐπὶ σοὶ Παναγία, ὁ τοῦ παντὸς αἴτιος, διʼ ὑπερβολὴν εὐσπλαγχνίας, θέλων ἐσκήνωσε, καὶ καθηγίασε, τὴν τῶν ἀνθρώπων οὐσίαν, παραβάσει πρότερον ἐξολισθήσασαν."

meMA.Ode3H.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3H.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3H.greekmelody = he.h.m8_gr_GR_cog.heHE.OuraniasApsidos.greekname
meMA.Ode3H.melody = he.h.m8_gr_GR_cog.heHE.OuraniasApsidos.melody
meMA.Ode3H.incipit = he.h.m8_gr_GR_cog.heHE.OuraniasApsidos.incipit
meMA.Ode3H.text = he.h.m8_gr_GR_cog.heHE.OuraniasApsidos.text

meMA.Ode3Kathisma1.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode3Kathisma1.greekmelody = he.a.m1_gr_GR_cog.heAU.TonTafonSouSotir.greekname
meMA.Ode3Kathisma1.melody = he.a.m1_gr_GR_cog.heAU.TonTafonSouSotir.name
meMA.Ode3Kathisma1.incipit = "Ἐκ βρέφους τῷ Θεῷ, ἀνετέθης…"
meMA.Ode3Kathisma1.text = "Ἐκ βρέφους τῷ Θεῷ, ἀνετέθης Ὁσία, νεκρώσασα σαρκός, ἐγκρατείᾳ τὰ πάθη, εἰς ὕψος δʼ ἀνέδραμες, μαρτυρίου περίδοξον, ἐναθλήσασα, Ἀναστασία νομίμως, καὶ τὸν δράκοντα, καταβαλοῦσα εἰς χάος, δυνάμει τοῦ Πνεύματος."

meMA.Ode3Kathisma2.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode3Kathisma2.greekmelody = he.a.m1_gr_GR_cog.heAU.TonTafonSouSotir.greekname
meMA.Ode3Kathisma2.melody = he.a.m1_gr_GR_cog.heAU.TonTafonSouSotir.name
meMA.Ode3Kathisma2.incipit = "Τὸν πάλαι Ἀβραάμ, ἐκμιμούμενος, Πάτερ…"
meMA.Ode3Kathisma2.text = "Τὸν πάλαι Ἀβραάμ, ἐκμιμούμενος, Πάτερ, ἐμάκρυνας σαυτόν, συγγενῶν κατὰ σάρκα, πειθόμενος Ἀβράμιε, τῷ καλοῦντι Θεῷ ἡμῶν· ὅθεν ἤσκησας, καὶ τὴν ψυχὴν λαμπροτέραν, ἐναπέδειξας, τῶν τοῦ ἡλίου ἀκτίνων, θεόφρον πανόλβιε."

meMA.Ode3KathismaT.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode3KathismaT.melody = he.a.m1_gr_GR_cog.heAU.TonTafonSouSotir.name
meMA.Ode3KathismaT.incipit = "Τὴν βάτον ἣν Μωσῆς, ἀκατάφλεκτον…"
meMA.Ode3KathismaT.text = "Τὴν βάτον ἣν Μωσῆς, ἀκατάφλεκτον εἶδε, τὸ ὄρος τοῦ Θεοῦ, τὴν ἁγίαν νεφέλην, σκηνὴν τὴν ἀμόλυντον, τὴν θεόδεκτον τράπεζαν, τὸ παλάτιον, τοῦ ὑψηλοῦ Βασιλέως, τὴν ὁλόφωτον, καὶ ἀδιόδευτον πύλην, Παρθένον ὑμνοῦμέν σε."

meMA.Ode3KathismaSt.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode3KathismaSt.melody = he.a.m1_gr_GR_cog.heAU.TonTafonSouSotir.name
meMA.Ode3KathismaSt.incipit = "Ὑμνῶ σου τὸν Σταυρόν, προσκυνῶ…"
meMA.Ode3KathismaSt.text = "Ὑμνῶ σου τὸν Σταυρόν, προσκυνῶ καὶ τὰ πάθη, δοξάζω καὶ τὴν σήν, ἀγαθοτητα Τέκνον· ἑκὼν γὰρ καθυπέμεινας, ἐπονείδιστον θάνατον. Τί τὸ ξένον σου, καὶ φρικωδέστατον Σῶτερ, τῆς ἀφράστου σου, οἰκονομίας τὸ βάθος; ἡ Μήτηρ ἐβόα σοι."

meMA.Ode4C11.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C11.greekmelody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.greekname
meMA.Ode4C11.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
meMA.Ode4C11.incipit = "Σὲ ζυγὸν ἐλαφρότατον, Μάρτυς τὴν…"
meMA.Ode4C11.text = "Σὲ ζυγὸν ἐλαφρότατον, Μάρτυς τὴν βαστάσασαν ἐκ νεότητος, σιδηροῦν κλοιὸν βαστάζειν σε, οἱ παρανομοῦντες κατεδίκασαν."

meMA.Ode4C12.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C12.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
meMA.Ode4C12.incipit = "Σταλαγμοὶ τῶν αἱμάτων σου, τῆς…"
meMA.Ode4C12.text = "Σταλαγμοὶ τῶν αἱμάτων σου, τῆς πολυθεΐας ἔσβεσαν ἄνθρακας, ὁ πυρσὸς δὲ τῶν θαυμάτων σου, τῶν παθῶν τὴν ὕλην ἀπετέφρωσεν."

meMA.Ode4C13.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C13.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
meMA.Ode4C13.incipit = "Οὐκ ἐγύμνωσε χάριτος, σῶμά σου…"
meMA.Ode4C13.text = "Οὐκ ἐγύμνωσε χάριτος, σῶμά σου γυμνώσας ὁ πονηρότατος, ὑφαντὸν δὲ μᾶλλον ἄνωθεν, ἀῤῥαγῆ χιτῶνα προεξένει σοι."

meMA.Ode4C14.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C14.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C14.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
meMA.Ode4C14.incipit = "Ὑπὲρ γῆν αἰρομένης σου, πῦρ…"
meMA.Ode4C14.text = "Ὑπὲρ γῆν αἰρομένης σου, πῦρ ἐπὶ τὰ στέρνα Μάρτυς ἐφήπτετο, ὑπεξάπτον σου τὸν ἔρωτα, τὸν πρὸς τὸν Δεσπότην καλλιπάρθενε."

meMA.Ode4C1T.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C1T.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
meMA.Ode4C1T.incipit = "Μετὰ τόκον διέμεινας, ἄφθορος Παρθένε…"
meMA.Ode4C1T.text = "Μετὰ τόκον διέμεινας, ἄφθορος Παρθένε ὡς πρὸ γεννήσεως· νέον βρέφος γὰρ ἐκύησας, τὸν πρὸ τῶν αἰώνων γνωριζόμενον."

meMA.Ode4C21.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C21.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C21.greekmelody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.greekname
meMA.Ode4C21.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
meMA.Ode4C21.incipit = "Ῥείθροις Πάτερ δακρύων σου, ψυχικὰς…"
meMA.Ode4C21.text = "Ῥείθροις Πάτερ δακρύων σου, ψυχικὰς κηλῖδας ἀπονιψάμενος, θεῖον γέγονας τοῦ Πνεύματος, καὶ σεπτὸν παμμάκαρ καταγώγιον."

meMA.Ode4C22.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C22.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C22.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
meMA.Ode4C22.incipit = "Ἀγρυπνίαν παννύχιον, προσευχὴν ἀέναον ἀπροσπάθειαν…"
meMA.Ode4C22.text = "Ἀγρυπνίαν παννύχιον, προσευχὴν ἀέναον ἀπροσπάθειαν, καὶ ἀγάπην ἀνυπόκριτον, καὶ τελείαν πίστιν Πάτερ ἤσκησας."

meMA.Ode4C23.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C23.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C23.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
meMA.Ode4C23.incipit = "Ἰατρεύειν νοσήματα, καὶ ἀποδιώκειν Ὅσιε…"
meMA.Ode4C23.text = "Ἰατρεύειν νοσήματα, καὶ ἀποδιώκειν Ὅσιε πνεύματα, χάριν εἴληφας Ἀβράμιε, ὡς Θεοῦ θεράπων ἀληθέστατος."

meMA.Ode4C2T.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C2T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C2T.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
meMA.Ode4C2T.incipit = "Ῥαθυμία κρατούμενον, καὶ τῇ ἁμαρτίᾳ…"
meMA.Ode4C2T.text = "Ῥαθυμία κρατούμενον, καὶ τῇ ἁμαρτίᾳ με βυθιζόμενον, τῇ πρεσβείᾳ σου διάσωσον, Θεοτόκε Μήτηρ ἀειπάρθενε."

meMA.Ode5C11.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C11.greekmelody = he.h.m8_gr_GR_cog.heHE.OrthrizontesVoomenSoi.greekname
meMA.Ode5C11.melody = he.h.m8_gr_GR_cog.heHE.OrthrizontesVoomenSoi.name
meMA.Ode5C11.incipit = "Ἀνάλωτος πυρί, τῷ ἐνύλῳ ἐδείχθης…"
meMA.Ode5C11.text = "Ἀνάλωτος πυρί, τῷ ἐνύλῳ ἐδείχθης Μάρτυς, πῦρ γὰρ σε ἐγκάρδιον, θείας ἀγάπης ἑδρόσιζε."

meMA.Ode5C12.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C12.melody = he.h.m8_gr_GR_cog.heHE.OrthrizontesVoomenSoi.name
meMA.Ode5C12.incipit = "Ῥαπίσματι τὸ πρόσωπον Μάρτυς ὡραιωθεῖσα…"
meMA.Ode5C12.text = "Ῥαπίσματι τὸ πρόσωπον Μάρτυς ὡραιωθεῖσα, ἐχθρῶν ἀπεῤῥύπωσας, δυσειδεστάτων τὰ πρόσωπα."

meMA.Ode5C13.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C13.melody = he.h.m8_gr_GR_cog.heHE.OrthrizontesVoomenSoi.name
meMA.Ode5C13.incipit = "Ταθεῖσα ἐπὶ ξύλου θεόφρον Ἀναστασία…"
meMA.Ode5C13.text = "Ταθεῖσα ἐπὶ ξύλου θεόφρον Ἀναστασία, πόθῳ τοῦ νυμφίου σου, τὸ θεῖον πάθος εἰκόνιζες."

meMA.Ode5C1T.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C1T.melody = he.h.m8_gr_GR_cog.heHE.OrthrizontesVoomenSoi.name
meMA.Ode5C1T.incipit = "Ὑμνοῦμέν σε πανύμνητε Δέσποινα Θεοτόκε…"
meMA.Ode5C1T.text = "Ὑμνοῦμέν σε πανύμνητε Δέσποινα Θεοτόκε, Θεὸν ὑπερύμνητον, σαρκὶ ἀσπόρως κυήσασαν."

meMA.Ode5C21.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C21.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C21.greekmelody = he.h.m8_gr_GR_cog.heHE.InaTiMeAposo.greekname
meMA.Ode5C21.melody = he.h.m8_gr_GR_cog.heHE.InaTiMeAposo.name
meMA.Ode5C21.incipit = "Ὡς κατάκαρπον κλῆμα, βότρυας ἐξήνθησας…"
meMA.Ode5C21.text = "Ὡς κατάκαρπον κλῆμα, βότρυας ἐξήνθησας Πάτερ Ἀβράμιε, ἀρετῶν ἐνθέων, κατανύξεως οἶνον πηγάζοντας, τῶν παθῶν τὴν μέθην, ἐκ τῶν ψυχῶν ἀποσοβοῦντα, καὶ καρδίας πιστῶν κατευφραίνοντα."

meMA.Ode5C22.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C22.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C22.melody = he.h.m8_gr_GR_cog.heHE.InaTiMeAposo.name
meMA.Ode5C22.incipit = "Πειρασμούς σοι ποικίλους, ὄφις ὁ…"
meMA.Ode5C22.text = "Πειρασμούς σοι ποικίλους, ὄφις ὁ παμπόνηρος Πάτερ ἐξήγειρεν, ἀλλʼ αὐτὸς τῷ ὅπλῳ, τοῦ Σταυροῦ θεοφόρε ἐνέκρωσας, καὶ τῆς νίκης στέφος, παρὰ Θεοῦ δικαίᾳ ψήφῳ, ἐκομίσω θεόφρον Ἀβράμιε."

meMA.Ode5C23.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C23.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C23.melody = he.h.m8_gr_GR_cog.heHE.InaTiMeAposo.name
meMA.Ode5C23.incipit = "Ἁγιώτατος οἶκος, Πάτερ θείου Πνεύματος…"
meMA.Ode5C23.text = "Ἁγιώτατος οἶκος, Πάτερ θείου Πνεύματος πίστει γενόμενος, ἐδομήσω οἶκον, ἱερὸν καὶ πανίερον σύστημα, ἀνιέρου πλάνης, μεταβαλὼν ταῖς νουθεσίαις, τῷ τῶν ὅλων Θεῷ καθιέρωσας."

meMA.Ode5C2T.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C2T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C2T.melody = he.h.m8_gr_GR_cog.heHE.InaTiMeAposo.name
meMA.Ode5C2T.incipit = "Μητροπάρθενε Κόρη, θείῳ φωτιζόμενοι Πνεύματι…"
meMA.Ode5C2T.text = "Μητροπάρθενε Κόρη, θείῳ φωτιζόμενοι Πνεύματι πάναγνε, ἱεροὶ Προφῆται, ἱεραῖς ἐν φωναῖς σε προήγγειλαν· ἐξ ἧς Θεὸς Λόγος, ὑπὲρ αἰτίαν τε καὶ λόγον, ἐσαρκώθη διʼ οἶκτον ἀμέτρητον."

meMA.Ode6C11.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C11.greekmelody = he.h.m8_gr_GR_cog.heHE.ChitonaMoiParaschou.greekname
meMA.Ode6C11.melody = he.h.m8_gr_GR_cog.heHE.ChitonaMoiParaschou.name
meMA.Ode6C11.incipit = "Στρεβλούμενον τὸ σῶμα αἰκισμοῖς, ἐδήλου…"
meMA.Ode6C11.text = "Στρεβλούμενον τὸ σῶμα αἰκισμοῖς, ἐδήλου τὸ ὄρθιον, σῆς προαιρέσεως, πρὸς Θεὸν Ἀναστασία πανεύφημε."

meMA.Ode6C12.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C12.melody = he.h.m8_gr_GR_cog.heHE.ChitonaMoiParaschou.name
meMA.Ode6C12.incipit = "Ἐκρέμασο μετέωρος πληγάς, Μάρτυς ὀποφέρουσα…"
meMA.Ode6C12.text = "Ἐκρέμασο μετέωρος πληγάς, Μάρτυς ὀποφέρουσα, ἄπληγον σῴζουσα, τῆς ψυχῆς τὸ εὐγενὲς ἀξιάγαστε."

meMA.Ode6C13.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C13.melody = he.h.m8_gr_GR_cog.heHE.ChitonaMoiParaschou.name
meMA.Ode6C13.incipit = "Ὑπήνεγκας μαζῶν τὴν ἐκκοπήν, πιστοὺς…"
meMA.Ode6C13.text = "Ὑπήνεγκας μαζῶν τὴν ἐκκοπήν, πιστοὺς διατρέφουσα, γάλακτι Ἔνδοξε, τῆς μιμήσεως τῆς σῆς ἐμφανέστατα."

meMA.Ode6C1T.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C1T.melody = he.h.m8_gr_GR_cog.heHE.ChitonaMoiParaschou.name
meMA.Ode6C1T.incipit = "Φιλάνθρωπον κυήσασα Θεόν, φιλάγαθε Δέσποινα…"
meMA.Ode6C1T.text = "Φιλάνθρωπον κυήσασα Θεόν, φιλάγαθε Δέσποινα, αὐτὸν ἱκέτευε, τῆς γεέννης τοῦ πυρὸς λυτρωθῆναί με."

meMA.Ode6C21.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C21.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C21.greekmelody = he.h.m8_gr_GR_cog.heHE.TinDeisinEkcheo.greekname
meMA.Ode6C21.melody = he.h.m8_gr_GR_cog.heHE.TinDeisinEkcheo.name
meMA.Ode6C21.incipit = "Φωστῆρά σε οἱ ἐν σκότει…"
meMA.Ode6C21.text = "Φωστῆρά σε οἱ ἐν σκότει τῆς πλάνης, ἀπλανῶς καθοδηγοῦντα εὑρόντες, πρὸς τὰς ὁδούς, τῆς ζωῆς θεοφόρε, τῆς ἀγνωσίας τὸ σκότος ἀπέλιπον, γενόμενοι φωτοειδεῖς, διὰ πίστεως θείας Ἀβράμιε."

meMA.Ode6C22.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C22.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C22.melody = he.h.m8_gr_GR_cog.heHE.TinDeisinEkcheo.name
meMA.Ode6C22.incipit = "Ἀγρύπνως ἐξευμενίσας τὸ θεῖον, ἐπιπέμψαι…"
meMA.Ode6C22.text = "Ἀγρύπνως ἐξευμενίσας τὸ θεῖον, ἐπιπέμψαι φωτισμὸν σωτηρίας, τοῖς ἐν νυκτί, τῆς εἰδωλομανίας, κατασχεθεῖσι θεόφρον Ἀβράμιε, ἀνέδειξας πάντας υἱούς, καὶ φωτός καὶ ἡμέρας ἐν χάριτι."

meMA.Ode6C23.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C23.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C23.melody = he.h.m8_gr_GR_cog.heHE.TinDeisinEkcheo.name
meMA.Ode6C23.incipit = "Ἡσύχως σου τὴν ζωὴν ἐκτελέσας…"
meMA.Ode6C23.text = "Ἡσύχως σου τὴν ζωὴν ἐκτελέσας, τὰς ἐν βίῳ τρικυμίας Παμμάκαρ, τῇ πρὸς Θεόν, πεποιθήσει παρῆλθες, καὶ γαληνοὺς πρὸς λιμένας κατήντησας, Ἀβράμιε τῆς οὐρανῶν, βασιλείας καὶ θείας λαμπρότητος."

meMA.Ode6C2T.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C2T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C2T.melody = he.h.m8_gr_GR_cog.heHE.TinDeisinEkcheo.name
meMA.Ode6C2T.incipit = "Βαρούμενον νυσταγμῷ ῥαθυμίας, διανάστησον τῇ…"
meMA.Ode6C2T.text = "Βαρούμενον νυσταγμῷ ῥαθυμίας, διανάστησον τῇ σῇ μεσιτείᾳ, Μήτηρ Θεοῦ, καὶ μὴ δῴης ὑπνῶσαι, τῆς ἁμαρτίας Παρθένε εἰς θάνατον· προστάτιν γὰρ καὶ ὁδηγόν, τῆς ἐμῆς σε ζωῆς ἐπιγράφομαι."

meMA.Ode6H.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6H.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6H.greekmelody = he.h.m8_gr_GR_cog.heHE.TinDeisinEkcheo.greekname
meMA.Ode6H.melody = he.h.m8_gr_GR_cog.heHE.TinDeisinEkcheo.melody
meMA.Ode6H.incipit = he.h.m8_gr_GR_cog.heHE.TinDeisinEkcheo.incipit
meMA.Ode6H.text = he.h.m8_gr_GR_cog.heHE.TinDeisinEkcheo.text

meMA.Kontakion1.mode = miscellanea_gr_GR_cog.misc.Mode3
meMA.Kontakion1.greekmelody = he.a.m3_gr_GR_cog.heAU.IParthenosSimeron.greekname
meMA.Kontakion1.melody = he.a.m3_gr_GR_cog.heAU.IParthenosSimeron.name
meMA.Kontakion1.incipit = "Παρθενίας νάμασι, καθηγνισμένη Ὁσία, μαρτυρίου…"
meMA.Kontakion1.text = "Παρθενίας νάμασι, καθηγνισμένη Ὁσία, μαρτυρίου αἵμασιν, Ἀναστασία πλυθεῖσα, παρέχεις τοῖς ἐν ἀνάγκαις τῶν νοσημάτων, ἴασιν καὶ σωτηρίαν τοῖς προσιοῦσιν, ἐκ καρδίας· ἰσχὺν γὰρ νέμει, Χριστὸς ὁ βρύων, χάριν ἀέναον."

meMA.Kontakion2.mode = miscellanea_gr_GR_cog.misc.Mode3
meMA.Kontakion2.greekmelody = he.a.m3_gr_GR_cog.heAU.IParthenosSimeron.greekname
meMA.Kontakion2.melody = he.a.m3_gr_GR_cog.heAU.IParthenosSimeron.name
meMA.Kontakion2.incipit = "Ἐν σαρκὶ ὡς ἄγγελος, ἐπὶ…"
meMA.Kontakion2.text = "Ἐν σαρκὶ ὡς ἄγγελος, ἐπὶ τῆς γῆς ἀνεφάνης, καὶ ἀσκήσας γέγονας, πεφυτευμένον ὡς ξύλον, ὕδατι τῆς ἐγκρατείας καλῶς αὐξήσας, ῥεύμασι τῶν σῶν δακρύων ῥύπον ἐκπλύνας· διὰ τοῦτο ἀνεδείχθης, δοχεῖον θεῖον, Ἀβράμιε Πνεύματος."

meMA.Oikos2.text = "Τὰ φθαρτὰ παριδών, τὴν ἀφθαρσίαν εἴληφας· τὰς τερπνὰς ἡδονὰς τοῦ σώματος ἐμίσησας σοφὲ ἀπὸ βρέφους, ποθήσας ἁγνείαν· ὅθεν θαλάμου καὶ κόσμου ἀπέδρασας, συζύγου τε εὔκλειαν, καὶ τῶν γονέων ἐξέκλινας, μόνου Θεοῦ σοφὲ τὸν ἔρωτα ἐπιποθήσας, καὶ ἀγαπήσας ἐξ ὅλης Πάτερ τῆς ψυχῆς, καὶ διανοίας ἀληθῶς· διὰ τοῦτο ἀνεδείχθης δοχεῖον θεῖον, Ἀβράμιε Πνεύματος."

meMA.Ode7C11.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode7C11.greekmelody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.greekname
meMA.Ode7C11.melody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.name
meMA.Ode7C11.incipit = "Ἦχος φωνῆς ἑορταζόντων, ἔνθα πέφυκε…"
meMA.Ode7C11.text = "Ἦχος φωνῆς ἑορταζόντων, ἔνθα πέφυκε χορεύεις, Ἀθληφόρε, σὺν παρθένων χοροῖς, τῷ Κτίστῃ μελῳδοῦσα· Εὐλογητὸς εἶ Κύριε, ὁ Θεὸς εἰς τοὺς αἰῶνας."

meMA.Ode7C12.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode7C12.melody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.name
meMA.Ode7C12.incipit = "Μέλη συνθλώμενα ὁρῶσα, καὶ ὀνύχων…"
meMA.Ode7C12.text = "Μέλη συνθλώμενα ὁρῶσα, καὶ ὀνύχων σπαραγμὸν προσκαρτεροῦσα, ἀνηνέχθης Χριστῷ, θυσία μελῳδοῦσα· Εὐλογητὸς εἶ Κύριε, ὁ Θεὸς εἰς τοὺς αἰῶνας."

meMA.Ode7C13.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode7C13.melody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.name
meMA.Ode7C13.incipit = "Ὤφθης ὡς ἄμπελος θεόφρον, χεῖρας…"
meMA.Ode7C13.text = "Ὤφθης ὡς ἄμπελος θεόφρον, χεῖρας πόδας τε ὡς κλάδους τεμνομένη, καὶ ἡμῖν νοητῶς, τὸν οἶνον γεωργοῦσα, καρδίας τὸν εὐφραίνοντα, καὶ παθῶν ὠθοῦντα μέθην."

meMA.Ode7C14.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C14.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode7C14.melody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.name
meMA.Ode7C14.incipit = "Πῦρ αἰσθητὸν οὐκ ἐπτοήθης, νεανίας…"
meMA.Ode7C14.text = "Πῦρ αἰσθητὸν οὐκ ἐπτοήθης, νεανίας τρεῖς τοὺς πάλαι μιμουμένη, θείαν δρόσον καὶ γάρ, ἐδέξω μελῳδοῦσα· Εὐλογητὸς εἶ Κύριε, ὁ Θεὸς εἰς τοὺς αἰῶνας."

meMA.Ode7C1T.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode7C1T.melody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.name
meMA.Ode7C1T.incipit = "Ὅλον τὸν ἄνθρωπον φορέσας, ὁ…"
meMA.Ode7C1T.text = "Ὅλον τὸν ἄνθρωπον φορέσας, ὁ ὑπέρθεος δίχα τῆς ἁμαρτίας, ἐκ γαστρός σου Ἁγνή, προῆλθε σαρκοφόρος· ὃν ἐκδυσώπει πίστει σε, τοὺς τιμῶντας διασῶσαι."

meMA.Ode7C21.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C21.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode7C21.greekmelody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.greekname
meMA.Ode7C21.melody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.name
meMA.Ode7C21.incipit = "Ἴνα τῆς ἄνω βασιλείας, ἐπιτύχῃς…"
meMA.Ode7C21.text = "Ἴνα τῆς ἄνω βασιλείας, ἐπιτύχῃς καὶ τῆς δόξης τῆς ἀῤῥήτου, πατουμένης Σοφέ, καὶ φθειρομένης δόξης, ὡς νουνεχὴς καὶ φρόνιμος, κατεφρόνησας ἐμφρόνως."

meMA.Ode7C22.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C22.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode7C22.melody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.name
meMA.Ode7C22.incipit = "Ὅτε ὁ λύκος ὁ πανοῦργος…"
meMA.Ode7C22.text = "Ὅτε ὁ λύκος ὁ πανοῦργος, τὴν ἀμνάδα σου ἐσπάραξε δολίως, τότε τούτου Σοφέ, τὰς μύλας διαθλάσας, ταύτην ποιμὴν ὡς ἄριστος, πρὸς ζωὴν ἐπανηγάγου."

meMA.Ode7C23.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C23.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode7C23.melody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.name
meMA.Ode7C23.incipit = "Νόμοις ὑπείκων τοῦ Δεσπότου, τὸ…"
meMA.Ode7C23.text = "Νόμοις ὑπείκων τοῦ Δεσπότου, τὸ πλανώμενον ἐζήτησας καὶ εὗρες, καὶ τοῖς ὤμοις τοῖς σοῖς, ἀράμενος Παμμάκαρ, οἷα ποιμὴν εἰσήγαγες, μετανοίας ἐν τῇ μάνδρᾳ."

meMA.Ode7C2T.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C2T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode7C2T.melody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.name
meMA.Ode7C2T.incipit = "Μόνη τὸν ἕνα τῆς Τριάδος…"
meMA.Ode7C2T.text = "Μόνη τὸν ἕνα τῆς Τριάδος, ἀπεγέννησας ἐν δύο ταῖς οὐσίαις, ὑποστάσει μιᾷ, ὁρώμενον Παρθένε, ᾧ μελῳδοῦμεν Πάναγνε, ἀσιγήτως εἰς αἰῶνας."

meMA.Ode8C11.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C11.greekmelody = he.h.m8_gr_GR_cog.heHE.EptaplasiosKaminon.greekname
meMA.Ode8C11.melody = he.h.m8_gr_GR_cog.heHE.EptaplasiosKaminon.name
meMA.Ode8C11.incipit = "Νεανικῶς ἠρίστευσας, Ἀθληφόρε πανεύφημε, καὶ…"
meMA.Ode8C11.text = "Νεανικῶς ἠρίστευσας, Ἀθληφόρε πανεύφημε, καὶ κατὰ τῆς πλάνης, ἀνεστήσω τρόπαια, χειρῶν στερουμένη γάρ, καὶ τῶν ποδῶν ὑπέφερες, καὶ τομῆς μαστῶν, καὶ ἐκριζώσεως Μάρτυς, ἠνέσχου τῶν ὀδόντων, ἐν χαρᾷ μελῳδοῦσα· Λαὸς ὑπερυψοῦτε, Χριστὸν εἰς τους αἰῶνας."

meMA.Ode8C12.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C12.melody = he.h.m8_gr_GR_cog.heHE.EptaplasiosKaminon.name
meMA.Ode8C12.incipit = "Ὁλολαμπὴς ὡς ἥλιος, παρθενίας λαμπρότησι…"
meMA.Ode8C12.text = "Ὁλολαμπὴς ὡς ἥλιος, παρθενίας λαμπρότησι, καὶ τῆς μαρτυρίας, καλλοναῖς ἐξήστραψας, καὶ κόσμον ἐφώτισας, ταῖς φωτοβόλοις λάμψεσι, σοῦ Ἀναστασία, τῆς πολλῆς καρτερίας, προθύμως ἐκβοῶσα· Ἱερεῖς εὐλογεῖτε, λαὸς ὑπερυψοῦτε, Χριστὸν εἰς τοὺς αἰῶνας."

meMA.Ode8C13.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C13.melody = he.h.m8_gr_GR_cog.heHE.EptaplasiosKaminon.name
meMA.Ode8C13.incipit = "Ὑλομανοῦσαν πάθεσι, τὴν ψυχήν μου…"
meMA.Ode8C13.text = "Ὑλομανοῦσαν πάθεσι, τὴν ψυχήν μου Θεόνυμφε, καὶ ἀμαυρουμένην, προσβολαῖς τοῦ ὄφεως, ταῖς σαῖς παρακλήσεσιν, ἐκ τῶν δεινῶν καθάρισον, καὶ φωτιστικῇ, ἐπισκιάσει σου Μάρτυς, καταύγασον βοῶντα· Ἱερεῖς ἀνυμνεῖτε, λαὸς ὑπερυψοῦτε, Χριστὸν εἰς τοὺς αἰῶνας."

meMA.Ode8C1T.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C1T.melody = he.h.m8_gr_GR_cog.heHE.EptaplasiosKaminon.name
meMA.Ode8C1T.incipit = "Σὲ τὴν Ἁγνὴν καὶ ἄμωμον…"
meMA.Ode8C1T.text = "Σὲ τὴν Ἁγνὴν καὶ ἄμωμον, ἡ Παρθένος ποθήσασα, ἄμωμον καὶ σῶμα, καὶ ψυχὴν ἐτήρησε, παθῶν ὑπεκκαύματα, ὑπομονῇ τεφρώσασα, πεῖράν τε πολλῶν, ὑπενεγκοῦσα βασάνων, καὶ νῦν ἐπουρανίοις, ἐν θαλάμοις χορεύει, σὺν σοὶ εὐφραινομένη, εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8C21.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C21.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C21.greekmelody = he.h.m8_gr_GR_cog.heHE.NikitaiTyrannou.greekname
meMA.Ode8C21.melody = he.h.m8_gr_GR_cog.heHE.NikitaiTyrannou.name
meMA.Ode8C21.incipit = "Ἀπωλείας βόθρῳ, τὴν ἁγνὴν συνώθησε…"
meMA.Ode8C21.text = "Ἀπωλείας βόθρῳ, τὴν ἁγνὴν συνώθησε περιστεράν σου, ὁ κακοῦργος ὄφις, ἀλλʼ αὐτὴν ἀνήγαγες Πάτερ πανσόφως· Εὐλογεῖτε μέλπων, τὰ ἔργα τὸν Κύριον."

meMA.Ode8C22.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C22.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C22.melody = he.h.m8_gr_GR_cog.heHE.NikitaiTyrannou.name
meMA.Ode8C22.incipit = "Καὶ ἐν βίῳ Πάτερ, περιὼν…"
meMA.Ode8C22.text = "Καὶ ἐν βίῳ Πάτερ, περιὼν Ἀβράμιε καὶ μετὰ τέλος, ἰατρὸς νοσούντων, ἀνεδείχθης Πνεύματος χάριτι θείᾳ· Εὐλογεῖτε μέλπων, τὰ ἔργα τὸν Κύριον."

meMA.Ode8C23.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C23.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C23.melody = he.h.m8_gr_GR_cog.heHE.NikitaiTyrannou.name
meMA.Ode8C23.incipit = "Ἀκακίᾳ Πάτερ, ἐν δικαιοσύνῃ τε…"
meMA.Ode8C23.text = "Ἀκακίᾳ Πάτερ, ἐν δικαιοσύνῃ τε καὶ σωφροσύνῃ, πίστει διαλάμψας, τοῖς, Ἀγγέλοις Ὅσιε συνηριθμήθης· Εὐλογεῖτε μέλπων, τὰ ἔργα τὸν Κύριον."

meMA.Ode8C2T.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C2T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C2T.melody = he.h.m8_gr_GR_cog.heHE.NikitaiTyrannou.name
meMA.Ode8C2T.incipit = "Ῥείθρου ζῶντος ὤφθης, πανάγιον σκήνωμα…"
meMA.Ode8C2T.text = "Ῥείθρου ζῶντος ὤφθης, πανάγιον σκήνωμα ἐξ οὗ πιόντες, οἱ θανατωθέντες, τὴν ζωὴν κληρούμεθα ἀναβοῶντες· Εὐλογεῖτε πάντα, τὰ ἒργα τὸν Κύριον."

meMA.Ode8H.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8H.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8H.greekmelody = he.h.m8_gr_GR_cog.heHE.NikitaiTyrannou.greekname
meMA.Ode8H.melody = he.h.m8_gr_GR_cog.heHE.NikitaiTyrannou.melody
meMA.Ode8H.incipit = he.h.m8_gr_GR_cog.heHE.NikitaiTyrannou.incipit
meMA.Ode8H.text = he.h.m8_gr_GR_cog.heHE.NikitaiTyrannou.text

meMA.Ode9C11.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode9C11.greekmelody = he.h.m8_gr_GR_cog.heHE.ExestiEpiTouto.greekname
meMA.Ode9C11.melody = he.h.m8_gr_GR_cog.heHE.ExestiEpiTouto.name
meMA.Ode9C11.incipit = "Ἰκρίῳ ἀνηρτήθης Λόγου Θεοῦ, εἰκονίζουσα…"
meMA.Ode9C11.text = "Ἰκρίῳ ἀνηρτήθης Λόγου Θεοῦ, εἰκονίζουσα πάθος σωτήριον, καὶ ἐκκοπήν, ἤνεγκας χειρῶν σου καὶ τῶν ποδῶν, ὀδόντων τὴν ἐκρίζωσιν, γλώσσης τὴν ἀφαίρεσιν καὶ μαστῶν, ἁγνὴ Ἀναστασία, Μοναζουσῶν τὸ κλέος, καὶ τῶν Μαρτύρων ἐγκαλλώπισμα."

meMA.Ode9C12.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode9C12.melody = he.h.m8_gr_GR_cog.heHE.ExestiEpiTouto.name
meMA.Ode9C12.incipit = "Ὡς νύμφη ὡραΐσθης καλλοποιῷ, ἁρμοσθεῖσα…"
meMA.Ode9C12.text = "Ὡς νύμφη ὡραΐσθης καλλοποιῷ, ἁρμοσθεῖσα νυμφίῳ παθήμασι, καλλοποιοῖς, ἔνδον τε θαλάμων φωτοειδῶν, ὡς ἐκλεκτὴ ἐσκήνωσας, φέρουσα λαμπάδα περθενικήν, καὶ νῦν συμβασιλεύεις, τῷ ζῶντι εἰς αἰῶνας, Ἀναστασία διαλάμπουσα."

meMA.Ode9C13.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode9C13.melody = he.h.m8_gr_GR_cog.heHE.ExestiEpiTouto.name
meMA.Ode9C13.incipit = "Σταλάζουσιν οἱ πόνοι σου γλυκασμόν…"
meMA.Ode9C13.text = "Σταλάζουσιν οἱ πόνοι σου γλυκασμόν, ἁμαρτίας πικρίαν ἐξαίροντες, ἡ δὲ σορός, βρύει ἰαμάτων τοὺς ποταμούς, καὶ κατακλύζει πάντοτε, πάθη καὶ νοσήματα χαλεπά, εἰς δόξαν τοῦ Σωτῆρος, σεμνὴ Ἀναστασία, τοῦ ἐπαξίως σε δοξάσαντος."

meMA.Ode9C14.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C14.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode9C14.melody = he.h.m8_gr_GR_cog.heHE.ExestiEpiTouto.name
meMA.Ode9C14.incipit = "Ἠρίστευσαν γυναῖκες τῷ σῷ Σταυρῷ…"
meMA.Ode9C14.text = "Ἠρίστευσαν γυναῖκες τῷ σῷ Σταυρῷ, κρατυνθεῖσαι Χριστὲ παντοδύναμε, καὶ τοῦ ἐχθροῦ, κάραν πολυμήχανον ἀνδρικῶς, Λόγε Θεοῦ συνέθλασαν, καὶ τοῦ Παραδείσου χαρμονικῶς, τὴν οἴκησιν λαχοῦσαι, θεώσεως μετέσχον, ἐπιτηδείως ἀνυμνοῦσαί σε."

meMA.Ode9C1T.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode9C1T.melody = he.h.m8_gr_GR_cog.heHE.ExestiEpiTouto.name
meMA.Ode9C1T.incipit = "Φορέσαντά σε σάρκα ἐκ γυναικός…"
meMA.Ode9C1T.text = "Φορέσαντά σε σάρκα ἐκ γυναικός, ὡς ἐπέγνω ἡ Μάρτυς σου Κύριε, παρθενικαῖς, κατηγλαϊσμένη μαρμαρυγαῖς, καὶ μαρτυρίου αἵματι, περιηνθισμένη σοι τῷ Χριστῷ, ὀπίσω τῆς Μητρός σου ἐνδόξως προσηνέχθη, ὡς βασιλεύοντι τῆς κτίσεως."

meMA.Ode9C21.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C21.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode9C21.greekmelody = he.h.m8_gr_GR_cog.heHE.KyriosTheotokon.greekname
meMA.Ode9C21.melody = he.h.m8_gr_GR_cog.heHE.KyriosTheotokon.name
meMA.Ode9C21.incipit = "Ἰδεῖν κατηξιώθης, Πάτερ θεοφόρε, τὰ…"
meMA.Ode9C21.text = "Ἰδεῖν κατηξιώθης, Πάτερ θεοφόρε, τὰ διʼ ἐλπίδος σοι πάλαι ποθούμενα, ἃ ὀφθαλμὸς οὐ κατεῖδε, καὶ οὓς οὐκ ἤκουσεν."

meMA.Ode9C22.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C22.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode9C22.melody = he.h.m8_gr_GR_cog.heHE.KyriosTheotokon.name
meMA.Ode9C22.incipit = "Ὡραῖος ἀπεφάνθης, θείαις ἀγλαΐαις, τῶν…"
meMA.Ode9C22.text = "Ὡραῖος ἀπεφάνθης, θείαις ἀγλαΐαις, τῶν ἀρετῶν διαλάμπων Ἀβράμιε, καὶ τῷ τῶν ὅλων Δεσπότῃ χαίρων παρίστασαι."

meMA.Ode9C23.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C23.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode9C23.melody = he.h.m8_gr_GR_cog.heHE.KyriosTheotokon.name
meMA.Ode9C23.incipit = "Συνήφθης ταῖς χορείαις, πάντων τῶν…"
meMA.Ode9C23.text = "Συνήφθης ταῖς χορείαις, πάντων τῶν Ὁσίων, καὶ σὺν αὐτοῖς τῆς θεώσεως Ὅσιε, ἐπιτυχὼν ἱκετεύεις, εἰς τὸ σωθῆναι ἡμᾶς."

meMA.Ode9C24.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C24.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode9C24.melody = he.h.m8_gr_GR_cog.heHE.KyriosTheotokon.name
meMA.Ode9C24.incipit = "Ἡμέραν εὐφροσύνης, ἄγει χαρμοσυνως, τῆς…"
meMA.Ode9C24.text = "Ἡμέραν εὐφροσύνης, ἄγει χαρμοσυνως, τῆς ἱερᾶς σου ὦ Πάτερ κοιμήσεως, ἡ Ἐκκλησία, τιμῶσα τὴν πολιτείαν σου."

meMA.Ode9C2T.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C2T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode9C2T.melody = he.h.m8_gr_GR_cog.heHE.KyriosTheotokon.name
meMA.Ode9C2T.incipit = "Φιλάγαθε Παρθένε, τὴν κεκακωμένην, τῇ…"
meMA.Ode9C2T.text = "Φιλάγαθε Παρθένε, τὴν κεκακωμένην, τῇ ἁμαρτίᾳ ψυχήν μου ἀγάθυνον, καὶ ἀγαθῶν αἰωνίων, μέτοχον ποίησον."

meMA.Ode9H.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9H.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode9H.greekmelody = he.h.m8_gr_GR_cog.heHE.KyriosTheotokon.greekname
meMA.Ode9H.melody = he.h.m8_gr_GR_cog.heHE.KyriosTheotokon.melody
meMA.Ode9H.incipit = he.h.m8_gr_GR_cog.heHE.KyriosTheotokon.incipit
meMA.Ode9H.text = he.h.m8_gr_GR_cog.heHE.KyriosTheotokon.text

meMA.Exaposteilarion1.mode = miscellanea_gr_GR_cog.misc.Mode3
meMA.Exaposteilarion1.greekmelody = he.a.m3_gr_GR_cog.heAU.OOuranonToisAstrois.greekname
meMA.Exaposteilarion1.melody = he.a.m3_gr_GR_cog.heAU.OOuranonToisAstrois.name
meMA.Exaposteilarion1.incipit = "Μοναζουσῶν τὸ κλέος, καὶ ὡραιότης…"
meMA.Exaposteilarion1.text = "Μοναζουσῶν τὸ κλέος, καὶ ὡραιότης Μαρτύρων, Ἀναστασία σὺ ὤφθης, ἐν παρθενίᾳ ἀσκοῦσα, καὶ καρτερῶς ἐναθλοῦσα, ὑπὲρ Χριστοῦ τῆς ἀγάπης."

meMA.Exaposteilarion2.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Exaposteilarion2.greekmelody = he.a.m2_gr_GR_cog.heAU.ToisMathitaisSynelthomen.greekname
meMA.Exaposteilarion2.melody = he.a.m2_gr_GR_cog.heAU.ToisMathitaisSynelthomen.name
meMA.Exaposteilarion2.incipit = "Ὡς ἐλυτρώσω Ὅσιε, τῷ Θεῷ…"
meMA.Exaposteilarion2.text = "Ὡς ἐλυτρώσω Ὅσιε, τῷ Θεῷ καὶ Σωτῆρι, ἐκ πλάνης κακοδαίμονος, σύστημα δυσσεβούντων, Ἀβράμιε θεοφόρε, καὶ τὴν πάλαι πεσοῦσαν, εἰς ἀπωλείας βάραθρον· καὶ ἡμᾶς ταῖς εὐχαῖς σου, ῥῦσαι πολλῶν, πειρασμῶν καὶ θλίψεων καὶ κινδύνων, τοὺς πόθῳ ἐκτελοῦντάς σου, τὴν σεβάσμιον μνήμην."

meMA.ExapTheotokion.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.ExapTheotokion.melody = he.a.m2_gr_GR_cog.heAU.ToisMathitaisSynelthomen.name
meMA.ExapTheotokion.incipit = "Θεοφανὴς Παράδεισος, ὤφθης ἔχουσα μέσον…"
meMA.ExapTheotokion.text = "Θεοφανὴς Παράδεισος, ὤφθης ἔχουσα μέσον, ξύλον ζωῆς τὸν Κύριον, ἄχραντε Θεοτόκε, ἐξ οὗ φαγόντες οὐκ ἔτι, ἀποθνῄσκομεν ὅλως, ἀλλὰ ζωὴν ἀμείνονα, διὰ σοῦ ζῶμεν πάντες, τὸ τοῦ Σταυροῦ, ὅπλον περιφέροντες καὶ νικῶντες, τὸν ἀποστάτην τύραννον, σὲ ὑμνοῦντες Παρθένε."

