meMA.Ode1C11.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C11.greekmelody = he.h.m8_gr_GR_cog.heHE.AsomenToKyrio.greekname
meMA.Ode1C11.melody = he.h.m8_gr_GR_cog.heHE.AsomenToKyrio.name
meMA.Ode1C11.incipit = "Δήγματι ἁμαρτίας…"
meMA.Ode1C11.text = "Δήγματι ἁμαρτίας, μεμελανωμένην τὴν καρδίαν μου, καταλεύκανον Πάτερ, μετανοίας τοῖς ὄμβροις πρεσβείαις σου."

meMA.Ode1C12.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C12.melody = he.h.m8_gr_GR_cog.heHE.AsomenToKyrio.name
meMA.Ode1C12.incipit = "Φόβῳ τῷ τοῦ Δεσπότου…"
meMA.Ode1C12.text = "Φόβῳ τῷ τοῦ Δεσπότου, σάρκα καθηλώσας ἅπαν νόημα, ἐμπαθὲς ἐκ καρδίας, θεοφόρε παμμάκαρ ἐξήλειψας."

meMA.Ode1C13.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C13.melody = he.h.m8_gr_GR_cog.heHE.AsomenToKyrio.name
meMA.Ode1C13.incipit = "Αὔλαξι διανοίας…"
meMA.Ode1C13.text = "Αὔλαξι διανοίας, κρύψας τὰ τοῦ λόγου Πάτερ σπέρματα, ἐγεώργησας σῖτον, θησαυροῖς ἀκενώτοις τηρούμενον."

meMA.Ode1C1T.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C1T.melody = he.h.m8_gr_GR_cog.heHE.AsomenToKyrio.name
meMA.Ode1C1T.incipit = "Ὅλον με διασῴζει…"
meMA.Ode1C1T.text = "Ὅλον με διασῴζει, Λόγος ὁ ὑπέρθεος χρηστότητι, βουληθεὶς ἐκ γαστρός σου, σαρκωθῆναι ἁγνὴ Μητροπάρθενε."

meMA.Ode3C11.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C11.greekmelody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.greekname
meMA.Ode3C11.melody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.name
meMA.Ode3C11.incipit = "Πνεύματι κινούμενος…"
meMA.Ode3C11.text = "Πνεύματι κινούμενος, πνευματικαῖς σοφὲ πράξεσιν, ἐγκαρτερῶν, τὰ τῆς πονηρίας, καθυπέταξας πνεύματα."

meMA.Ode3C12.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C12.melody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.name
meMA.Ode3C12.incipit = "Σθένει δυναμούμενος…"
meMA.Ode3C12.text = "Σθένει δυναμούμενος, θεαρχικῷ Μωσῆ Ὅσιε, τὸν ἰσχυρόν, ἄσαρκος καθάπερ, ἐταπείνωσας δράκοντα."

meMA.Ode3C13.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C13.melody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.name
meMA.Ode3C13.incipit = "Ὄμβροις τῶν δακρύων σου…"
meMA.Ode3C13.text = "Ὄμβροις τῶν δακρύων σου, πυρκαϊὰν παθῶν ἔσβεσας, καὶ ποταμός, ὤφθης χαρισμάτων, πεπλησμένος τοῦ Πνεύματος."

meMA.Ode3C1T.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C1T.melody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.name
meMA.Ode3C1T.incipit = "Ἔχων σε βοήθειαν…"
meMA.Ode3C1T.text = "Ἔχων σε βοήθειαν, τῶν δυσμενῶν ὁρμὰς Ἄχραντε, οὐ δειλιῶ· ἔχων σε προστάτιν, τὰς αὐτῶν τρέπω φάλαγγας."

meMA.Ode3H.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3H.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3H.greekmelody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.greekname
meMA.Ode3H.melody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.melody
meMA.Ode3H.incipit = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.incipit
meMA.Ode3H.text = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.text

meMA.Ode3Kathisma1.mode = miscellanea_gr_GR_cog.misc.Mode3
meMA.Ode3Kathisma1.greekmelody = he.a.m3_gr_GR_cog.heAU.TheiasPisteos.greekname
meMA.Ode3Kathisma1.melody = he.a.m3_gr_GR_cog.heAU.TheiasPisteos.name
meMA.Ode3Kathisma1.incipit = "Φέγγος ἄδυτον…"
meMA.Ode3Kathisma1.text = "Φέγγος ἄδυτον εἰσδεδεγμένος, ἀπημαύρωσας τῶν νοουμένων, Αἰθιόπων θεοφόρε τὰ πρόσωπα, καὶ τὰς αὐτῶν κακουργίας διέλυσας, ταῖς πρὸς τὸ θεῖον ἀπαύστοις σου νεύσεσι. Πάτερ Ὅσιε, Χριστὸν τὸν Θεὸν ἱκέτευε, δωρήσασθαι ἡμῖν τὸ μέγα ἔλεος."

meMA.Ode3KathismaT.mode = miscellanea_gr_GR_cog.misc.Mode3
meMA.Ode3KathismaT.melody = he.a.m3_gr_GR_cog.heAU.TheiasPisteos.name
meMA.Ode3KathismaT.text = me.m01.d25_gr_GR_cog.meMA.Kathisma1T.text

meMA.Ode3KathismaSt.mode = miscellanea_gr_GR_cog.misc.Mode3
meMA.Ode3KathismaSt.melody = he.a.m3_gr_GR_cog.heAU.TheiasPisteos.name
meMA.Ode3KathismaSt.incipit = me.m01.d29_gr_GR_cog.meMA.Ode3KathismaSt.incipit
meMA.Ode3KathismaSt.text = me.m01.d29_gr_GR_cog.meMA.Ode3KathismaSt.text

meMA.Ode4C11.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C11.greekmelody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.greekname
meMA.Ode4C11.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
meMA.Ode4C11.incipit = "Τὸν φιλόσαρκον δαίμονα…"
meMA.Ode4C11.text = "Τὸν φιλόσαρκον δαίμονα, ταῖς ἀδιαλείπτοις Πάτερ δεήσεσι, καὶ τῶν πόνων ἐπιδόσεσιν, ἐκ ψυχῆς παμμάκαρ ἀπεδίωξας."

meMA.Ode4C12.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C12.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
meMA.Ode4C12.incipit = "Μεταθεὶς τὴν διάνοιαν…"
meMA.Ode4C12.text = "Μεταθεὶς τὴν διάνοιαν, πρὸς τὰ ὑπὲρ νοῦν τε καὶ λόγον Ὅσιε, τῆς ἀσκήσεως τὸν καύσωνα, ὥσπερ θείαν δρόσον καθυπέφερες."

meMA.Ode4C13.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C13.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
meMA.Ode4C13.incipit = "Θεωρίαις καὶ πράξεσιν…"
meMA.Ode4C13.text = "Θεωρίαις καὶ πράξεσιν, ἐκλελαμπρυσμένη Πάτερ πανόλβιε, ἡ καρδία σου τοῦ Πνεύματος, καθαρὸν ἐδείχθη ἐνδιαίτημα."

meMA.Ode4C1T.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C1T.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
meMA.Ode4C1T.incipit = "Παρθενίας κειμήλιον…"
meMA.Ode4C1T.text = "Παρθενίας κειμήλιον, καὶ τῆς ἀχωρήτου φύσεως σκήνωμα, τὴν ψυχήν μου φωταγώγησον, τὴν ἐσκοτισμένην Θεονύμφευτε."

meMA.Ode5C11.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C11.greekmelody = he.h.m8_gr_GR_cog.heHE.OrthrizontesVoomenSoi.greekname
meMA.Ode5C11.melody = he.h.m8_gr_GR_cog.heHE.OrthrizontesVoomenSoi.name
meMA.Ode5C11.incipit = "Τῷ ἄνθρακι πυρούμενος…"
meMA.Ode5C11.text = "Τῷ ἄνθρακι πυρούμενος, μάκαρ τῆς ἀπαθείας, ὕλην εὐκατάπρηστον, τὴν τῶν παθῶν ἀπετέφρωσας."

meMA.Ode5C12.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C12.melody = he.h.m8_gr_GR_cog.heHE.OrthrizontesVoomenSoi.name
meMA.Ode5C12.incipit = "Ἀστὴρ τῆς ἐγκρατείας…"
meMA.Ode5C12.text = "Ἀστὴρ τῆς ἐγκρατείας ὑψώμασι, διαπρέπων, ὤφθης τὰς ψυχὰς ἡμῶν, καταφωτίζων ἀοίδιμε."

meMA.Ode5C13.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C13.melody = he.h.m8_gr_GR_cog.heHE.OrthrizontesVoomenSoi.name
meMA.Ode5C13.incipit = "Τεθρίππῳ ἀρετῶν…"
meMA.Ode5C13.text = "Τεθρίππῳ ἀρετῶν ἐπιβέβηκας, καὶ πρὸς νύσσαν, ἔφθασας οὐράνιον, Πάτερ Μωσῆ ἀξιάγαστε."

meMA.Ode5C1T.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C1T.melody = he.h.m8_gr_GR_cog.heHE.OrthrizontesVoomenSoi.name
meMA.Ode5C1T.incipit = "Παρθένον μετὰ τόκον…"
meMA.Ode5C1T.text = "Παρθένον μετὰ τόκον ὑμνοῦμέν σε, Θεοτόκε· σὺ γὰρ τὸν Θεὸν Λόγον, σαρκὶ τῷ κόσμῳ ἐκύησας."

meMA.Ode6C11.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C11.greekmelody = he.h.m8_gr_GR_cog.heHE.IlasthitiMoiSotir.greekname
meMA.Ode6C11.melody = he.h.m8_gr_GR_cog.heHE.IlasthitiMoiSotir.name
meMA.Ode6C11.incipit = "Νυκτί με ἁμαρτιῶν…"
meMA.Ode6C11.text = "Νυκτί με ἁμαρτιῶν, καὶ ἡδονῶν ἀμαυρότητι, κυκλούμενον ταῖς ἐν σοί, τοῦ Πνεύματος λάμψεσιν, ὅλον φωταγώγησον, ὅλον πρὸς λιμένα, σωτηρίας Πάτερ ἴθυνον."

meMA.Ode6C12.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C12.melody = he.h.m8_gr_GR_cog.heHE.IlasthitiMoiSotir.name
meMA.Ode6C12.incipit = "Ὡς μέλισσα φιλεργός…"
meMA.Ode6C12.text = "Ὡς μέλισσα φιλεργός, τῷ σίμβλῳ τῆς διανοίας σου, τὰ ἄνθη τῶν ἀρετῶν, ἀθροίσας ἀνέβλυσας, γλυκασμὸν ἀθάνατον, τὴν πικρίαν Πάτερ, τῶν δαιμόνων ἀπελαύνοντα."

meMA.Ode6C13.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C13.melody = he.h.m8_gr_GR_cog.heHE.IlasthitiMoiSotir.name
meMA.Ode6C13.incipit = "Ἐρήμοις ἐγκαρτερῶν…"
meMA.Ode6C13.text = "Ἐρήμοις ἐγκαρτερῶν, τὴν ἄνω πόλιν κεκλήρωσαι, τὴν σάρκα δουλαγωγῶν, νηστείαις θαυμάσιε, τρυφὴν πρὸς ἀδάπανον, καὶ πρὸς Παραδείσου, τὰς σκηνώσεις ἐξεδήμησας."

meMA.Ode6C1T.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C1T.melody = he.h.m8_gr_GR_cog.heHE.IlasthitiMoiSotir.name
meMA.Ode6C1T.incipit = "Ἁγίασμα νοητόν…"
meMA.Ode6C1T.text = "Ἁγίασμα νοητόν, καὶ ἄψαυστον ἱλαστήριον, λυχνίαν χρυσοειδῆ, καὶ τράπεζαν ἔμψυχον, τὸν ἄρτον βαστάσασαν, τῆς ζωῆς Παρθένε, οἱ πιστοί σε ὀνομάζομεν."

meMA.Ode6H.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6H.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6H.greekmelody = he.h.m8_gr_GR_cog.heHE.IlasthitiMoiSotir.greekname
