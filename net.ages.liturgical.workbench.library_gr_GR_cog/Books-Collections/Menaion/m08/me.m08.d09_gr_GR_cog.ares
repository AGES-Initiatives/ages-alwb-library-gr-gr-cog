meMA.C1.verse = ""

meMA.Ode1C11.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C11.mode = misc_gr_GR_cog.Mode8
meMA.Ode1C11.melody = he.h.m8_gr_GR_cog.AsomenToKyrio.incipit
meMA.Ode1C11.incipit = "Ὕμνοις σου τὴν φωσφόρον, μέλποντα"
meMA.Ode1C11.text = "Ὕμνοις σου τὴν φωσφόρον, μέλποντα Ματθία νῦν πανήγυριν, οὐρανόθεν μοι αἴγλην, σωτηρίας δοθῆναι ἱκέτευε."

meMA.Ode1C12.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C12.mode = misc_gr_GR_cog.Mode8
meMA.Ode1C12.melody = he.h.m8_gr_GR_cog.AsomenToKyrio.incipit
meMA.Ode1C12.incipit = "Μέγας ἥλιος ὤφθης, φωτὶ τῷ"
meMA.Ode1C12.text = "Μέγας ἥλιος ὤφθης, φωτὶ τῷ μεγάλῳ ἀξιάγαστε, μεθ' ἡμῶν γενομένῳ, ὁμιλήσας ἀμέσως, Ἀπόστολε."

meMA.Ode1C13.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C13.mode = misc_gr_GR_cog.Mode8
meMA.Ode1C13.melody = he.h.m8_gr_GR_cog.AsomenToKyrio.incipit
meMA.Ode1C13.incipit = "Νόμοις τοῖς τοῦ Δεσπότου, Μάκαρ"
meMA.Ode1C13.text = "Νόμοις τοῖς τοῦ Δεσπότου, Μάκαρ καθυπείκων τῇ σαγήνῃ σου, συλλαμβάνεις τῆς γλώττης, ἐκ βυθοῦ ἀγνωσίας τὰ πέρατα."

meMA.Ode1C1T.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C1T.mode = misc_gr_GR_cog.Mode8
meMA.Ode1C1T.melody = he.h.m8_gr_GR_cog.AsomenToKyrio.incipit
meMA.Ode1C1T.incipit = "Ὅλον με διασῴζει, Λόγος ὁ"
meMA.Ode1C1T.text = "Ὅλον με διασῴζει, Λόγος ὁ ὑπέρθεος χρηστότητι, βουληθεὶς ἐκ γαστρός σου, σαρκωθῆναι ἁγνὴ Μητροπάρθενε."

meMA.Ode3C11.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C11.mode = misc_gr_GR_cog.Mode8
meMA.Ode3C11.melody = he.h.m8_gr_GR_cog.SyEiToStereoma.incipit
meMA.Ode3C11.incipit = "Ἴσχυσας σθενούμενος, ζωαρχικῷ σοφὲ νεύματι,"
meMA.Ode3C11.text = "Ἴσχυσας σθενούμενος, ζωαρχικῷ σοφὲ νεύματι, τοῦ ἰσχυροῦ σκῦλα διαρπάσαι, καὶ δουλῶσαι τῇ χάριτι."

meMA.Ode3C12.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C12.mode = misc_gr_GR_cog.Mode8
meMA.Ode3C12.melody = he.h.m8_gr_GR_cog.SyEiToStereoma.incipit
meMA.Ode3C12.incipit = "Στέργων τὸν Διδάσκαλον, καθυπουργῶν αὐτοῦ"
meMA.Ode3C12.text = "Στέργων τὸν Διδάσκαλον, καθυπουργῶν αὐτοῦ νεύμασι, παντουργικοῖς πλάνης διασῴζεις, τοὺς βροτοὺς ἀξιάγαστε."

meMA.Ode3C13.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C13.mode = misc_gr_GR_cog.Mode8
meMA.Ode3C13.melody = he.h.m8_gr_GR_cog.SyEiToStereoma.incipit
meMA.Ode3C13.incipit = "Σὲ τὸν δωδεκάριθμον, τῶν Μαθητῶν"
meMA.Ode3C13.text = "Σὲ τὸν δωδεκάριθμον, τῶν Μαθητῶν χορὸν ἅγιον, τὸ παντουργόν, Πνεῦμα συμπληροῦντα, θεοῤῥῆμον ἐκλέγεται."

meMA.Ode3C1T.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C1T.mode = misc_gr_GR_cog.Mode8
meMA.Ode3C1T.melody = he.h.m8_gr_GR_cog.SyEiToStereoma.incipit
meMA.Ode3C1T.incipit = "Ἔχων σε βοήθειαν, τῶν δυσμενῶν"
meMA.Ode3C1T.text = "Ἔχων σε βοήθειαν, τῶν δυσμενῶν ὁρμὰς Ἄχραντε, οὐ δειλιῶ· ἔχων σε προστάτιν, τὰς αὐτῶν τρέπω φάλαγγας."

meMA.Ode3H.ode = misc_gr_GR_cog.Ode3
meMA.Ode3H.mode = misc_gr_GR_cog.Mode8
meMA.Ode3H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode3H.incipit = he.h.m8_gr_GR_cog.SyEiToStereoma.incipit
meMA.Ode3H.text = he.h.m8_gr_GR_cog.SyEiToStereoma.text

meMA.Ode3Kathisma1.mode = misc_gr_GR_cog.Mode8
meMA.Ode3Kathisma1.melody = he.a.m8_gr_GR_cog.TinSofianKaiLogon.incipit
meMA.Ode3Kathisma1.incipit = "Ἀστραπαῖς σου τῶν λόγων τῶν"
meMA.Ode3Kathisma1.text = "Ἀστραπαῖς σου τῶν λόγων τῶν θεϊκῶν, καταφλέξας τὴν πλάνην τὴν πονηράν, πιστοὺς κατεφώτισας, ἀνυμνεῖν τὴν ἐν σώματι, παρουσίαν Μάκαρ, τοῦ πάντων δεσπόζοντος, καὶ αὐτοῦ τὸ πάθος τὸ θεῖον ἐζήλωσας· ὅθεν συνελθόντες, τὴν ἁγίαν σου μνήμην, τελοῦμεν γηθόμενοι, καὶ συμφώνως βοῶμέν σοι· Ματθία Ἀπόστολε, πρέσβευε Χριστῷ τῷ Θεῷ, τῶν πταισμάτων ἄφεσιν δωρήσασθαι, τοῖς ἑορτάζουσι πόθῳ, τὴν ἁγίαν μνήμην σου."

meMA.Ode3Kathisma2.mode = misc_gr_GR_cog.Mode4
meMA.Ode3Kathisma2.melody = he.a.m4_gr_GR_cog.EpefanisSimeron.incipit
meMA.Ode3Kathisma2.incipit = me.m08.d07_gr_GR_cog.meMA.Kathisma11.incipit
meMA.Ode3Kathisma2.text = me.m08.d07_gr_GR_cog.meMA.Kathisma11.text

meMA.Ode4C11.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C11.mode = misc_gr_GR_cog.Mode8
meMA.Ode4C11.melody = he.h.m8_gr_GR_cog.EisakikoaKyrie.incipit
meMA.Ode4C11.incipit = "Μίαν φύσιν Θεότητος, μίαν βασιλείαν"
meMA.Ode4C11.text = "Μίαν φύσιν Θεότητος, μίαν βασιλείαν καὶ κυριότητα, καταγγέλλων παμμακάριστε, τῶν ἐθνῶν ἀγέλας ἐσαγήνευσας."

meMA.Ode4C12.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C12.mode = misc_gr_GR_cog.Mode8
meMA.Ode4C12.melody = he.h.m8_gr_GR_cog.EisakikoaKyrie.incipit
meMA.Ode4C12.incipit = "Ἐπεβίβασε Κύριος, σὲ ἐν τῇ"
meMA.Ode4C12.text = "Ἐπεβίβασε Κύριος, σὲ ἐν τῇ θαλάσσῃ τοῦ βίου πάνσοφε, ὥσπερ, ἵππον συνταράσσοντα, τῆς πολυθεΐας πικρὰ ὕδατα."

meMA.Ode4C13.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C13.mode = misc_gr_GR_cog.Mode8
meMA.Ode4C13.melody = he.h.m8_gr_GR_cog.EisakikoaKyrie.incipit
meMA.Ode4C13.incipit = "Λόγῳ θείῳ ἐνέωσας, τὰς κεχερσωμένας"
meMA.Ode4C13.text = "Λόγῳ θείῳ ἐνέωσας, τὰς κεχερσωμένας καρδίας πάνσοφε, κατασπείρας τὴν εὐσέβειαν, καὶ τῆς ἀληθείας τὴν ἐπίγνωσιν."

meMA.Ode4C1T.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C1T.mode = misc_gr_GR_cog.Mode8
meMA.Ode4C1T.melody = he.h.m8_gr_GR_cog.EisakikoaKyrie.incipit
meMA.Ode4C1T.incipit = "Παρθενίας κειμήλιον, καὶ τῆς ἀχωρήτου"
meMA.Ode4C1T.text = "Παρθενίας κειμήλιον, καὶ τῆς ἀχωρήτου φύσεως σκήνωμα, τὴν ψυχήν μου φωταγώγησον, τὴν ἐσκοτισμένην πολλοῖς πάθεσιν."

meMA.Ode5C11.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C11.mode = misc_gr_GR_cog.Mode8
meMA.Ode5C11.melody = he.h.m8_gr_GR_cog.InaTiMeAposo.incipit
meMA.Ode5C11.incipit = "Ὡς πολύφορον κλῆμα, ἡ ἀληθινὴ"
meMA.Ode5C11.text = "Ὡς πολύφορον κλῆμα, ἡ ἀληθινὴ σε ἐξέτεινεν ἄμπελος, γεωργοῦντα βότρυν, τὸν προχέοντα οἶνον σωτήριον, ὃν πιόντες Μάκαρ, οἱ συσχεθέντες ἀγνωσίᾳ, τῆς ἀπάτης τὴν μέθην ἀπέῤῥιψαν."

meMA.Ode5C12.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C12.mode = misc_gr_GR_cog.Mode8
meMA.Ode5C12.melody = he.h.m8_gr_GR_cog.InaTiMeAposo.incipit
meMA.Ode5C12.incipit = "Μαθητῶν δωδεκάδος, ὤφθης συμπληρῶν τὸν"
meMA.Ode5C12.text = "Μαθητῶν δωδεκάδος, ὤφθης συμπληρῶν τὸν χορὸν τὸν μακάριον, ἐξ οὗ ὁ προδότης, ἑαυτὸν ἀποῤῥίψας ἐκέρδησε, τὴν πικρὰν ἀγχόνην, κατὰ Χριστοῦ ἐπάρας πτέρναν, μυστολέκτα Ματθία Ἀπόστολε."

meMA.Ode5C13.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C13.mode = misc_gr_GR_cog.Mode8
meMA.Ode5C13.melody = he.h.m8_gr_GR_cog.InaTiMeAposo.incipit
meMA.Ode5C13.incipit = "Ἅλας θεῖον ἐβλήθης, Μάκαρ εἰς"
meMA.Ode5C13.text = "Ἅλας θεῖον ἐβλήθης, Μάκαρ εἰς τὸν κόσμον Ματθία Ἀπόστολε, σηπεδόνα πλάνης, ἐκκαθαίρων νοστίμοις διδάγμασιν, ἐκδιώκων νόσους, καὶ ἀπελαύνων μαλακίας, τῶν ψυχῶν καὶ τῶν σωμάτων πανεύφημε."

meMA.Ode5C1T.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C1T.mode = misc_gr_GR_cog.Mode8
meMA.Ode5C1T.melody = he.h.m8_gr_GR_cog.InaTiMeAposo.incipit
meMA.Ode5C1T.incipit = "Τοὺς ἀμόρφους χιτῶνας, καὶ τὴν"
meMA.Ode5C1T.text = "Τοὺς ἀμόρφους χιτῶνας, καὶ τὴν παλαιὰν ἐξεδύσατο νέκρωσιν, ὁ Ἀδὰμ Παρθένε, τῷ ἀχράντῳ σου τόκῳ Θεόνυμφε, καὶ στολὴν ἁγίαν, ἀληθινὴν μετημφιέσθη, μηδαμῶς ῥυπουμένην παθήμασιν."

meMA.Ode6C11.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C11.mode = misc_gr_GR_cog.Mode8
meMA.Ode6C11.melody = he.h.m8_gr_GR_cog.IlasthitiMoiSotir.incipit
meMA.Ode6C11.incipit = "Θεὸς ἐγένου θετός, ἑνώσει θείᾳ"
meMA.Ode6C11.text = "Θεὸς ἐγένου θετός, ἑνώσει θείᾳ θεούμενος, καὶ πρωτουργῶς ἐκ Θεοῦ, ἀκτῖνας δεξάμενος, πιστοὺς κατεφώτισας, καὶ τὴν γῆν ἀχλύος, τῶν εἰδώλων ἐξεκάθηρας."

meMA.Ode6C12.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C12.mode = misc_gr_GR_cog.Mode8
meMA.Ode6C12.melody = he.h.m8_gr_GR_cog.IlasthitiMoiSotir.incipit
meMA.Ode6C12.incipit = "Ἰδοὺ σοι ὑποχωρεῖ, τῷ Μαθητῇ"
meMA.Ode6C12.text = "Ἰδοὺ σοι ὑποχωρεῖ, τῷ Μαθητῇ πᾶσα πρόῤῥησις, τῶν ἱερῶν Προφητῶν· αὐτόπτης ἐγένου γάρ, οὗ πάλαι προήγγειλαν, ὑπουργὸς Ματθία, μιμητής τε καὶ Ἀπόστολος."

meMA.Ode6C1T.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C1T.mode = misc_gr_GR_cog.Mode8
meMA.Ode6C1T.melody = he.h.m8_gr_GR_cog.IlasthitiMoiSotir.incipit
meMA.Ode6C1T.incipit = "Ἁγίασμα νοητόν, καὶ ἄψαυστον ἱλαστήριον,"
meMA.Ode6C1T.text = "Ἁγίασμα νοητόν, καὶ ἄψαυστον ἱλαστήριον, λυχνίαν χρυσοειδῆ, καὶ τράπεζαν ἔμψυχον, τὸν ἄρτον βαστάσασαν, τῆς ζωῆς Παρθένε, οἱ πιστοὶ σε ὀνομάζομεν."

meMA.Ode6H.ode = misc_gr_GR_cog.Ode6
meMA.Ode6H.mode = misc_gr_GR_cog.Mode8
meMA.Ode6H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode6H.incipit = he.h.m8_gr_GR_cog.IlasthitiMoiSotir.incipit
meMA.Ode6H.text = he.h.m8_gr_GR_cog.IlasthitiMoiSotir.text

meMA.Ode7C11.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C11.mode = misc_gr_GR_cog.Mode8
meMA.Ode7C11.melody = he.h.m8_gr_GR_cog.PaidesEvraion.incipit
meMA.Ode7C11.incipit = "Χύσιν ἐξήρανας ἀπάτης, τῇ πλημμύρᾳ"
meMA.Ode7C11.text = "Χύσιν ἐξήρανας ἀπάτης, τῇ πλημμύρᾳ σου τῶν λόγων τῆς σοφίας, καὶ χειμάῤῥους τρυφῆς, ἐπότισας τοὺς πίστει, ἀναβοῶντας ἔνδοξε· ὁ Θεὸς εὐλογητὸς εἶ."

meMA.Ode7C12.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C12.mode = misc_gr_GR_cog.Mode8
meMA.Ode7C12.melody = he.h.m8_gr_GR_cog.PaidesEvraion.incipit
meMA.Ode7C12.incipit = "Ῥώμῃ Θεοῦ παντοδυνάμῳ, ἐξησθένησαν ἐχθροῦ"
meMA.Ode7C12.text = "Ῥώμῃ Θεοῦ παντοδυνάμῳ, ἐξησθένησαν ἐχθροῦ μηχανουργίαι, πρὸ προσώπου τῆς σῆς, Ἀπόστολε πορείας, καὶ διεθρύβη ὄρη τε, καὶ βουνοὶ τῆς ἀθεΐας."

meMA.Ode7C1T.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C1T.mode = misc_gr_GR_cog.Mode8
meMA.Ode7C1T.melody = he.h.m8_gr_GR_cog.PaidesEvraion.incipit
meMA.Ode7C1T.incipit = "Ἴδε ἣν ἔφησε Παρθένον, ἐν"
meMA.Ode7C1T.text = "Ἴδε ἣν ἔφησε Παρθένον, ἐν τῷ Πνεύματι ὁ μέγας Ἡσαΐας, ἐν γαστρὶ τὸν Θεόν, συνέλαβε καὶ τίκτει· ᾧ μελῳδοῦμεν, Κύριε, ὁ Θεὸς εὐλογητὸς εἶ."

meMA.Ode8C11.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C11.mode = misc_gr_GR_cog.Mode8
meMA.Ode8C11.melody = he.h.m8_gr_GR_cog.EptaplasiosKaminon.incipit
meMA.Ode8C11.incipit = "Στόμα Θεοῦ γενόμενος, ἐκ τοῦ"
meMA.Ode8C11.text = "Στόμα Θεοῦ γενόμενος, ἐκ τοῦ φάρυγγος ἥρπασας, τοῦ ἀνθρωποκτόνου, οὓς δεινῶς κατέπιε, καὶ βρῶμα εἰργάσατο, τῆς ἑαυτοῦ κακίας σοφέ, καὶ διὰ λουτροῦ, ἀναγεννήσεως τούτους, προσήγαγες Κυρίῳ, μελῳδοῦντας ἀπαύστως· Λαὸς ὑπερυψοῦτε, Χριστὸν εἰς τοὺς αἰῶνας."

meMA.Ode8C12.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C12.mode = misc_gr_GR_cog.Mode8
meMA.Ode8C12.melody = he.h.m8_gr_GR_cog.EptaplasiosKaminon.incipit
meMA.Ode8C12.incipit = "Τοὺς τῆς ἀπάτης ἄξονας, τῆς"
meMA.Ode8C12.text = "Τοὺς τῆς ἀπάτης ἄξονας, τῆς κακίας τὰ ἅρματα, ἅρμα γεγονώς, Θεοῦ τοῦ Λόγου ἔνδοξε, εἰς τέλος ἐλέπτυνας, εἰδωλικούς, προῤῥίζους τε, στήλας καὶ ναοὺς θείᾳ δυνάμει καθεῖλες, ναοὺς δὲ τοὺς βοῶντας, τῆς Τριάδος εἰργάσω· Λαὸς ὑπερυψοῦτε, Χριστὸν εἰς τοὺς αἰῶνας."

meMA.Ode8C13.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C13.mode = misc_gr_GR_cog.Mode8
meMA.Ode8C13.melody = he.h.m8_gr_GR_cog.EptaplasiosKaminon.incipit
meMA.Ode8C13.incipit = "Ὁ λογικὸς ἐπέφανεν, οὐρανὸς διηγούμενος,"
meMA.Ode8C13.text = "Ὁ λογικὸς ἐπέφανεν, οὐρανὸς διηγούμενος, τοῦ μονογενοῦς, Θεοῦ δόξαν τὴν ἄῤῥητον, Ματθίας ὁ πάνσοφος, ἡ ἀστραπὴ τοῦ Πνεύματος ὁ σαγηνευτής, τῶν πλανωμένων ὁ λύχνος, τῆς θείας φωταυγίας, τῶν ἀῤῥήτων ὁ μύστης, αὐτὸν ἐν εὐφροσύνῃ, ὑμνήσωμεν συμφώνως."

meMA.Ode8C1T.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C1T.mode = misc_gr_GR_cog.Mode8
meMA.Ode8C1T.melody = he.h.m8_gr_GR_cog.EptaplasiosKaminon.incipit
meMA.Ode8C1T.incipit = "Ὑπερφυῶς συνέλαβες, ὑπὲρ λόγον ἐκύησας,"
meMA.Ode8C1T.text = "Ὑπερφυῶς συνέλαβες, ὑπὲρ λόγον ἐκύησας, τὸν Δημιουργόν, τῆς ἀνθρωπίνης φύσεως, γενόμενον ἄνθρωπον, τὸν τοῦ Πατρὸς ἀχώριστον Δέσποινα ἁγνή, ᾧ μελῳδεῖ πᾶσα κτίσις· οἱ παῖδες εὐλογεῖτε, Ἱερεῖς ἀνυμνεῖτε, λαὸς ὑπερυψοῦτε, Χριστὸν εἰς τοὺς αἰῶνας."

meMA.Ode8H.ode = misc_gr_GR_cog.Ode8
meMA.Ode8H.mode = misc_gr_GR_cog.Mode8
meMA.Ode8H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode8H.incipit = he.h.m8_gr_GR_cog.EptaplasiosKaminon.incipit
meMA.Ode8H.text = he.h.m8_gr_GR_cog.EptaplasiosKaminon.text

meMA.Ode9C11.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C11.mode = misc_gr_GR_cog.Mode8
meMA.Ode9C11.melody = he.h.m8_gr_GR_cog.EfrixePasaAkoi.incipit
meMA.Ode9C11.incipit = "Φίλον σε ἔφη ὁ Σωτήρ,"
meMA.Ode9C11.text = "Φίλον σε ἔφη ὁ Σωτήρ, τοῖς αὐτοῦ καθυπουργοῦντα προστάγμασι, Μάκαρ Ἀπόστολε, καὶ κληρονόμον τῆς βασιλείας αὐτοῦ, ἐν τῇ μελλούσῃ φοβερᾷ, ἡμέρᾳ συγκάθεδρον, Ματθία πάνσοφε, δωδεκάδος Μαθητῶν τὸ συμπλήρωμα."

meMA.Ode9C12.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C12.mode = misc_gr_GR_cog.Mode8
meMA.Ode9C12.melody = he.h.m8_gr_GR_cog.EfrixePasaAkoi.incipit
meMA.Ode9C12.incipit = "Ἱστίῳ Μάκαρ τοῦ Σταυροῦ, διελθὼν"
meMA.Ode9C12.text = "Ἱστίῳ Μάκαρ τοῦ Σταυροῦ, διελθὼν τὴν ἀγριαίνουσαν θάλασσαν, τοῦ βίου ἔφθασας, πρὸς τοὺς λιμένας τῆς ἀναπαύσεως, καὶ ἀκροτάτῳ ἐφετῶν, χαίρων νῦν παρίστασαι, σὺν Ἀποστόλων χορῷ, δυσωπῶν ὑπὲρ ἡμῶν τὸν φιλάνθρωπον."

meMA.Ode9C13.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C13.mode = misc_gr_GR_cog.Mode8
meMA.Ode9C13.melody = he.h.m8_gr_GR_cog.EfrixePasaAkoi.incipit
meMA.Ode9C13.incipit = "Λυχνία ὄντως χρυσαυγής, θρυαλλίδι τῇ"
meMA.Ode9C13.text = "Λυχνία ὄντως χρυσαυγής, θρυαλλίδι τῇ τοῦ Πνεύματος λάμπουσα, ὤφθη ἡ γλῶσσά σου, καταπιμπρῶσα ἔκφυλα δόγματα, κατασβεννύουσα σοφέ, τὸ πῦρ τὸ ἀλλότριον, φῶς ἀπαστράπτουσα, τοῖς ἐν σκότει ἀγνωσίας καθεύδουσιν."

meMA.Ode9C1T.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C1T.mode = misc_gr_GR_cog.Mode8
meMA.Ode9C1T.melody = he.h.m8_gr_GR_cog.EfrixePasaAkoi.incipit
meMA.Ode9C1T.incipit = "Ἔπαυσας μόνη γυναικῶν, τὴν ἀρὰν"
meMA.Ode9C1T.text = "Ἔπαυσας μόνη γυναικῶν, τὴν ἀρὰν τῶν πρωτοπλάστων Θεόνυμφε, τὸν ἀπερίγραπτον, σαρκὶ τεκοῦσα περιγραφόμενον, ἐκαινοτόμησας θεσμοὺς φύσεως ἀμόλυντε, τὰ διεστῶτα πρίν, παραδόξῳ μεσιτείᾳ σου ἥνωσας."

meMA.Ode9H.ode = misc_gr_GR_cog.Ode9
meMA.Ode9H.mode = misc_gr_GR_cog.Mode8
meMA.Ode9H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode9H.incipit = he.h.m8_gr_GR_cog.EfrixePasaAkoi.incipit
meMA.Ode9H.text = he.h.m8_gr_GR_cog.EfrixePasaAkoi.text

meMA.Exaposteilarion1.mode = misc_gr_GR_cog.Mode2
meMA.Exaposteilarion1.melody = he.a.m2_gr_GR_cog.ToisMathitaisSynelthomen.incipit
meMA.Exaposteilarion1.incipit = "Ἰούδας ὁ παράνομος, αὐθαιρέτῳ τῇ"
meMA.Exaposteilarion1.text = "Ἰούδας ὁ παράνομος, αὐθαιρέτῳ τῇ γνώμῃ, ἀποῤῥαγεὶς ἐκπέπτωκε, δωδεκάδος τῆς θείας, τῶν Ἱερῶν Ἀποστόλων, ἐκλογῇ δὲ τῇ θείᾳ, Ματθίας ὁ πανεύφημος, προκριθείς ἀντεισήχθη, ὡς Μαθητής, ὡς Χριστοῦ Ἀπόστολος, καὶ ὡς πᾶσαν, τὴν γῆν λαμπρύνας δόγμασι, τῆς Ἁγίας Τριάδος."

meMA.Exaposteilarion2.mode = misc_gr_GR_cog.Mode2
meMA.Exaposteilarion2.melody = he.a.m2_gr_GR_cog.ToisMathitaisSynelthomen.incipit
meMA.Exaposteilarion2.incipit = me.m08.d07_gr_GR_cog.meMA.Exaposteilarion2.incipit
meMA.Exaposteilarion2.text = me.m08.d07_gr_GR_cog.meMA.Exaposteilarion2.text

meMA.Aposticha1.mode = misc_gr_GR_cog.Mode2
meMA.Aposticha1.melody = he.a.m2_gr_GR_cog.OikosTouEfratha.incipit
meMA.Aposticha1.incipit = "Ἐν ὄρει τῷ Θαβώρ, ἡ"
meMA.Aposticha1.text = "Ἐν ὄρει τῷ Θαβώρ, ἡ τῆς Τριάδος δόξα, ἀναφανδὸν ἐγνώσθη, σοῦ μεταμορφουμένου, Σωτήρ μου ὑπεράγαθε."

meMA.Aposticha2.mode = misc_gr_GR_cog.Mode2
meMA.Aposticha2.melody = he.a.m2_gr_GR_cog.OikosTouEfratha.incipit
meMA.Aposticha2.incipit = "Νεφέλη φωτεινή, φαιδρῶς ἐφαπλωθεῖσα, ἐν"
meMA.Aposticha2.text = "Νεφέλη φωτεινή, φαιδρῶς ἐφαπλωθεῖσα, ἐν τῇ Μεταμορφώσει, κατέπληττε τοὺς θείους, τῶν Μαθητῶν ἀκραίμονας."

meMA.Aposticha3.mode = misc_gr_GR_cog.Mode2
meMA.Aposticha3.melody = he.a.m2_gr_GR_cog.OikosTouEfratha.incipit
meMA.Aposticha3.incipit = "Ἠλίας σὺν Μωσεῖ, παρέστησάν σοι"
meMA.Aposticha3.text = "Ἠλίας σὺν Μωσεῖ, παρέστησάν σοι Λόγε, ἐν τῇ Μεταμορφώσει, Πατὴρ δὲ ἐμαρτύρει, ἐξ οὐρανοῦ φθεγγόμενος."

meMA.AposGlory.mode = misc_gr_GR_cog.Mode2
meMA.AposGlory.melody = he.a.m2_gr_GR_cog.OikosTouEfratha.incipit
