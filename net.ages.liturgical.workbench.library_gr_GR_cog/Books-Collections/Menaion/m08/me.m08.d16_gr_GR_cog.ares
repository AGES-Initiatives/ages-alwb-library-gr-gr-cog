meVE.Stichera01.mode = miscellanea_gr_GR_cog.misc.Mode2
meVE.Stichera01.greekmelody = he.a.m2_gr_GR_cog.heAU.PoioisEffimionStemmasin.greekname
meVE.Stichera01.melody = he.a.m2_gr_GR_cog.heAU.PoioisEffimionStemmasin.name
meVE.Stichera01.incipit = "Ποίοις οἱ γηγενεῖς…"
meVE.Stichera01.text = "Ποίοις οἱ γηγενεῖς ὄμμασιν, ἐποψόμεθά σου τὴν Εἰκόνα; ἣν τὰ τῶν Ἀγγέλων στρατεύματα, βλέπειν ἀδεῶς οὐ δεδύνηνται, θεϊκῷ φωτὶ ἀστραπτομένην· ἀπαίρει γάρ, ἀπὸ γῆς ἀπίστων σήμερον, καὶ πόλιν, πρὸς Βασιλίδα καὶ λαὸν εὐσεβῆ, ἐπιδημεῖ θείᾳ νεύσει· ἧς περ τῇ εἰσόδῳ, ἐπευφραίνονται βασιλεῖς, προσπίπτοντες ταύτῃ, μετὰ φόβου Χριστὲ καὶ πίστεως."

meVE.Stichera02.mode = miscellanea_gr_GR_cog.misc.Mode2
meVE.Stichera02.melody = he.a.m2_gr_GR_cog.heAU.PoioisEffimionStemmasin.name
meVE.Stichera02.incipit = "Ποίαις οἱ χοϊκοί…"
meVE.Stichera02.text = "Ποίαις οἱ χοϊκοὶ ψαύσομεν, τῆς Εἰκόνος σου Λόγε παλάμαις; οἱ ῥερυπωμένοι τοῖς πταίσμασι, τοῦ ἀναμαρτήτου Θεοῦ ἡμῶν; οἱ ἐν μολυσμοῖς, τοῦ ἀπροσίτου; Καλύπτει, τὰ Χερουβὶμ τὰς ὄψεις τρέμοντα· οὐ φέρει, τὰ Σεραφὶμ ὁρᾶν τὴν δόξαν σου· φόβῳ δουλεύει σοι κτίσις. Μὴ οὖν κατακρίνῃς, ἀναξίως σου τὴν Μορφήν, Χριστὲ τὴν φρικτήν, ἀσπαζομένους ἡμᾶς ἐκ πίστεως."

meVE.Stichera03.mode = miscellanea_gr_GR_cog.misc.Mode2
meVE.Stichera03.melody = he.a.m2_gr_GR_cog.heAU.PoioisEffimionStemmasin.name
meVE.Stichera03.incipit = "Πάλιν Δεσποτικῆς…"
meVE.Stichera03.text = "Πάλιν Δεσποτικῆς πάρεστι, πανηγύρεως θεία ἡμέρα· ὁ γὰρ ἐν ὑψίστοις καθήμενος, νῦν ἡμᾶς σαφῶς ἐπεσκέψατο, διὰ τῆς σεπτῆς αὑτοῦ Εἰκόνος· ὁ ἄνω, τοῖς Χερουβὶμ ὢν ἀθεώρητος, ὁρᾶται, διὰ γραφῆς οἷς περ ὡμοίωται, Πατρὸς ἀχράντῳ δακτύλῳ, μορφωθεὶς ἀῤῥήτως, καθʼ ὁμοίωσιν τὴν αὐτοῦ· ἣν πίστει καὶ πόθῳ, προσκυνοῦντες ἁγιαζόμεθα."

meVE.Stichera04.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.Stichera04.greekmelody = he.a.m4_gr_GR_cog.heAU.OsGennaionEnMartysi.greekname
meVE.Stichera04.melody = he.a.m4_gr_GR_cog.heAU.OsGennaionEnMartysi.name
meVE.Stichera04.incipit = "Ἰατρεύειν παθήματα…"
meVE.Stichera04.text = "Ἰατρεύειν παθήματα, φυγαδεύειν νοσήματα, ἀπελαύνειν πνεύματα πονηρότατα, τῇ συνεργείᾳ τοῦ Πνεύματος, παμμάκαρ ἠξίωσαι, ἀφανίσας ἀνδρικῶς, τὰ ἐκείνων σεβάσματα, ταῖς ἐνστάσεσι, τῆς ἀθλήσεως Μάρτυς στεφηφόρε, συμπολῖτα τῶν Ἀγγέλων, τῶν ἀθλοφόρων ὁμόσκηνε."

meVE.Stichera05.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.Stichera05.melody = he.a.m4_gr_GR_cog.heAU.OsGennaionEnMartysi.name
meVE.Stichera05.incipit = "Τὸν ἀγῶνα ἠγώνισαι…"
meVE.Stichera05.text = "Τὸν ἀγῶνα ἠγώνισαι, καὶ τὸν δρόμον ἐτέλεσας, καὶ τὴν πίστιν ἔνδοξε συνετήρησας, καὶ πρὸς Θεὸν προσεχώρησας, βραβεῖον τὰ θαύματα, παρʼ αὐτοῦ ἀπολαβών, καὶ τῆς νίκης τὸν στέφανον, τὸν ἀμάραντον, ὡς ἀήττητος Μάρτυς, ὡς τῆς ἄνω, βασιλείας κληρονόμος, ὑπὲρ τοῦ κόσμου δεόμενος."

meVE.Stichera06.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.Stichera06.melody = he.a.m4_gr_GR_cog.heAU.OsGennaionEnMartysi.name
meVE.Stichera06.incipit = "Ἐν ὁδῷ μαρτυρίου σου…"
meVE.Stichera06.text = "Ἐν ὁδῷ μαρτυρίου σου, ὡς ἐν πλούτῳ τερπόμενος, Διομήδης Κύριε τῇ βουλήσει σου, αὐτοῦ τὸ πνεῦμα εἰς χεῖράς σου, Χριστὲ παραδέδωκεν, ἐκτμηθεὶς τὴν κεφαλήν, μετὰ τέλος μακάριον, καὶ γενόμενος, ὁδηγὸς ἀληθείας τοῖς ἀθέοις, πηρωθεῖσι παραδόξως, καὶ διὰ πίστεως βλέψασι."

meVE.SticGlory.mode = miscellanea_gr_GR_cog.misc.Mode6
meVE.SticGlory.melody = ""
meVE.SticGlory.incipit = "Δεῦτε τὴν παγκόσμιον…"
meVE.SticGlory.text = "Δεῦτε τὴν παγκόσμιον Κοίμησιν, τῆς παναμώμου Θεοτόκου ἑορτάσωμεν· σήμερον γὰρ Ἄγγελοι πανηγυρίζουσι, τὴν σεπτὴν Μετάστασιν τῆς Θεομήτορος, καὶ πρὸς εὐωχίαν ἡμᾶς τοὺς γηγενεῖς συγκαλοῦσι, τοῦ βοᾶν ἀσιγήτῳ φωνῇ· Χαῖρε, ἡ μεταστᾶσα ἀπὸ γῆς, καὶ πρὸς οὐρανίους μονὰς μετοικήσασα. Χαίροις, ἡ τῶν Μαθητῶν τὸν χορόν, διὰ νεφέλης κούφης εἰς ἓν συναγαγοῦσα. Χαίροις, ἡ ἐλπὶς καὶ προστασία ἡμῶν. Σὲ γὰρ Χριστιανῶν τὸ γένος, ἀπαύστως μακαρίζομεν."

meVE.Aposticha1.mode = miscellanea_gr_GR_cog.misc.Mode2
meVE.Aposticha1.greekmelody = he.a.m2_gr_GR_cog.heAU.OikosTouEfratha.greekname
meVE.Aposticha1.melody = he.a.m2_gr_GR_cog.heAU.OikosTouEfratha.name
meVE.Aposticha1.incipit = "Δῆμος τῶν Μαθητῶν…"
meVE.Aposticha1.text = "Δῆμος τῶν Μαθητῶν, ἤθροισται ἐκ περάτων, Γεθσημανῇ χωρίῳ, Μῆτερ Θεοῦ κηδεῦσαι, τὸ θεοδόχον σῶμά σου."

meVE.Aposticha2.mode = miscellanea_gr_GR_cog.misc.Mode2
meVE.Aposticha2.melody = he.a.m2_gr_GR_cog.heAU.OikosTouEfratha.name
meVE.Aposticha2.incipit = "Ὢ τῶν ὑπερφυῶν…"
meVE.Aposticha2.text = "Ὢ τῶν ὑπερφυῶν, Ἁγνή σου μυστηρίων! τοῦ Θεοῦ γὰρ ὡς Μήτηρ, πρὸς τοῦτον Θεοτόκε, λαμπρῶς νῦν ἐξεδήμησας."

meVE.Aposticha3.mode = miscellanea_gr_GR_cog.misc.Mode2
meVE.Aposticha3.melody = he.a.m2_gr_GR_cog.heAU.OikosTouEfratha.name
meVE.Aposticha3.incipit = "Κλῖμαξ πρὸς οὐρανόν…"
meVE.Aposticha3.text = "Κλῖμαξ πρὸς οὐρανόν, ὁ τάφος τῆς Παρθένου, καὶ Θεοτόκου πέλει, ἀνάγουσα τοὺς πίστει, αὐτὴν ἀεὶ δοξάζοντας."

meVE.AposGlory.mode = miscellanea_gr_GR_cog.misc.Mode2
meVE.AposGlory.melody = he.a.m2_gr_GR_cog.heAU.OikosTouEfratha.name
meVE.AposGlory.incipit = "Ὅτε πρὸς τὸν ἐκ σοῦ…"
meVE.AposGlory.text = "Ὅτε πρὸς τὸν ἐκ σοῦ, τεχθέντα μετετέθης, συνῆλθον ἐν νεφέλαις, τὸ σῶμά σου κηδεῦσαι, Παρθένε οἱ Ἀπόστολοι."

meVE.Apolytikion1.mode = miscellanea_gr_GR_cog.misc.Mode2
meVE.Apolytikion1.melody = ""
meVE.Apolytikion1.incipit = oc.m2.d4_gr_GR_cog.ocMA.Kathisma21.incipit
meVE.Apolytikion1.text = oc.m2.d4_gr_GR_cog.ocMA.Kathisma21.text

meMA.Kathisma11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Kathisma11.greekmelody = he.a.m4_gr_GR_cog.heAU.KateplagiIosif.greekname
meMA.Kathisma11.melody = he.a.m4_gr_GR_cog.heAU.KateplagiIosif.name
meMA.Kathisma11.incipit = "Καθορῶσαι ἀληθῶς…"
meMA.Kathisma11.text = "Καθορῶσαι ἀληθῶς, αἱ τῶν Ἀγγέλων στρατιαί, τὴν Μετάστασιν τὴν σήν, ἄχραντε Μῆτερ τοῦ Θεοῦ, εὐλογημένη πανύμνητε Παναγία, ἔχοντες βουλὴν τοῦ τεχθέντος ἐκ σοῦ, στῖφος Μαθητῶν συναθροίσασαι, ἐν εὐφροσύνῃ ἔφερον τὸ τίμιον, εἰς τὸν Παράδεισον σῶμά σου, Χριστὸν ὑμνοῦσαι, τὸν ζωοδότην, ὄντα εἰς τοὺς αἰῶνας."

meMA.Kathisma21.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Kathisma21.greekmelody = he.a.m4_gr_GR_cog.heAU.KateplagiIosif.greekname
meMA.Kathisma21.melody = he.a.m4_gr_GR_cog.heAU.KateplagiIosif.name
meMA.Kathisma21.incipit = "Ἐν χερσὶ τοῦ διʼ ἡμᾶς…"
meMA.Kathisma21.text = "Ἐν χερσὶ τοῦ διʼ ἡμᾶς, ἐνανθρωπήσαντος ἐκ σοῦ, παραθεμένην τὴν ψυχήν, ὡς πλαστουργός σου καὶ Θεός, πρὸς τὴν ἀκήρατον μετέστησεν εὐφροσύνην· ὅθεν σε σεπτῶς μακαρίζομεν, τὴν μόνην καθαρὰν καὶ ἀμόλυντον, καὶ Θεοτόκον ἅπαντες κυρίως, ὁμολογοῦντες κραυγάζομεν· Χριστὸν δυσώπει, πρὸς ὃν μετέστης, σῶσαι τὰς ψυχάς ἡμῶν."

