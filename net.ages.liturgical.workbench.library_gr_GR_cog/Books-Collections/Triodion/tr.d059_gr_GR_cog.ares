trMA.Ode2C11.ode = miscellanea_gr_GR_cog.misc.Ode2
trMA.Ode2C11.mode = miscellanea_gr_GR_cog.misc.Mode6
trMA.Ode2C11.greekmelody = he.h.m6_gr_GR_cog.heHE.IdeteIdete.greekname
trMA.Ode2C11.melody = he.h.m6_gr_GR_cog.heHE.IdeteIdete.name
trMA.Ode2C11.incipit = "Θέλων ἑπτώχευσας, ὁ φύσει πλούσιος…"
trMA.Ode2C11.text = "Θέλων ἑπτώχευσας, ὁ φύσει πλούσιος Χριστέ, ἐπείνασας θελήσει, ὁ ἐμπιπλῶν πᾶν ζῶον· διό με κόρεσον πεινῶντά σου τὴν χάριν, καὶ δεῖξον τῆς ἐκεῖ, τραπέζης Λόγε κοινωνόν."

trMA.Ode2C12.ode = miscellanea_gr_GR_cog.misc.Ode2
trMA.Ode2C12.mode = miscellanea_gr_GR_cog.misc.Mode6
trMA.Ode2C12.melody = he.h.m6_gr_GR_cog.heHE.IdeteIdete.name
trMA.Ode2C12.incipit = "Λάζαρον πτωχόν με, ἁμαρτιῶν δεῖξον…"
trMA.Ode2C12.text = "Λάζαρον πτωχόν με, ἁμαρτιῶν δεῖξον Χριστέ, καὶ σκόρπισον τὸν πλοῦτον, ὅν περ κακῶς συνῆξα, καὶ τὴν τελείαν σου ἀγάπησιν οἰκτίρμον, ἐμπλήσας τῆς ἐκεῖ, ῥῦσαι κολάσεως φρικτῆς."

trMA.Ode2C13.ode = miscellanea_gr_GR_cog.misc.Ode2
trMA.Ode2C13.mode = miscellanea_gr_GR_cog.misc.Mode6
trMA.Ode2C13.melody = he.h.m6_gr_GR_cog.heHE.IdeteIdete.name
trMA.Ode2C13.incipit = "Παῖδας στομώσασα, δυνατωτέρους τοῦ πυρός…"
trMA.Ode2C13.text = "Παῖδας στομώσασα, δυνατωτέρους τοῦ πυρός, εἰργάσατο νηστεία, ποτὲ ἐν Βαβυλῶνι, αὐτοὺς ζηλοῦσα ὦ ψυχή μου, μὴ ἀθύμει, καὶ πῦρ τῶν ἡδονῶν, πνεύματος σβέσεις δροσισμῷ."

trMA.Ode2C1T.ode = miscellanea_gr_GR_cog.misc.Ode2
trMA.Ode2C1T.mode = miscellanea_gr_GR_cog.misc.Mode6
trMA.Ode2C1T.melody = he.h.m6_gr_GR_cog.heHE.IdeteIdete.name
trMA.Ode2C1T.incipit = "Χαῖρέ σοι βοῶμεν, τῇ κυησάσῃ…"
trMA.Ode2C1T.text = "Χαῖρέ σοι βοῶμεν, τῇ κυησάσῃ τὴν χαράν, ἡ Κεχαριτωμένη, Θεοτόκε Παρθένε, Θεὸν ὃν ἔτεκες, δυσώπει λυτρωθῆναι, κινδύνων καὶ φθορᾶς, τοὺς ἀνυμνοῦντάς σε ἀεί."

trMA.Ode2C21.ode = miscellanea_gr_GR_cog.misc.Ode2
trMA.Ode2C21.mode = miscellanea_gr_GR_cog.misc.Mode5
trMA.Ode2C21.greekmelody = he.h.m5_gr_GR_cog.heHE.IdeteIdete.greekname
trMA.Ode2C21.melody = he.h.m5_gr_GR_cog.heHE.IdeteIdete.name
trMA.Ode2C21.incipit = "Ἴδετε ἴδετε, ὅτι ἐγώ εἰμι…"
trMA.Ode2C21.text = "Ἴδετε ἴδετε, ὅτι ἐγώ εἰμι Θεός, ὁ πέραν, Ἰορδάνου διατρίβων ἐπακούσας, ὅτι Λάζαρος νοσεῖ, καὶ φήσας ὅτι οὐ θνῄσκει, ἀλλʹ ἔστι τοῦτο ὑπὲρ τῆς δόξης μου."

trMA.Ode2C22.ode = miscellanea_gr_GR_cog.misc.Ode2
trMA.Ode2C22.mode = miscellanea_gr_GR_cog.misc.Mode5
trMA.Ode2C22.melody = he.h.m5_gr_GR_cog.heHE.IdeteIdete.name
trMA.Ode2C22.incipit = "Θρήνοις βαλλόμεναι, αἱ τοῦ Λαζάρου…"
trMA.Ode2C22.text = "Θρήνοις βαλλόμεναι, αἱ τοῦ Λαζάρου ἀδελφαί, τὸ πάθος, σοὶ τῷ Γνώστῃ τῶν ἁπάντων φανεροῦσιν, ἀλλὰ μένεις πρὸς μικρόν, ἵνα ἐπανάγῃς τὸ θαῦμα, καὶ ὑποδείξῃς τοῖς Μαθηταῖς σου φρικτά."

trMA.Ode2C2Tr.ode = miscellanea_gr_GR_cog.misc.Ode2
trMA.Ode2C2Tr.mode = miscellanea_gr_GR_cog.misc.Mode5
trMA.Ode2C2Tr.melody = he.h.m5_gr_GR_cog.heHE.IdeteIdete.name
trMA.Ode2C2Tr.incipit = "Ὁμοβασίλειε, ἡ τρισυπόστατος Μονάς, κυρία…"
trMA.Ode2C2Tr.text = "Ὁμοβασίλειε, ἡ τρισυπόστατος Μονάς, κυρία, βασιλεία τῶν αἰώνων, σὲ δοξάζει τὸν Πατέρα, καὶ Υἱόν, καὶ τὸ ἅγιον Πνεῦμα, πληθὺς Ἀγγέλων, καὶ πᾶσα φύσις βροτῶν."

trMA.Ode2C2T.ode = miscellanea_gr_GR_cog.misc.Ode2
trMA.Ode2C2T.mode = miscellanea_gr_GR_cog.misc.Mode5
trMA.Ode2C2T.melody = he.h.m5_gr_GR_cog.heHE.IdeteIdete.name
trMA.Ode2C2T.incipit = "Τίς μὴ θαυμάσει, βλέπων ἐν…"
trMA.Ode2C2T.text = "Τίς μὴ θαυμάσει, βλέπων ἐν σοὶ τὸν Πλαστουργόν, Παρθένε ἀναπλάσαντα Ἀδὰμ τὸν πεπτωκότα καὶ ἀφράστῳ ἑνώσει, ἐκ σοῦ σαρκὶ τεχθέντα, ἀναλλοιώτως εἰς σωτηρίαν ἡμῶν;"

trMA.Ode2C2A1.ode = miscellanea_gr_GR_cog.misc.Ode2
trMA.Ode2C2A1.mode = miscellanea_gr_GR_cog.misc.Mode5
trMA.Ode2C2A1.melody = he.h.m5_gr_GR_cog.heHE.IdeteIdete.name
trMA.Ode2C2A1.incipit = "Νῦν εὐτρεπίσθητι, ὦ Βηθανία ἐν…"
trMA.Ode2C2A1.text = "Νῦν εὐτρεπίσθητι, ὦ Βηθανία ἐν χαρᾷ ποιοῦσα, τὴν δοχὴν τοῦ Βασιλέως τῶν ἁπάντων, ὅτι ἥξει ἐπὶ σέ, ἵνα τὸν Λάζαρον δείξῃ, παλινδρομοῦντα ἐκ τῆς φθορᾶς πρός ζωήν."

trMA.Ode2C2H.ode = miscellanea_gr_GR_cog.misc.Ode2
trMA.Ode2C2H.mode = miscellanea_gr_GR_cog.misc.Mode5
trMA.Ode2C2H.greekmelody = he.h.m5_gr_GR_cog.heHE.IdeteIdete.greekname
trMA.Ode2C2H.melody = he.h.m5_gr_GR_cog.heHE.IdeteIdete.melody
trMA.Ode2C2H.incipit = he.h.m5_gr_GR_cog.heHE.IdeteIdete.incipit
trMA.Ode2C2H.text = he.h.m5_gr_GR_cog.heHE.IdeteIdete.text

trMA.Ode8C11.ode = miscellanea_gr_GR_cog.misc.Ode8
trMA.Ode8C11.mode = miscellanea_gr_GR_cog.misc.Mode6
trMA.Ode8C11.greekmelody = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.greekname
trMA.Ode8C11.melody = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.name
trMA.Ode8C11.incipit = "Ἐννοῶν μου τὰ πλήθη τῶν…"
trMA.Ode8C11.text = "Ἐννοῶν μου τὰ πλήθη τῶν παραπτώσεων, καὶ πληττόμενος κέντρῳ τῆς συνειδήσεως, ὥσπερ ἐν φλογί, ὀδυνῶμαι ὁ ἄθλιος. Οἴκτειρόν με Λόγε, Θεοῦ τῷ σῷ ἐλέει."

trMA.Ode8C12.ode = miscellanea_gr_GR_cog.misc.Ode8
trMA.Ode8C12.mode = miscellanea_gr_GR_cog.misc.Mode6
trMA.Ode8C12.melody = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.name
trMA.Ode8C12.incipit = "Παραβλέψας Λαζάρου τὰ κατορθώματα, τοῦ…"
trMA.Ode8C12.text = "Παραβλέψας Λαζάρου τὰ κατορθώματα, τοῦ ἀσπλάγχνου Πλουσίου τρόπους ἐζήλωσα, εὔσπλαγχνε Θεέ, ἐπιστρέψας με οἴκτειρον, ἵνα σε δοξάζω, εἰς πάντας τοὺς αἰῶνας."

trMA.Ode8C13.ode = miscellanea_gr_GR_cog.misc.Ode8
trMA.Ode8C13.mode = miscellanea_gr_GR_cog.misc.Mode6
trMA.Ode8C13.melody = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.name
trMA.Ode8C13.incipit = "Ψυχικῇ ἀσθενείᾳ περικρατούμενος, καὶ τεθνήξεσθαι…"
trMA.Ode8C13.text = "Ψυχικῇ ἀσθενείᾳ περικρατούμενος, καὶ τεθνήξεσθαι μέλλων διʹ ἀπογνώσεως, τῆς σῆς Ἰησοῦ, ἐπισκέψεως δέομαι, τῆς ζωοποιούσης, τοὺς σὲ προσκαλουμένους."

trMA.Ode8C1T.ode = miscellanea_gr_GR_cog.misc.Ode8
trMA.Ode8C1T.mode = miscellanea_gr_GR_cog.misc.Mode6
trMA.Ode8C1T.melody = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.name
trMA.Ode8C1T.incipit = "Παναγία Παρθένε σύ με διάσωσον…"
trMA.Ode8C1T.text = "Παναγία Παρθένε σύ με διάσωσον, τῆς ἐμῆς ἀσθενείας γενοῦ βοήθεια, ἡ τὸν θελητὴν τοῦ ἐλέους κυήσασα ὃν ὑπερυψοῦμεν, εἰς πάντας τοὺς αἰῶνας."

trMA.Ode8C21.ode = miscellanea_gr_GR_cog.misc.Ode8
trMA.Ode8C21.mode = miscellanea_gr_GR_cog.misc.Mode5
trMA.Ode8C21.greekmelody = he.h.m5_gr_GR_cog.heHE.OiOsioiSouPaides.greekname
trMA.Ode8C21.melody = he.h.m5_gr_GR_cog.heHE.OiOsioiSouPaides.name
trMA.Ode8C21.incipit = "Ἀγάλλου Βηθανία, ἡ τοῦ Λαζάρου…"
trMA.Ode8C21.text = "Ἀγάλλου Βηθανία, ἡ τοῦ Λαζάρου πατρίς· ἐπὶ σοὶ γὰρ Χριστὸς ἐπιστάς, μεγαλεῖον τελέσει, ζωώσας τὸν Λάζαρον."

trMA.Ode8C22.ode = miscellanea_gr_GR_cog.misc.Ode8
trMA.Ode8C22.mode = miscellanea_gr_GR_cog.misc.Mode5
trMA.Ode8C22.melody = he.h.m5_gr_GR_cog.heHE.OiOsioiSouPaides.name
trMA.Ode8C22.incipit = "Λάζαρος νοσηλεύεται, ἵνα σὺ δοξασθῇς…"
trMA.Ode8C22.text = "Λάζαρος νοσηλεύεται, ἵνα σὺ δοξασθῇς, ὁ Υἱὸς τοῦ Θεοῦ ἐν αὐτῷ, καὶ ὑμνεῖ σε ἀπαύστως, τὰ ἔργα τὸν Κύριον."

trMA.Ode8C2Tr.ode = miscellanea_gr_GR_cog.misc.Ode8
trMA.Ode8C2Tr.mode = miscellanea_gr_GR_cog.misc.Mode5
trMA.Ode8C2Tr.melody = he.h.m5_gr_GR_cog.heHE.OiOsioiSouPaides.name
trMA.Ode8C2Tr.incipit = "Τριάδα παναγίαν, τὴν ἐν Πατρί…"
trMA.Ode8C2Tr.text = "Τριάδα παναγίαν, τὴν ἐν Πατρί, καὶ Υἱῷ, καὶ Πνεύματι, ὑμνοῦντες ψάλλομεν· Εὐλογεῖτε, τὰ ἔργα τὸν Κύριον."

trMA.Ode8C2T.ode = miscellanea_gr_GR_cog.misc.Ode8
trMA.Ode8C2T.mode = miscellanea_gr_GR_cog.misc.Mode5
trMA.Ode8C2T.melody = he.h.m5_gr_GR_cog.heHE.OiOsioiSouPaides.name
trMA.Ode8C2T.incipit = "Τὸν ἄῤῥητόν σου τόκον, πάντες…"
trMA.Ode8C2T.text = "Τὸν ἄῤῥητόν σου τόκον, πάντες ὑμνοῦμεν Ἁγνή, καὶ σέβοντες αὐτὸν ὡς Θεόν, εὐλογοῦμεν ὑμνοῦμεν, ἀπαύστως τὸν Κύριον."

trMA.Ode8C2A1.ode = miscellanea_gr_GR_cog.misc.Ode8
trMA.Ode8C2A1.mode = miscellanea_gr_GR_cog.misc.Mode5
trMA.Ode8C2A1.melody = he.h.m5_gr_GR_cog.heHE.OiOsioiSouPaides.name
trMA.Ode8C2A1.incipit = "Τῶν ἀρετῶν τοὺς κλάδους, πρὸς…"
trMA.Ode8C2A1.text = "Τῶν ἀρετῶν τοὺς κλάδους, πρὸς ὑπαντὴν τοῦ Χριστοῦ, εὐτρεπίζοντες νῦν κράζομεν· Εὐλογεῖτε τὰ ἔργα, Κυρίου τὸν Κύριον."

