A_Resource_Whose_Name = menaion.verses_gr_GR_cog 

psLI.m01.d01.antiphon1.v1.info = ps_gr_GR_cog.psa65.v1.number //"65:1"
psLI.m01.d01.antiphon1.v1.text = "Ἀλαλάξατε τῷ Κυρίῳ πᾶσα ἡ γῆ."
psLI.m01.d01.antiphon1.v2.info = ps_gr_GR_cog.psa65.v2.number //"65:2"
psLI.m01.d01.antiphon1.v2.text = "Ψάλατε δὴ τῷ ὀνόματι αὐτοῦ."
psLI.m01.d01.antiphon1.v3.info = ps_gr_GR_cog.psa104.v2.number //"104:2"
psLI.m01.d01.antiphon1.v3.text = "Διηγήσασθε πάντα τὰ θαυμάσια αὐτοῦ."
psLI.m01.d01.antiphon1.v4.info = ps_gr_GR_cog.psa65.v3.number //"65:3"
psLI.m01.d01.antiphon1.v4.text = "Εἴπατε τῷ Θεῷ· ὡς φοβερὰ τὰ ἔργα σου."
psLI.m01.d01.antiphon2.v1.info = ps_gr_GR_cog.psa95.v11.number //"95:11"
psLI.m01.d01.antiphon2.v1.text = "Εὐφραινέσθωσαν οἱ οὐρανοὶ καὶ ἀγαλλιάσθω ἡ γῆ."
psLI.m01.d01.antiphon2.v2.info = ps_gr_GR_cog.psa95.v1.number //"95:1"
psLI.m01.d01.antiphon2.v2.text = "Ἄσατε τῷ Κυρίῳ ᾆσμα καινόν, ᾄσατε τῷ Κυρίῳ πᾶσα ἡ γῆ."
psLI.m01.d01.antiphon2.v3.info = ps_gr_GR_cog.psa49.v2.number //"49:2"
psLI.m01.d01.antiphon2.v3.text = "Ἐκ Σιὼν ἡ εὐπρέπεια τῆς ὡραιότητος αὐτοῦ."
psLI.m01.d01.antiphon2.v4.info = ps_gr_GR_cog.psa113.v11.number //"113:11"
psLI.m01.d01.antiphon2.v4.text = "Ὁ Θεὸς ἡμῶν ἐν τῷ οὐρανῷ καὶ ἐν τῇ γῇ, πάντα, ὅσα ἠθέλησεν, ἐποίησε."
psLI.m01.d01.antiphon3.v1.info = ps_gr_GR_cog.psa88.v2.number //"88:2"
psLI.m01.d01.antiphon3.v1.text = "Τὰ ἐλέη σου, Κύριε, εἰς τὸν αἰῶνα ᾄσομαι."
psLI.m01.d01.antiphon3.v2.info = ps_gr_GR_cog.psa88.v2.number //"88:2"
psLI.m01.d01.antiphon3.v2.text = "Ἀπαγγελῶ τὴν ἀλήθειάν σου ἐν τῷ στόματί μου."
psLI.m01.d01.antiphon3.v3.info = ps_gr_GR_cog.psa44.v8.number //"44:8"
psLI.m01.d01.antiphon3.v3.text = "Ἠγάπησας δικαιοσύνην καὶ ἐμίσησας ἀνομίαν."
psLI.m01.d01.prokeimenon.v1.info = ps_gr_GR_cog.psa48.v4.number //"48:4"
psLI.m01.d01.prokeimenon.v1.text = "Τὸ στόμα μου λαλήσει σοφίαν καὶ ἡ μελέτη τῆς καρδίας μου σύνεσιν."
psLI.m01.d01.prokeimenon.v2.info = ps_gr_GR_cog.psa48.v2.number //"48:2"
psLI.m01.d01.prokeimenon.v2.text = "Ἀκούσατε ταῦτα, πάντα τὰ ἔθνη, ἐνωτίσασθε πάντες οἱ κατοικοῦντες τὴν οἰκουμένην."
psLI.m01.d01.alleluia.v1.info = ps_gr_GR_cog.psa79.v2.number //"79:2"
psLI.m01.d01.alleluia.v1.text = "Ὁ ποιμαίνων τὸν Ἰσραήλ, πρόσχες, ὁ ὁδηγῶν ὡσεὶ πρόβατα τὸν Ἰωσήφ."
psLI.m01.d01.alleluia.v2.info = ps_gr_GR_cog.psa36.v30.number //"36:30"
psLI.m01.d01.alleluia.v2.text = "Στόμα δικαίου μελετήσει σοφίαν, καὶ ἡ γλῶσσα αὐτοῦ λαλήσει κρίσιν."

psLI.m01.d02.prokeimenon.v1.info = ps_gr_GR_cog.psa115.v5.number 
psLI.m01.d02.prokeimenon.v1.text = "Τίμιος ἐναντίον Κυρίου ὁ θάνατος τοῦ ὁσίου αὐτοῦ."
psLI.m01.d02.prokeimenon.v2.info = ps_gr_GR_cog.psa115.v3.number 
psLI.m01.d02.prokeimenon.v2.text = "Τί ἀνταποδώσω τῷ Κυρίῳ περὶ πάντων;"

psVL.m01.d06.prokeimenon.v1.info = ps_gr_GR_cog.psa26.v1.number
psVL.m01.d06.prokeimenon.v1.text = "Κύριος φωτισμός μου καὶ σωτήρ μου."
psVL.m01.d06.prokeimenon.v2.info = ps_gr_GR_cog.psa26.v1.number
psVL.m01.d06.prokeimenon.v2.text = "Κύριος ὑπερασπιστὴς τῆς ζωῆς μου."
psVL.m01.d06.alleluia.v1.info = ps_gr_GR_cog.psa44.v2.number
psVL.m01.d06.alleluia.v1.text = "Ἐξηρεύξατο ἡ καρδία μου λόγον ἀγαθόν."
psVL.m01.d06.alleluia.v2.info = ps_gr_GR_cog.psa44.v3.number
psVL.m01.d06.alleluia.v2.text = "Ὡραῖος κάλλει παρὰ τοὺς υἱοὺς τῶν ἀνθρώπων."
psGW.m01.d06.prokeimenon.v1.info = ps_gr_GR_cog.psa26.v1.number
psGW.m01.d06.prokeimenon.v1.text = "Κύριος φωτισμός μου καὶ σωτήρ μου."
psGW.m01.d06.prokeimenon.v2.info = ps_gr_GR_cog.psa26.v1.number
psGW.m01.d06.prokeimenon.v2.text = "Κύριος ὑπερασπιστὴς τῆς ζωῆς μου."
psGW.m01.d06.alleluia.v1.info = ps_gr_GR_cog.psa44.v2.number
psGW.m01.d06.alleluia.v1.text = "Ἐξηρεύξατο ἡ καρδία μου λόγον ἀγαθόν"
psGW.m01.d06.alleluia.v2.info = ps_gr_GR_cog.psa44.v3.number
psGW.m01.d06.alleluia.v2.text = "Ὡραῖος κάλλει παρὰ τοὺς υἱοὺς τῶν ἀνθρώπων."
psLI.m01.d06.antiphon1.v1.info = ps_gr_GR_cog.psa113.v1.number //"113:1"
psLI.m01.d06.antiphon1.v1.text = "Ἐν ἐξόδῳ Ἰσραὴλ ἐξ Αἰγύπτου, οἴκου Ἰακώβ."
psLI.m01.d06.antiphon1.v2.info = ps_gr_GR_cog.psa113.v2.number //"113:2"
psLI.m01.d06.antiphon1.v2.text = "Ἐγενήθη Ἰουδαία ἁγίασμα αὐτοῦ."
psLI.m01.d06.antiphon1.v3.info = ps_gr_GR_cog.psa113.v3.number //"113:3"
psLI.m01.d06.antiphon1.v3.text = "Ἡ θάλασσα εἶδε καὶ ἔφυγεν· ὁ Ἰορδάνης ἐστράφη εἰς τὰ ὀπίσω."
psLI.m01.d06.antiphon1.v4.info = ps_gr_GR_cog.psa113.v5.number //"113:5"
psLI.m01.d06.antiphon1.v4.text = "Τί σοί ἐστι, θάλασσα, ὅτι ἔφυγες, καὶ σύ, Ἰορδάνη, ὅτι ἐστράφης εἰς τὰ ὀπίσω;"
psLI.m01.d06.antiphon2.v1.info = ps_gr_GR_cog.psa114.v1.number //"114:1"
psLI.m01.d06.antiphon2.v1.text = "Ἠγάπησα, ὅτι εἰσακούσεται Κύριος τῆς φωνῆς τῆς δεήσεώς μου."
psLI.m01.d06.antiphon2.v2.info = ps_gr_GR_cog.psa114.v2.number //"114:2"
psLI.m01.d06.antiphon2.v2.text = "Ὅτι ἔκλινε τὸ οὖς αὐτοῦ ἐμοί, καὶ ἐν ταῖς ἡμέραις μου ἐπικαλέσομαι."
psLI.m01.d06.antiphon2.v3.info = ps_gr_GR_cog.psa114.v3.number //"114:3"
psLI.m01.d06.antiphon2.v3.text = "Περιέσχον με ὠδῖνες θανάτου, κίνδυνοι ᾅδου εὕροσάν με."
psLI.m01.d06.antiphon2.v4.info = ps_gr_GR_cog.psa114.v5.number //"114:5"
psLI.m01.d06.antiphon2.v4.text = "Ἐλεήμων ὁ Κύριος καὶ δίκαιος, καὶ ὁ Θεὸς ἡμῶν ἐλεεῖ."
psLI.m01.d06.antiphon3.v1.info = ps_gr_GR_cog.psa117.v1.number //"117:1"
psLI.m01.d06.antiphon3.v1.text = "Ἐξομολογεῖσθε τῷ Κυρίῳ, ὅτι ἀγαθός, ὅτι εἰς τὸν αἰῶνα τὸ ἔλεος αὐτοῦ."
psLI.m01.d06.antiphon3.v2.info = ps_gr_GR_cog.psa117.v2.number //"117:2"
psLI.m01.d06.antiphon3.v2.text = "Εἰπάτω δὴ οἶκος Ἰσραήλ, ὅτι ἀγαθός, ὅτι εἰς τὸν αἰῶνα τὸ ἔλεος αὐτοῦ."
psLI.m01.d06.antiphon3.v3.info = ps_gr_GR_cog.psa117.v3.number //"117:3"
psLI.m01.d06.antiphon3.v3.text = "Εἰπάτω δὴ οἶκος Ἀαρών, ὅτι ἀγαθός, ὅτι εἰς τὸν αἰῶνα τὸ ἔλεος αὐτοῦ."
psLI.m01.d06.antiphon3.v4.info = ps_gr_GR_cog.psa117.v4.number //"117:4"
psLI.m01.d06.antiphon3.v4.text = "Εἰπάτωσαν δὴ πάντες οἱ φοβούμενοι τὸν Κύριον, ὅτι ἀγαθός, ὅτι εἰς τὸν αἰῶνα τὸ ἔλεος αὐτοῦ."
psLI.m01.d06.entrance.v1.info = ps_gr_GR_cog.psa117.v26.number //"117:26-27"
psLI.m01.d06.entrance.v1.text = "Εὐλογημένος ὁ ἐρχόμενος ἐν ὀνόματι Κυρίου. Θεὸς Κύριος καὶ ἐπέφανεν ἡμῖν."
psLI.m01.d06.prokeimenon.v1.info = ps_gr_GR_cog.psa117.v26.number //"117:26"
psLI.m01.d06.prokeimenon.v1.text = "Εὐλογημένος ὁ ἐρχόμενος ἐν ὀνόματι Κυρίου."
psLI.m01.d06.prokeimenon.v2.info = ps_gr_GR_cog.psa117.v1.number //"117:1"
psLI.m01.d06.prokeimenon.v2.text = "Ἐξομολογεῖσθε τῷ Κυρίῳ, ὅτι ἀγαθός, ὅτι εἰς τὸν αἰῶνα τὸ ἔλεος αὐτοῦ."
psLI.m01.d06.alleluia.v1.info = ps_gr_GR_cog.psa28.v2.number //"28:2"
psLI.m01.d06.alleluia.v1.text = "Ἐνέγκατε τῷ Κυρίῳ δόξαν ὀνόματι αὐτοῦ, προσκυνήσατε τῷ Κυρίῳ ἐν αὐλῇ ἁγίᾳ αὐτοῦ."
psLI.m01.d06.alleluia.v2.info = ps_gr_GR_cog.psa28.v4.number //"28:4"
psLI.m01.d06.alleluia.v2.text = "Φωνὴ Κυρίου ἐν ἰσχύϊ, φωνὴ Κυρίου ἐν μεγαλοπρεπείᾳ."
psLI.m01.d06.communion.v1.info = le.ep.os_gr_GR_cog.leosLI.Jan06_CommunionHymn.chapverse //"Τίτ. 2:11"
psLI.m01.d06.communion.v1.text = "Ἐπεφάνη ἡ χάρις τοῦ Θεοῦ, ἡ σωτήριος πᾶσιν ἀνθρώποις."

psLI.m01.d07.prokeimenon.v1.info = ps_gr_GR_cog.psa63.v11.number
psLI.m01.d07.prokeimenon.v1.text = "Εὐφρανθήσεται δίκαιος ἐν Κυρίῳ."
psLI.m01.d07.prokeimenon.v2.info = ps_gr_GR_cog.psa63.v2.number
psLI.m01.d07.prokeimenon.v2.text = "Εἰσάκουσον, ὁ Θεός, τῆς φωνῆς μου."
psLI.m01.d07.alleluia.v1.info = ps_gr_GR_cog.psa91.v13.number
psLI.m01.d07.alleluia.v1.text = "Δίκαιος ὡς φοῖνιξ ἀνθήσει."
psLI.m01.d07.alleluia.v2.info = ps_gr_GR_cog.psa91.v14.number
psLI.m01.d07.alleluia.v2.text = "Πεφυτευμένοι ἐν τῷ οἴκῳ Κυρίου."

psLI.m01.d09.prokeimenon.v1.info = ps_gr_GR_cog.psa67.v36.number
psLI.m01.d09.prokeimenon.v1.text = "Θαυμαστὸς ὁ Θεὸς ἐν τοῖς ἁγίοις αὐτοῦ."
psLI.m01.d09.prokeimenon.v2.info = ps_gr_GR_cog.psa67.v27.number
psLI.m01.d09.prokeimenon.v2.text = "Ἐν ἐκκλησίαις εὐλογεῖτε τὸν Θεόν."
psLI.m01.d09.alleluia.v1.info = ps_gr_GR_cog.psa43.v2.number
psLI.m01.d09.alleluia.v1.text = "Ὁ Θεός, ἐν τοῖς ὠσὶν ἡμῶν ἠκούσαμεν."
psLI.m01.d09.alleluia.v2.info = ps_gr_GR_cog.psa33.v18.number
psLI.m01.d09.alleluia.v2.text = "Ἐκέκραξαν οἱ δίκαιοι, καὶ ὁ Κύριος εἰσήκουσεν αὐτῶν."

psLI.m01.d11.prokeimenon.v1.info = ps_gr_GR_cog.psa115.v5.number
psLI.m01.d11.prokeimenon.v1.text = "Τίμιος ἐναντίον Κυρίου ὁ θάνατος τοῦ ὁσίου αὐτοῦ."
psLI.m01.d11.prokeimenon.v2.info = ps_gr_GR_cog.psa115.v3.number
psLI.m01.d11.prokeimenon.v2.text = "Τί ἀνταποδώσωμεν τῷ Κυρίῳ περὶ πάντων;"
psLI.m01.d11.alleluia.v1.info = ps_gr_GR_cog.psa111.v1.number
psLI.m01.d11.alleluia.v1.text = "Μακάριος ἀνὴρ ὁ φοβούμενος τὸν Κύριον."
psLI.m01.d11.alleluia.v2.info = ps_gr_GR_cog.psa111.v9.number
psLI.m01.d11.alleluia.v2.text = "Ἡ δικαιοσύνη αὐτοῦ μένει εἰς τὸν αἰῶνα."

psLI.m01.dSatAE.prokeimenon.v1.info = ps_gr_GR_cog.psa31.v11.number
psLI.m01.dSatAE.prokeimenon.v1.text = "Εὐφράνθητε ἐπὶ Κύριον καὶ ἀγαλλιᾶσθε, δίκαιοι."
psLI.m01.dSatAE.prokeimenon.v2.info = ps_gr_GR_cog.psa31.v1.number
psLI.m01.dSatAE.prokeimenon.v2.text = "Μακάριοι ὧν ἀφέθησαν αἱ ἀνομίαι."
psLI.m01.dSatAE.alleluia.v1.info = ps_gr_GR_cog.psa64.v5.number
psLI.m01.dSatAE.alleluia.v1.text = "Μακάριοι οὓς ἐξελέξω καὶ προσελάβου, Κύριε."
psLI.m01.dSatAE.alleluia.v2.info = ps_gr_GR_cog.psa101.v13.number
psLI.m01.dSatAE.alleluia.v2.text = "Τὸ μνημόσυνον αὐτῶν εἰς γενεὰν καὶ γενεάν."

psLI.m01.dSunAE.prokeimenon.v1.info = ps_gr_GR_cog.psa32.v22.number
psLI.m01.dSunAE.prokeimenon.v1.text = "Γένοιτο, Κύριε, τὸ ἔλεός σου ἐφ' ἡμᾶς."
psLI.m01.dSunAE.prokeimenon.v2.info = ps_gr_GR_cog.psa32.v1.number
psLI.m01.dSunAE.prokeimenon.v2.text = "Ἀγαλλιᾶσθε, δίκαιοι, ἐν Κυρίῳ."
psLI.m01.dSunAE.alleluia.v1.info = ps_gr_GR_cog.psa88.v2.number
psLI.m01.dSunAE.alleluia.v1.text = "Τὰ ἐλέη σου, Κύριε, εἰς τὸν αἰῶνα ᾄσομαι."
psLI.m01.dSunAE.alleluia.v2.info = ps_gr_GR_cog.psa88.v3.number
psLI.m01.dSunAE.alleluia.v2.text = "Ὅτι εἶπας· Εἰς τὸν αἰῶνα ἔλεος οἰκοδομηθήσεται."

psLI.m01.d16.prokeimenon.v1.info = ps_gr_GR_cog.psa18.v5.number
psLI.m01.d16.prokeimenon.v1.text = "Εἰς πᾶσαν τὴν γῆν ἐξῆλθεν ὁ φθόγγος αὐτοῦ."
psLI.m01.d16.prokeimenon.v2.info = ps_gr_GR_cog.psa18.v2.number
psLI.m01.d16.prokeimenon.v2.text = "Οἱ οὐρανοὶ διηγοῦνται δόξαν Θεοῦ."

psLI.m01.d17.prokeimenon.v1.info = ps_gr_GR_cog.psa115.v5.number
psLI.m01.d17.prokeimenon.v1.text = "Τίμιος ἐναντίον Κυρίου ὁ θάνατος τοῦ ὁσίου αὐτοῦ."
psLI.m01.d17.prokeimenon.v2.info = ps_gr_GR_cog.psa115.v3.number
psLI.m01.d17.prokeimenon.v2.text = "Τί ἀνταποδώσωμεν τῷ Κυρίῳ περὶ πάντων;"
psLI.m01.d17.alleluia.v1.info = ps_gr_GR_cog.psa111.v1.number
psLI.m01.d17.alleluia.v1.text = "Μακάριος ἀνὴρ ὁ φοβούμενος τὸν Κύριον."
psLI.m01.d17.alleluia.v2.info = ps_gr_GR_cog.psa111.v9.number
psLI.m01.d17.alleluia.v2.text = "Ἡ δικαιοσύνη αὐτοῦ μένει εἰς τὸν αἰῶνα."

psLI.m01.d18.prokeimenon.v1.info = le.pr.os_gr_GR_cog.leosLI.pe.d113.EpistleProkeimenon.book //"(Dan 3:52a, 27)"
psLI.m01.d18.prokeimenon.v1.text = "Εὐλογητὸς εἶ Κύριε ὁ Θεὸς τῶν πατέρων ἡμῶν."
psLI.m01.d18.prokeimenon.v2.info = le.pr.os_gr_GR_cog.leosLI.pe.d113.EpistleProkeimenon.book //"(Dan 3:52a, 27)"
psLI.m01.d18.prokeimenon.v2.text = "Ὅτι δίκαιος εἶ ἐπὶ πᾶσιν, οἷς ἐποίησας ἡμῖν."
psLI.m01.d18.alleluia.v1.info = ps_gr_GR_cog.psa43.v2.number
psLI.m01.d18.alleluia.v1.text = "Ὁ Θεός, ἐν τοῖς ὠσὶν ἡμῶν ἠκούσαμεν."
psLI.m01.d18.alleluia.v2.info = ps_gr_GR_cog.psa33.v18.number
psLI.m01.d18.alleluia.v2.text = "Ἐκέκραξαν οἱ δίκαιοι, καὶ ὁ Κύριος εἰσήκουσεν αὐτῶν."

psLI.m01.d20.prokeimenon.v1.info = ps_gr_GR_cog.psa149.v5.number
psLI.m01.d20.prokeimenon.v1.text = "Καυχήσονται ὅσιοι ἐν δόξῃ."
psLI.m01.d20.prokeimenon.v2.info = ps_gr_GR_cog.psa149.v1.number
psLI.m01.d20.prokeimenon.v2.text = "Ἄσατε τῷ Κυρίῳ ᾆσμα καινόν."
psLI.m01.d20.alleluia.v1.info = ps_gr_GR_cog.psa111.v1.number
psLI.m01.d20.alleluia.v1.text = "Μακάριος ἀνὴρ ὁ φοβούμενος τὸν Κύριον."
psLI.m01.d20.alleluia.v2.info = ps_gr_GR_cog.psa111.v9.number
psLI.m01.d20.alleluia.v2.text = "Ἡ δικαιοσύνη αὐτοῦ μένει εἰς τὸν αἰῶνα."

psLI.m01.d21.prokeimenon.v1.info = ps_gr_GR_cog.psa115.v5.number
psLI.m01.d21.prokeimenon.v1.text = "Τίμιος ἐναντίον Κυρίου ὁ θάνατος τοῦ ὁσίου αὐτοῦ."
psLI.m01.d21.prokeimenon.v2.info = ps_gr_GR_cog.psa115.v3.number
psLI.m01.d21.prokeimenon.v2.text = "Τί ἀνταποδώσωμεν τῷ Κυρίῳ περὶ πάντων;"

psLI.m01.d22.prokeimenon.v1.info = ps_gr_GR_cog.psa18.v5.number
psLI.m01.d22.prokeimenon.v1.text = "Εἰς πᾶσαν τὴν γῆν ἐξῆλθεν ὁ φθόγγος αὐτοῦ."
psLI.m01.d22.prokeimenon.v2.info = ps_gr_GR_cog.psa18.v2.number
psLI.m01.d22.prokeimenon.v2.text = "Οἱ οὐρανοὶ διηγοῦνται δόξαν Θεοῦ."
psLI.m01.d22.alleluia.v1.info = ps_gr_GR_cog.psa88.v6.number
psLI.m01.d22.alleluia.v1.text = "Ἐξομολογήσονται οἱ οὐρανοὶ τὰ θαυμάσιά σου, Κύριε."
psLI.m01.d22.alleluia.v2.info = ps_gr_GR_cog.psa88.v8.number
psLI.m01.d22.alleluia.v2.text = "Ὁ Θεὸς ἐνδοξαζόμενος ἐν βουλῇ ἁγίων."

psLI.m01.d25.prokeimenon.v1.info = ps_gr_GR_cog.psa48.v4.number
psLI.m01.d25.prokeimenon.v1.text = "Τὸ στόμα μου λαλήσει σοφίαν."
psLI.m01.d25.prokeimenon.v2.info = ps_gr_GR_cog.psa48.v2.number
psLI.m01.d25.prokeimenon.v2.text = "Ἀκούσατε ταῦτα, πάντα τὰ ἔθνη."
psLI.m01.d25.alleluia.v1.info = ps_gr_GR_cog.psa77.v1.number
psLI.m01.d25.alleluia.v1.text = "Προσέχετε, λαός μου, τῷ νόμῳ μου."
psLI.m01.d25.alleluia.v2.info = ps_gr_GR_cog.psa77.v2.number
psLI.m01.d25.alleluia.v2.text = "Ἀνοίξω ἐν παραβολαῖς τὸ στόμα μου."

psLI.m01.d27.prokeimenon.v1.info = ps_gr_GR_cog.psa48.v4.number
psLI.m01.d27.prokeimenon.v1.text = "Τὸ στόμα μου λαλήσει σοφίαν."
psLI.m01.d27.prokeimenon.v2.info = ps_gr_GR_cog.psa48.v2.number
psLI.m01.d27.prokeimenon.v2.text = "Ἀκούσατε ταῦτα, πάντα τὰ ἔθνη."
psLI.m01.d27.alleluia.v1.info = ps_gr_GR_cog.psa36.v30.number
psLI.m01.d27.alleluia.v1.text = "Στόμα δικαίου μελετήσει σοφίαν."
psLI.m01.d27.alleluia.v2.info = ps_gr_GR_cog.psa36.v31.number
psLI.m01.d27.alleluia.v2.text = "Ὁ νόμος τοῦ Θεοῦ αὐτοῦ ἐν καρδίᾳ αὐτοῦ."

psLI.m01.d30.prokeimenon.v1.info = ps_gr_GR_cog.psa18.v5.number
psLI.m01.d30.prokeimenon.v1.text = "Εἰς πᾶσαν τὴν γῆν ἐξῆλθεν ὁ φθόγγος αὐτοῦ."
psLI.m01.d30.prokeimenon.v2.info = ps_gr_GR_cog.psa18.v2.number
psLI.m01.d30.prokeimenon.v2.text = "Οἱ οὐρανοὶ διηγοῦνται δόξαν Θεοῦ."
psLI.m01.d30.alleluia.v1.info = ps_gr_GR_cog.psa88.v6.number
psLI.m01.d30.alleluia.v1.text = "Ἐξομολογήσονται οἱ οὐρανοὶ τὰ θαυμάσιά σου, Κύριε."
psLI.m01.d30.alleluia.v2.info = ps_gr_GR_cog.psa88.v8.number
psLI.m01.d30.alleluia.v2.text = "Ὁ Θεὸς ἐνδοξαζόμενος ἐν βουλῇ ἁγίων."

psLI.m01.d31.prokeimenon.v1.info = ps_gr_GR_cog.psa15.v3.number
psLI.m01.d31.prokeimenon.v1.text = "Τοῖς ἁγίοις τοῖς ἐν τῇ γῇ αὐτοῦ ἐθαυμάστωσεν ὁ Κύριος."
psLI.m01.d31.prokeimenon.v2.info = ps_gr_GR_cog.psa15.v8.number
psLI.m01.d31.prokeimenon.v2.text = "Προωρώμην τὸν Κύριον ἐνώπιόν μου διαπαντός."
psLI.m01.d31.alleluia.v1.info = ps_gr_GR_cog.psa132.v1.number
psLI.m01.d31.alleluia.v1.text = "Ἰδοὺ δὴ τί καλὸν ἢ τί τερπνόν, ἀλλ' ἢ τὸ κατοικεῖν ἀδελφοὺς ἐπὶ τὸ αὐτό;"
psLI.m01.d31.alleluia.v2.info = ps_gr_GR_cog.psa132.v3.number
psLI.m01.d31.alleluia.v2.text = "Ὅτι ἐκεῖ ἐνετείλατο Κύριος τὴν εὐλογίαν, ζωὴν ἕως τοῦ αἰῶνος."


//February

psLI.m02.d01.prokeimenon.v1.info = ps_gr_GR_cog.psa15.v3.number
psLI.m02.d01.prokeimenon.v1.text = "Τοῖς ἁγίοις τοῖς ἐν τῇ γῇ αὐτοῦ ἐθαυμάστωσεν ὁ Κύριος."
psLI.m02.d01.prokeimenon.v2.info = ps_gr_GR_cog.psa67.v27.number
psLI.m02.d01.prokeimenon.v2.text = "Ἐν ἐκκλησίαις εὐλογεῖτε τὸν Θεόν."
psLI.m02.d01.alleluia.v1.info = ps_gr_GR_cog.psa91.v13.number
psLI.m02.d01.alleluia.v1.text = "Δίκαιος ὡς φοῖνιξ ἀνθήσει."
psLI.m02.d01.alleluia.v2.info = ps_gr_GR_cog.psa91.v14.number
psLI.m02.d01.alleluia.v2.text = "Πεφυτευμένοι ἐν τῷ οἴκῳ Κυρίου."

psLI.m02.d28.antiphon1.v1.info = ps_gr_GR_cog.psa44.v2.number //"44:2"
psLI.m02.d02.antiphon1.v1.text = "Ἐξηρεύξατο ἡ καρδία μου λόγον ἀγαθόν· λέγω ἐγὼ τὰ ἔργα μου τῷ βασιλεῖ."
psLI.m02.d02.antiphon1.v2.info = ps_gr_GR_cog.psa44.v2.number //"44:2"
psLI.m02.d02.antiphon1.v2.text = "Ἡ γλῶσσά μου κάλαμος γραμματέως ὀξυγράφου."
psLI.m02.d02.antiphon1.v3.info = ps_gr_GR_cog.psa44.v3.number //"44:3"
psLI.m02.d02.antiphon1.v3.text = "Ὡραῖος κάλλει παρὰ τοὺς υἱοὺς τῶν ἀνθρώπων, ἐξεχύθη χάρις ἐν χείλεσί σου"
psLI.m02.d02.antiphon1.v4.info = ps_gr_GR_cog.psa44.v3.number //"44:3"
psLI.m02.d02.antiphon1.v4.text = "Διὰ τοῦτο εὐλόγησέ σε ὁ Θεὸς εἰς τὸν αἰῶνα."
psLI.m02.d02.antiphon2.v1.info = ps_gr_GR_cog.psa44.v4.number //"44:4"
psLI.m02.d02.antiphon2.v1.text = "Περίζωσαι τὴν ρομφαίαν σου ἐπὶ τὸν μηρόν σου, δυνατέ, τῇ ὡραιότητί σου καὶ τῷ κάλλει σου."
psLI.m02.d02.antiphon2.v2.info = ps_gr_GR_cog.psa44.v5.number //"44:5"
psLI.m02.d02.antiphon2.v2.text = "Καὶ ἔντεινον καὶ κατευοδοῦ καὶ βασίλευε ἕνεκεν ἀληθείας καὶ πρᾳότητος καὶ δικαιοσύνης."
psLI.m02.d02.antiphon2.v3.info = ps_gr_GR_cog.psa44.v6.number //"44:6"
psLI.m02.d02.antiphon2.v3.text = "Τὰ βέλη σου ἠκονημένα, δυνατέ, λαοὶ ὑποκάτω σου πεσοῦνται."
psLI.m02.d02.antiphon2.v4.info = ps_gr_GR_cog.psa44.v7.number //"44:7"
psLI.m02.d02.antiphon2.v4.text = "Ὁ θρόνος σου, ὁ Θεός, εἰς τὸν αἰῶνα τοῦ αἰῶνος, ράβδος εὐθύτητος ἡ ῥάβδος τῆς βασιλείας σου."
psLI.m02.d02.antiphon3.v1.info = ps_gr_GR_cog.psa44.v11.number //"44:11"
psLI.m02.d02.antiphon3.v1.text = "Ἄκουσον, θύγατερ, καὶ ἴδε καὶ κλῖνον τὸ οὖς σου καὶ ἐπιλάθου τοῦ λαοῦ σου καὶ τοῦ οἴκου τοῦ πατρός σου."
psLI.m02.d02.antiphon3.v2.info = ps_gr_GR_cog.psa44.v13.number //"44:13"
psLI.m02.d02.antiphon3.v2.text = "Τὸ πρόσωπόν σου λιτανεύσουσιν οἱ πλούσιοι τοῦ λαοῦ."
psLI.m02.d02.antiphon3.v3.info = ps_gr_GR_cog.psa44.v18.number //"44:18"
psLI.m02.d02.antiphon3.v3.text = "Μνησθήσονται τοῦ ὀνόματός σου ἐν πάσῃ γενεᾷ καὶ γενεᾷ."
psLI.m02.d02.entrance.v1.info = ps_gr_GR_cog.psa97.v2.number //"97:2"
psLI.m02.d02.entrance.v1.text = "Ἐγνώρισε Κύριος τὸ σωτήριον αὐτοῦ ἐναντίον πάντων τῶν ἐθνῶν."
psLI.m02.d02.prokeimenon.v1.info = ps_gr_GR_cog.bode9_mag.v1.number //"Ὠδὴ 9a:1"
psLI.m02.d02.prokeimenon.v1.text = "Μεγαλύνει ἡ ψυχή μου τὸν Κύριον, καὶ ἠγαλλίασε τὸ πνεῦμά μου ἐπὶ τῷ Θεῷ τῷ σωτῆρί μου."
psLI.m02.d02.prokeimenon.v2.info = ps_gr_GR_cog.bode9_mag.v1.number //"Ὠδὴ 9a:2"
psLI.m02.d02.prokeimenon.v2.text = "Ὅτι ἐπέβλεψεν ἐπὶ τὴν ταπείνωσιν τῆς δούλης αὐτοῦ· ἰδού γάρ, ἀπὸ τοῦ νῦν μακαριοῦσί με πᾶσαι αἱ γενεαί."
psLI.m02.d02.alleluia.v1.info = le.go.os_gr_GR_cog.leosVE.Prayer_NynApolyeis.chapverse //"Λουκᾶ 2:29-31"
psLI.m02.d02.alleluia.v1.text = "Νῦν ἀπολύεις τον δοῦλόν σου, Δέσποτα, κατὰ τὸ ῥῆμά σου, ἐν εἰρήνῃ· ὅτι εἶδον οἱ ὀφθαλμοί μου τὸ σωτήριόν σου, ὃ ἡτοίμασας κατὰ πρόσωπον πάντων τῶν λαῶν."
psLI.m02.d02.alleluia.v2.info = le.go.os_gr_GR_cog.leosVE.Prayer_NynApolyeis.chapverse //"Λουκᾶ 2:32"
psLI.m02.d02.alleluia.v2.text = "Φῶς εἰς ἀποκάλυψιν ἐθνῶν, καὶ δόξαν λαοῦ σου Ἰσραήλ."

psLI.m02.d03.prokeimenon.v1.info = ps_gr_GR_cog.psa63.v11.number
psLI.m02.d03.prokeimenon.v1.text = "Εὐφρανθήσεται δίκαιος ἐν Κυρίῳ."
psLI.m02.d03.prokeimenon.v2.info = ps_gr_GR_cog.psa63.v2.number
psLI.m02.d03.prokeimenon.v2.text = "Εἰσάκουσον, ὁ Θεός, τῆς φωνῆς μου."
psLI.m02.d03.alleluia.v1.info = ps_gr_GR_cog.psa96.v11.number
psLI.m02.d03.alleluia.v1.text = "Φῶς ἀνέτειλε τῷ δικαίῳ."
psLI.m02.d03.alleluia.v2.info = ps_gr_GR_cog.psa111.v9.number
psLI.m02.d03.alleluia.v2.text = "Ἐσκόρπισεν, ἔδωκε τοῖς πένησιν."

psLI.m02.d06.prokeimenon.v1.info = ps_gr_GR_cog.psa48.v4.number
psLI.m02.d06.prokeimenon.v1.text = "Τὸ στόμα μου λαλήσει σοφίαν."
psLI.m02.d06.prokeimenon.v2.info = ps_gr_GR_cog.psa48.v2.number
psLI.m02.d06.prokeimenon.v2.text = "Ἀκούσατε ταῦτα, πάντα τὰ ἔθνη."
psLI.m02.d06.alleluia.v1.info = ps_gr_GR_cog.psa36.v30.number
psLI.m02.d06.alleluia.v1.text = "Στόμα δικαίου μελετήσει σοφίαν."
psLI.m02.d06.alleluia.v2.info = ps_gr_GR_cog.psa36.v31.number
psLI.m02.d06.alleluia.v2.text = "Ὁ νόμος τοῦ Θεοῦ αὐτοῦ ἐν καρδίᾳ αὐτοῦ."

psLI.m02.d08.prokeimenon.v1.info = ps_gr_GR_cog.psa67.v36.number
psLI.m02.d08.prokeimenon.v1.text = "Θαυμαστὸς ὁ Θεὸς ἐν τοῖς ἁγίοις αὐτοῦ."
psLI.m02.d08.prokeimenon.v2.info = ps_gr_GR_cog.psa67.v27.number
psLI.m02.d08.prokeimenon.v2.text = "Ἐν ἐκκλησίαις εὐλογεῖτε τὸν Θεόν."
psLI.m02.d08.alleluia.v1.info = ps_gr_GR_cog.psa33.v18.number
psLI.m02.d08.alleluia.v1.text = "Ἐκέκραξαν οἱ δίκαιοι."
psLI.m02.d08.alleluia.v2.info = ps_gr_GR_cog.psa111.v1.number
psLI.m02.d08.alleluia.v2.text = "Μακάριος ἀνὴρ ὁ φοβούμενος τὸν Κύριον."

psLI.m02.d09.prokeimenon.v1.info = ps_gr_GR_cog.psa67.v36.number
psLI.m02.d09.prokeimenon.v1.text = "Θαυμαστὸς ὁ Θεὸς ἐν τοῖς ἁγίοις αὐτοῦ."
psLI.m02.d09.prokeimenon.v2.info = ps_gr_GR_cog.psa67.v27.number
psLI.m02.d09.prokeimenon.v2.text = "Ἐν ἐκκλησίαις εὐλογεῖτε τὸν Θεόν."
psLI.m02.d09.alleluia.v1.info = ps_gr_GR_cog.psa33.v18.number
psLI.m02.d09.alleluia.v1.text = "Ἐκέκραξαν οἱ δίκαιοι."
psLI.m02.d09.alleluia.v2.info = ps_gr_GR_cog.psa111.v1.number
psLI.m02.d09.alleluia.v2.text = "Μακάριος ἀνὴρ ὁ φοβούμενος τὸν Κύριον."

psLI.m02.d10.prokeimenon.v1.info = ps_gr_GR_cog.psa63.v11.number
psLI.m02.d10.prokeimenon.v1.text = "Εὐφρανθήσεται δίκαιος ἐν Κυρίῳ."
psLI.m02.d10.prokeimenon.v2.info = ps_gr_GR_cog.psa63.v2.number
psLI.m02.d10.prokeimenon.v2.text = "Εἰσάκουσον, ὁ Θεός, τῆς φωνῆς μου."
psLI.m02.d10.alleluia.v1.info = ps_gr_GR_cog.psa91.v13.number
psLI.m02.d10.alleluia.v1.text = "Δίκαιος ὡς φοῖνιξ ἀνθήσει."
psLI.m02.d10.alleluia.v2.info = ps_gr_GR_cog.psa91.v14.number
psLI.m02.d10.alleluia.v2.text = "Πεφυτευμένος ἐν τῷ οἴκῳ Κυρίου."

psLI.m02.d17.prokeimenon.v1.info = ps_gr_GR_cog.psa63.v11.number
psLI.m02.d17.prokeimenon.v1.text = "Εὐφρανθήσεται δίκαιος ἐν Κυρίῳ."
psLI.m02.d17.prokeimenon.v2.info = ps_gr_GR_cog.psa63.v2.number
psLI.m02.d17.prokeimenon.v2.text = "Εἰσάκουσον, ὁ Θεός, τῆς φωνῆς μου."
psLI.m02.d17.alleluia.v1.info = ps_gr_GR_cog.psa91.v13.number
psLI.m02.d17.alleluia.v1.text = "Δίκαιος ὡς φοῖνιξ ἀνθήσει."
psLI.m02.d17.alleluia.v2.info = ps_gr_GR_cog.psa91.v14.number
psLI.m02.d17.alleluia.v2.text = "Πεφυτευμένος ἐν τῷ οἴκῳ Κυρίου."

psLI.m02.d19.prokeimenon.v1.info = ps_gr_GR_cog.psa67.v36.number
psLI.m02.d19.prokeimenon.v1.text = "Θαυμαστὸς ὁ Θεὸς ἐν τοῖς ἁγίοις αὐτοῦ."
psLI.m02.d19.prokeimenon.v2.info = ps_gr_GR_cog.psa67.v27.number
psLI.m02.d19.prokeimenon.v2.text = "Ἐν ἐκκλησίαις εὐλογεῖτε τὸν Θεόν."
psLI.m02.d19.alleluia.v1.info = ps_gr_GR_cog.psa39.v2.number
psLI.m02.d19.alleluia.v1.text = "Ὑπομένων ὑπέμεινα τὸν Κύριον."
psLI.m02.d19.alleluia.v2.info = ps_gr_GR_cog.psa39.v3.number
psLI.m02.d19.alleluia.v2.text = "Καὶ ἀνήγαγέ με ἐκ λάκκου ταλαιπωρίας."

psLI.m02.d23.prokeimenon.v1.info = ps_gr_GR_cog.psa115.v5.number
psLI.m02.d23.prokeimenon.v1.text = "Τίμιος ἐναντίον Κυρίου ὁ θάνατος τοῦ ὁσίου αὐτοῦ."
psLI.m02.d23.prokeimenon.v2.info = ps_gr_GR_cog.psa115.v8.number
psLI.m02.d23.prokeimenon.v2.text = "Τὰς εὐχάς μου τῷ Κυρίῳ ἀποδώσω ἐναντίον παντὸς τοῦ λαοῦ αὐτοῦ."
psLI.m02.d23.alleluia.v1.info = ps_gr_GR_cog.psa131.v9.number
psLI.m02.d23.alleluia.v1.text = "Οἱ ἱερεῖς σου, Κύριε, ἐνδύσονται δικαιοσύνην."
psLI.m02.d23.alleluia.v2.info = ps_gr_GR_cog.psa131.v13.number
psLI.m02.d23.alleluia.v2.text = "Ὅτι ἐξελέξατο Κύριος τὴν Σιών."

psLI.m02.d24.prokeimenon.v1.info = ps_gr_GR_cog.psa63.v11.number
psLI.m02.d24.prokeimenon.v1.text = "Εὐφρανθήσεται δίκαιος ἐν Κυρίῳ."
psLI.m02.d24.prokeimenon.v2.info = ps_gr_GR_cog.psa63.v2.number
psLI.m02.d24.prokeimenon.v2.text = "Εἰσάκουσον, ὁ Θεός, τῆς φωνῆς μου."
psLI.m02.d24.alleluia.v1.info = ps_gr_GR_cog.psa91.v13.number
psLI.m02.d24.alleluia.v1.text = "Δίκαιος ὡς φοῖνιξ ἀνθήσει."
psLI.m02.d24.alleluia.v2.info = ps_gr_GR_cog.psa91.v14.number
psLI.m02.d24.alleluia.v2.text = "Πεφυτευμένος ἐν τῷ οἴκῳ Κυρίου."


//March

psLI.m03.d09.prokeimenon.v1.info = ps_gr_GR_cog.psa11.v8.number
psLI.m03.d09.prokeimenon.v1.text = "Σύ, Κύριε, φυλάξαις ἡμᾶς καὶ διατηρήσαις ἡμᾶς."
psLI.m03.d09.prokeimenon.v2.info = ps_gr_GR_cog.psa11.v2.number
psLI.m03.d09.prokeimenon.v2.text = "Σῶσον με, Κύριε, ὅτι ἐκλέλοιπεν ὅσιος."
psLI.m03.d09.alleluia.v1.info = ps_gr_GR_cog.psa65.v1.number
psLI.m03.d09.alleluia.v1.text = "Ἀλαλάξατε τῷ Κυρίῳ πᾶσα ἡ γῆ."
psLI.m03.d09.alleluia.v2.info = ps_gr_GR_cog.psa65.v10.number
psLI.m03.d09.alleluia.v2.text = "Ὅτι ἐδοκίμασας ἡμᾶς, ὁ Θεός, ἐπύρωσας ἡμᾶς."

psLI.m03.d25.antiphon1.v1.info = ps_gr_GR_cog.psa71.v1.number //"71:1"
psLI.m03.d25.antiphon1.v1.text = "Ὁ Θεός, τὸ κρίμα σου τῷ βασιλεῖ δὸς καὶ τὴν δικαιοσύνην σου τῷ υἱῷ τοῦ βασιλέως."
psLI.m03.d25.antiphon1.v2.info = ps_gr_GR_cog.psa71.v3.number //"71:3"
psLI.m03.d25.antiphon1.v2.text = "Ἀναλαβέτω τὰ ὄρη εἰρήνην τῷ λαῷ σου καὶ οἱ βουνοὶ δικαιοσύνην."
psLI.m03.d25.antiphon1.v3.info = ps_gr_GR_cog.psa95.v2.number //"95:2"
psLI.m03.d25.antiphon1.v3.text = "Εὐαγγελίζεσθε ἡμέραν ἐξ ἡμέρας τὸ σωτήριον τοῦ Θεοῦ ἡμῶν."
psLI.m03.d25.antiphon1.v4.info = ps_gr_GR_cog.psa131.v11.number //"131:11"
psLI.m03.d25.antiphon1.v4.text = "Ὤμοσε Κύριος τῷ Δαυῒδ ἀλήθειαν καὶ οὐ μὴ ἀθετήσει αὐτήν."
psLI.m03.d25.antiphon2.v1.info = ps_gr_GR_cog.psa71.v6.number //"71:6"
psLI.m03.d25.antiphon2.v1.text = "Καταβήσεται ὡς ὑετὸς ἐπὶ πόκον καὶ ὡσεὶ σταγὼν ἡ στάζουσα ἐπὶ τὴν γῆν."
psLI.m03.d25.antiphon2.v2.info = ps_gr_GR_cog.psa45.v5.number //"45:5"
psLI.m03.d25.antiphon2.v2.text = "Ἡγίασε τὸ σκήνωμα αὐτοῦ ὁ Ὕψιστος."
psLI.m03.d25.antiphon2.v3.info = ps_gr_GR_cog.psa49.v3.number //"49:3"
psLI.m03.d25.antiphon2.v3.text = "Ὁ Θεὸς ἐμφανῶς ἥξει, ὁ Θεὸς ἡμῶν, καὶ οὐ παρασιωπήσεται."
psLI.m03.d25.antiphon2.v4.info = ps_gr_GR_cog.psa71.v7.number //"71:7"
psLI.m03.d25.antiphon2.v4.text = "Ἀνατελεῖ ἐν ταῖς ἡμέραις αὐτοῦ δικαιοσύνη καὶ πλῆθος εἰρήνης."
psLI.m03.d25.antiphon3.v1.info = ps_gr_GR_cog.psa71.v17.number //"71:17"
psLI.m03.d25.antiphon3.v1.text = "Ἔσται τὸ ὄνομα αὐτοῦ εὐλογημένον εἰς τοὺς αἰῶνας."
psLI.m03.d25.antiphon3.v2.info = ps_gr_GR_cog.psa71.v18.number //"71:18"
psLI.m03.d25.antiphon3.v2.text = "Εὐλογητὸς Κύριος, ὁ Θεὸς τοῦ Ἰσραήλ, ὁ ποιῶν θαυμάσια μόνος,"
psLI.m03.d25.antiphon3.v3.info = ps_gr_GR_cog.psa71.v19.number //"71:19"
psLI.m03.d25.antiphon3.v3.text = "Εὐλογημένον τὸ ὄνομα τῆς δόξης αὐτοῦ εἰς τὸν αἰῶνα καὶ εἰς τὸν αἰῶνα τοῦ αἰῶνος."
psLI.m03.d25.entrance.v1.info = ps_gr_GR_cog.psa95.v2.number //"95:2"
psLI.m03.d25.entrance.v1.text = "Εὐαγγελίζεσθε ἡμέραν ἐξ ἡμέρας τὸ σωτήριον τοῦ Θεοῦ ἡμῶν."
psLI.m03.d25.prokeimenon.v1.info = ps_gr_GR_cog.bode9_mag.v1.number //"Ὠδὴ 9a:1"
psLI.m03.d25.prokeimenon.v1.text = "Μεγαλύνει ἡ ψυχή μου τὸν Κύριον, καὶ ἠγαλλίασε τὸ πνεῦμά μου ἐπὶ τῷ Θεῷ τῷ σωτῆρί μου."
psLI.m03.d25.prokeimenon.v2.info = ps_gr_GR_cog.bode9_mag.v2.number //"Ὠδὴ 9a:2"
psLI.m03.d25.prokeimenon.v2.text = "Ὅτι ἐπέβλεψεν ἐπὶ τὴν ταπείνωσιν τῆς δούλης αὐτοῦ· ἰδού γάρ, ἀπὸ τοῦ νῦν μακαριοῦσί με πᾶσαι αἱ γενεαί."
psLI.m03.d25.alleluia.v1.info = ps_gr_GR_cog.psa71.v6.number //"71:6"
psLI.m03.d25.alleluia.v1.text = "Καταβήσεται ὡς ὑετὸς ἐπὶ πόκον καὶ ὡσεὶ σταγὼν ἡ στάζουσα ἐπὶ τὴν γῆν."
psLI.m03.d25.alleluia.v2.info = ps_gr_GR_cog.psa71.v17.number //"71:17"
psLI.m03.d25.alleluia.v2.text = "Ἔσται τὸ ὄνομα αὐτοῦ εὐλογημένον εἰς τοὺς αἰῶνας."
psLI.m03.d25.communion.v1.info = ps_gr_GR_cog.psa131.v13.number //"131:13"
psLI.m03.d25.communion.v1.text = "Ἐξελέξατο Κύριος τὴν Σιών, ᾑρετίσατο αὐτὴν εἰς κατοικίαν ἑαυτῷ."

psLI.m03.d26.prokeimenon.v1.info = ps_gr_GR_cog.psa103.v4.number
psLI.m03.d26.prokeimenon.v1.text = "Ὁ ποιῶν τοὺς ἀγγέλους αὐτοῦ πνεύματα καὶ τοὺς λειτουργοὺς αὐτοῦ πυρὸς φλόγα."
psLI.m03.d26.prokeimenon.v2.info = ps_gr_GR_cog.psa103.v1.number
psLI.m03.d26.prokeimenon.v2.text = "Εὐλόγει, ἡ ψυχή μου, τὸν Κύριον."
psLI.m03.d26.alleluia.v1.info = ps_gr_GR_cog.psa148.v1.number
psLI.m03.d26.alleluia.v1.text = "Αἰνεῖτε τὸν Κύριον ἐκ τῶν οὐρανῶν· αἰνεῖτε αὐτὸν ἐν τοῖς ὑψίστοις."
psLI.m03.d26.alleluia.v2.info = ps_gr_GR_cog.psa148.v2.number
psLI.m03.d26.alleluia.v2.text = "Αἰνεῖτε αὐτόν, πάντες οἱ ἄγγελοι αὐτοῦ· αἰνεῖτε αὐτόν, πᾶσαι αἱ δυνάμεις αὐτοῦ."

psLI.m03.d30.prokeimenon.v1.info = ps_gr_GR_cog.psa115.v5.number
psLI.m03.d30.prokeimenon.v1.text = "Τίμιος ἐναντίον Κυρίου ὁ θάνατος τῶν ὁσίου αὐτοῦ."
psLI.m03.d30.prokeimenon.v2.info = ps_gr_GR_cog.psa115.v3.number
psLI.m03.d30.prokeimenon.v2.text = "Τί ἀνταποδώσω τῷ Κυρίῳ περὶ πάντων;"
psLI.m03.d30.alleluia.v1.info = ps_gr_GR_cog.psa33.v18.number
psLI.m03.d30.alleluia.v1.text = "Ἐκέκραξαν οἱ δίκαιοι, καὶ ὁ Κύριος εἰσήκουσεν αὐτῶν."
psLI.m03.d30.alleluia.v2.info = ps_gr_GR_cog.psa111.v1.number
psLI.m03.d30.alleluia.v2.text = "Μακάριος ἀνὴρ ὁ φοβούμενος τὸν Κύριον."


//April

psLI.m04.d01.prokeimenon.v1.info = ps_gr_GR_cog.psa67.v36.number
psLI.m04.d01.prokeimenon.v1.text = "Θαυμαστὸς ὁ Θεὸς ἐν τοῖς ἁγίοις αὐτοῦ."
psLI.m04.d01.prokeimenon.v2.info = ps_gr_GR_cog.psa67.v27.number
psLI.m04.d01.prokeimenon.v2.text = "Ἐν ἐκκλησίαις εὐλογεῖτε τὸν Θεόν."
psLI.m04.d01.alleluia.v1.info = ps_gr_GR_cog.psa39.v2.number
psLI.m04.d01.alleluia.v1.text = "Ὑπομένων ὑπέμεινα τὸν Κύριον."
psLI.m04.d01.alleluia.v2.info = ps_gr_GR_cog.psa39.v3.number
psLI.m04.d01.alleluia.v2.text = "Καὶ ἔστησεν ἐπὶ πέτραν τοὺς πόδας μου."

psLI.m04.d10.prokeimenon.v1.info = ps_gr_GR_cog.psa48.v4.number
psLI.m04.d10.prokeimenon.v1.text = "Τὸ στόμα μου λαλήσει σοφίαν."
psLI.m04.d10.prokeimenon.v2.info = ps_gr_GR_cog.psa48.v2.number
psLI.m04.d10.prokeimenon.v2.text = "Ἀκούσατε ταῦτα, πάντα τὰ ἔθνη."
psLI.m04.d10.alleluia.v1.info = ps_gr_GR_cog.psa36.v30.number
psLI.m04.d10.alleluia.v1.text = "Στόμα δικαίου μελετήσει σοφίαν."
psLI.m04.d10.alleluia.v2.info = ps_gr_GR_cog.psa36.v31.number
psLI.m04.d10.alleluia.v2.text = "Ὁ νόμος τοῦ Θεοῦ αὐτοῦ ἐν καρδίᾳ αὐτοῦ."

psLI.m04.d23.prokeimenon.v1.info = ps_gr_GR_cog.psa63.v11.number
psLI.m04.d23.prokeimenon.v1.text = "Εὐφρανθήσεται δίκαιος ἐν Κυρίῳ."
psLI.m04.d23.prokeimenon.v2.info = ps_gr_GR_cog.psa63.v2.number
psLI.m04.d23.prokeimenon.v2.text = "Εἰσάκουσον, ὁ Θεός, τῆς φωνῆς μου."
psLI.m04.d23.alleluia.v1.info = ps_gr_GR_cog.psa91.v13.number
psLI.m04.d23.alleluia.v1.text = "Δίκαιος ὡς φοῖνιξ ἀνθήσει."
psLI.m04.d23.alleluia.v2.info = ps_gr_GR_cog.psa91.v14.number
psLI.m04.d23.alleluia.v2.text = "Πεφυτευμένος ἐν τῷ οἴκῳ Κυρίου."

psLI.m04.d25.prokeimenon.v1.info = ps_gr_GR_cog.psa18.v5.number
psLI.m04.d25.prokeimenon.v1.text = "Εἰς πᾶσαν τὴν γῆν ἐξῆλθεν ὁ φθόγγος αὐτοῦ."
psLI.m04.d25.prokeimenon.v2.info = ps_gr_GR_cog.psa18.v2.number
psLI.m04.d25.prokeimenon.v2.text = "Οἱ οὐρανοὶ διηγοῦνται δόξαν Θεοῦ."
psLI.m04.d25.alleluia.v1.info = ps_gr_GR_cog.psa88.v6.number
psLI.m04.d25.alleluia.v1.text = "Ἐξομολογήσονται οἱ οὐρανοὶ τὰ θαυμάσιά σου, Κύριε."
psLI.m04.d25.alleluia.v2.info = ps_gr_GR_cog.psa88.v8.number
psLI.m04.d25.alleluia.v2.text = "Ὁ Θεὸς ἐνδοξαζόμενος ἐν βουλῇ ἁγίων."

psLI.m04.d30.prokeimenon.v1.info = ps_gr_GR_cog.psa18.v5.number
psLI.m04.d30.prokeimenon.v1.text = "Εἰς πᾶσαν τὴν γῆν ἐξῆλθεν ὁ φθόγγος αὐτοῦ."
psLI.m04.d30.prokeimenon.v2.info = ps_gr_GR_cog.psa18.v2.number
psLI.m04.d30.prokeimenon.v2.text = "Οἱ οὐρανοὶ διηγοῦνται δόξαν Θεοῦ."
psLI.m04.d30.alleluia.v1.info = ps_gr_GR_cog.psa88.v6.number
psLI.m04.d30.alleluia.v1.text = "Ἐξομολογήσονται οἱ οὐρανοὶ τὰ θαυμάσιά σου, Κύριε."
psLI.m04.d30.alleluia.v2.info = ps_gr_GR_cog.psa88.v8.number
psLI.m04.d30.alleluia.v2.text = "Ὁ Θεὸς ἐνδοξαζόμενος ἐν βουλῇ ἁγίων."


//May

psLI.m05.d02.prokeimenon.v1.info = le.pr.os_gr_GR_cog.leosLI.pe.d113.EpistleProkeimenon.book //"(Dan 3:52a, 27)"
psLI.m05.d02.prokeimenon.v1.text = "Εὐλογητὸς εἶ Κύριε ὁ Θεὸς τῶν πατέρων ἡμῶν."
psLI.m05.d02.prokeimenon.v2.info = le.pr.os_gr_GR_cog.leosLI.pe.d113.EpistleProkeimenon.book //"(Dan 3:52a, 27)"
psLI.m05.d02.prokeimenon.v2.text = "Ὅτι δίκαιος εἶ ἐπὶ πᾶσιν, οἷς ἐποίησας ἡμῖν."
psLI.m05.d02.alleluia.v1.info = ps_gr_GR_cog.psa131.v9.number
psLI.m05.d02.alleluia.v1.text = "Οἱ ἱερεῖς σου, Κύριε, ἐνδύσονται δικαιοσύνην."
psLI.m05.d02.alleluia.v2.info = ps_gr_GR_cog.psa131.v13.number
psLI.m05.d02.alleluia.v2.text = "Ὅτι ἐξελέξατο Κύριος τὴν Σιών."

psLI.m05.d07.prokeimenon.v1.info = ps_gr_GR_cog.psa27.v9.number
psLI.m05.d07.prokeimenon.v1.text = "Σῶσον, Κύριε, τὸν λαόν σου καὶ εὐλόγησον τὴν κληρονομίαν σου."
psLI.m05.d07.prokeimenon.v2.info = ps_gr_GR_cog.psa27.v1.number
psLI.m05.d07.prokeimenon.v2.text = "Πρὸς σέ, Κύριε, κεκράξομαι, ὁ Θεός μου."
psLI.m05.d07.alleluia.v1.info = ps_gr_GR_cog.psa73.v2.number
psLI.m05.d07.alleluia.v1.text = "Μνήσθητι τῆς συναγωγῆς σου, ἧς ἐκτήσω ἀπ' ἀρχῆς."
psLI.m05.d07.alleluia.v2.info = ps_gr_GR_cog.psa73.v12.number
psLI.m05.d07.alleluia.v2.text = "Ὁ δὲ Θεὸς βασιλεὺς ἡμῶν πρὸ αἰώνων, εἰργάσατο σωτηρίαν ἐν μέσῳ τῆς γῆς."

psLI.m05.d08.prokeimenon.v1.info = ps_gr_GR_cog.psa18.v5.number
psLI.m05.d08.prokeimenon.v1.text = "Εἰς πᾶσαν τὴν γῆν ἐξῆλθεν ὁ φθόγγος αὐτοῦ."
psLI.m05.d08.prokeimenon.v2.info = ps_gr_GR_cog.psa18.v2.number
psLI.m05.d08.prokeimenon.v2.text = "Οἱ οὐρανοὶ διηγοῦνται δόξαν Θεοῦ."
psLI.m05.d08.alleluia.v1.info = ps_gr_GR_cog.psa88.v6.number
psLI.m05.d08.alleluia.v1.text = "Ἐξομολογήσονται οἱ οὐρανοὶ τὰ θαυμάσιά σου, Κύριε."
psLI.m05.d08.alleluia.v2.info = ps_gr_GR_cog.psa88.v8.number
psLI.m05.d08.alleluia.v2.text = "Ὁ Θεὸς ἐνδοξαζόμενος ἐν βουλῇ ἁγίων."

psLI.m05.d21.prokeimenon.v1.info = ps_gr_GR_cog.psa18.v5.number
psLI.m05.d21.prokeimenon.v1.text = "Εἰς πᾶσαν τὴν γῆν ἐξῆλθεν ὁ φθόγγος αὐτοῦ."
psLI.m05.d21.prokeimenon.v2.info = ps_gr_GR_cog.psa18.v2.number
psLI.m05.d21.prokeimenon.v2.text = "Οἱ οὐρανοὶ διηγοῦνται δόξαν Θεοῦ."
psLI.m05.d21.alleluia.v1.info = ps_gr_GR_cog.psa88.v20.number
psLI.m05.d21.alleluia.v1.text = "Ὕψωσα ἐκλεκτὸν ἐκ τοῦ λαοῦ μου."
psLI.m05.d21.alleluia.v2.info = ps_gr_GR_cog.psa88.v22.number
psLI.m05.d21.alleluia.v2.text = "Ἡ γὰρ χείρ μου συναντιλήψεται αὐτῷ."

psLI.m05.d25.prokeimenon.v1.info = ps_gr_GR_cog.psa63.v11.number
psLI.m05.d25.prokeimenon.v1.text = "Εὐφρανθήσεται δίκαιος ἐν Κυρίῳ."
psLI.m05.d25.prokeimenon.v2.info = ps_gr_GR_cog.psa63.v2.number
psLI.m05.d25.prokeimenon.v2.text = "Εἰσάκουσον, ὁ Θεός, τῆς φωνῆς μου."
psLI.m05.d25.alleluia.v1.info = ps_gr_GR_cog.psa91.v13.number
psLI.m05.d25.alleluia.v1.text = "Δίκαιος ὡς φοῖνιξ ἀνθήσει."
psLI.m05.d25.alleluia.v2.info = ps_gr_GR_cog.psa91.v14.number
psLI.m05.d25.alleluia.v2.text = "Πεφυτευμένος ἐν τῷ οἴκῳ Κυρίου."


//June

psLI.m06.d02.prokeimenon.v1.info = ps_gr_GR_cog.psa115.v5.number
psLI.m06.d02.prokeimenon.v1.text = "Τίμιος ἐναντίον Κυρίου ὁ θάνατος τῶν ὁσίου αὐτοῦ."
psLI.m06.d02.prokeimenon.v2.info = ps_gr_GR_cog.psa115.v3.number
psLI.m06.d02.prokeimenon.v2.text = "Τί ἀνταποδώσω τῷ Κυρίῳ περὶ πάντων;"
psLI.m06.d02.alleluia.v1.info = ps_gr_GR_cog.psa131.v9.number
psLI.m06.d02.alleluia.v1.text = "Οἱ ἱερεῖς σου, Κύριε, ἐνδύσονται δικαιοσύνην."
psLI.m06.d02.alleluia.v2.info = ps_gr_GR_cog.psa131.v13.number
psLI.m06.d02.alleluia.v2.text = "Ὅτι ἐξελέξατο Κύριος τὴν Σιών."

psLI.m06.d04.prokeimenon.v1.info = ps_gr_GR_cog.psa48.v4.number
psLI.m06.d04.prokeimenon.v1.text = "Τὸ στόμα μου λαλήσει σοφίαν."
psLI.m06.d04.prokeimenon.v2.info = ps_gr_GR_cog.psa48.v2.number
psLI.m06.d04.prokeimenon.v2.text = "Ἀκούσατε ταῦτα, πάντα τὰ ἔθνη."
psLI.m06.d04.alleluia.v1.info = ps_gr_GR_cog.psa36.v30.number
psLI.m06.d04.alleluia.v1.text = "Στόμα δικαίου μελετήσει σοφίαν."
psLI.m06.d04.alleluia.v2.info = ps_gr_GR_cog.psa36.v31.number
psLI.m06.d04.alleluia.v2.text = "Ὁ νόμος τοῦ Θεοῦ αὐτοῦ ἐν καρδίᾳ αὐτοῦ."

psLI.m06.d09.prokeimenon.v1.info = ps_gr_GR_cog.psa48.v4.number
psLI.m06.d09.prokeimenon.v1.text = "Τὸ στόμα μου λαλήσει σοφίαν."
psLI.m06.d09.prokeimenon.v2.info = ps_gr_GR_cog.psa48.v2.number
psLI.m06.d09.prokeimenon.v2.text = "Ἀκούσατε ταῦτα, πάντα τὰ ἔθνη."
psLI.m06.d09.alleluia.v1.info = ps_gr_GR_cog.psa36.v30.number
psLI.m06.d09.alleluia.v1.text = "Στόμα δικαίου μελετήσει σοφίαν."
psLI.m06.d09.alleluia.v2.info = ps_gr_GR_cog.psa36.v31.number
psLI.m06.d09.alleluia.v2.text = "Ὁ νόμος τοῦ Θεοῦ αὐτοῦ ἐν καρδίᾳ αὐτοῦ."

psLI.m06.d14a.prokeimenon.v1.info = ps_gr_GR_cog.psa109.v4.number
psLI.m06.d14a.prokeimenon.v1.text = "Σὺ ἱερεὺς εἰς τὸν αἰῶνα κατὰ τὴν τάξιν Μελχισεδέκ."
psLI.m06.d14a.prokeimenon.v2.info = ps_gr_GR_cog.psa109.v1.number
psLI.m06.d14a.prokeimenon.v2.text = "Εἶπεν ὁ Κύριος τῷ Κυρίῳ μου· Κάθου ἐκ δεξιῶν μου, ἕως ἂν θῶ τοὺς ἐχθρούς σου ὑποπόδιον τῶν ποδῶν σου."
psLI.m06.d14a.alleluia.v1.info = ps_gr_GR_cog.psa111.v1.number
psLI.m06.d14a.alleluia.v1.text = "Μακάριος ἀνὴρ ὁ φοβούμενος τὸν Κύριον."
psLI.m06.d14a.alleluia.v2.info = ps_gr_GR_cog.psa111.v9.number
psLI.m06.d14a.alleluia.v2.text = "Ἡ δικαιοσύνη αὐτοῦ μένει εἰς τὸν αἰῶνα."

psLI.m06.d14b.prokeimenon.v1.info = ps_gr_GR_cog.psa115.v5.number
psLI.m06.d14b.prokeimenon.v1.text = "Τίμιος ἐναντίον Κυρίου ὁ θάνατος τοῦ ὁσίου αὐτοῦ."
psLI.m06.d14b.prokeimenon.v2.info = ps_gr_GR_cog.psa115.v3.number
psLI.m06.d14b.prokeimenon.v2.text = "Τί ἀνταποδώσω τῷ Κυρίῳ περὶ πάντων, ὧν ἀνταπέδωκέ μοι;"

psLI.m06.d19.prokeimenon.v1.info = ps_gr_GR_cog.psa18.v5.number
psLI.m06.d19.prokeimenon.v1.text = "Εἰς πᾶσαν τὴν γῆν ἐξῆλθεν ὁ φθόγγος αὐτοῦ."
psLI.m06.d19.prokeimenon.v2.info = ps_gr_GR_cog.psa18.v2.number
psLI.m06.d19.prokeimenon.v2.text = "Οἱ οὐρανοὶ διηγοῦνται δόξαν Θεοῦ."
psLI.m06.d19.alleluia.v1.info = ps_gr_GR_cog.psa88.v6.number
psLI.m06.d19.alleluia.v1.text = "Ἐξομολογήσονται οἱ οὐρανοὶ τὰ θαυμάσιά σου, Κύριε."
psLI.m06.d19.alleluia.v2.info = ps_gr_GR_cog.psa88.v8.number
psLI.m06.d19.alleluia.v2.text = "Ὁ Θεὸς ἐνδοξαζόμενος ἐν βουλῇ ἁγίων."

psLI.m06.d24.prokeimenon.v1.info = ps_gr_GR_cog.psa63.v11.number
psLI.m06.d24.prokeimenon.v1.text = "Εὐφρανθήσεται δίκαιος ἐν Κυρίῳ."
psLI.m06.d24.prokeimenon.v2.info = ps_gr_GR_cog.psa63.v2.number
psLI.m06.d24.prokeimenon.v2.text = "Εἰσάκουσον, ὁ Θεός, τῆς φωνῆς μου."
psLI.m06.d24.alleluia.v1.info = ps_gr_GR_cog.bode9.v1.number
psLI.m06.d24.alleluia.v1.text = "Εὐλογητὸς Κύριος ὁ Θεὸς τοῦ Ἰσραήλ, ὅτι ἐπεσκέψατο καὶ ἐποίησε λύτρωσιν τῷ λαῷ αὐτοῦ."
psLI.m06.d24.alleluia.v2.info = ps_gr_GR_cog.bode9.v8.number
psLI.m06.d24.alleluia.v2.text = "Καὶ σύ, Παιδίον, Προφήτης Ὑψίστου κληθήσῃ· προπορεύσῃ γὰρ πρὸ προσώπου Κυρίου ἑτοιμάσαι ὁδοὺς αὐτοῦ."

psLI.m06.d25.prokeimenon.v1.info = ps_gr_GR_cog.psa67.v36.number
psLI.m06.d25.prokeimenon.v1.text = "Θαυμαστὸς ὁ Θεὸς ἐν τοῖς ἁγίοις αὐτοῦ."
psLI.m06.d25.prokeimenon.v2.info = ps_gr_GR_cog.psa67.v27.number
psLI.m06.d25.prokeimenon.v2.text = "Ἐν ἐκκλησίαις εὐλογεῖτε τὸν Θεόν."
psLI.m06.d25.alleluia.v1.info = ps_gr_GR_cog.psa39.v2.number
psLI.m06.d25.alleluia.v1.text = "Ὑπομένων ὑπέμεινα τὸν Κύριον."
psLI.m06.d25.alleluia.v2.info = ps_gr_GR_cog.psa39.v3.number
psLI.m06.d25.alleluia.v2.text = "Καὶ ἀνήγαγέ με ἐκ λάκκου ταλαιπωρίας."

psLI.m06.d28.prokeimenon.v1.info = ps_gr_GR_cog.psa15.v3.number
psLI.m06.d28.prokeimenon.v1.text = "Τοῖς ἁγίοις τοῖς ἐν τῇ γῇ αὐτοῦ ἐθαυμάστωσεν ὁ Κύριος."
psLI.m06.d28.prokeimenon.v2.info = ps_gr_GR_cog.psa15.v8.number
psLI.m06.d28.prokeimenon.v2.text = "Προωρώμην τὸν Κύριον ἐνώπιόν μου διαπαντός."
psLI.m06.d28.alleluia.v1.info = ps_gr_GR_cog.psa132.v1.number
psLI.m06.d28.alleluia.v1.text = "Ἰδοὺ δὴ τί καλὸν ἢ τί τερπνόν, ἀλλ' ἢ τὸ κατοικεῖν ἀδελφοὺς ἐπὶ τὸ αὐτό;"
psLI.m06.d28.alleluia.v2.info = ps_gr_GR_cog.psa132.v3.number
psLI.m06.d28.alleluia.v2.text = "Ὅτι ἐκεῖ ἐνετείλατο Κύριος τὴν εὐλογίαν, ζωὴν ἕως τοῦ αἰῶνος."

psLI.m06.d29.prokeimenon.v1.info = ps_gr_GR_cog.psa18.v5.number
psLI.m06.d29.prokeimenon.v1.text = "Εἰς πᾶσαν τὴν γῆν ἐξῆλθεν ὁ φθόγγος αὐτῶν."
psLI.m06.d29.prokeimenon.v2.info = ps_gr_GR_cog.psa18.v2.number
psLI.m06.d29.prokeimenon.v2.text = "Οἱ οὐρανοὶ διηγοῦνται δόξαν Θεοῦ."
psLI.m06.d29.alleluia.v1.info = ps_gr_GR_cog.psa88.v6.number
psLI.m06.d29.alleluia.v1.text = "Ἐξομολογήσονται οἱ οὐρανοὶ τὰ θαυμάσιά σου, Κύριε."
psLI.m06.d29.alleluia.v2.info = ps_gr_GR_cog.psa88.v8.number
psLI.m06.d29.alleluia.v2.text = "Ὁ Θεὸς ἐνδοξαζόμενος ἐν βουλῇ ἁγίων."

psLI.m06.d30.prokeimenon.v1.info = ps_gr_GR_cog.psa18.v5.number
psLI.m06.d30.prokeimenon.v1.text = "Εἰς πᾶσαν τὴν γῆν ἐξῆλθεν ὁ φθόγγος αὐτῶν."
psLI.m06.d30.prokeimenon.v2.info = ps_gr_GR_cog.psa18.v2.number
psLI.m06.d30.prokeimenon.v2.text = "Οἱ οὐρανοὶ διηγοῦνται δόξαν Θεοῦ."
psLI.m06.d30.alleluia.v1.info = ps_gr_GR_cog.psa88.v6.number
psLI.m06.d30.alleluia.v1.text = "Ἐξομολογήσονται οἱ οὐρανοὶ τὰ θαυμάσιά σου, Κύριε."
psLI.m06.d30.alleluia.v2.info = ps_gr_GR_cog.psa88.v8.number
psLI.m06.d30.alleluia.v2.text = "Ὁ Θεὸς ἐνδοξαζόμενος ἐν βουλῇ ἁγίων."


//July

psLI.m07.d01.prokeimenon.v1.info = ps_gr_GR_cog.psa15.v3.number
psLI.m07.d01.prokeimenon.v1.text = "Τοῖς ἁγίοις τοῖς ἐν τῇ γῇ αὐτοῦ ἐθαυμάστωσεν ὁ Κύριος."
psLI.m07.d01.prokeimenon.v2.info = ps_gr_GR_cog.psa15.v8.number
psLI.m07.d01.prokeimenon.v2.text = "Προωρώμην τὸν Κύριον ἐνώπιόν μου διαπαντός."
psLI.m07.d01.alleluia.v1.info = ps_gr_GR_cog.psa132.v1.number
psLI.m07.d01.alleluia.v1.text = "Ἰδοὺ δὴ τί καλὸν ἢ τί τερπνόν, ἀλλ' ἢ τὸ κατοικεῖν ἀδελφοὺς ἐπὶ τὸ αὐτό;"
psLI.m07.d01.alleluia.v2.info = ps_gr_GR_cog.psa132.v3.number
psLI.m07.d01.alleluia.v2.text = "Ὅτι ἐκεῖ ἐνετείλατο Κύριος τὴν εὐλογίαν, ζωὴν ἕως τοῦ αἰῶνος."

psLI.m07.d02.prokeimenon.v1.info = ps_gr_GR_cog.bode9_mag.v1.number //"Ὠδὴ 9a:1"
psLI.m07.d02.prokeimenon.v1.text = "Μεγαλύνει ἡ ψυχή μου τὸν Κύριον, καὶ ἠγαλλίασε τὸ πνεῦμά μου ἐπὶ τῷ Θεῷ τῷ σωτῆρί μου."
psLI.m07.d02.prokeimenon.v2.info = ps_gr_GR_cog.bode9_mag.v1.number //"Ὠδὴ 9a:2"
psLI.m07.d02.prokeimenon.v2.text = "Ὅτι ἐπέβλεψεν ἐπὶ τὴν ταπείνωσιν τῆς δούλης αὐτοῦ· ἰδού γάρ, ἀπὸ τοῦ νῦν μακαριοῦσί με πᾶσαι αἱ γενεαί."
psLI.m07.d02.alleluia.v1.info = ps_gr_GR_cog.psa44.v11.number
psLI.m07.d02.alleluia.v1.text = "Ἄκουσον, θύγατερ, καὶ ἴδε καὶ κλῖνον τὸ οὖς σου."
psLI.m07.d02.alleluia.v2.info = ps_gr_GR_cog.psa44.v13.number
psLI.m07.d02.alleluia.v2.text = "Τὸ πρόσωπόν σου λιτανεύσουσιν οἱ πλούσιοι τοῦ λαοῦ."

psLI.m07.d05.prokeimenon.v1.info = ps_gr_GR_cog.psa149.v5.number
psLI.m07.d05.prokeimenon.v1.text = "Καυχήσονται ὅσιοι ἐν δόξῃ."
psLI.m07.d05.prokeimenon.v2.info = ps_gr_GR_cog.psa149.v1.number
psLI.m07.d05.prokeimenon.v2.text = "Ἄσατε τῷ Κυρίῳ ᾆσμα καινόν."
psLI.m07.d05.alleluia.v1.info = ps_gr_GR_cog.psa111.v1.number
psLI.m07.d05.alleluia.v1.text = "Μακάριος ἀνὴρ ὁ φοβούμενος τὸν Κύριον."
psLI.m07.d05.alleluia.v2.info = ps_gr_GR_cog.psa111.v9.number
psLI.m07.d05.alleluia.v2.text = "Ἡ δικαιοσύνη αὐτοῦ μένει εἰς τὸν αἰῶνα τοῦ αἰῶνος."

psLI.m07.d09.prokeimenon.v1.info = ps_gr_GR_cog.psa149.v5.number
psLI.m07.d09.prokeimenon.v1.text = "Καυχήσονται ὅσιοι ἐν δόξῃ."
psLI.m07.d09.prokeimenon.v2.info = ps_gr_GR_cog.psa149.v1.number
psLI.m07.d09.prokeimenon.v2.text = "Ἄσατε τῷ Κυρίῳ ᾆσμα καινόν."
psLI.m07.d09.alleluia.v1.info = ps_gr_GR_cog.psa131.v9.number
psLI.m07.d09.alleluia.v1.text = "Οἱ ἱερεῖς σου, Κύριε, ἐνδύσονται δικαιοσύνην."
psLI.m07.d09.alleluia.v2.info = ps_gr_GR_cog.psa131.v13.number
psLI.m07.d09.alleluia.v2.text = "Ὅτι ἐξελέξατο Κύριος τὴν Σιών."

psLI.m07.d13.prokeimenon.v1.info = ps_gr_GR_cog.psa103.v4.number
psLI.m07.d13.prokeimenon.v1.text = "Ὁ ποιῶν τοὺς ἀγγέλους αὐτοῦ πνεύματα."
psLI.m07.d13.prokeimenon.v2.info = ps_gr_GR_cog.psa103.v1.number
psLI.m07.d13.prokeimenon.v2.text = "Εὐλόγει, ἡ ψυχή μου, τὸν Κύριον."
psLI.m07.d13.alleluia.v1.info = ps_gr_GR_cog.psa148.v1.number
psLI.m07.d13.alleluia.v1.text = "Αἰνεῖτε τὸν Κύριον ἐκ τῶν οὐρανῶν."
psLI.m07.d13.alleluia.v2.info = ps_gr_GR_cog.psa148.v2.number
psLI.m07.d13.alleluia.v2.text = "Αἰνεῖτε αὐτόν, πάντες οἱ ἄγγελοι αὐτοῦ."

psLI.m07.dHF.prokeimenon.v1.info = ps_gr_GR_cog.psa31.v11.number
psLI.m07.dHF.prokeimenon.v1.text = "Εὐφράνθητε ἐπὶ Κύριον καὶ ἀγαλλιᾶσθε, δίκαιοι."
psLI.m07.dHF.prokeimenon.v2.info = ps_gr_GR_cog.psa31.v1.number
psLI.m07.dHF.prokeimenon.v2.text = "Μακάριοι ὧν ἀφέθησαν αἱ ἀνομίαι."
psLI.m07.dHF.alleluia.v1.info = ps_gr_GR_cog.psa43.v2.number
psLI.m07.dHF.alleluia.v1.text = "Ὁ Θεός, ἐν τοῖς ὠσὶν ἡμῶν ἠκούσαμεν"
psLI.m07.dHF.alleluia.v2.info = ps_gr_GR_cog.psa43.v8.number
psLI.m07.dHF.alleluia.v2.text = "Ἔσωσας γὰρ ἡμᾶς ἐκ τῶν θλιβόντων ἡμᾶς."

psLI.m07.d15.prokeimenon.v1.info = ps_gr_GR_cog.psa67.v36.number
psLI.m07.d15.prokeimenon.v1.text = "Θαυμαστὸς ὁ Θεὸς ἐν τοῖς ἁγίοις αὐτοῦ."
psLI.m07.d15.prokeimenon.v2.info = ps_gr_GR_cog.psa67.v27.number
psLI.m07.d15.prokeimenon.v2.text = "Ἐν ἐκκλησίαις εὐλογεῖτε τὸν Θεόν."
psLI.m07.d15.alleluia.v1.info = ps_gr_GR_cog.psa33.v18.number
psLI.m07.d15.alleluia.v1.text = "Ἐκέκραξαν οἱ δίκαιοι, καὶ ὁ Κύριος εἰσήκουσεν αὐτῶν."
psLI.m07.d15.alleluia.v2.info = ps_gr_GR_cog.psa33.v20.number
psLI.m07.d15.alleluia.v2.text = "Πολλαὶ αἱ θλίψεις τῶν δικαίων."

psLI.m07.d20.prokeimenon.v1.info = ps_gr_GR_cog.psa109.v4.number
psLI.m07.d20.prokeimenon.v1.text = "Σὺ ἱερεὺς εἰς τὸν αἰῶνα κατὰ τὴν τάξιν Μελχισεδέκ."
psLI.m07.d20.prokeimenon.v2.info = ps_gr_GR_cog.psa109.v1.number
psLI.m07.d20.prokeimenon.v2.text = "Εἶπεν ὁ Κύριος τῷ Κυρίῳ μου· Κάθου ἐκ δεξιῶν μου."
psLI.m07.d20.alleluia.v1.info = ps_gr_GR_cog.psa98.v6.number
psLI.m07.d20.alleluia.v1.text = "Μωυσῆς καὶ Ἀαρὼν ἐν τοῖς ἱερεῦσιν αὐτοῦ."
psLI.m07.d20.alleluia.v2.info = ps_gr_GR_cog.psa33.v18.number
psLI.m07.d20.alleluia.v2.text = "Ἐκέκραξαν οἱ δίκαιοι, καὶ ὁ Κύριος εἰσήκουσεν αὐτῶν."

psLI.m07.d21.prokeimenon.v1.info = ps_gr_GR_cog.psa39.v2.number
psLI.m07.d21.prokeimenon.v1.text = "Ὑπομένων ὑπέμεινα τὸν Κύριον."
psLI.m07.d21.prokeimenon.v2.info = ps_gr_GR_cog.psa33.v20.number
psLI.m07.d21.prokeimenon.v2.text = "Πολλαὶ αἱ θλίψεις τῶν δικαίων."
psLI.m07.d21.alleluia.v1.info = ps_gr_GR_cog.psa111.v1.number
psLI.m07.d21.alleluia.v1.text = "Μακάριος ἀνὴρ ὁ φοβούμενος τὸν Κύριον."
psLI.m07.d21.alleluia.v2.info = ps_gr_GR_cog.psa111.v9.number
psLI.m07.d21.alleluia.v2.text = "Ἡ δικαιοσύνη αὐτοῦ μένει εἰς τὸν αἰῶνα."

psLI.m07.d22.prokeimenon.v1.info = ps_gr_GR_cog.psa67.v36.number
psLI.m07.d22.prokeimenon.v1.text = "Θαυμαστὸς ὁ Θεὸς ἐν τοῖς ἁγίοις αὐτοῦ."
psLI.m07.d22.prokeimenon.v2.info = ps_gr_GR_cog.psa67.v27.number
psLI.m07.d22.prokeimenon.v2.text = "Ἐν ἐκκλησίαις εὐλογεῖτε τὸν Θεόν."
psLI.m07.d22.alleluia.v1.info = ps_gr_GR_cog.psa39.v2.number
psLI.m07.d22.alleluia.v1.text = "Ὑπομένων ὑπέμεινα τὸν Κύριον."
psLI.m07.d22.alleluia.v2.info = ps_gr_GR_cog.psa33.v20.number
psLI.m07.d22.alleluia.v2.text = "Πολλαὶ αἱ θλίψεις τῶν δικαίων."

psLI.m07.d25.prokeimenon.v1.info = ps_gr_GR_cog.psa67.v36.number
psLI.m07.d25.prokeimenon.v1.text = "Θαυμαστὸς ὁ Θεὸς ἐν τοῖς ἁγίοις αὐτοῦ."
psLI.m07.d25.prokeimenon.v2.info = ps_gr_GR_cog.psa67.v27.number
psLI.m07.d25.prokeimenon.v2.text = "Ἐν ἐκκλησίαις εὐλογεῖτε τὸν Θεόν."
psLI.m07.d25.alleluia.v1.info = ps_gr_GR_cog.psa39.v2.number
psLI.m07.d25.alleluia.v1.text = "Ὑπομένων ὑπέμεινα τὸν Κύριον."
psLI.m07.d25.alleluia.v2.info = ps_gr_GR_cog.psa33.v20.number
psLI.m07.d25.alleluia.v2.text = "Πολλαὶ αἱ θλίψεις τῶν δικαίων."

psLI.m07.d26a.prokeimenon.v1.info = ps_gr_GR_cog.psa115.v3.number
psLI.m07.d26a.prokeimenon.v1.text = "Τί ἀνταποδώσω τῷ Κυρίῳ περὶ πάντων, ὧν ἀνταπέδωκέ μοι;"
psLI.m07.d26a.prokeimenon.v2.info = ps_gr_GR_cog.psa115.v5.number
psLI.m07.d26a.prokeimenon.v2.text = "Τίμιος ἐναντίον Κυρίου ὁ θάνατος τῶν ὁσίων αὐτοῦ."
psLI.m07.d26a.alleluia.v1.info = ps_gr_GR_cog.psa131.v9.number
psLI.m07.d26a.alleluia.v1.text = "Οἱ ἱερεῖς σου, Κύριε, ἐνδύσονται δικαιοσύνην"
psLI.m07.d26a.alleluia.v2.info = ps_gr_GR_cog.psa131.v13.number
psLI.m07.d26a.alleluia.v2.text = "Ὅτι ἐξελέξατο Κύριος τὴν Σιών"

psLI.m07.d26b.prokeimenon.v1.info = ps_gr_GR_cog.psa67.v36.number
psLI.m07.d26b.prokeimenon.v1.text = "Θαυμαστὸς ὁ Θεὸς ἐν τοῖς ἁγίοις αὐτοῦ."
psLI.m07.d26b.prokeimenon.v2.info = ps_gr_GR_cog.psa67.v27.number
psLI.m07.d26b.prokeimenon.v2.text = "Ἐν ἐκκλησίαις εὐλογεῖτε τὸν Θεόν."
psLI.m07.d26b.alleluia.v1.info = ps_gr_GR_cog.psa39.v2.number
psLI.m07.d26b.alleluia.v1.text = "Ὑπομένων ὑπέμεινα τὸν Κύριον."
psLI.m07.d26b.alleluia.v2.info = ps_gr_GR_cog.psa39.v3.number
psLI.m07.d26b.alleluia.v2.text = "Καὶ ἔστησεν ἐπὶ πέτραν τοὺς πόδας μου."

psLI.m07.d27.prokeimenon.v1.info = ps_gr_GR_cog.psa63.v11.number
psLI.m07.d27.prokeimenon.v1.text = "Εὐφρανθήσεται δίκαιος ἐν Κυρίῳ."
psLI.m07.d27.prokeimenon.v2.info = ps_gr_GR_cog.psa63.v2.number
psLI.m07.d27.prokeimenon.v2.text = "Εἰσάκουσον, ὁ Θεός, τῆς φωνῆς μου."
psLI.m07.d27.alleluia.v1.info = ps_gr_GR_cog.psa91.v13.number
psLI.m07.d27.alleluia.v1.text = "Δίκαιος ὡς φοῖνιξ ἀνθήσει."
psLI.m07.d27.alleluia.v2.info = ps_gr_GR_cog.psa91.v14.number
psLI.m07.d27.alleluia.v2.text = "Πεφυτευμένος ἐν τῷ οἴκῳ Κυρίου."

psLI.m07.d28.prokeimenon.v1.info = ps_gr_GR_cog.psa18.v5.number
psLI.m07.d28.prokeimenon.v1.text = "Εἰς πᾶσαν τὴν γῆν ἐξῆλθεν ὁ φθόγγος αὐτῶν."
psLI.m07.d28.prokeimenon.v2.info = ps_gr_GR_cog.psa18.v2.number
psLI.m07.d28.prokeimenon.v2.text = "Οἱ οὐρανοὶ διηγοῦνται δόξαν Θεοῦ."
psLI.m07.d28.alleluia.v1.info = ps_gr_GR_cog.psa88.v6.number
psLI.m07.d28.alleluia.v1.text = "Ἐξομολογήσονται οἱ οὐρανοὶ τὰ θαυμάσιά σου, Κύριε."
psLI.m07.d28.alleluia.v2.info = ps_gr_GR_cog.psa88.v8.number
psLI.m07.d28.alleluia.v2.text = "Ὁ Θεὸς ἐνδοξαζόμενος ἐν βουλῇ ἁγίων."

psLI.m07.d29.prokeimenon.v1.info = ps_gr_GR_cog.psa63.v11.number
psLI.m07.d29.prokeimenon.v1.text = "Εὐφρανθήσεται δίκαιος ἐν Κυρίῳ."
psLI.m07.d29.prokeimenon.v2.info = ps_gr_GR_cog.psa63.v2.number
psLI.m07.d29.prokeimenon.v2.text = "Εἰσάκουσον, ὁ Θεός, τῆς φωνῆς μου."


//August

psLI.m08.d01.prokeimenon.v1.info = ps_gr_GR_cog.psa67.v36.number
psLI.m08.d01.prokeimenon.v1.text = "Θαυμαστὸς ὁ Θεὸς ἐν τοῖς ἁγίοις αὐτοῦ."
psLI.m08.d01.prokeimenon.v2.info = ps_gr_GR_cog.psa67.v27.number
psLI.m08.d01.prokeimenon.v2.text = "Ἐν ἐκκλησίαις εὐλογεῖτε τὸν Θεόν."
psLI.m08.d01.alleluia.v1.info = ps_gr_GR_cog.psa33.v18.number
psLI.m08.d01.alleluia.v1.text = "Ἐκέκραξαν οἱ δίκαιοι, καὶ ὁ Κύριος εἰσήκουσεν αὐτῶν."
psLI.m08.d01.alleluia.v2.info = ps_gr_GR_cog.psa33.v20.number
psLI.m08.d01.alleluia.v2.text = "Πολλαὶ αἱ θλίψεις τῶν δικαίων."

psLI.m08.d06.antiphon1.v1.info = ps_gr_GR_cog.psa47.v2.number //"47:2"
psLI.m08.d06.antiphon1.v1.text = "Μέγας Κύριος καὶ αἰνετὸς σφόδρα ἐν πόλει τοῦ Θεοῦ ἡμῶν, ἐν ὄρει ἁγίῳ αὐτοῦ."
psLI.m08.d06.antiphon1.v2.info = ps_gr_GR_cog.psa64.v7.number //"64:7"
psLI.m08.d06.antiphon1.v2.text = "Ἑτοιμάζων ὄρη ἐν τῇ ἰσχύϊ αὐτοῦ, περιεζωσμένος ἐν δυναστείᾳ."
psLI.m08.d06.antiphon1.v3.info = ps_gr_GR_cog.psa103.v1.number //"103:1-2"
psLI.m08.d06.antiphon1.v3.text = "Ἐξομολόγησιν καὶ μεγαλοπρέπειαν ἐνεδύσω, ἀναβαλλόμενος φῶς ὡς ἱμάτιον."
psLI.m08.d06.antiphon1.v4.info = ps_gr_GR_cog.psa97.v8.number //"97:8"
psLI.m08.d06.antiphon1.v4.text = "Τὰ ὄρη ἀγαλλιάσονται ἀπὸ προσώπου Κυρίου."
psLI.m08.d06.antiphon2.v1.info = ps_gr_GR_cog.psa86.v1.number //"86:1"
psLI.m08.d06.antiphon2.v1.text = "Οἱ θεμέλιοι αὐτοῦ ἐν τοῖς ὄρεσι τοῖς ἁγίοις."
psLI.m08.d06.antiphon2.v2.info = ps_gr_GR_cog.psa86.v2.number //"86:2"
psLI.m08.d06.antiphon2.v2.text = "Ἀγαπᾷ Κύριος τὰς πύλας Σιών, ὑπὲρ πάντα τὰ σκηνώματα Ἰακώβ."
psLI.m08.d06.antiphon2.v3.info = ps_gr_GR_cog.psa86.v3.number //"86:3"
psLI.m08.d06.antiphon2.v3.text = "Δεδοξασμένα ἐλαλήθη περὶ, σοῦ ἡ πόλις τοῦ Θεοῦ."
psLI.m08.d06.antiphon2.v4.info = ps_gr_GR_cog.psa86.v5.number //"86:5"
psLI.m08.d06.antiphon2.v4.text = "Μήτηρ Σιών, ἐρεῖ ἄνθρωπος, καὶ ἄνθρωπος ἐγενήθη ἐν αὐτῇ."
psLI.m08.d06.antiphon3.v1.info = ps_gr_GR_cog.psa88.v2.number //"88:2"
psLI.m08.d06.antiphon3.v1.text = "Τὰ ἐλέη σου, Κύριε, εἰς τὸν αἰῶνα ᾄσομαι."
psLI.m08.d06.antiphon3.v2.info = ps_gr_GR_cog.psa88.v6.number //"88:6"
psLI.m08.d06.antiphon3.v2.text = "Ἐξομολογήσονται οἱ οὐρανοὶ τὰ θαυμάσιά σου, Κύριε."
psLI.m08.d06.antiphon3.v3.info = ps_gr_GR_cog.psa88.v16.number //"88:16"
psLI.m08.d06.antiphon3.v3.text = "Μακάριος ὁ λαὸς ὁ γινώσκων ἀλαλαγμόν."
psLI.m08.d06.antiphon3.v4.info = ps_gr_GR_cog.psa88.v16.number //"88:16-17"
psLI.m08.d06.antiphon3.v4.text = "Κύριε, ἐν τῷ φωτὶ τοῦ προσώπου σου πορεύσονται, καὶ ἐν τῷ ὀνόματί σου ἀγαλλιάσονται ὅλην τὴν ἡμέραν."
psLI.m08.d06.entrance.v1.info = ps_gr_GR_cog.psa35.v10.number //"35:10"
psLI.m08.d06.entrance.v1.text = "Ὅτι παρὰ σοὶ πηγὴ ζωῆς, Κύριε, ἐν τῷ φωτί σου ὀψόμεθα φῶς."
psLI.m08.d06.entrance.v2.info = ps_gr_GR_cog.psa88.v13.number //"88:13"
psLI.m08.d06.entrance.v2.text = "Θαβὼρ καὶ Ἑρμὼν ἐν τῷ ὀνόματί σου ἀγαλλιάσονται."
psLI.m08.d06.prokeimenon.v1.info = ps_gr_GR_cog.psa103.v24.number //"103:24"
psLI.m08.d06.prokeimenon.v1.text = "Ὡς ἐμεγαλύνθη τὰ ἔργα σου, Κύριε· πάντα ἐν σοφίᾳ ἐποίησας."
psLI.m08.d06.prokeimenon.v2.info = ps_gr_GR_cog.psa103.v1.number //"103:1"
psLI.m08.d06.prokeimenon.v2.text = "Εὐλόγει, ἡ ψυχή μου, τὸν Κύριον. Κύριε ὁ Θεός μου, ἐμεγαλύνθης σφόδρα."
psLI.m08.d06.alleluia.v1.info = ps_gr_GR_cog.psa88.v12.number //"88:12"
psLI.m08.d06.alleluia.v1.text = "Σοί εἰσιν οἱ οὐρανοί, καὶ σή ἐστιν ἡ γῆ· τὴν οἰκουμένην καὶ τὸ πλήρωμα αὐτῆς σὺ ἐθεμελίωσας."
psLI.m08.d06.alleluia.v2.info = ps_gr_GR_cog.psa143.v15.number //"143:15"
psLI.m08.d06.alleluia.v2.text = "Μακάριος ὁ λαός, οὗ Κύριος ὁ Θεὸς αὐτοῦ."
psLI.m08.d06.communion.v1.info = ps_gr_GR_cog.psa88.v16.number //"88:16"
psLI.m08.d06.communion.v1.text = "Ἐν τῷ φωτὶ τῆς δόξης τοῦ προσώπου σου, Κύριε, πορευσόμεθα εἰς τὸν αἰῶνα."

psLI.m08.d09.prokeimenon.v1.info = ps_gr_GR_cog.psa18.v5.number
psLI.m08.d09.prokeimenon.v1.text = "Εἰς πᾶσαν τὴν γῆν ἐξῆλθεν ὁ φθόγγος αὐτοῦ."
psLI.m08.d09.prokeimenon.v2.info = ps_gr_GR_cog.psa18.v2.number
psLI.m08.d09.prokeimenon.v2.text = "Οἱ οὐρανοὶ διηγοῦνται δόξαν Θεοῦ."
psLI.m08.d09.alleluia.v1.info = ps_gr_GR_cog.psa88.v6.number
psLI.m08.d09.alleluia.v1.text = "Ἐξομολογήσονται οἱ οὐρανοὶ τὰ θαυμάσιά σου, Κύριε."
psLI.m08.d09.alleluia.v2.info = ps_gr_GR_cog.psa88.v8.number
psLI.m08.d09.alleluia.v2.text = "Ὁ Θεὸς ἐνδοξαζόμενος ἐν βουλῇ ἁγίων."

psLI.m08.d15.antiphon1.v1.info = ps_gr_GR_cog.psa99.v1.number //"99:1"
psLI.m08.d15.antiphon1.v1.text = "Ἀλαλάξατε τῷ Κυρίῳ πᾶσα ἡ γῆ."
psLI.m08.d15.antiphon1.v2.info = ps_gr_GR_cog.psa99.v4.number //"99:4"
psLI.m08.d15.antiphon1.v2.text = "Ἐξομολογεῖσθε αὐτῷ, αἰνεῖτε τὸ ὄνομα αὐτοῦ."
psLI.m08.d15.antiphon1.v3.info = ps_gr_GR_cog.psa47.v9.number //"47:9"
psLI.m08.d15.antiphon1.v3.text = "Ἐν πόλει Κυρίου τῶν δυνάμεων, ἐν πόλει τοῦ Θεοῦ ἡμῶν."
psLI.m08.d15.antiphon1.v4.info = ps_gr_GR_cog.psa75.v3.number //"75:3"
psLI.m08.d15.antiphon1.v4.text = "Ἐγενήθη ἐν εἰρήνῃ ὁ τόπος αὐτοῦ, καὶ τὸ κατοικητήριον αὐτοῦ ἐν Σιών."
psLI.m08.d15.antiphon2.v1.info = ps_gr_GR_cog.psa86.v2.number //"86:2"
psLI.m08.d15.antiphon2.v1.text = "Ἀγαπᾷ Κύριος τὰς πύλας Σιών, ὑπὲρ πάντα τὰ σκηνώματα Ἰακώβ."
psLI.m08.d15.antiphon2.v2.info = ps_gr_GR_cog.psa86.v3.number //"86:3"
psLI.m08.d15.antiphon2.v2.text = "Δεδοξασμένα ἐλαλήθη περὶ σοῦ ἡ πόλις τοῦ Θεοῦ."
psLI.m08.d15.antiphon2.v3.info = ps_gr_GR_cog.psa47.v9.number //"47:9"
psLI.m08.d15.antiphon2.v3.text = "Ὁ Θεὸς ἐθεμελίωσεν αὐτὴν εἰς τὸν αἰῶνα."
psLI.m08.d15.antiphon2.v4.info = ps_gr_GR_cog.psa45.v5.number //"45:5"
psLI.m08.d15.antiphon2.v4.text = "Ἡγίασε τὸ σκήνωμα αὐτοῦ ὁ Ὕψιστος."
psLI.m08.d15.antiphon3.v1.info = ps_gr_GR_cog.psa107.v2.number //"107:2"
psLI.m08.d15.antiphon3.v1.text = "Ἑτοίμη ἡ καρδία μου, ὁ Θεός, ἑτοίμη ἡ καρδία μου."
psLI.m08.d15.antiphon3.v2.info = ps_gr_GR_cog.psa115.v3.number //"115:3"
psLI.m08.d15.antiphon3.v2.text = "Τί ἀνταποδώσω τῷ Κυρίῳ περὶ πάντων, ὧν ἀνταπέδωκέ μοι;"
psLI.m08.d15.antiphon3.v3.info = ps_gr_GR_cog.psa115.v4.number //"115:4"
psLI.m08.d15.antiphon3.v3.text = "Ποτήριον σωτηρίου λήψομαι, καὶ τὸ ὄνομα Κυρίου ἐπικαλέσομαι."
psLI.m08.d15.prokeimenon.v1.info = ps_gr_GR_cog.bode9_mag.v1.number //"Ὠδὴ 9a:1"
psLI.m08.d15.prokeimenon.v1.text = "Μεγαλύνει ἡ ψυχή μου τὸν Κύριον, καὶ ἠγαλλίασε τὸ πνεῦμά μου ἐπὶ τῷ Θεῷ τῷ σωτῆρί μου."
psLI.m08.d15.prokeimenon.v2.info = ps_gr_GR_cog.bode9_mag.v2.number //"Ὠδὴ 9a:2"
psLI.m08.d15.prokeimenon.v2.text = "Ὅτι ἐπέβλεψεν ἐπὶ τὴν ταπείνωσιν τῆς δούλης αὐτοῦ· ἰδού γάρ, ἀπὸ τοῦ νῦν μακαριοῦσί με πᾶσαι αἱ γενεαί."
psLI.m08.d15.alleluia.v1.info = ps_gr_GR_cog.psa131.v8.number //"131:8"
psLI.m08.d15.alleluia.v1.text = "Ἀνάστηθι, Κύριε, εἰς τὴν ἀνάπαυσίν σου, σὺ καὶ ἡ κιβωτὸς τοῦ ἁγιάσματός σου."
psLI.m08.d15.alleluia.v2.info = ps_gr_GR_cog.psa131.v11.number //"131:11"
psLI.m08.d15.alleluia.v2.text = "Ὤμοσε Κύριος τῷ Δαυῒδ ἀλήθειαν καὶ οὐ μὴ ἀθετήσει αὐτήν· Ἐκ καρποῦ τῆς κοιλίας σου θήσομαι ἐπὶ τοῦ θρόνου σου."
psLI.m08.d15.communion.v1.info = ps_gr_GR_cog.psa115.v4.number //"115:4"
psLI.m08.d15.communion.v1.text = "Ποτήριον σωτηρίου λήψομαι, καὶ τὸ ὄνομα Κυρίου ἐπικαλέσομαι."

psLI.m08.d16.prokeimenon.v1.info = ps_gr_GR_cog.psa68.v18.number
psLI.m08.d16.prokeimenon.v1.text = "Μὴ ἀποστρέψῃς τὸ πρόσωπόν σου ἀπὸ τοῦ παιδός σου"
psLI.m08.d16.prokeimenon.v2.info = ps_gr_GR_cog.psa68.v30.number
psLI.m08.d16.prokeimenon.v2.text = "Ἡ σωτηρία σου, ὁ Θεός, ἀντιλάβοιτό μου."
psLI.m08.d16.alleluia.v1.info = ps_gr_GR_cog.psa64.v2.number
psLI.m08.d16.alleluia.v1.text = "Σοὶ πρέπει ὕμνος, ὁ Θεός, ἐν Σιών."
psLI.m08.d16.alleluia.v2.info = ps_gr_GR_cog.psa64.v5.number
psLI.m08.d16.alleluia.v2.text = "Πλησθησόμεθα ἐν τοῖς ἀγαθοῖς τοῦ οἴκου σου."

psLI.m08.d18.prokeimenon.v1.info = ps_gr_GR_cog.psa67.v35.number
psLI.m08.d18.prokeimenon.v1.text = "Θαυμαστὸς ὁ Θεὸς ἐν τοῖς ἁγίοις αὐτοῦ."
psLI.m08.d18.prokeimenon.v2.info = ps_gr_GR_cog.psa67.v27.number
psLI.m08.d18.prokeimenon.v2.text = "Ἐν ἐκκλησίαις εὐλογεῖτε τὸν Θεόν."
psLI.m08.d18.alleluia.v1.info = ps_gr_GR_cog.psa33.v18.number
psLI.m08.d18.alleluia.v1.text = "Ἐκέκραξαν οἱ δίκαιοι, καὶ ὁ Κύριος εἰσήκουσεν αὐτῶν."
psLI.m08.d18.alleluia.v2.info = ps_gr_GR_cog.psa111.v1.number
psLI.m08.d18.alleluia.v2.text = "Μακάριος ἀνὴρ ὁ φοβούμενος τὸν Κύριον"

psLI.m08.d20.prokeimenon.v1.info = ps_gr_GR_cog.psa109.v4.number
psLI.m08.d20.prokeimenon.v1.text = "Σὺ ἱερεὺς εἰς τὸν αἰῶνα κατὰ τὴν τάξιν Μελχισεδέκ."
psLI.m08.d20.prokeimenon.v2.info = ps_gr_GR_cog.psa109.v1.number
psLI.m08.d20.prokeimenon.v2.text = "Εἶπεν ὁ Κύριος τῷ Κυρίῳ μου· Κάθου ἐκ δεξιῶν μου."
psLI.m08.d20.alleluia.v1.info = ps_gr_GR_cog.psa98.v6.number
psLI.m08.d20.alleluia.v1.text = " Μωυσῆς καὶ Ἀαρὼν ἐν τοῖς ἱερεῦσιν αὐτοῦ."
psLI.m08.d20.alleluia.v2.info = ps_gr_GR_cog.psa33.v18.number
psLI.m08.d20.alleluia.v2.text = "Ἐκέκραξαν οἱ δίκαιοι, καὶ ὁ Κύριος εἰσήκουσεν αὐτῶν"

psLI.m08.d25.prokeimenon.v1.info = ps_gr_GR_cog.psa18.v5.number
psLI.m08.d25.prokeimenon.v1.text = "Εἰς πᾶσαν τὴν γῆν ἐξῆλθεν ὁ φθόγγος αὐτοῦ."
psLI.m08.d25.prokeimenon.v2.info = ps_gr_GR_cog.psa18.v2.number
psLI.m08.d25.prokeimenon.v2.text = "Οἱ οὐρανοὶ διηγοῦνται δόξαν Θεοῦ."
psLI.m08.d25.alleluia.v1.info = ps_gr_GR_cog.psa88.v6.number
psLI.m08.d25.alleluia.v1.text = "Ἐξομολογήσονται οἱ οὐρανοὶ τὰ θαυμάσιά σου, Κύριε."
psLI.m08.d25.alleluia.v2.info = ps_gr_GR_cog.psa88.v8.number
psLI.m08.d25.alleluia.v2.text = "Ὁ Θεὸς ἐνδοξαζόμενος ἐν βουλῇ ἁγίων."

psLI.m08.d26.prokeimenon.v1.info = ps_gr_GR_cog.psa67.v36.number
psLI.m08.d26.prokeimenon.v1.text = "Θαυμαστὸς ὁ Θεὸς ἐν τοῖς ἁγίοις αὐτοῦ."
psLI.m08.d26.prokeimenon.v2.info = ps_gr_GR_cog.psa67.v27.number
psLI.m08.d26.prokeimenon.v2.text = "Ἐν ἐκκλησίαις εὐλογεῖτε τὸν Θεόν."
psLI.m08.d26.alleluia.v1.info = ps_gr_GR_cog.psa33.v18.number
psLI.m08.d26.alleluia.v1.text = "Ἐκέκραξαν οἱ δίκαιοι, καὶ ὁ Κύριος εἰσήκουσεν αὐτῶν."
psLI.m08.d26.alleluia.v2.info = ps_gr_GR_cog.psa33.v20.number
psLI.m08.d26.alleluia.v2.text = "Πολλαὶ αἱ θλίψεις τῶν δικαίων."

psLI.m08.d29.prokeimenon.v1.info = ps_gr_GR_cog.psa63.v11.number
psLI.m08.d29.prokeimenon.v1.text = "Εὐφρανθήσεται δίκαιος ἐν Κυρίῳ."
psLI.m08.d29.prokeimenon.v2.info = ps_gr_GR_cog.psa63.v2.number
psLI.m08.d29.prokeimenon.v2.text = "Εἰσάκουσον, ὁ Θεός, τῆς φωνῆς μου."
psLI.m08.d29.alleluia.v1.info = ps_gr_GR_cog.psa91.v13.number
psLI.m08.d29.alleluia.v1.text = "Δίκαιος ὡς φοῖνιξ ἀνθήσει."
psLI.m08.d29.alleluia.v2.info = ps_gr_GR_cog.psa91.v14.number
psLI.m08.d29.alleluia.v2.text = "Πεφυτευμένος ἐν τῷ οἴκῳ Κυρίου."

psLI.m08.d31.prokeimenon.v1.info = ps_gr_GR_cog.bode9_mag.v1.number //"Ὠδὴ 9a:1"
psLI.m08.d31.prokeimenon.v1.text = "Μεγαλύνει ἡ ψυχή μου τὸν Κύριον, καὶ ἠγαλλίασε τὸ πνεῦμά μου ἐπὶ τῷ Θεῷ τῷ σωτῆρί μου."
psLI.m08.d31.prokeimenon.v2.info = ps_gr_GR_cog.bode9_mag.v2.number //"Ὠδὴ 9a:2"
psLI.m08.d31.prokeimenon.v2.text = "Ὅτι ἐπέβλεψεν ἐπὶ τὴν ταπείνωσιν τῆς δούλης αὐτοῦ· ἰδού γάρ, ἀπὸ τοῦ νῦν μακαριοῦσί με πᾶσαι αἱ γενεαί."
psLI.m08.d31.alleluia.v1.info = ps_gr_GR_cog.psa131.v8.number //"131:8"
psLI.m08.d31.alleluia.v1.text = "Ἀνάστηθι, Κύριε, εἰς τὴν ἀνάπαυσίν σου, σὺ καὶ ἡ κιβωτὸς τοῦ ἁγιάσματός σου."
psLI.m08.d31.alleluia.v2.info = ps_gr_GR_cog.psa44.v11.number //"44:11"
psLI.m08.d31.alleluia.v2.text = "Ἄκουσον, θύγατερ, καὶ ἴδε καὶ κλῖνον τὸ οὖς σου καὶ ἐπιλάθου τοῦ λαοῦ σου καὶ τοῦ οἴκου τοῦ πατρός σου."

