meMA.Ode1C11.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode1C11.greekmelody = he.h.m4_gr_GR_cog.heHE.ThalassisToErythraion.greekname
meMA.Ode1C11.melody = he.h.m4_gr_GR_cog.heHE.ThalassisToErythraion.name
meMA.Ode1C11.incipit = "Θεόθεν τὴν φωτοδότιν Κύριλλε, χάριν…"
meMA.Ode1C11.text = "Θεόθεν τὴν φωτοδότιν Κύριλλε, χάριν δεξάμενος, καὶ τηλαυγὴς φωστὴρ ἀναδειχθείς, λογικὰς ἀπαστράψαις ἡμῖν, μαρμαρυγάς, τὴν μνήμην σου, ὅπως ἀξίως ἀνυμνήσωμεν."

meMA.Ode1C12.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode1C12.melody = he.h.m4_gr_GR_cog.heHE.ThalassisToErythraion.name
meMA.Ode1C12.incipit = "Ἐπλήσθης τῆς δᾳδουχίας Κύριλλε, τοῦ…"
meMA.Ode1C12.text = "Ἐπλήσθης τῆς δᾳδουχίας Κύριλλε, τοῦ θείου Πνεύματος, τῶν ἐντολῶν ὡς φύλαξ τοῦ Χριστοῦ, καὶ παθῶν καθηράμενος, τῆς ὑπερθέου γέγονας, ὄντως Τριάδος ἐνδιαίτημα."

meMA.Ode1C13.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode1C13.melody = he.h.m4_gr_GR_cog.heHE.ThalassisToErythraion.name
meMA.Ode1C13.incipit = "Ὡς ζήλου καὶ παῤῥησίας ἔμπλεως…"
meMA.Ode1C13.text = "Ὡς ζήλου καὶ παῤῥησίας ἔμπλεως, Κύριλλε ἔνδοξε, τὰς θεομάχους πάσας ἀψευδῶς, γλωσσαλγίας διέλυσας, τῶν δυσσεβῶν αἱρέσεων, ὀρθοδοξίας προϊστάμενος."

meMA.Ode1C1T.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode1C1T.melody = he.h.m4_gr_GR_cog.heHE.ThalassisToErythraion.name
meMA.Ode1C1T.incipit = "Ἰάσω τὴν συντριβὴν Πανάμωμε, τῆς…"
meMA.Ode1C1T.text = "Ἰάσω τὴν συντριβὴν Πανάμωμε, τῆς ἀνθρωπότητος, τὸν ποιητήν τεκοῦσα τοῦ παντός, τὸν ἡμᾶς ἀνορθώσαντα, καταῤῥαγέντας πτώματι, τῶν προπατόρων Θεονύμφευτε."

meMA.Ode3C11.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C11.greekmelody = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.greekname
meMA.Ode3C11.melody = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.name
meMA.Ode3C11.incipit = "Ῥωννύμενος ἐν Χριστῷ, τὰς μηχανὰς…"
meMA.Ode3C11.text = "Ῥωννύμενος ἐν Χριστῷ, τὰς μηχανὰς τοῦ σκολιοῦ δράκοντος, γνώμῃ στεῤῥᾷ Κύριλλε, ὡς ἱστὸν ἀράχνης διέλυσας."

meMA.Ode3C12.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C12.melody = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.name
meMA.Ode3C12.incipit = "Ἰλὺν παθῶν ἐκ ψυχῆς, ἐξετινάξω…"
meMA.Ode3C12.text = "Ἰλὺν παθῶν ἐκ ψυχῆς, ἐξετινάξω καθελὼν Κύριλλε, πᾶν λογισμῶν ὕψωμα, κατὰ τοῦ Χριστοῦ ἐπαιρόμενον."

meMA.Ode3C13.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C13.melody = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.name
meMA.Ode3C13.incipit = "Ὡς εὔστοχος ἀθλητής, Πάτερ ἐξέκλινας…"
meMA.Ode3C13.text = "Ὡς εὔστοχος ἀθλητής, Πάτερ ἐξέκλινας στεῤῥῶς ἅπασαν, τὴν τῆς σαρκὸς εὐπάθειαν, ὡς ἐπιβλαβῆ καὶ ἐπίβουλον."

meMA.Ode3C1T.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C1T.melody = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.name
meMA.Ode3C1T.incipit = "Τὴν μόνην ἐν γυναιξίν, εὐλογημένην…"
meMA.Ode3C1T.text = "Τὴν μόνην ἐν γυναιξίν, εὐλογημένην ὡς Θεοῦ σκήνωμα, πανευπρεπὲς ἅπαντες, ἐν ἀγαλλιάσει δοξάσωμεν."

meMA.Ode3H.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3H.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3H.greekmelody = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.greekname
meMA.Ode3H.melody = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.melody
meMA.Ode3H.incipit = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.incipit
meMA.Ode3H.text = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.text

meMA.Ode3Kathisma1.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3Kathisma1.greekmelody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.greekname
meMA.Ode3Kathisma1.melody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.name
meMA.Ode3Kathisma1.incipit = "Τὴν σοφίαν τὴν θείαν πεπλουτηκώς…"
meMA.Ode3Kathisma1.text = "Τὴν σοφίαν τὴν θείαν πεπλουτηκώς, ἐξηρεύξω δογμάτων Πάτερ πηγάς, πλυνούσας αἱρέσεων, δυσωδέστατον βόρβορον, καὶ πιστῶν καρδίας, ἀρδευούσας ἐν χάριτι, καὶ τῆς Ἐκκλησίας τὰς αὔλακας Κύριλλε, ἤδη μεθυσκούσας· διὰ τοῦτό σε πίστει, τιμῶντες τὴν μνήμην σου, ἐκτελοῦμεν θεόπνευστε, Ἱεράρχα πανεύφημε· Πρέσβευε Χριστῷ τῷ Θεῷ, τῶν πταισμάτων ἄφεσιν δωρήσασθαι, τοῖς ἑορτάζουσι πόθω, τὴν ἁγίαν μνήμην σου."

meMA.Ode3KathismaT.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3KathismaT.melody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.name
meMA.Ode3KathismaT.incipit = "Τὴν Σοφίαν καὶ Λόγον…"
meMA.Ode3KathismaT.text = "Τὴν Σοφίαν καὶ Λόγον ἐν σῇ γαστρί, συλλαβοῦσα ἀφράστως Μήτηρ Θεοῦ, τῷ κόσμῳ ἐκύησας, τὸν τὸν κόσμον κατέχοντα· καὶ ἐν ἀγκάλαις ἔσχες, τὸν πάντα συνέχοντα, καὶ ἐκ μαζῶν ἐθήλασας, τὸν πάντας ἐκτρέφοντα. Ὅθεν δυσωπῶ σε, Παναγία Παρθένε, ῥυσθῆναι πταισμάτων μου, ὅταν μέλλω παρίστασθαι, πρὸ προσώπου τοῦ Κτίστου μου. Δέσποινα Παρθένε ἁγνή, τὴν σὴν βοήθειαν τότε μοι δώρησαι· καὶ γὰρ δύνασαι, ὅσα θέλεις πανύμνητε."

meMA.Ode3KathismaSt.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3KathismaSt.melody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.name
meMA.Ode3KathismaSt.incipit = me.m01.d24_gr_GR_cog.meMA.Ode3KathismaSt.incipit
meMA.Ode3KathismaSt.text = me.m01.d24_gr_GR_cog.meMA.Ode3KathismaSt.text

meMA.Ode4C11.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C11.greekmelody = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.greekname
meMA.Ode4C11.melody = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.name
meMA.Ode4C11.incipit = "Νενικητότα τὰ πάθη τὰ ψυχοφθόρα…"
meMA.Ode4C11.text = "Νενικητότα τὰ πάθη τὰ ψυχοφθόρα, καὶ λογισμῷ κρατήσαντα σαρκὸς θεοφόρε, Κύριλλε ἰδὼν ὁ Χριστός, τῆς θείας σε πρόεδρον, Πάτερ Ἐκκλησίας καθίστησιν."

meMA.Ode4C12.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C12.melody = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.name
meMA.Ode4C12.incipit = "Κληρονομήσας ὡς γόνος ἠγαπημένος, τὴν…"
meMA.Ode4C12.text = "Κληρονομήσας ὡς γόνος ἠγαπημένος, τὴν πατρικὴν τοῦ Μάρκου, ἀρετὴν καὶ τοῦ θρόνου, γέγονας διάδοχος, τοῦ θείου τοῖς ἴχνεσιν, Εὐαγγελιστοῦ παρεπόμενος."

meMA.Ode4C13.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C13.melody = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.name
meMA.Ode4C13.incipit = "Ὑπὸ Χριστοῦ μὲν ὡς πρόβατον…"
meMA.Ode4C13.text = "Ὑπὸ Χριστοῦ μὲν ὡς πρόβατον ἐποιμάνθης, ὡς δὲ ποιμὴν ἐποίμανας, τὸ ποίμνιον Πάτερ, λόγοις ψυχοτρόφοις σου, ὡς πόαις καὶ ἄνθεσι, δαψιλῶς ἐκτρέφων τῇ χάριτι."

meMA.Ode4C1T.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C1T.melody = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.name
meMA.Ode4C1T.incipit = "Σεσαρκωμένον τὸν Λόγον ἐκ σοῦ…"
meMA.Ode4C1T.text = "Σεσαρκωμένον τὸν Λόγον ἐκ σοῦ Παρθένε, θεοπρεπῶς ἐγέννησας, καὶ μένεις Παρθένος· ὅθεν ἀσιγήτοις φωναῖς, τὸ Χαῖρέ σοι Δέσποινα, πίστει ἀδιστάκτῳ κραυγάζομεν."

meMA.Ode5C11.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C11.greekmelody = he.h.m4_gr_GR_cog.heHE.SyKyrieMouFos.greekname
meMA.Ode5C11.melody = he.h.m4_gr_GR_cog.heHE.SyKyrieMouFos.name
meMA.Ode5C11.incipit = "Ῥεῦμα ὄντος σφοδρόν, πλημμυροῦντος χειμάῤῥου…"
meMA.Ode5C11.text = "Ῥεῦμα ὄντος σφοδρόν, πλημμυροῦντος χειμάῤῥου ὁ σός, ὦ Κύριλλε ἐκμιμεῖται, λόγος, τῶν κακοδόξων ἐκτρέπων μηχανήματα."

meMA.Ode5C12.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C12.melody = he.h.m4_gr_GR_cog.heHE.SyKyrieMouFos.name
meMA.Ode5C12.incipit = "Ἴσως τὴν βδελυράν, τοῦ Ἀρείου…"
meMA.Ode5C12.text = "Ἴσως τὴν βδελυράν, τοῦ Ἀρείου διαίρεσιν, ὦ Κύριλλε μιαράν τε, Σαβελλίου ἐκ βάθρων, συναλοιφὴν καθῄρηκας."

meMA.Ode5C13.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C13.melody = he.h.m4_gr_GR_cog.heHE.SyKyrieMouFos.name
meMA.Ode5C13.incipit = "Λόγους πλουτοποιούς, ὡς χρυσὸν καταλέλοιπας…"
meMA.Ode5C13.text = "Λόγους πλουτοποιούς, ὡς χρυσὸν καταλέλοιπας, ὦ Κύριλλε θησαυροὺς δέ, ὑπὲρ λίθους τιμίους, τοὺς σοὺς τῇ Ἐκκλησία Χριστοῦ."

meMA.Ode5C1T.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C1T.melody = he.h.m4_gr_GR_cog.heHE.SyKyrieMouFos.name
meMA.Ode5C1T.incipit = "Τίς δύναται τὸ σόν, ἑρμηνεῦσαι…"
meMA.Ode5C1T.text = "Τίς δύναται τὸ σόν, ἑρμηνεῦσαι μυστήριον, Πανάμωμε; τὸν γὰρ κτίσει, μὴ χωρούμενον πάσῃ, ἐν μήτρᾳ σου ἐχώρησας."

meMA.Ode6C11.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C11.greekmelody = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.greekname
meMA.Ode6C11.melody = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.name
meMA.Ode6C11.incipit = "Λαβίδι, Σεραφὶμ τῷ Προφήτῃ τὸν…"
meMA.Ode6C11.text = "Λαβίδι, Σεραφὶμ τῷ Προφήτῃ τὸν ἄνθρακα, τῇ Ἐκκλησία Χριστοῦ δέ, ταῖς χερσὶ προσνέμεις Ἱερομύστα, τὸ ἐκ θείου, πυρὸς ἠνθρακωμένον καθάρσιον."

meMA.Ode6C12.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C12.melody = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.name
meMA.Ode6C12.incipit = "Ὁμόρους, ὡς Σαμψὼν ἀλλοφύλους οὐκ…"
meMA.Ode6C12.text = "Ὁμόρους, ὡς Σαμψὼν ἀλλοφύλους οὐκ ἔθραυσας, τὰ δὲ ἀλλόκοτα πάντα, τῶν ἑτεροδόξων διδάγματα, καὶ τὸ κράτος, τοῖς ὀρθοδόξοις δέδωκας Κύριλλε."

meMA.Ode6C13.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C13.melody = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.name
meMA.Ode6C13.incipit = "Συνόδου, θεολέκτου σαφῶς προηγούμενος, τὸ…"
meMA.Ode6C13.text = "Συνόδου, θεολέκτου σαφῶς προηγούμενος, τὸ δυσσεβὲς Νεστορίου, χριστομάχον θράσος Πάτερ καθεῖλες, ζήλῳ πνέων, ὑπὲρ τῆς ἀληθοῦς Θεομήτορος."

meMA.Ode6C1T.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C1T.melody = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.name
meMA.Ode6C1T.incipit = "Ὅλην σε, περιστερὰν τελείαν καὶ…"
meMA.Ode6C1T.text = "Ὅλην σε, περιστερὰν τελείαν καὶ πάγκαλον, καὶ τηλαυγὲς εὑρὼν κρίνον, καὶ κοιλάδων ἄνθος ὦ Θεομῆτορ, ὁ Νυμφίος, ὁ νοητὸς ἐν σοὶ κατεσκήνωσεν."

meMA.Ode6H.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6H.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6H.greekmelody = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.greekname
meMA.Ode6H.melody = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.melody
meMA.Ode6H.incipit = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.incipit
meMA.Ode6H.text = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.text

meMA.Kontakion1.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Kontakion1.greekmelody = he.a.m6_gr_GR_cog.heAU.TinYperImon.greekname
meMA.Kontakion1.melody = he.a.m6_gr_GR_cog.heAU.TinYperImon.name
meMA.Kontakion1.incipit = "Ἄβυσσον ἡμῖν…"
meMA.Kontakion1.text = "Ἄβυσσον ἡμῖν δογμάτων θεολογίας, ἔβλυσας σαφῶς ἐκ τῶν πηγῶν τοῦ Σωτῆρος, τὰς αἱρέσεις κατακλύζουσαν μακάριε, καὶ τὸ ποίμνιον ἀλώβητον, τρικυμίαις διασῴζουσαν· τῶν περάτων καὶ γὰρ Ὅσιε, ὑπάρχεις καθηγητής, ὡς τὰ θεῖα σαφῶν."

meMA.Ode7C11.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C11.greekmelody = he.h.m4_gr_GR_cog.heHE.EnTiKaminoAvramiaioi.greekname
meMA.Ode7C11.melody = he.h.m4_gr_GR_cog.heHE.EnTiKaminoAvramiaioi.name
meMA.Ode7C11.incipit = "Ἐν τῷ Σιναίῳ, ἀκρωτηρίῳ πάλαι…"
meMA.Ode7C11.text = "Ἐν τῷ Σιναίῳ, ἀκρωτηρίῳ πάλαι γνόφον εἰσδύς, νόμον εἷλε μάκαρ Κύριλλε ὁ Μωσῆς, ἀναιροῦντα, τὸν τοῦ γράμματος, σὺ δὲ ἀνέπτυξας, τὸ κεκρυμμένον βάθος τοῦ Πνεύματος."

meMA.Ode7C12.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C12.melody = he.h.m4_gr_GR_cog.heHE.EnTiKaminoAvramiaioi.name
meMA.Ode7C12.incipit = "Νομοθεσίας, διʼ αἰνιγμάτων ὡς νηπίοις…"
meMA.Ode7C12.text = "Νομοθεσίας, διʼ αἰνιγμάτων ὡς νηπίοις τὸ πρίν, Πάτερ δεδομένης κάλυξι καλυπτά, ὥσπερ ῥόδα τὰ νοήματα, σὺ ἀπεκάλυψας, ἑρμηνεύσας τοὺς τύπους σαφέστατα."

meMA.Ode7C13.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C13.melody = he.h.m4_gr_GR_cog.heHE.EnTiKaminoAvramiaioi.name
meMA.Ode7C13.incipit = "Θεοκηρύκων, καὶ θεολήπτων τὰ δυσέφικτα…"
meMA.Ode7C13.text = "Θεοκηρύκων, καὶ θεολήπτων τὰ δυσέφικτα, τεύχη Προφητῶν τῇ χάριτι ἐμπνευσθείς, τοῦ ἐκείνους ἐμφορήσαντος, Πνεύματος Κύριλλε, εἰλικρινῶς ἡμῖν διεσάφησας."

meMA.Ode7C14.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C14.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C14.melody = he.h.m4_gr_GR_cog.heHE.EnTiKaminoAvramiaioi.name
meMA.Ode7C14.incipit = "Ἐν ἀσυγχύτῳ, ἑνώσει θείᾳ καὶ…"
meMA.Ode7C14.text = "Ἐν ἀσυγχύτῳ, ἑνώσει θείᾳ καὶ ἀτμήτῳ σαφῶς, Πάτερ διαιρέσεις, τρεῖς μὲν θεολογῶν, ὑποστάσεις ἔφης Κύριλλε, ἀπαραλλάκτους δέ, ἐν οὐσίᾳ μιᾷ καὶ Θεότητι."

meMA.Ode7C1T.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C1T.melody = he.h.m4_gr_GR_cog.heHE.EnTiKaminoAvramiaioi.name
meMA.Ode7C1T.incipit = "Ἐπὶ τοῦ θρόνου, τοῦ ἐπηρμένου…"
meMA.Ode7C1T.text = "Ἐπὶ τοῦ θρόνου, τοῦ ἐπηρμένου ὁ καθήμενος, θρόνον ἐπὶ γῆς Παρθένε θεοπρεπῆ, εὑρηκώς σε ἀνεπαύσατο· Εὐλογημένη σύ, Θεογεννῆτορ ἄχραντε Δέσποινα."

meMA.Ode8C11.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C11.greekmelody = he.h.m4_gr_GR_cog.heHE.CheirasEkpetasasDaniil.greekname
meMA.Ode8C11.melody = he.h.m4_gr_GR_cog.heHE.CheirasEkpetasasDaniil.name
meMA.Ode8C11.incipit = "Ὡς κῦμα θαλάσσης ἀληθῶς, ἐν…"
meMA.Ode8C11.text = "Ὡς κῦμα θαλάσσης ἀληθῶς, ἐν σοὶ ἐξέβλυσεν, ἡ τῆς σοφίας πηγή· καὶ γὰρ ἀέναοι ἔῤῥευσαν, ποταμοὶ ἐκ τῆς κοιλίας σου, Πάτερ δογμάτων εὐσεβῶν, καθὼς προεῖπε Χριστός· ὑπερβαίνει ψάμμον γάρ, τεύχη τὰ σὰ τὰ θεόλεκτα."

meMA.Ode8C12.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C12.melody = he.h.m4_gr_GR_cog.heHE.CheirasEkpetasasDaniil.name
meMA.Ode8C12.incipit = "Νομεὺς καὶ διδάσκαλος σοφός, τῆς…"
meMA.Ode8C12.text = "Νομεὺς καὶ διδάσκαλος σοφός, τῆς Ἐκκλησίας δειχθείς, τὰς δύο Κύριλλε, σαφῶς ἀνέπτυξας Ὅσιε, Διαθήκας καὶ ὡς ἄγκυραν, καὶ πατρικὸν κλῆρον τοὺς σούς, ἔχουσα λόγους βοᾷ· Εὐλογεῖτε, πάντα τὰ ἔργα Κυρίου τὸν Κύριον."

meMA.Ode8C13.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C13.melody = he.h.m4_gr_GR_cog.heHE.CheirasEkpetasasDaniil.name
meMA.Ode8C13.incipit = "Λόγου σαρκωθέντος διʼ ἡμᾶς, τοῦ…"
meMA.Ode8C13.text = "Λόγου σαρκωθέντος διʼ ἡμᾶς, τοῦ παντεχνήμονος, σὺ τὴν ἀπόῤῥητον, διδάσκεις ἕνωσιν Κύριλλε, ἀδιαίρετον ἀσύγχυτον, τὰς ἐφʼ ἑκάτερον ῥοπάς, ἴσως ἐκκλίνας βοᾷς· Εὐλογεῖτε, πάντα τὰ ἔργα Κυρίου τὸν Κύριον."

meMA.Ode8C1T.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C1T.melody = he.h.m4_gr_GR_cog.heHE.CheirasEkpetasasDaniil.name
meMA.Ode8C1T.incipit = "Κυρίως γεννήσασα Θεόν, κυρίως Πάναγνε…"
meMA.Ode8C1T.text = "Κυρίως γεννήσασα Θεόν, κυρίως Πάναγνε, Μήτηρ ἐδείχθης Θεοῦ, ἐπαληθεύουσαν φέρουσα, καταλλήλως τῷ γεννήματι, θεωνυμίαν, οἱ πιστοί· ὅθεν δοξάζομεν, Θεοτόκον σε, θεοφρόνως πανάμωμε Δέσποινα."

meMA.Ode8H.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8H.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8H.greekmelody = he.h.m4_gr_GR_cog.heHE.CheirasEkpetasasDaniil.greekname
