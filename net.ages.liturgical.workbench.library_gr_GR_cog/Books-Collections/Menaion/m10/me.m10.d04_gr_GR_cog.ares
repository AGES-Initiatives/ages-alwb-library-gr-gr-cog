meMA.Ode9H.text = he.h.m8_gr_GR_cog.heHE.ExestiEpiTouto.text

meMA.Exaposteilarion1.mode = miscellanea_gr_GR_cog.misc.Mode3
meMA.Exaposteilarion1.greekmelody = he.a.m3_gr_GR_cog.heAU.OOuranonToisAstrois.greekname
meMA.Exaposteilarion1.melody = he.a.m3_gr_GR_cog.heAU.OOuranonToisAstrois.name
meMA.Exaposteilarion1.incipit = "Ὅτε ἡ Μήτηρ τοῦ Λόγου…"
meMA.Exaposteilarion1.text = "Ὅτε ἡ Μήτηρ τοῦ Λόγου, πρὸς τὰς ἐκεῖ καταπαύσεις, μεταναστεύουσα ὤφθη, σὺν τοῖς λοιποῖς Ἀποστόλοις, σὺ Ἱερόθεε ὕμνοις, ἐπέστης ταύτην προπέμπων."

