alt1.meMA.PostGospelIdiomelon.mode = miscellanea_gr_GR_cog.misc.Mode6
alt1.meMA.PostGospelIdiomelon.melody = ""
alt1.meMA.PostGospelIdiomelon.incipit = "Σήμερον ἡ Οἰκουμένη πᾶσα, ταῖς…"
alt1.meMA.PostGospelIdiomelon.text = "Σήμερον ἡ Οἰκουμένη πᾶσα, ταῖς τῆς Θεοτόκου αὐγάζετε ἁκτῖσι, καὶ τῶν Κυθηραίων ἡ Νῆσος, τοῖς ἄνθεσι ὡραϊζομένη, μυρσίνης ἀναμέλπει· Παρθένε Ἁγνή, Ὀρθοδόξων τὸ καύχημα, μὴ διαλείπῃς πρεσβεύειν ὑπὲρ ἡμῶν πρὸς τὸν Κύριον."

alt1.meMA.C1.acrostic.title = "Ὁ Κανών,"
alt1.meMA.C1.acrostic.notice1 = "οὖ ἡ Ἀκροστιχίς, ἄνευ τῶν Εἱρμῶν·"
alt1.meMA.C1.acrostic.text = "Κυθηραίων ὑμνῶ Μυρτιδιώτισσαν κλέος."
