ocMA.OdeW1C11.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW1C11.ode = miscellanea_gr_GR_cog.misc.Ode1
ocMA.OdeW1C11.melody = he.h.m8_gr_GR_cog.heHE.AsomenToKyrio.name
ocMA.OdeW1C11.incipit = "Τίμιοι μαργαρῖται, ὤφθητε τὸν στέφανον…"
ocMA.OdeW1C11.text = "Τίμιοι μαργαρῖται, ὤφθητε τὸν στέφανον φαιδρύνοντες, τῆς σεπτῆς Ἐκκλησίας, ἀθλοφόροι Χριστοῦ γενναιότατοι."

ocMA.OdeW1C12.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW1C12.ode = miscellanea_gr_GR_cog.misc.Ode1
ocMA.OdeW1C12.melody = he.h.m8_gr_GR_cog.heHE.AsomenToKyrio.name
ocMA.OdeW1C12.incipit = "Ἤστραψαν τῶν δογμάτων, καὶ τῶν…"
ocMA.OdeW1C12.text = "Ἤστραψαν τῶν δογμάτων, καὶ τῶν ἀρετῶν θείαις λαμπρότησιν, οἱ σοφοὶ Ἱεράρχαι, καὶ πιστῶν τὰς καρδίας ἐφώτισαν."

ocMA.OdeW1C13.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW1C13.ode = miscellanea_gr_GR_cog.misc.Ode1
ocMA.OdeW1C13.melody = he.h.m8_gr_GR_cog.heHE.AsomenToKyrio.name
ocMA.OdeW1C13.incipit = "Σὲ τὸν ἐν τοῖς Προφήταις…"
ocMA.OdeW1C13.text = "Σὲ τὸν ἐν τοῖς Προφήταις, καὶ ἐν τοῖς Ὁσίοις θαυμαζόμενον, ἱκετεύομεν Λόγε, ταῖς αὐτῶν ἡμᾶς σῶσον δεήσεσιν."

ocMA.OdeW1C14.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW1C14.ode = miscellanea_gr_GR_cog.misc.Ode1
ocMA.OdeW1C14.melody = he.h.m8_gr_GR_cog.heHE.AsomenToKyrio.name
ocMA.OdeW1C14.incipit = "Ὅταν κρίσιν δικαίαν, ἔλθῃς τοῦ…"
ocMA.OdeW1C14.text = "Ὅταν κρίσιν δικαίαν, ἔλθῃς τοῦ ποιῆσαι δικαιότατε, ἀκατακρίτους Λόγε, τοὺς πρὸς σὲ μεταστάντας ἀπέργασαι."

ocMA.OdeW1C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW1C1T.ode = miscellanea_gr_GR_cog.misc.Ode1
ocMA.OdeW1C1T.melody = he.h.m8_gr_GR_cog.heHE.AsomenToKyrio.name
ocMA.OdeW1C1T.incipit = "Κύριον τετοκυῖαν, γυναικῶν χορός σε…"
ocMA.OdeW1C1T.text = "Κύριον τετοκυῖαν, γυναικῶν χορός σε ἐπιστάμενος, σοῦ ὀπίσω Παρθένε, ἐναθλήσας αὐτῷ προσενήνεκται."

ocMA.OdeW1C21.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW1C21.ode = miscellanea_gr_GR_cog.misc.Ode1
ocMA.OdeW1C21.greekmelody = he.h.m8_gr_GR_cog.heHE.YgranDiodefsas.greekname
ocMA.OdeW1C21.melody = he.h.m8_gr_GR_cog.heHE.YgranDiodefsas.name
ocMA.OdeW1C21.incipit = "Θανάτῳ τὸν θάνατον τοῦ Χριστοῦ…"
ocMA.OdeW1C21.text = "Θανάτῳ τὸν θάνατον τοῦ Χριστοῦ, καὶ πάθεσι πάθος, μιμησάμενοι τὸ σεπτόν, οἱ Μάρτυρες πάντες τῆς ἐνθέου, καὶ μακαρίας ζωῆς ἔτυχον."

ocMA.OdeW1C22.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW1C22.ode = miscellanea_gr_GR_cog.misc.Ode1
ocMA.OdeW1C22.melody = he.h.m8_gr_GR_cog.heHE.YgranDiodefsas.name
ocMA.OdeW1C22.incipit = "Νεότητος πταίσματα παρορῶν, καὶ τὰς…"
ocMA.OdeW1C22.text = "Νεότητος πταίσματα παρορῶν, καὶ τὰς ἁμαρτίας, ὑπερβαίνων Χριστὲ Σωτήρ, τῶν προκοιμηθέντων οἰκετῶν σου, τοῖς ἐκλεκτοῖς σου συναρίθμησον."

ocMA.OdeW1C23.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW1C23.ode = miscellanea_gr_GR_cog.misc.Ode1
ocMA.OdeW1C23.melody = he.h.m8_gr_GR_cog.heHE.YgranDiodefsas.name
ocMA.OdeW1C23.incipit = "Ἧς ἔτυχον δόξης τε καὶ…"
ocMA.OdeW1C23.text = "Ἧς ἔτυχον δόξης τε καὶ χαρᾶς, οἱ τὴν μακαρίαν κεκτημένοι διαγωγήν, μετάδος πλουσίως τοῖς σοῖς δούλοις, οὓς προσελάβου Πολυέλεε."

ocMA.OdeW1C2T.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW1C2T.ode = miscellanea_gr_GR_cog.misc.Ode1
ocMA.OdeW1C2T.melody = he.h.m8_gr_GR_cog.heHE.YgranDiodefsas.name
ocMA.OdeW1C2T.incipit = "Συνέλαβες Λόγον τὸν τοῦ Πατρός…"
ocMA.OdeW1C2T.text = "Συνέλαβες Λόγον τὸν τοῦ Πατρός, σαρκὶ ἑνωθέντα, καθʼ ὑπόστασιν τῇ ἐκ σοῦ, πανάμωμε Κόρη καὶ τὸν ᾅδην, θείᾳ δυνάμει καταργήσαντα."

ocMA.OdeW3C1H.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW3C1H.ode = miscellanea_gr_GR_cog.misc.Ode3
ocMA.OdeW3C1H.greekmelody = he.h.m8_gr_GR_cog.heHE.OStereosasLogo.greekname
ocMA.OdeW3C1H.melody = he.h.m8_gr_GR_cog.heHE.OStereosasLogo.melody
ocMA.OdeW3C1H.incipit = he.h.m8_gr_GR_cog.heHE.OStereosasLogo.incipit
ocMA.OdeW3C1H.text = he.h.m8_gr_GR_cog.heHE.OStereosasLogo.text

ocMA.OdeW3C11.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW3C11.ode = miscellanea_gr_GR_cog.misc.Ode3
ocMA.OdeW3C11.melody = he.h.m8_gr_GR_cog.heHE.OStereosasLogo.name
ocMA.OdeW3C11.incipit = "Τὰς μυσαρὰς θυσίας οἱ Ἀθληταί…"
ocMA.OdeW3C11.text = "Τὰς μυσαρὰς θυσίας οἱ Ἀθληταί, στεῤῥοτάτῃ γνώμῃ ἀποσεισάμενοι, θυσία καθαρωτάτη, Λόγῳ τῷ τυθέντι γεγόνασιν."

ocMA.OdeW3C12.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW3C12.ode = miscellanea_gr_GR_cog.misc.Ode3
ocMA.OdeW3C12.melody = he.h.m8_gr_GR_cog.heHE.OStereosasLogo.name
ocMA.OdeW3C12.incipit = "Ἀνανεοῦντες λόγοις ἱερουργοῖς, τοὺς παλαιωθέντας…"
ocMA.OdeW3C12.text = "Ἀνανεοῦντες λόγοις ἱερουργοῖς, τοὺς παλαιωθέντας παντοίοις παθήμασι, τοῦ ἀναπλάσαντος κόσμον, θεῖοι Μαθηταὶ ἀπεφάνθητε."

ocMA.OdeW3C13.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW3C13.ode = miscellanea_gr_GR_cog.misc.Ode3
ocMA.OdeW3C13.melody = he.h.m8_gr_GR_cog.heHE.OStereosasLogo.name
ocMA.OdeW3C13.incipit = "Ἡ τοῖς Προφήταις πρότερον ἐμφανῶς…"
ocMA.OdeW3C13.text = "Ἡ τοῖς Προφήταις πρότερον ἐμφανῶς, δεδομένη χάρις τοῦ θείου Πνεύματος, τοὺς ἀσκητὰς ἐπʼ ἐσχάτων, θείων χαρισμάτων ἠξίωσε."

ocMA.OdeW3C14.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW3C14.ode = miscellanea_gr_GR_cog.misc.Ode3
ocMA.OdeW3C14.melody = he.h.m8_gr_GR_cog.heHE.OStereosasLogo.name
ocMA.OdeW3C14.incipit = "Χοροῖς Ἁγίοις σύνταξον ὁ Θεός…"
ocMA.OdeW3C14.text = "Χοροῖς Ἁγίοις σύνταξον ὁ Θεός, τοὺς πιστῶς τοῦ βίου μεταχωρήσαντας, καὶ Παραδείσου πολίτας, ποίησον διʼ ἄφατον ἔλεος."

ocMA.OdeW3C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW3C1T.ode = miscellanea_gr_GR_cog.misc.Ode3
ocMA.OdeW3C1T.melody = he.h.m8_gr_GR_cog.heHE.OStereosasLogo.name
ocMA.OdeW3C1T.incipit = "Ὁ μηδαμοῦ χωρούμενος Ἰησοῦς, ἀστενοχωρήτως…"
ocMA.OdeW3C1T.text = "Ὁ μηδαμοῦ χωρούμενος Ἰησοῦς, ἀστενοχωρήτως τὴν σὴν κατώκησεν, ἡγιασμένην γαστέρα, ἄχραντε Παρθένε πανύμνητε."

ocMA.OdeW3C21.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW3C21.ode = miscellanea_gr_GR_cog.misc.Ode3
ocMA.OdeW3C21.greekmelody = he.h.m8_gr_GR_cog.heHE.OuraniasApsidos.greekname
ocMA.OdeW3C21.melody = he.h.m8_gr_GR_cog.heHE.OuraniasApsidos.name
ocMA.OdeW3C21.incipit = "Καθαρθέντες τῆς πάλαι, προγονικῆς πτώσεως…"
ocMA.OdeW3C21.text = "Καθαρθέντες τῆς πάλαι, προγονικῆς πτώσεως, διὰ τοῦ Βαπτίσματος καὶ τῆς ἀναγεννήσεως, ῥείθροις αἱμάτων τε, τοῖς ἑαυτῶν ῥαντισθέντες, τῷ Χριστῷ Μακάριοι συμβασιλεύετε."

ocMA.OdeW3C22.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW3C22.ode = miscellanea_gr_GR_cog.misc.Ode3
ocMA.OdeW3C22.melody = he.h.m8_gr_GR_cog.heHE.OuraniasApsidos.name
ocMA.OdeW3C22.incipit = "Ὁ νεκρὸς ἑκουσίως, κατατεθεὶς μνήματι…"
ocMA.OdeW3C22.text = "Ὁ νεκρὸς ἑκουσίως, κατατεθεὶς μνήματι, καὶ τοὺς κατοικοῦντας ἐν τάφοις ἀνακαλούμενος, Σῶτερ εὐδόκησον, οὓς ἐξ ἡμῶν προσελάβου, ἐν σκηναῖς δικαίων σου, τούτους αὐλίζεσθαι."

ocMA.OdeW3C23.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW3C23.ode = miscellanea_gr_GR_cog.misc.Ode3
ocMA.OdeW3C23.melody = he.h.m8_gr_GR_cog.heHE.OuraniasApsidos.name
ocMA.OdeW3C23.incipit = "Ὑπὸ τῆς εὐσπλαγχνίας, τῆς θεϊκῆς…"
ocMA.OdeW3C23.text = "Ὑπὸ τῆς εὐσπλαγχνίας, τῆς θεϊκῆς Δέσποτα, καὶ τῆς συμφυῶς νοουμένης σου ἀγαθότητος, παρακαλούμενος, ἁμαρτημάτων τὴν λύσιν, παρασχὼν τοῖς δούλοις σου, Σῶτερ ἀνάπαυσον."

ocMA.OdeW3C2T.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW3C2T.ode = miscellanea_gr_GR_cog.misc.Ode3
ocMA.OdeW3C2T.melody = he.h.m8_gr_GR_cog.heHE.OuraniasApsidos.name
ocMA.OdeW3C2T.incipit = "Σαρκωθεὶς ἐκ γαστρός σου, καὶ…"
ocMA.OdeW3C2T.text = "Σαρκωθεὶς ἐκ γαστρός σου, καὶ γεγονὼς ἄνθρωπος, ὁ διαφερόντως ὑπάρχων μόνος Φιλάνθρωπος, σώζει τὸν ἄνθρωπον, ἐκ τῶν πυλῶν τοῦ θανάτου, Θεομῆτορ ἄχραντε, μόνη πανύμνητε."

ocMA.OdeW4C1H.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW4C1H.ode = miscellanea_gr_GR_cog.misc.Ode4
ocMA.OdeW4C1H.greekmelody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.greekname
ocMA.OdeW4C1H.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.melody
ocMA.OdeW4C1H.incipit = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.incipit
ocMA.OdeW4C1H.text = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.text

ocMA.OdeW4C11.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW4C11.ode = miscellanea_gr_GR_cog.misc.Ode4
ocMA.OdeW4C11.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
ocMA.OdeW4C11.incipit = "Ὑπελθόντες τὸ στάδιον, τὸ τῆς…"
ocMA.OdeW4C11.text = "Ὑπελθόντες τὸ στάδιον, τὸ τῆς μαρτυρίας ζέοντι πνεύματι, Ἀθληταὶ θεομακάριστοι, τὴν ὑλώδη πλάνην κατεφλέξατε."

ocMA.OdeW4C12.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW4C12.ode = miscellanea_gr_GR_cog.misc.Ode4
ocMA.OdeW4C12.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
ocMA.OdeW4C12.incipit = "Τοὺς Ὁσίους σου Κύριε, καὶ…"
ocMA.OdeW4C12.text = "Τοὺς Ὁσίους σου Κύριε, καὶ τοὺς Ἱεράρχας λαμπρῶς ἐδόξασας· τούτων θείαις παρακλήσεσι, κοινωνόν με δόξης τῆς σῆς ποίησον."

ocMA.OdeW4C13.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW4C13.ode = miscellanea_gr_GR_cog.misc.Ode4
ocMA.OdeW4C13.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
ocMA.OdeW4C13.incipit = "Ἡ τοῦ θείου ἐπίπνοια, Πνεύματος…"
ocMA.OdeW4C13.text = "Ἡ τοῦ θείου ἐπίπνοια, Πνεύματος Προφήτας καταλαμπρύνασα, γυναιξὶν ἰσχὺν δεδώρηται, τοῦ ἐχθροῦ ὀλέσαι τὰ φρυάγματα."

ocMA.OdeW4C14.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW4C14.ode = miscellanea_gr_GR_cog.misc.Ode4
ocMA.OdeW4C14.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
ocMA.OdeW4C14.incipit = "Συγχορεύειν ἀξίωσον, πᾶσι τοῖς Ἁγίοις…"
ocMA.OdeW4C14.text = "Συγχορεύειν ἀξίωσον, πᾶσι τοῖς Ἁγίοις οὕς περ μετέστησας, Ὑπεράγαθε, οἰκέτας σου, ἐν σκηναῖς Ἁγίων δυσωπούμενος."

ocMA.OdeW4C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW4C1T.ode = miscellanea_gr_GR_cog.misc.Ode4
ocMA.OdeW4C1T.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
ocMA.OdeW4C1T.incipit = "Νόμου θείου τὸν πάροχον, φύσεως…"
ocMA.OdeW4C1T.text = "Νόμου θείου τὸν πάροχον, φύσεως νομίμων ἄνευθεν τέτοκας, ἀναπλάττοντα Πανάμωμε, τῶν βροτῶν τὴν φύσιν ὀλισθήσασαν."

ocMA.OdeW4C21.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW4C21.ode = miscellanea_gr_GR_cog.misc.Ode4
ocMA.OdeW4C21.greekmelody = he.h.m8_gr_GR_cog.heHE.SyMouIschys.greekname
ocMA.OdeW4C21.melody = he.h.m8_gr_GR_cog.heHE.SyMouIschys.name
ocMA.OdeW4C21.incipit = "Ἵνα τὴν σήν, Δέσποτα δόξαν…"
ocMA.OdeW4C21.text = "Ἵνα τὴν σήν, Δέσποτα δόξαν θεάσωνται, καὶ τῆς αἴγλης, σοῦ τῆς φωτοδότιδος, ἐν οὐρανοῖς τύχωσι φαιδρῶς, ἅπασαν βασάνων, πεῖραν ἀθλοῦντες ὑπήνεγκαν, οἱ Μάρτυρες οἱ θεῖοι, σοὶ Χριστὲ μελῳδοῦντες· Τῇ δυνάμει σου δόξα Φιλάνθρωπε."

ocMA.OdeW4C22.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW4C22.ode = miscellanea_gr_GR_cog.misc.Ode4
ocMA.OdeW4C22.melody = he.h.m8_gr_GR_cog.heHE.SyMouIschys.name
ocMA.OdeW4C22.incipit = "Πολλαὶ μοναί, παρὰ σοὶ Σῶτερ…"
ocMA.OdeW4C22.text = "Πολλαὶ μοναί, παρὰ σοὶ Σῶτερ πεφύκασι, κατʼ ἀξίαν, πᾶσι μεριζόμεναι, κατὰ τὸ μέτρον τῆς ἀρετῆς· ταύτης οὖν Οἰκτίρμον, πληρῶσαι τούτους ἀξίωσον, τοὺς πίστει μεταστάντας, εὐσεβῶς σοι βοῶντας· Τῇ δυνάμει σου δόξα Φιλάνθρωπε."

ocMA.OdeW4C23.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW4C23.ode = miscellanea_gr_GR_cog.misc.Ode4
ocMA.OdeW4C23.melody = he.h.m8_gr_GR_cog.heHE.SyMouIschys.name
ocMA.OdeW4C23.incipit = "Ἴσος ἡμῖν, ἄνθρωπος ὤφθης Ἀθάνατε…"
ocMA.OdeW4C23.text = "Ἴσος ἡμῖν, ἄνθρωπος ὤφθης Ἀθάνατε, πᾶσιν ἴσον, θάνατον ὑπέμεινας, καὶ τῆς ζωῆς ἔδειξας ὁδούς· ἧς τοὺς μεταστάντας, ἀξίωσον ὡς Φιλάνθρωπος, συγχώρησιν πταισμάτων, ὡς Δεσπότης παρέχων, καὶ φωτὸς μετουσίαν δωρούμενος."

ocMA.OdeW4C2T.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW4C2T.ode = miscellanea_gr_GR_cog.misc.Ode4
ocMA.OdeW4C2T.melody = he.h.m8_gr_GR_cog.heHE.SyMouIschys.name
ocMA.OdeW4C2T.incipit = "Σὺ τῶν πιστῶν, καύχημα πέλεις…"
ocMA.OdeW4C2T.text = "Σὺ τῶν πιστῶν, καύχημα πέλεις Ἀνύμφευτε, σὺ προστάτις, σὺ καὶ καταφύγιον, Χριστιανῶν τεῖχος καὶ λιμήν· πρὸς γὰρ τὸν Υἱόν σου, ἐντεύξεις φέρεις Πανάμωμε, καὶ σώζεις ἐκ κινδύνων, τοὺς ἐν πίστει καὶ πόθῳ, Θεοτόκον ἁγνήν σε γινώσκοντας."

ocMA.OdeW5C1H.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW5C1H.ode = miscellanea_gr_GR_cog.misc.Ode5
ocMA.OdeW5C1H.greekmelody = he.h.m8_gr_GR_cog.heHE.FotisonImas.greekname
ocMA.OdeW5C1H.melody = he.h.m8_gr_GR_cog.heHE.FotisonImas.melody
ocMA.OdeW5C1H.incipit = he.h.m8_gr_GR_cog.heHE.FotisonImas.incipit
ocMA.OdeW5C1H.text = he.h.m8_gr_GR_cog.heHE.FotisonImas.text

ocMA.OdeW5C11.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW5C11.ode = miscellanea_gr_GR_cog.misc.Ode5
ocMA.OdeW5C11.melody = he.h.m8_gr_GR_cog.heHE.FotisonImas.name
ocMA.OdeW5C11.incipit = "Ἔφερον πληγάς, τῆς σαρκὸς οἱ…"
ocMA.OdeW5C11.text = "Ἔφερον πληγάς, τῆς σαρκὸς οἱ θεῖοι Μάρτυρες· καὶ διὰ τοῦτο τὰς πληγὰς ἡμῶν, ἀεὶ ἰῶνται, τραυματίζοντες τοὺς δαίμονας."

ocMA.OdeW5C12.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW5C12.ode = miscellanea_gr_GR_cog.misc.Ode5
ocMA.OdeW5C12.melody = he.h.m8_gr_GR_cog.heHE.FotisonImas.name
ocMA.OdeW5C12.incipit = "Ἅγιοι Θεοῦ, Ἱεράρχαι ἱκετεύσατε, μετὰ…"
ocMA.OdeW5C12.text = "Ἅγιοι Θεοῦ, Ἱεράρχαι ἱκετεύσατε, μετὰ πάντων τῶν Ὁσίων Χριστόν, ἁμαρτημάτων, δωρηθῆναι ἡμῖν ἄφεσιν."

ocMA.OdeW5C13.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW5C13.ode = miscellanea_gr_GR_cog.misc.Ode5
ocMA.OdeW5C13.melody = he.h.m8_gr_GR_cog.heHE.FotisonImas.name
ocMA.OdeW5C13.incipit = "Σάρκα αἰκισμοῖς, καὶ ἀσκήσει δαπανήσασαι…"
ocMA.OdeW5C13.text = "Σάρκα αἰκισμοῖς, καὶ ἀσκήσει δαπανήσασαι, γυναικῶν αἱ Ὅσιαι καὶ Μάρτυρες, τῶν ἐν ἐλπίσιν, ἀγαθῶν κατηξιώθησαν."

ocMA.OdeW5C14.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW5C14.ode = miscellanea_gr_GR_cog.misc.Ode5
ocMA.OdeW5C14.melody = he.h.m8_gr_GR_cog.heHE.FotisonImas.name
ocMA.OdeW5C14.incipit = "Θάνατον ἑλών τῷ θανάτῳ σου…"
ocMA.OdeW5C14.text = "Θάνατον ἑλών τῷ θανάτῳ σου Ἀθάνατε, τοὺς θανόντας ἐπʼ ἐλπίδι ζωῆς, πιστούς σου δούλους, ὡς φιλάνθρωπος ἀνάπαυσον."

ocMA.OdeW5C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW5C1T.ode = miscellanea_gr_GR_cog.misc.Ode5
ocMA.OdeW5C1T.melody = he.h.m8_gr_GR_cog.heHE.FotisonImas.name
ocMA.OdeW5C1T.incipit = "Ἔπαυσας Ἁγνή, προπατόρων τὸ κατάκριμα…"
ocMA.OdeW5C1T.text = "Ἔπαυσας Ἁγνή, προπατόρων τὸ κατάκριμα, τὸν τοὺς πάντας δικαιώσαντα, σαρκὶ τεκοῦσα, Ἰησοῦν τὸν μόνον Κύριον."

ocMA.OdeW5C21.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW5C21.ode = miscellanea_gr_GR_cog.misc.Ode5
ocMA.OdeW5C21.greekmelody = he.h.m8_gr_GR_cog.heHE.InaTiMeAposo.greekname
ocMA.OdeW5C21.melody = he.h.m8_gr_GR_cog.heHE.InaTiMeAposo.name
ocMA.OdeW5C21.incipit = "Τῆς ἀφράστου σου δόξης, καὶ…"
ocMA.OdeW5C21.text = "Τῆς ἀφράστου σου δόξης, καὶ τῆς ὑπὲρ λόγον σοῦ μακαριότητος, ἐν σκηναῖς Ἁγίων, ἔνθα ἦχος τερπνὸς ἑορτάζοντος, τοὺς μεταθεμένους, πρὸς ἀπαθῆ ζωὴν οἰκτείρας, καταξίωσον μόνε Φιλάνθρωπε."

ocMA.OdeW5C22.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW5C22.ode = miscellanea_gr_GR_cog.misc.Ode5
ocMA.OdeW5C22.melody = he.h.m8_gr_GR_cog.heHE.InaTiMeAposo.name
ocMA.OdeW5C22.incipit = "Ὅπου τάξεις Ἀγγέλων, ὅπου τῶν…"
ocMA.OdeW5C22.text = "Ὅπου τάξεις Ἀγγέλων, ὅπου τῶν δικαίων, οἱ δῆμοι ἀγάλλονται, Ἀβραὰμ ἐν κόλποις, κατασκήνωσον Σῶτερ τοὺς δούλους σου· καὶ σὺν παῤῥησίᾳ, τῷ φοβερῷ καὶ θείῳ θρόνῳ, παρεστάναι εὐδόκησον Εὔσπλαγχνε."

ocMA.OdeW5C23.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW5C23.ode = miscellanea_gr_GR_cog.misc.Ode5
ocMA.OdeW5C23.melody = he.h.m8_gr_GR_cog.heHE.InaTiMeAposo.name
ocMA.OdeW5C23.incipit = "Ἱλασμὸς ἡμῖν ὤφθης, καὶ δικαιοσύνη…"
ocMA.OdeW5C23.text = "Ἱλασμὸς ἡμῖν ὤφθης, καὶ δικαιοσύνη τε καὶ ἀπολύτρωσις, καὶ τῷ μώλωπί σου, τὰς ἡμῶν ἀσθενείας ἰάτρευσας· αὐτὸς οὖν Οἰκτίρμον, ὡς ἀγαθὸς τοὺς μεταστάντας, ἐν τρυφῇ Παραδείσου κατάταξον."

ocMA.OdeW5C2T.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW5C2T.ode = miscellanea_gr_GR_cog.misc.Ode5
ocMA.OdeW5C2T.melody = he.h.m8_gr_GR_cog.heHE.InaTiMeAposo.name
ocMA.OdeW5C2T.incipit = "Συμπαθῶς Ἐλεῆμον, τὸ τῆς ἀνθρωπότητος…"
ocMA.OdeW5C2T.text = "Συμπαθῶς Ἐλεῆμον, τὸ τῆς ἀνθρωπότητος μέτρον ἀνέλαβες, ὁ τῆς θείας δόξης, ὑπερτάτοις ἐμπρέπων ὑψώμασιν, ἐκ Παρθένου μήτρας, σάρκα λαβὼν ἐμψυχωμένην, λογικήν διʼ ἧς θάνατον ὤλεσας."

ocMA.OdeW6C1H.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW6C1H.ode = miscellanea_gr_GR_cog.misc.Ode6
ocMA.OdeW6C1H.greekmelody = he.h.m8_gr_GR_cog.heHE.IlasthitiMoiSotir.greekname
ocMA.OdeW6C1H.melody = he.h.m8_gr_GR_cog.heHE.IlasthitiMoiSotir.melody
ocMA.OdeW6C1H.incipit = he.h.m8_gr_GR_cog.heHE.IlasthitiMoiSotir.incipit
ocMA.OdeW6C1H.text = he.h.m8_gr_GR_cog.heHE.IlasthitiMoiSotir.text

ocMA.OdeW6C11.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW6C11.ode = miscellanea_gr_GR_cog.misc.Ode6
ocMA.OdeW6C11.melody = he.h.m8_gr_GR_cog.heHE.IlasthitiMoiSotir.name
ocMA.OdeW6C11.incipit = "Ἰχῶρσιν ὑμῶν σεπτοῖς, ψυχῶν ἰᾶσθε…"
ocMA.OdeW6C11.text = "Ἰχῶρσιν ὑμῶν σεπτοῖς, ψυχῶν ἰᾶσθε νοσήματα, καὶ σηπεδόνας ἀεί, σωμάτων ἐξαίρετε, κατατραυματίζοντες, πονηρῶν δαιμόνων, τὴν πληθύν, ἅγιοι Μάρτυρες."

ocMA.OdeW6C12.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW6C12.ode = miscellanea_gr_GR_cog.misc.Ode6
ocMA.OdeW6C12.melody = he.h.m8_gr_GR_cog.heHE.IlasthitiMoiSotir.name
ocMA.OdeW6C12.incipit = "Ὁ τῶν Ὁσίων χορός, Ἱεραρχῶν…"
ocMA.OdeW6C12.text = "Ὁ τῶν Ὁσίων χορός, Ἱεραρχῶν τε ὁ σύλλογος, καὶ Γυναικῶν ἱερῶν, ὁ θεῖος κατάλογος, Χριστὸν ἀγαπήσαντες, τὰς ἐπουρανίους, ἀντιδόσεις ἐκληρώσαντο."

ocMA.OdeW6C13.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW6C13.ode = miscellanea_gr_GR_cog.misc.Ode6
ocMA.OdeW6C13.melody = he.h.m8_gr_GR_cog.heHE.IlasthitiMoiSotir.name
ocMA.OdeW6C13.incipit = "Νεκρώσαντες Ἀσκηταί, τὴν σάρκα ζωῆς…"
ocMA.OdeW6C13.text = "Νεκρώσαντες Ἀσκηταί, τὴν σάρκα ζωῆς ἐτύχετε· ποιμάναντες δὲ καλῶς, Κυρίου τὸ ποίμνιον, Ἱεράρχαι πάνσοφοι, ἀθανάτου δόξης, μετὰ τέλος ἠξιώθητε."

ocMA.OdeW6C14.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW6C14.ode = miscellanea_gr_GR_cog.misc.Ode6
ocMA.OdeW6C14.melody = he.h.m8_gr_GR_cog.heHE.IlasthitiMoiSotir.name
ocMA.OdeW6C14.incipit = "Τῶν ζώντων Λόγε ζωή, καὶ…"
ocMA.OdeW6C14.text = "Τῶν ζώντων Λόγε ζωή, καὶ τῶν θανόντων ἀνάπαυσις, ὑπάρχων τοὺς ἐξ ἡμῶν, τῷ θείῳ σου νεύματι, μεταστάντας δούλους σου, Ἀβραὰμ ἐν κόλποις, τοῦ θεράποντός σου σκήνωσον."

ocMA.OdeW6C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW6C1T.ode = miscellanea_gr_GR_cog.misc.Ode6
ocMA.OdeW6C1T.melody = he.h.m8_gr_GR_cog.heHE.IlasthitiMoiSotir.name
ocMA.OdeW6C1T.incipit = "Ἐσκήνωσεν ἐπὶ σοί, Πατρὸς Ἁγνὴ…"
ocMA.OdeW6C1T.text = "Ἐσκήνωσεν ἐπὶ σοί, Πατρὸς Ἁγνὴ τὸ ἀπαύγασμα, καὶ ταῖς ἀΰλοις αὐγαῖς, αὐτοῦ τῆς Θεότητος, τὸ σκότος διέλυσε, τῆς πολυθεΐας, καὶ τὸν κόσμον κατεφώτισεν."

ocMA.OdeW6C21.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW6C21.ode = miscellanea_gr_GR_cog.misc.Ode6
ocMA.OdeW6C21.greekmelody = he.h.m8_gr_GR_cog.heHE.IlasthitiMoiSotir.greekname
ocMA.OdeW6C21.melody = he.h.m8_gr_GR_cog.heHE.IlasthitiMoiSotir.name
ocMA.OdeW6C21.incipit = "Οἱ Μάρτυρές σου Σωτήρ, πολλὰς…"
ocMA.OdeW6C21.text = "Οἱ Μάρτυρές σου Σωτήρ, πολλὰς βασάνους ὑπέμειναν, τῷ πόθῳ σου τὰς ψυχάς, τρωθέντες οἱ Ἅγιοι, τῆς σῆς ὀρεγόμενοι, ἀϊδίου δόξης, καὶ γλυκείας σου μεθέξεως."

ocMA.OdeW6C22.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW6C22.ode = miscellanea_gr_GR_cog.misc.Ode6
ocMA.OdeW6C22.melody = he.h.m8_gr_GR_cog.heHE.IlasthitiMoiSotir.name
ocMA.OdeW6C22.incipit = "Γαστέρα τοῦ δυσμενοῦς, θανάτου Σῶτερ…"
ocMA.OdeW6C22.text = "Γαστέρα τοῦ δυσμενοῦς, θανάτου Σῶτερ διέῤῥηξας, καὶ πάντας τοὺς ἐν αὐτῇ, δεσμώτας ἀνέστησας, ζωὴν χαρισάμενος· ἧς τοὺς μεταστάντας, Εὐεργέτα καταξίωσον."

ocMA.OdeW6C23.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW6C23.ode = miscellanea_gr_GR_cog.misc.Ode6
ocMA.OdeW6C23.melody = he.h.m8_gr_GR_cog.heHE.IlasthitiMoiSotir.name
ocMA.OdeW6C23.incipit = "Δακρύων καὶ στεναγμῶν, τῶν ἐν…"
ocMA.OdeW6C23.text = "Δακρύων καὶ στεναγμῶν, τῶν ἐν τῷ ᾅδῃ τοὺς δούλους σου, ἀπάλλαξον Ἀγαθέ· ὡς μόνος γὰρ εὔσπλαγχνος, ἀφεῖλες πᾶν δάκρυον, ἐκ παντὸς προσώπου, τῶν πιστῶς θεολογούντων σε."

ocMA.OdeW6C2T.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW6C2T.ode = miscellanea_gr_GR_cog.misc.Ode6
ocMA.OdeW6C2T.melody = he.h.m8_gr_GR_cog.heHE.IlasthitiMoiSotir.name
ocMA.OdeW6C2T.incipit = "Ὁ φύσει Δημιουργός, δημιουργεῖται ἐν…"
ocMA.OdeW6C2T.text = "Ὁ φύσει Δημιουργός, δημιουργεῖται ἐν μήτρᾳ σου· ὁ πλήρης καθʼ ὃ Θεός, κενοῦται Πανάμωμε· ὁ μόνος ἀθάνατος, τὸν ὑπὲρ τῆς πάντων, σωτηρίας θνήσκει θάνατον."

ocMA.OdeW7C1H.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW7C1H.ode = miscellanea_gr_GR_cog.misc.Ode7
ocMA.OdeW7C1H.greekmelody = he.h.m8_gr_GR_cog.heHE.PaidesTheoseveis.greekname
ocMA.OdeW7C1H.melody = he.h.m8_gr_GR_cog.heHE.PaidesTheoseveis.melody
ocMA.OdeW7C1H.incipit = he.h.m8_gr_GR_cog.heHE.PaidesTheoseveis.incipit
ocMA.OdeW7C1H.text = he.h.m8_gr_GR_cog.heHE.PaidesTheoseveis.text

ocMA.OdeW7C11.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW7C11.ode = miscellanea_gr_GR_cog.misc.Ode7
ocMA.OdeW7C11.melody = he.h.m8_gr_GR_cog.heHE.PaidesTheoseveis.name
ocMA.OdeW7C11.incipit = "Λύσαντες τὸν φραγμὸν τῆς δυσσεβείας…"
ocMA.OdeW7C11.text = "Λύσαντες τὸν φραγμὸν τῆς δυσσεβείας, δεσμοῖς ἱεροῖς ὑμῶν πανεύφημοι, τῶν ἡμῶν ἐγκλημάτων, φορτία σκορπίσατε, καὶ βοῶντας Μάρτυρες σώσατε· Εὐλογητὸς ὁ Θεός, ὁ τῶν πατέρων ἡμῶν."

ocMA.OdeW7C12.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW7C12.ode = miscellanea_gr_GR_cog.misc.Ode7
ocMA.OdeW7C12.melody = he.h.m8_gr_GR_cog.heHE.PaidesTheoseveis.name
ocMA.OdeW7C12.incipit = "Ὄμβροις τῶν ἱερῶν δογμάτων, αἱρέσεων…"
ocMA.OdeW7C12.text = "Ὄμβροις τῶν ἱερῶν δογμάτων, αἱρέσεων πῦρ ἐναπεσβέσατε, καὶ φλογὶ Ἱεράρχαι, σοφοὶ κατεφλέξατε, ἀσεβείας πλάνης ζιζάνια, καὶ τῶν πιστῶν τὰς ψυχάς, κατεφωτίσατε."

ocMA.OdeW7C13.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW7C13.ode = miscellanea_gr_GR_cog.misc.Ode7
ocMA.OdeW7C13.melody = he.h.m8_gr_GR_cog.heHE.PaidesTheoseveis.name
ocMA.OdeW7C13.incipit = "Σάρκα οἱ ἀσκηταὶ διʼ ἐγκρατείας…"
ocMA.OdeW7C13.text = "Σάρκα οἱ ἀσκηταὶ διʼ ἐγκρατείας, νεκρώσαντες ζῶσι μετὰ θάνατον· Προφητῶν τε Δικαίων, χορὸς καὶ κατάλογος, Γυναικῶν ἀθλήσας δεδόξασται. Αὐτῶν πρεσβείαις Χριστέ, κινδύνων ῥῦσαι ἡμᾶς."

ocMA.OdeW7C14.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW7C14.ode = miscellanea_gr_GR_cog.misc.Ode7
ocMA.OdeW7C14.melody = he.h.m8_gr_GR_cog.heHE.PaidesTheoseveis.name
ocMA.OdeW7C14.incipit = "Πάντων τῶν ἐπʼ ἐλπίδι κοιμηθέντων…"
ocMA.OdeW7C14.text = "Πάντων τῶν ἐπʼ ἐλπίδι κοιμηθέντων, ζωῆς τὰς ψυχὰς Χριστὲ ἀνάπαυσον, παρορῶν τὰ ἐν βίῳ, αὐτῶν πλημμελήματα, διὰ τὴν πολλὴν εὐσπλαγχνίαν σου, εὔσπλαγχνε μόνε Σωτήρ, ὁ τῶν πατέρων Θεός."

ocMA.OdeW7C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW7C1T.ode = miscellanea_gr_GR_cog.misc.Ode7
ocMA.OdeW7C1T.melody = he.h.m8_gr_GR_cog.heHE.PaidesTheoseveis.name
ocMA.OdeW7C1T.incipit = "Ὅλη ὡς καθαρὰ καθυπεδέξω, ἐν…"
ocMA.OdeW7C1T.text = "Ὅλη ὡς καθαρὰ καθυπεδέξω, ἐν μήτρᾳ τὸν Λόγον σωματούμενον· αὐτὸν γοῦν ἐκδυσώπει, καθάραι πταισμάτων με, καὶ ψυχῆς καὶ σώματος Πάναγνε, τὸν ἐπὶ σοὶ καθαρᾷ πίστει προστρέχοντα."

ocMA.OdeW7C21.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW7C21.ode = miscellanea_gr_GR_cog.misc.Ode7
ocMA.OdeW7C21.greekmelody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.greekname
ocMA.OdeW7C21.melody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.name
ocMA.OdeW7C21.incipit = "Ὅλος ὁ πόθος τῶν Μαρτύρων…"
ocMA.OdeW7C21.text = "Ὅλος ὁ πόθος τῶν Μαρτύρων, ἀνατέτακται πρὸς μόνον τὸν Δεσπότην, διʼ ἀγάπης αὐτῷ, ἑνούμενος καὶ μέλπων· Εὐλογητὸς εἶ Κύριε, ὁ Θεὸς εἰς τοὺς αἰῶνας."

ocMA.OdeW7C22.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW7C22.ode = miscellanea_gr_GR_cog.misc.Ode7
ocMA.OdeW7C22.melody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.name
ocMA.OdeW7C22.incipit = "Νέμοις τοῖς πίστει μεταστᾶσι, τὴν…"
ocMA.OdeW7C22.text = "Νέμοις τοῖς πίστει μεταστᾶσι, τὴν φαιδρότητα τῆς θείας Βασιλείας, ἀφθαρσίας στολήν, δωρούμενος βοῶσιν· Εὐλογητὸς εἶ Κύριε, ὁ Θεὸς εἰς τοὺς αἰῶνας."

ocMA.OdeW7C23.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW7C23.ode = miscellanea_gr_GR_cog.misc.Ode7
ocMA.OdeW7C23.melody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.name
ocMA.OdeW7C23.incipit = "Πλῆσον χαρᾶς καὶ εὐφροσύνης, οὓς…"
ocMA.OdeW7C23.text = "Πλῆσον χαρᾶς καὶ εὐφροσύνης, οὓς μετέστησας οἰκέτας σου Οἰκτίρμον, ἀξιώσας αὐτούς, κραυγάζειν σοι καὶ μέλπειν· Εὐλογητὸς εἶ Κύριε, ὁ Θεὸς εἰς τοὺς αἰῶνας."

ocMA.OdeW7C2T.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW7C2T.ode = miscellanea_gr_GR_cog.misc.Ode7
ocMA.OdeW7C2T.melody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.name
ocMA.OdeW7C2T.incipit = "Λύων τῆς Εὔας τὴν κατάραν…"
ocMA.OdeW7C2T.text = "Λύων τῆς Εὔας τὴν κατάραν, τὴν πανάμωμον κατῴκησας Παρθένον, εὐλογίας πηγήν, βλυστάνων τοῖς βοῶσιν· Εὐλογημένος Πάναγνε, ὁ καρπὸς τῆς σῆς κοιλίας."

ocMA.OdeW8C1H.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW8C1H.ode = miscellanea_gr_GR_cog.misc.Ode8
ocMA.OdeW8C1H.greekmelody = he.h.m8_gr_GR_cog.heHE.OiTheorrimonesPaides.greekname
ocMA.OdeW8C1H.melody = he.h.m8_gr_GR_cog.heHE.OiTheorrimonesPaides.melody
ocMA.OdeW8C1H.incipit = he.h.m8_gr_GR_cog.heHE.OiTheorrimonesPaides.incipit
ocMA.OdeW8C1H.text = he.h.m8_gr_GR_cog.heHE.OiTheorrimonesPaides.text

ocMA.OdeW8C11.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW8C11.ode = miscellanea_gr_GR_cog.misc.Ode8
ocMA.OdeW8C11.melody = he.h.m8_gr_GR_cog.heHE.OiTheorrimonesPaides.name
ocMA.OdeW8C11.incipit = "Ναοὺς εἰδώλων καθείλετε ἀθλοῦντες, καὶ…"
ocMA.OdeW8C11.text = "Ναοὺς εἰδώλων καθείλετε ἀθλοῦντες, καὶ ἑαυτοὺς τῆς Τριάδος, θείους ναοὺς ἐδομήσασθε, Ἀθλοφόροι Κυρίου, Ἀγγέλων συνόμιλοι."

ocMA.OdeW8C12.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW8C12.ode = miscellanea_gr_GR_cog.misc.Ode8
ocMA.OdeW8C12.melody = he.h.m8_gr_GR_cog.heHE.OiTheorrimonesPaides.name
ocMA.OdeW8C12.incipit = "Οἱ ἱερεῖς σου Χριστὲ δικαιοσύνην…"
ocMA.OdeW8C12.text = "Οἱ ἱερεῖς σου Χριστὲ δικαιοσύνην, ἐνδεδυμένοι ὁσίοις, τοῖς ἀπʼ αἰῶνος νῦν συναγάλλονται, σοῦ τὸ κάλλος ὁρῶντες, τὸ θεῖον τρανότερον."

ocMA.OdeW8C13.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW8C13.ode = miscellanea_gr_GR_cog.misc.Ode8
ocMA.OdeW8C13.melody = he.h.m8_gr_GR_cog.heHE.OiTheorrimonesPaides.name
ocMA.OdeW8C13.incipit = "Ἱερωτάτων πρεσβείαις Προφητῶν σου, καὶ…"
ocMA.OdeW8C13.text = "Ἱερωτάτων πρεσβείαις Προφητῶν σου, καὶ Γυναικῶν ἀοιδίμων, τῶν ἀπʼ αἰῶνος δικαίων τε, τὰ ἐλέη σου Λόγε, τῷ κόσμῳ σου δώρησαι."

ocMA.OdeW8C14.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW8C14.ode = miscellanea_gr_GR_cog.misc.Ode8
ocMA.OdeW8C14.melody = he.h.m8_gr_GR_cog.heHE.OiTheorrimonesPaides.name
ocMA.OdeW8C14.incipit = "Δικαιοκρῖτα ἡνίκα μέλλεις κρῖναι, οὓς…"
ocMA.OdeW8C14.text = "Δικαιοκρῖτα ἡνίκα μέλλεις κρῖναι, οὓς ἐξ ἡμῶν προσελάβου, ἀκατακρίτους συντήρησον, παραβλέψας τὰ τούτων, ἐγκλήματα Δέσποτα."

ocMA.OdeW8C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW8C1T.ode = miscellanea_gr_GR_cog.misc.Ode8
ocMA.OdeW8C1T.melody = he.h.m8_gr_GR_cog.heHE.OiTheorrimonesPaides.name
ocMA.OdeW8C1T.incipit = "Ἐπιφανεῖσα Παρθένε τῆς ψυχῆς μου…"
ocMA.OdeW8C1T.text = "Ἐπιφανεῖσα Παρθένε τῆς ψυχῆς μου, τὰ σκοτεινότατα νέφη, τῷ φωτισμῷ σου ἀπέλασον, ἡ τῆς δικαιοσύνης, τεκοῦσα τὸν ἥλιον."

ocMA.OdeW8C21.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW8C21.ode = miscellanea_gr_GR_cog.misc.Ode8
ocMA.OdeW8C21.greekmelody = he.h.m8_gr_GR_cog.heHE.MousikonOrganon.greekname
ocMA.OdeW8C21.melody = he.h.m8_gr_GR_cog.heHE.MousikonOrganon.name
ocMA.OdeW8C21.incipit = "Ἐπιγείους ἄθλους διελθόντες, οὐρανίους ἔλαβον…"
ocMA.OdeW8C21.text = "Ἐπιγείους ἄθλους διελθόντες, οὐρανίους ἔλαβον στεφάνους, οἱ Μάρτυρες οἱ εὐκλεεῖς, βοῶντές σοι ἀπαύστως· Τὸν Κύριον ὑμνεῖτε, καὶ ὑπερυψοῦτε εἰς πάντας τοὺς αἰῶνας."

ocMA.OdeW8C22.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW8C22.ode = miscellanea_gr_GR_cog.misc.Ode8
ocMA.OdeW8C22.melody = he.h.m8_gr_GR_cog.heHE.MousikonOrganon.name
ocMA.OdeW8C22.incipit = "Καταβὰς ἐν λάκκῳ κατωτάτῳ, τοὺς…"
ocMA.OdeW8C22.text = "Καταβὰς ἐν λάκκῳ κατωτάτῳ, τοὺς ἐν τοῖς μνημείοις κατοικοῦντας, ἀνέστησας ζωαρχικῇ, παλάμῃ, καὶ παρέσχες, τοῖς δούλοις σου τοῖς πίστει, προκεκοιμημένοις ἀνάπαυσιν Οἰκτίρμον."

ocMA.OdeW8C23.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW8C23.ode = miscellanea_gr_GR_cog.misc.Ode8
ocMA.OdeW8C23.melody = he.h.m8_gr_GR_cog.heHE.MousikonOrganon.name
ocMA.OdeW8C23.incipit = "Ὡς πηγὴ ζωῆς τῆς ἀϊδίου…"
ocMA.OdeW8C23.text = "Ὡς πηγὴ ζωῆς τῆς ἀϊδίου, ὡς τρυφῆς ὑπάρχων ὁ χειμάῤῥους, τοὺς δούλους σου τοὺς εὐσεβῶς, πρὸς σὲ μετατεθέντας, ἀξίωσον ὑμνεῖν σε, καὶ δοξολογεῖν σε εἰς πάντας τοὺς αἰῶνας."

ocMA.OdeW8C2T.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW8C2T.ode = miscellanea_gr_GR_cog.misc.Ode8
ocMA.OdeW8C2T.melody = he.h.m8_gr_GR_cog.heHE.MousikonOrganon.name
ocMA.OdeW8C2T.incipit = "Μαρία Παρθένε Θεοτόκε, ἡ Θεὸν…"
ocMA.OdeW8C2T.text = "Μαρία Παρθένε Θεοτόκε, ἡ Θεὸν Σωτῆρα τοῖς ἀνθρώποις, γεννήσασα σωματικῶς, περίσωζε τοὺς πίστει, ὑμνοῦντάς σου τὸν τόκον, καὶ ὑπερυψοῦντας εἰς πάντας τοὺς αἰῶνας."

ocMA.OdeW9C1H.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW9C1H.ode = miscellanea_gr_GR_cog.misc.Ode9
ocMA.OdeW9C1H.greekmelody = he.h.m8_gr_GR_cog.heHE.SeTinApeirogamon.greekname
ocMA.OdeW9C1H.melody = he.h.m8_gr_GR_cog.heHE.SeTinApeirogamon.melody
ocMA.OdeW9C1H.incipit = he.h.m8_gr_GR_cog.heHE.SeTinApeirogamon.incipit
ocMA.OdeW9C1H.text = he.h.m8_gr_GR_cog.heHE.SeTinApeirogamon.text

ocMA.OdeW9C11.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW9C11.ode = miscellanea_gr_GR_cog.misc.Ode9
ocMA.OdeW9C11.melody = he.h.m8_gr_GR_cog.heHE.SeTinApeirogamon.name
ocMA.OdeW9C11.incipit = "Ἵσταντο οἱ Μάρτυρες, πρὸ τῶν…"
ocMA.OdeW9C11.text = "Ἵσταντο οἱ Μάρτυρες, πρὸ τῶν ἀδίκων κριτηρίων, πᾶσαν ἀδικίαν χάριτι, τοῦ Χριστοῦ κατακρίνοντες, καὶ τοὺς ὑπʼ αὐτῶν κεκρατημένους ἐλέγχοντες, καὶ στεφάνους δικαιοσύνης κομιζόμενοι."

ocMA.OdeW9C12.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW9C12.ode = miscellanea_gr_GR_cog.misc.Ode9
ocMA.OdeW9C12.melody = he.h.m8_gr_GR_cog.heHE.SeTinApeirogamon.name
ocMA.OdeW9C12.incipit = "Ὤφθητε ὡς οἴακες, τῆς Ἐκκλησίας…"
ocMA.OdeW9C12.text = "Ὤφθητε ὡς οἴακες, τῆς Ἐκκλησίας τὴν ὁλκάδα, πᾶσαν εὐσεβῶς ἰθύναντες, εἰς λιμένα Θεοῦ ἐντολῶν, πάντες Ἱεράρχαι καὶ Ποιμένες μακάριοι· διὰ τοῦτο ὡς κυβερνήτας ὑμᾶς σέβομεν."

ocMA.OdeW9C13.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW9C13.ode = miscellanea_gr_GR_cog.misc.Ode9
ocMA.OdeW9C13.melody = he.h.m8_gr_GR_cog.heHE.SeTinApeirogamon.name
ocMA.OdeW9C13.incipit = "Στῖφος ἱκετεύει σε, τῶν Προφητῶν…"
ocMA.OdeW9C13.text = "Στῖφος ἱκετεύει σε, τῶν Προφητῶν καὶ τῶν Ὁσίων, σύλλογος δικαίων Κύριε, ἀθλησάντων λαμπρότατα, καὶ διαπρεψάντων ἐν ἀσκήσει Φιλάνθρωπε, δυσωπεῖ σε τοὺς οἰκτιρμούς σου ἡμῖν δώρησαι."

ocMA.OdeW9C14.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW9C14.ode = miscellanea_gr_GR_cog.misc.Ode9
ocMA.OdeW9C14.melody = he.h.m8_gr_GR_cog.heHE.SeTinApeirogamon.name
ocMA.OdeW9C14.incipit = "Ἧς κατηξιώθησαν, τιμῆς καὶ δόξης…"
ocMA.OdeW9C14.text = "Ἧς κατηξιώθησαν, τιμῆς καὶ δόξης αἰωνίου, ἅπαντες Χριστοῦ οἱ Ἅγιοι, τοὺς πιστῶς ἐκδημήσαντας, τοῦ ματαίου βίου μετασχεῖν καταξίωσον, Ἐλεῆμον ταῖς ἱκεσίαις τῶν Ἁγίων σου."

ocMA.OdeW9C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW9C1T.ode = miscellanea_gr_GR_cog.misc.Ode9
ocMA.OdeW9C1T.melody = he.h.m8_gr_GR_cog.heHE.SeTinApeirogamon.name
ocMA.OdeW9C1T.incipit = "Φέρουσα τὸ πλήρωμα, τῶν ἀγαθῶν…"
ocMA.OdeW9C1T.text = "Φέρουσα τὸ πλήρωμα, τῶν ἀγαθῶν ἐν ταῖς ἀγκάλαις, πλήρωσον ἡμῶν Πανάμωμε, τὰς ἐντεύξεις τῶν δούλων σου, καὶ τὰς πρὸς Θεὸν ἡμῶν πορείας κατεύθυνον, ἐναρέτως ἐνδυναμοῦσα πολιτεύεσθαι."

ocMA.OdeW9C21.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW9C21.ode = miscellanea_gr_GR_cog.misc.Ode9
ocMA.OdeW9C21.greekmelody = he.h.m8_gr_GR_cog.heHE.EfrixePasaAkoi.greekname
ocMA.OdeW9C21.melody = he.h.m8_gr_GR_cog.heHE.EfrixePasaAkoi.name
ocMA.OdeW9C21.incipit = "Ἔχοντες Μάρτυρες Χριστοῦ, τὴν ἀνίκητον…"
ocMA.OdeW9C21.text = "Ἔχοντες Μάρτυρες Χριστοῦ, τὴν ἀνίκητον ἰσχὺν καὶ ἀήττητον, ἐξεφαυλίσατε, τὸ τῶν τυράννων, ἄθεον πρόσταγμα, καὶ Βασιλείας οὐρανῶν, σαφῶς ἠξιώθητε, καταλαμπόμενοι, τῆς Τριάδος ταῖς αὐγαῖς ἀξιάγαστοι."

ocMA.OdeW9C22.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW9C22.ode = miscellanea_gr_GR_cog.misc.Ode9
ocMA.OdeW9C22.melody = he.h.m8_gr_GR_cog.heHE.EfrixePasaAkoi.name
ocMA.OdeW9C22.incipit = "Λέλυται ᾅδης ὁ πικρός, καταλύσαντος…"
ocMA.OdeW9C22.text = "Λέλυται ᾅδης ὁ πικρός, καταλύσαντος αὐτὸν σοῦ Φιλάνθρωπε, καὶ ἀναστήσαντος, τοὺς ἀπʼ αἰῶνος ἐκεῖ καθεύδοντας· ἀλλὰ καὶ νῦν ὡς ἀγαθός, τοὺς μεταχωρήσαντας, πρὸς σὲ τὸν Εὔσπλαγχνον, ἀνεσπέρου σου φωτὸς καταξίωσον."

ocMA.OdeW9C23.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW9C23.ode = miscellanea_gr_GR_cog.misc.Ode9
ocMA.OdeW9C23.melody = he.h.m8_gr_GR_cog.heHE.EfrixePasaAkoi.name
ocMA.OdeW9C23.incipit = "Ὅλος εἶ Σῶτερ γλυκασμός, ὅλος…"
ocMA.OdeW9C23.text = "Ὅλος εἶ Σῶτερ γλυκασμός, ὅλος εἶ ἐπιθυμία καὶ ἔφεσις, ὄντως ἀκόρεστος, ὅλος ὑπάρχεις, κάλλος ἀμήχανον· τοὺς μεταστάντας οὖν πρὸς σέ, τῆς σῆς ὡραιότητος, τρυφᾷν εὐδόκησον, καὶ τῆς θείας καλλονῆς σου ἀξίωσον."

ocMA.OdeW9C2T.mode = miscellanea_gr_GR_cog.misc.Mode8
ocMA.OdeW9C2T.ode = miscellanea_gr_GR_cog.misc.Ode9
ocMA.OdeW9C2T.melody = he.h.m8_gr_GR_cog.heHE.EfrixePasaAkoi.name
ocMA.OdeW9C2T.incipit = "Σῶσόν με Μῆτερ τοῦ Θεοῦ…"
ocMA.OdeW9C2T.text = "Σῶσόν με Μῆτερ τοῦ Θεοῦ, ἡ γεννήσασα Χριστὸν τὸν Σωτῆρά μου, Θεὸν καὶ ἄνθρωπον, διπλοῦν τὴν φύσιν, οὐ τὴν ὑπόστασιν, μονογενῆ μὲν ἐκ Πατρός, ἐκ σοῦ δὲ πρωτότοκον, πάσης τῆς κτίσεως· ὃν ἐν φύσεσι διτταῖς μεγαλύνομεν."

