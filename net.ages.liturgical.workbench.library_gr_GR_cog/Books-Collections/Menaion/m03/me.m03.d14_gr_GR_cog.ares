meMA.Ode3C1H.text = he.h.m2_gr_GR_cog.heHE.StereosonImasEnSoiKyrie.text

meMA.Ode3C11.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C11.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode3C11.greekmelody = he.h.m2_gr_GR_cog.heHE.StereosonImasEnSoiKyrie.greekname
meMA.Ode3C11.melody = he.h.m2_gr_GR_cog.heHE.StereosonImasEnSoiKyrie.name
meMA.Ode3C11.incipit = "Ναμάτων ζωτικῶν τοῦ θείου Πνεύματος…"
meMA.Ode3C11.text = "Ναμάτων ζωτικῶν τοῦ θείου Πνεύματος, πλησθεῖσα Βενέδικτε ἡ ψυχή σου, ποταμοὺς θαυμάτων ἔβλυσε, νοσημάτων ξηραίνοντας ἐπήρειαν."

meMA.Ode3C12.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C12.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode3C12.melody = he.h.m2_gr_GR_cog.heHE.StereosonImasEnSoiKyrie.name
meMA.Ode3C12.incipit = "Πρὸς πλάτος Παραδείσου κατεσκήνωσας, Παμμάκαρ…"
meMA.Ode3C12.text = "Πρὸς πλάτος Παραδείσου κατεσκήνωσας, Παμμάκαρ ὁδεύσας, στενὴν τὴν τρίβον, καὶ δαιμόνων πανουργεύματα, καὶ πορείας ἀτάκτους ἀπεστένωσας."

meMA.Ode3C13.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C13.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode3C13.melody = he.h.m2_gr_GR_cog.heHE.StereosonImasEnSoiKyrie.name
meMA.Ode3C13.incipit = "Ῥοαῖς σου τῶν δακρύων ἀρδευόμενος…"
meMA.Ode3C13.text = "Ῥοαῖς σου τῶν δακρύων ἀρδευόμενος, ὡς δένδρον Βενέδικτε καρποφόρον, εὐκαρπίαν θείαν ἤνεγκας, ἀρετῶν καὶ θαυμάτων θείᾳ χάριτι."

meMA.Ode3C1T.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C1T.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode3C1T.melody = he.h.m2_gr_GR_cog.heHE.StereosonImasEnSoiKyrie.name
meMA.Ode3C1T.incipit = "Ὁ μόνος ἀγαθὸς τὴν σὴν…"
meMA.Ode3C1T.text = "Ὁ μόνος ἀγαθὸς τὴν σὴν σαρκούμενος, ὑπέδυ Παναμώμητε γαστέρα, καὶ βροτὸς ὡράθη τέλειος, ἱκέτευε σῶσαι τὰς ψυχὰς ἡμῶν."

meMA.Ode3Kathisma1.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode3Kathisma1.greekmelody = he.a.m1_gr_GR_cog.heAU.TouLithouSfragisthentos.greekname
meMA.Ode3Kathisma1.melody = he.a.m1_gr_GR_cog.heAU.TouLithouSfragisthentos.name
meMA.Ode3Kathisma1.incipit = "Μονάσας θεαρέστως, ἐναρέτως ἐβίωσας, καὶ…"
meMA.Ode3Kathisma1.text = "Μονάσας θεαρέστως, ἐναρέτως ἐβίωσας, καὶ χάριν ἰαμάτων, ἐκομίσω Βενέδικτε, θαυμάσια τελέσας φοβερά, Μονὴν δὲ συγκροτήσας ἱεράν, προσενήνοχας Κυρίῳ τῶν σῳζομένων πληθὺν παναοίδιμε. Δόξα τῷ σὲ λαμπρύναντι Θεῷ, δόξα τῷ σὲ στεφανώσαντι, δόξα τῷ ἐνεργοῦντι διὰ σοῦ πᾶσιν ἰάματα."

meMA.Ode3KathismaT.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode3KathismaT.melody = he.a.m1_gr_GR_cog.heAU.TouLithouSfragisthentos.name
meMA.Ode3KathismaT.incipit = me.m02.d13_gr_GR_cog.meMA.Ode3KathismaT.incipit
meMA.Ode3KathismaT.text = me.m02.d13_gr_GR_cog.meMA.Ode3KathismaT.text

meMA.Ode3KathismaSt.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode3KathismaSt.melody = he.a.m1_gr_GR_cog.heAU.TouLithouSfragisthentos.name
meMA.Ode3KathismaSt.incipit = "Σταυρῷ σου προσπαγέντος, ὑπὸ τῶν…"
meMA.Ode3KathismaSt.text = "Σταυρῷ σου προσπαγέντος, ὑπὸ τῶν παρανόμων, καὶ στρατιωτῶν Σῶτερ λόγχῃ, τὴν πλευρὰν ὀρυγέντος, ἡ Πάναγνος ὠδύρετο πικρῶς, τὰ σπλάγχνα κοπτομένη μητρικῶς, καὶ τὸ πολύ σου καὶ φρικτόν, τῆς ἀνοχῆς ἐξίστατο βοῶσα, Δόξα τῇ πρὸς ἀνθρώπους σου στοργῇ, δόξα τῇ σῇ χρηστότητι, δόξα τῷ ἐν τῷ θανάτῳ σου βροτοὺς ἀθανατίζοντι."

meMA.Ode4C1H.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C1H.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode4C1H.greekmelody = he.h.m2_gr_GR_cog.heHE.YmnoSeAkoi.greekname
meMA.Ode4C1H.melody = he.h.m2_gr_GR_cog.heHE.YmnoSeAkoi.melody
meMA.Ode4C1H.incipit = he.h.m2_gr_GR_cog.heHE.YmnoSeAkoi.incipit
meMA.Ode4C1H.text = he.h.m2_gr_GR_cog.heHE.YmnoSeAkoi.text

meMA.Ode4C11.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C11.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode4C11.greekmelody = he.h.m2_gr_GR_cog.heHE.YmnoSeAkoi.greekname
meMA.Ode4C11.melody = he.h.m2_gr_GR_cog.heHE.YmnoSeAkoi.name
meMA.Ode4C11.incipit = "Σταυρώσας, σεαυτὸν τοῖς πάθεσι, καὶ…"
meMA.Ode4C11.text = "Σταυρώσας, σεαυτὸν τοῖς πάθεσι, καὶ τῷ κόσμῳ θεόφρον Πάτερ, τὸν ἐν Σταυρῷ τείναντα ἐθελουσίως τὰς παλάμας Χριστόν, ἐθεράπευσας Βενέδικτε, ὃν ἐκδυσώπει σῶσαι τὰς ψυχὰς ἡμῶν."

meMA.Ode4C12.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C12.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode4C12.melody = he.h.m2_gr_GR_cog.heHE.YmnoSeAkoi.name
meMA.Ode4C12.incipit = "Ἀγῶσιν, ἐγκρατείας Ὅσιε, τῆς σαρκὸς…"
meMA.Ode4C12.text = "Ἀγῶσιν, ἐγκρατείας Ὅσιε, τῆς σαρκὸς νεκρώσας τὰ μέλη, νεκροὺς εὐχῇ ἤγειρας, καὶ παρειμένοις εὐδρομίαν πιστῶς, θαυμαζόμενος ἀπένειμας, καὶ πᾶσαν νόσον Πάτερ ἐθεράπευσας."

meMA.Ode4C13.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C13.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode4C13.melody = he.h.m2_gr_GR_cog.heHE.YmnoSeAkoi.name
meMA.Ode4C13.incipit = "Ξηράς τε, καὶ ἀνίκους Ὅσιε…"
meMA.Ode4C13.text = "Ξηράς τε, καὶ ἀνίκους Ὅσιε, ἀπειργάσω ψυχὰς γονίμους, τῷ ζωτικῷ λόγῳ σου, καὶ τῶν θαυμάτων ἐπιδείξει, Ποιμὴν θεοπρόβλητος γενόμενος, καὶ μοναζόντων κόσμος ὡραιότατος."

meMA.Ode4C1T.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C1T.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode4C1T.melody = he.h.m2_gr_GR_cog.heHE.YmnoSeAkoi.name
meMA.Ode4C1T.incipit = "Ὡς ὄμβρος ἐπὶ πόκον Ἄχραντε…"
meMA.Ode4C1T.text = "Ὡς ὄμβρος ἐπὶ πόκον Ἄχραντε, τῇ σεπτῇ σου γαστρὶ ὁ Λόγος, κατῆλθε σαρκούμενος, καὶ ἔπαυσε πολυθεΐας σαφῶς, τὰ ὀμβρήματα Πανάμωμε, καὶ τὸν πικρὸν χειμῶνα διεσκέδασεν."

meMA.Ode5C1H.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C1H.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode5C1H.greekmelody = he.h.m2_gr_GR_cog.heHE.OTouFotosChorigos.greekname
