meMA.Ode1C11.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C11.mode = misc_gr_GR_cog.Mode6
meMA.Ode1C11.melody = he.h.m6_gr_GR_cog.OsEnIpeiroPezefsas.name
meMA.Ode1C11.incipit = "Συντετριμμένης καρδίας τὸν στεναγμόν, ἐπακούσας…"
meMA.Ode1C11.text = "Συντετριμμένης καρδίας τὸν στεναγμόν, ἐπακούσας Δέσποτα, τῆς ψυχῆς μου τὴν δεινήν, συντριβὴν θεράπευσον εὐχαῖς, τοῦ Ὁσίου σου Χριστέ, ὡς παντοδύναμος."

meMA.Ode1C12.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C12.mode = misc_gr_GR_cog.Mode6
meMA.Ode1C12.melody = he.h.m6_gr_GR_cog.OsEnIpeiroPezefsas.name
meMA.Ode1C12.incipit = "Ἐπιβὰς θεοσόφως τῆς πρακτικῆς, ἀρετῆς…"
meMA.Ode1C12.text = "Ἐπιβὰς θεοσόφως τῆς πρακτικῆς, ἀρετῆς, Ἰσίδωρε, θεωρίας ἀκραιφνοῦς, τὴν τερπνὴν ἐμφάνειαν, Θεῷ, ὁμιλῶν διαπαντὸς σαφῶς ἐπλούτησας."

meMA.Ode1C13.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C13.mode = misc_gr_GR_cog.Mode6
meMA.Ode1C13.melody = he.h.m6_gr_GR_cog.OsEnIpeiroPezefsas.name
meMA.Ode1C13.incipit = "Τῷ τῆς σοφίας κρατῆρι προσαγαγών…"
meMA.Ode1C13.text = "Τῷ τῆς σοφίας κρατῆρι προσαγαγών, τὸ σὸν στόμα Πάνσοφε, τῶν ἐκεῖθεν δωρεῶν, τὴν ῥοὴν ἑξήντλησας Θεῷ, ἐπινίκιον ᾠδὴν ἀνακρουόμενος."

meMA.Ode1C1T.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C1T.mode = misc_gr_GR_cog.Mode6
meMA.Ode1C1T.melody = he.h.m6_gr_GR_cog.OsEnIpeiroPezefsas.name
meMA.Ode1C1T.incipit = "Ὡς ἐπὶ κούφης νεφέλης ὁ…"
meMA.Ode1C1T.text = "Ὡς ἐπὶ κούφης νεφέλης ὁ Πλαστουργός, ἐπὶ σοὶ ἐλήλυθε, καθελεῖν ὡς δυνατός, τὰ Αἰγύπτου Δέσποινα ἁγνή, χειροποίητα Χριστὸς ὡς παντοδύναμος."

meMA.Ode3C1H.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C1H.mode = misc_gr_GR_cog.Mode6
meMA.Ode3C1H.melody = he.h.m6_gr_GR_cog.OukEstinAgios.melody
meMA.Ode3C1H.incipit = he.h.m6_gr_GR_cog.OukEstinAgios.incipit
meMA.Ode3C1H.text = he.h.m6_gr_GR_cog.OukEstinAgios.text

meMA.Ode3C11.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C11.mode = misc_gr_GR_cog.Mode6
meMA.Ode3C11.melody = he.h.m6_gr_GR_cog.OukEstinAgios.name
meMA.Ode3C11.incipit = "Νοὸς ὀξύτητι σαφῶς, τὴν τῶν…"
meMA.Ode3C11.text = "Νοὸς ὀξύτητι σαφῶς, τὴν τῶν ὄντων ἀθροίσας, Παναοίδιμε φύσιν, διʼ αὐτῆς τὴν ἀρχικήν, αἰτίαν ὡς καθαρός, θεολόγος ἀπλανῶς κατέλαβες."

meMA.Ode3C12.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C12.mode = misc_gr_GR_cog.Mode6
meMA.Ode3C12.melody = he.h.m6_gr_GR_cog.OukEstinAgios.name
meMA.Ode3C12.incipit = "Μελέτην θέμενος τρανῶς, τοῦ θανάτου…"
meMA.Ode3C12.text = "Μελέτην θέμενος τρανῶς, τοῦ θανάτου τὸν βίον, θεοφόρε τρισμάκαρ, τῶν παθῶν τὰς ὑλικάς, ἐνέκρωσας ἀφορμάς, ἀπαθείας συσχεθεὶς τῷ ἔρωτι."

meMA.Ode3C13.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C13.mode = misc_gr_GR_cog.Mode6
meMA.Ode3C13.melody = he.h.m6_gr_GR_cog.OukEstinAgios.name
meMA.Ode3C13.incipit = "Ὀπίσω χαίρων τοῦ Χριστοῦ, τὸν…"
meMA.Ode3C13.text = "Ὀπίσω χαίρων τοῦ Χριστοῦ, τὸν σταυρόν σου βαστάσας, ἠκολούθησας Πάτερ, ἐν ἀσκήσει καρτερᾷ, καὶ τούτῳ ὡς δυνατόν, ὡμοιώθης βίου καθαρότητι."

meMA.Ode3C1T.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C1T.mode = misc_gr_GR_cog.Mode6
meMA.Ode3C1T.melody = he.h.m6_gr_GR_cog.OukEstinAgios.name
meMA.Ode3C1T.incipit = "Νομίμων φύσεως ἐκτός, τὸν Δεσπότην…"
meMA.Ode3C1T.text = "Νομίμων φύσεως ἐκτός, τὸν Δεσπότην τεκοῦσα, Θεοτόκε Παρθένε, τῆς πάλαι παρακοῆς, διέλυσας τὴν ἀράν, εὐλογίας τὴν πηγὴν βλυστάνουσα."

meMA.Ode3Kathisma1.mode = misc_gr_GR_cog.Mode3
meMA.Ode3Kathisma1.melody = he.a.m3_gr_GR_cog.TheiasPisteos.name
meMA.Ode3Kathisma1.incipit = "Βίβλος γνώσεως διδασκαλίας, πλούτῳ πίστεως…"
meMA.Ode3Kathisma1.text = "Βίβλος γνώσεως διδασκαλίας, πλούτῳ πίστεως συντεταγμένη, τῷ πανάγνῳ ἀνεδείχθης ἐν Πνεύματι, ἀνακαλύπτων τὰ θεῖα τοῖς χρῄζουσι, καὶ τὴν ζωὴν θησαυρίζων τοῖς θέλουσι. Πάτερ Ὅσιε, Χριστὸν τὸν Θεὸν ἱκέτευε, δωρήσασθαι ἡμῖν τὸ μέγα ἔλεος."

meMA.Ode3Kathisma2.mode = misc_gr_GR_cog.Mode4
meMA.Ode3Kathisma2.melody = he.a.m8_gr_GR_cog.AnestisEkNekron.name
meMA.Ode3Kathisma2.incipit = "Ἐτέχθης ἐκ Μητρός, ὁ προάναρχος…"
meMA.Ode3Kathisma2.text = "Ἐτέχθης ἐκ Μητρός, ὁ προάναρχος Λόγος, προσήχθης τῷ ναῷ, ἀκατάληπτος μένων· Χαίρων δὲ ὁ Πρεσβύτης, ἐν ταῖς ἀγκάλαις σε ὑπεδέξατο, κράζων· Νῦν ἀπολύεις, ὃν ἐπεσκέψω κατὰ τὸ ῥῆμά σου, ὁ εὐδοκήσας σῶσαι ὡς Θεός, τὸ γένος τῶν ἀνθρώπων."

meMA.Ode4C1H.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C1H.mode = misc_gr_GR_cog.Mode6
meMA.Ode4C1H.melody = he.h.m6_gr_GR_cog.ChristosMouDynamis.melody
meMA.Ode4C1H.incipit = he.h.m6_gr_GR_cog.ChristosMouDynamis.incipit
meMA.Ode4C1H.text = he.h.m6_gr_GR_cog.ChristosMouDynamis.text

meMA.Ode4C11.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C11.mode = misc_gr_GR_cog.Mode6
meMA.Ode4C11.melody = he.h.m6_gr_GR_cog.ChristosMouDynamis.name
meMA.Ode4C11.incipit = "Ἀφθόνως Ὅσιε, Χριστῷ πειθόμενος, τὸν…"
meMA.Ode4C11.text = "Ἀφθόνως Ὅσιε, Χριστῷ πειθόμενος, τὸν δοθέντα σοι πλοῦτον ὡς εὐπειθής, δοῦλος διαδέδωκας, καὶ τῆς ἀφράστου σε χαράς, ὁ Δεσπότης κατηξίωσε."

meMA.Ode4C12.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C12.mode = misc_gr_GR_cog.Mode6
meMA.Ode4C12.melody = he.h.m6_gr_GR_cog.ChristosMouDynamis.name
meMA.Ode4C12.incipit = "Σοφίας πέλαγος, διανηξάμενος, τὸν καλὸν…"
meMA.Ode4C12.text = "Σοφίας πέλαγος, διανηξάμενος, τὸν καλὸν μαργαρίτην ὡς ἀγαθός, ἔμπορος ἐκέρδησας, καὶ τοῦτον μόνον θησαυρόν, ἀδαπάνητον ἐπλούτησας."

meMA.Ode4C13.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C13.mode = misc_gr_GR_cog.Mode6
meMA.Ode4C13.melody = he.h.m6_gr_GR_cog.ChristosMouDynamis.name
meMA.Ode4C13.incipit = "Τῷ θείῳ Πνεύματι, καταλαμπόμενος, τὰς…"
meMA.Ode4C13.text = "Τῷ θείῳ Πνεύματι, καταλαμπόμενος, τὰς ἀκτῖνας τῶν λόγων ἡλιακῶς, πᾶσιν ἐξαπέστειλας, τῆς σωτηρίας τῶν πιστῶν θεοφάντορ ὀρεγόμενος."

meMA.Ode4C1T.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C1T.mode = misc_gr_GR_cog.Mode6
meMA.Ode4C1T.melody = he.h.m6_gr_GR_cog.ChristosMouDynamis.name
meMA.Ode4C1T.incipit = "Ὡς ὄντως ἄφθεγκτα, καὶ ἀκατάληπτα…"
meMA.Ode4C1T.text = "Ὡς ὄντως ἄφθεγκτα, καὶ ἀκατάληπτα, τὰ τῆς σῆς Θεοτόκε θεοπρεποῦς, πέφυκε κυήσεως, τοῖς ἐπὶ γῆς καὶ οὐρανοῦ, ἀειπάρθενε Μυστήρια."

meMA.Ode5C1H.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C1H.mode = misc_gr_GR_cog.Mode6
meMA.Ode5C1H.melody = he.h.m6_gr_GR_cog.ToTheioFengeiSou.melody
meMA.Ode5C1H.incipit = he.h.m6_gr_GR_cog.ToTheioFengeiSou.incipit
meMA.Ode5C1H.text = he.h.m6_gr_GR_cog.ToTheioFengeiSou.text

meMA.Ode5C11.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C11.mode = misc_gr_GR_cog.Mode6
meMA.Ode5C11.melody = he.h.m6_gr_GR_cog.ToTheioFengeiSou.name
meMA.Ode5C11.incipit = "Νεκρώσει Πάνσοφε τῶν παθῶν, ἔξω…"
meMA.Ode5C11.text = "Νεκρώσει Πάνσοφε τῶν παθῶν, ἔξω γεγονὼς καὶ τῆς σαρκός, πόθῳ Θεῷ προσωμίλησας, τῷ καθαρωτάτῳ καὶ λαμπροτάτῳ φωτί, καὶ μόνην ἀπαιτοῦντι τὴν καθαρότητα."

meMA.Ode5C12.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C12.mode = misc_gr_GR_cog.Mode6
meMA.Ode5C12.melody = he.h.m6_gr_GR_cog.ToTheioFengeiSou.name
meMA.Ode5C12.incipit = "Τῷ θείῳ φέγγει σου ἀγαθέ…"
meMA.Ode5C12.text = "Τῷ θείῳ φέγγει σου ἀγαθέ, νῦν ὁ θεοφόρος αὐγασθείς, στῦλος ὁλόφωτος γέγονε, πᾶσι πέμπων θείου φωτός, τὰς θείας αὐγάς, καὶ ζόφον τῆς ἀγνοίας ἀποσειόμενος."

meMA.Ode5C13.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C13.mode = misc_gr_GR_cog.Mode6
meMA.Ode5C13.melody = he.h.m6_gr_GR_cog.ToTheioFengeiSou.name
meMA.Ode5C13.incipit = "Ὁ θέλων πάντας ὡς ἀγαθός…"
meMA.Ode5C13.text = "Ὁ θέλων πάντας ὡς ἀγαθός, πάνσοφε σωθῆναι ποδηγόν, τοῖς πλανομένοις σε ἔδειξε, πολλοὺς ἐπιστρέφειν πρὸς σωτηρίας ὁδόν, Ἰσίδωρε τρισμάκαρ, Πάτερ πανόλβιε."

meMA.Ode5C1T.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C1T.mode = misc_gr_GR_cog.Mode6
meMA.Ode5C1T.melody = he.h.m6_gr_GR_cog.ToTheioFengeiSou.name
meMA.Ode5C1T.incipit = "Κυρίως στόματι καὶ ψυχῇ, Δέσποινα…"
meMA.Ode5C1T.text = "Κυρίως στόματι καὶ ψυχῇ, Δέσποινα τοῦ κόσμου ἀγαθή, ὁμολογῶ σε πανάμωμε, σεσωματωμένον Θεὸν τεκοῦσαν Ἁγνή, καὶ βίου σε προστάτιν πίστει προβάλλομαι."

meMA.Ode6C1H.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C1H.mode = misc_gr_GR_cog.Mode6
meMA.Ode6C1H.melody = he.h.m6_gr_GR_cog.TouViouTinThalassan.melody
meMA.Ode6C1H.incipit = he.h.m6_gr_GR_cog.TouViouTinThalassan.incipit
meMA.Ode6C1H.text = he.h.m6_gr_GR_cog.TouViouTinThalassan.text

meMA.Ode6C11.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C11.mode = misc_gr_GR_cog.Mode6
meMA.Ode6C11.melody = he.h.m6_gr_GR_cog.TouViouTinThalassan.name
meMA.Ode6C11.incipit = "Λογίῳ τῶν κρίσεων, κοσμηθεὶς ὡς…"
meMA.Ode6C11.text = "Λογίῳ τῶν κρίσεων, κοσμηθεὶς ὡς Ἱερεύς, λογιστικῶς ἐδίκασας, θεοφόρε καὶ σώματι καὶ ψυχῇ, τὸ χεῖρον τῷ κρείττονι, ὑποτάξας ἐμφρόνως ἀξιάγαστε."

meMA.Ode6C12.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C12.mode = misc_gr_GR_cog.Mode6
meMA.Ode6C12.melody = he.h.m6_gr_GR_cog.TouViouTinThalassan.name
meMA.Ode6C12.incipit = "Εὐτόνως τὴν Αἴγυπτον, τῶν παθῶν…"
meMA.Ode6C12.text = "Εὐτόνως τὴν Αἴγυπτον, τῶν παθῶν καὶ τῆς τρυφῆς, σὺ Μοναστῶν τὸ καύχημα, τῆς ἐγκρατείας Ἔνδοξε ταῖς πληγαῖς, ἐτάζων ἐμάστιξας, τοῖς πιστοῖς ὁμαλίζων τὴν διάβασιν."

meMA.Ode6C1T.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C1T.mode = misc_gr_GR_cog.Mode6
meMA.Ode6C1T.melody = he.h.m6_gr_GR_cog.TouViouTinThalassan.name
meMA.Ode6C1T.incipit = "Ὁ φέρων τὰ σύμπαντα, δυναστείᾳ…"
meMA.Ode6C1T.text = "Ὁ φέρων τὰ σύμπαντα, δυναστείᾳ θεϊκῇ, καὶ συγκρατῶν ὡς εὔσπλαγχνος, σαῖς ἀγκάλαις Πανάμωμε σαρκωθείς, ὡς βρέφος βαστάζεται, ὁ Πατρὶ κατʼ οὐσίαν συναΐδιος."

meMA.Kontakion1.mode = misc_gr_GR_cog.Mode1
meMA.Kontakion1.melody = ""
meMA.Kontakion1.incipit = me.m02.d02_gr_GR_cog.meMA.Kontakion1.incipit
meMA.Kontakion1.text = me.m02.d02_gr_GR_cog.meMA.Kontakion1.text

meMA.Ode7C1H.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C1H.mode = misc_gr_GR_cog.Mode6
meMA.Ode7C1H.melody = he.h.m6_gr_GR_cog.DrosovolonMenTinKaminon.melody
meMA.Ode7C1H.incipit = he.h.m6_gr_GR_cog.DrosovolonMenTinKaminon.incipit
meMA.Ode7C1H.text = he.h.m6_gr_GR_cog.DrosovolonMenTinKaminon.text

meMA.Ode7C11.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C11.mode = misc_gr_GR_cog.Mode6
meMA.Ode7C11.melody = he.h.m6_gr_GR_cog.DrosovolonMenTinKaminon.name
meMA.Ode7C11.incipit = "Στερεώματι τῆς πίστεως ἐνέθετο, καθάπερ…"
meMA.Ode7C11.text = "Στερεώματι τῆς πίστεως ἐνέθετο, καθάπερ Πάτερ ἥλιον, ὁ Δεσπότης σε καταλάμπειν φέγγει νοητῷ, ψυχὰς τῶν βοώντων ἐκτενῶς· Εὐλογητὸς εἶ ὁ Θεός, ὁ τῶν Πατέρων ἤμων."

meMA.Ode7C12.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C12.mode = misc_gr_GR_cog.Mode6
meMA.Ode7C12.melody = he.h.m6_gr_GR_cog.DrosovolonMenTinKaminon.name
meMA.Ode7C12.incipit = "Μολυσμοὺς σαρκὸς καὶ πνεύματος ἀπέπλυνας…"
meMA.Ode7C12.text = "Μολυσμοὺς σαρκὸς καὶ πνεύματος ἀπέπλυνας, Μάκαρ δακρύων χεύμασι, καὶ γενόμενος Θεοφόρε φῶς τοῖς ἐπὶ γῆς, τοὺς πάντας ἐδίδαξας βοᾶν· Εὐλογητὸς εἶ ὁ Θεός, ὁ τῶν Πατέρων ἡμῶν."

meMA.Ode7C1T.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C1T.mode = misc_gr_GR_cog.Mode6
meMA.Ode7C1T.melody = he.h.m6_gr_GR_cog.DrosovolonMenTinKaminon.name
meMA.Ode7C1T.incipit = "Ἐπὶ ἄσειστον κρηπῖδα ὄντες πίστεως…"
meMA.Ode7C1T.text = "Ἐπὶ ἄσειστον κρηπῖδα ὄντες πίστεως, σὲ Θεομῆτορ ἄχραντε, ὁμολογοῦμεν Θεοτόκον. Λόγον γὰρ Θεοῦ, ἀσπόρως ἐγέννησας ἡμῖν. Εὐλογημένος ὁ καρπός, τῆς σῆς κοιλίας Ἁγνή."

meMA.Ode8C1H.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C1H.mode = misc_gr_GR_cog.Mode6
meMA.Ode8C1H.melody = he.h.m6_gr_GR_cog.EkFlogosToisOsiois.melody
meMA.Ode8C1H.incipit = he.h.m6_gr_GR_cog.EkFlogosToisOsiois.incipit
meMA.Ode8C1H.text = he.h.m6_gr_GR_cog.EkFlogosToisOsiois.text

meMA.Ode8C11.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C11.mode = misc_gr_GR_cog.Mode6
meMA.Ode8C11.melody = he.h.m6_gr_GR_cog.EkFlogosToisOsiois.name
meMA.Ode8C11.incipit = "Λογικὴν ὡς θυσίαν πάντα τὸν…"
meMA.Ode8C11.text = "Λογικὴν ὡς θυσίαν πάντα τὸν βίον σου, εἰς ὀσμὴν εὐωδίας Πάτερ προσήγαγες, τῷ ἐπὶ Σταυροῦ, κρεμασθέντι Χριστῷ τῷ Θεῷ, ὃν ὑπερυψοῦμεν, εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8C12.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C12.mode = misc_gr_GR_cog.Mode6
meMA.Ode8C12.melody = he.h.m6_gr_GR_cog.EkFlogosToisOsiois.name
meMA.Ode8C12.incipit = "Πλεονάζεις τὸν πλοῦτον καὶ μετὰ…"
meMA.Ode8C12.text = "Πλεονάζεις τὸν πλοῦτον καὶ μετὰ θάνατον, τοὺς ἐμψύχους σου λόγους, ὥσπερ πολύτιμον, πᾶσι τοῖς πιστοῖς, κληροδοσίαν λιπών, τοῖς ὑπερυψοῦσι, Χριστὸν εἰς τοὺς αἰῶνας."

meMA.Ode8C13.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C13.mode = misc_gr_GR_cog.Mode6
meMA.Ode8C13.melody = he.h.m6_gr_GR_cog.EkFlogosToisOsiois.name
meMA.Ode8C13.incipit = "Ὡραιώθης Παμμάκαρ κάλλει τῶν λόγων…"
meMA.Ode8C13.text = "Ὡραιώθης Παμμάκαρ κάλλει τῶν λόγων σου· τοῦ Ἁγίου γὰρ ὤφθης Πνεύματος κάλαμος, γράφων εὐσεβῶς, τούτου γνῶσιν τὴν ἔνθεον, τοῖς ὑπερυψοῦσι, Χριστὸν εἰς τοὺς αἰῶνας."

meMA.Ode8C1T.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C1T.mode = misc_gr_GR_cog.Mode6
meMA.Ode8C1T.melody = he.h.m6_gr_GR_cog.EkFlogosToisOsiois.name
meMA.Ode8C1T.incipit = "Πρὸς τὴν σὴν καταφεύγω σκέπην…"
meMA.Ode8C1T.text = "Πρὸς τὴν σὴν καταφεύγω σκέπην Πανάμωμε, καὶ προστάτιν ζωῆς μου τανῦν προβάλλομαι, σὲ τὴν ὑπὲρ νοῦν, Θεὸν Λόγον κυήσασαν, ὃν ὑπερυψοῦμεν, εἰς πάντας τοὺς αἰῶνας."

meMA.Ode9C1H.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C1H.mode = misc_gr_GR_cog.Mode6
meMA.Ode9C1H.melody = he.h.m6_gr_GR_cog.TheonAnthropois.melody
meMA.Ode9C1H.incipit = he.h.m6_gr_GR_cog.TheonAnthropois.incipit
meMA.Ode9C1H.text = he.h.m6_gr_GR_cog.TheonAnthropois.text

meMA.Ode9C11.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C11.mode = misc_gr_GR_cog.Mode6
meMA.Ode9C11.melody = he.h.m6_gr_GR_cog.TheonAnthropois.name
meMA.Ode9C11.incipit = "Ἀκτίς καθάπερ φωτὸς ἐνήστραψε, σοῦ…"
meMA.Ode9C11.text = "Ἀκτίς καθάπερ φωτὸς ἐνήστραψε, σοῦ τῇ ψυχῇ τῆς χάριτος ὁ λόγος πανόλβιε, διὰ σοῦ δὲ πᾶσα ἡ γῆ πεπλήρωται, τούτου τῆς λαμπηδόνος, ἧς οἱ μετέχοντες, τὸν τῆς σῆς σοφίας θησαυρόν, Πάτερ θαυμάζομεν."

meMA.Ode9C12.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C12.mode = misc_gr_GR_cog.Mode6
meMA.Ode9C12.melody = he.h.m6_gr_GR_cog.TheonAnthropois.name
meMA.Ode9C12.incipit = "Τρισὶ προσώποις μονάδα φύσεως, τοῖς…"
meMA.Ode9C12.text = "Τρισὶ προσώποις μονάδα φύσεως, τοῖς πατρικοῖς ἑπόμενος θεόφρον διδάγμασι, θεοφρόνως προσκυνεῖν ἐδίδαξας, σέβειν θεολογοῦντας, ἄκτιστον ἄναρχον, πᾶσι τοῖς πιστοῖς τὸν φωτισμὸν ἀναπηγάζουσαν."

meMA.Ode9C13.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C13.mode = misc_gr_GR_cog.Mode6
meMA.Ode9C13.melody = he.h.m6_gr_GR_cog.TheonAnthropois.name
meMA.Ode9C13.incipit = "Εὑρὼν τῶν πόνων τῶν σῶν…"
meMA.Ode9C13.text = "Εὑρὼν τῶν πόνων τῶν σῶν τὰ ἔπαθλα, ἐν οὐρανοῖς ζωῆς ἀτελευτήτου λαβόμενος, τοῖς ὑμνοῦσι πάνσοφε τὴν μνήμην σου, αἴτησαι σωτηρίαν, μάκαρ Ἰσίδωρε, θρόνῳ τοῦ Σωτῆρος μέτʼ Ἀγγέλων παριστάμενος."

meMA.Ode9C1T.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C1T.mode = misc_gr_GR_cog.Mode6
meMA.Ode9C1T.melody = he.h.m6_gr_GR_cog.TheonAnthropois.name
meMA.Ode9C1T.incipit = "Ῥευστὴν ὁ Λόγος Θεοῦ ὁ…"
meMA.Ode9C1T.text = "Ῥευστὴν ὁ Λόγος Θεοῦ ὁ ἄῤῥευστος, μορφὴν λαβὼν ἀνθρώπους ἀφθαρσίαν ἐνέδυσεν, εὐδοκίᾳ πατρικῇ σκηνώσας ἐν σοί, τῇ κεχαριτωμένῃ· ὅθεν Πανάχραντε, σὺν ταῖς οὐρανίαις, Στρατιαῖς, σὲ μεγαλύνομεν."

meMA.Exaposteilarion1.mode = misc_gr_GR_cog.Mode2
meMA.Exaposteilarion1.melody = he.a.m2_gr_GR_cog.GynaikesAkoutisthite.name
meMA.Exaposteilarion1.incipit = "Ἱδρῶσι τῆς ἀσκήσεως, παθῶν τὴν…"
meMA.Exaposteilarion1.text = "Ἱδρῶσι τῆς ἀσκήσεως, παθῶν τὴν φλόγα ἔσβεσας, καὶ ἀπαθείας πρὸς ὕψος, ἀναδραμὼν Θεοφόρε, τῷ τοῦ Χριστοῦ νῦν βήματι, παρέστης ἀγαλλόμενος, ὑπὲρ ἡμῶν δεόμενος, Ἰσίδωρε τῶν τελούντων, τὴν παναγίαν σου μνήμην."

meMA.Exaposteilarion2.mode = misc_gr_GR_cog.Mode2
meMA.Exaposteilarion2.melody = he.a.m2_gr_GR_cog.GynaikesAkoutisthite.name
meMA.Exaposteilarion2.incipit = "Ἐκ σοῦ Θεογεννήτρια, ὁ Κτίστης…"
meMA.Exaposteilarion2.text = "Ἐκ σοῦ Θεογεννήτρια, ὁ Κτίστης οὐρανοῦ καὶ γῆς, τεχθεὶς ἀφράστως ὡς οἶδεν, ἐν τῷ ναῷ νῦν ὡς βρέφος, εἰσήχθη ἐπωλένιος, ὃν Συμεὼν δεξάμενος, Θεὸς ὑπάρχεις Δέσποτα, Σωτὴρ καὶ ῥύστης ἐβόα, βροτῶν τοῦ γένους Χριστέ μου."

