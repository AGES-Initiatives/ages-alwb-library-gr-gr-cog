meMA.PostGospelIdiomelon.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.PostGospelIdiomelon.melody = ""
meMA.PostGospelIdiomelon.incipit = "Σταυρὲ τοῦ Χριστοῦ…"
meMA.PostGospelIdiomelon.text = "Σταυρὲ τοῦ Χριστοῦ, Χριστιανῶν ἡ ἐλπίς, πεπλανημένων ὁδηγέ, χειμαζομένων λιμήν, ἐν πολέμοις νῖκος, οἰκουμένης ἀσφάλεια, ἀσθενούντων ἰατρέ, νεκρῶν ἡ ἀνάστασις, ἐλέησον ἡμᾶς."

meMA.C1.acrostic.title = "Ὁ Κανών,"
meMA.C1.acrostic.notice1 = "ὃς τὴν δε τὴν Ἀκροστιχίδα φέρει·"
meMA.C1.acrostic.text = "Σταυρῷ πεποιθώς, ὕμνον ἐξερεύγομαι."
