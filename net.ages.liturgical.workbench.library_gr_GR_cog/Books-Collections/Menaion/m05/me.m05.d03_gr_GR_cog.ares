meVE.Stichera01.mode = miscellanea_gr_GR_cog.misc.Mode1
meVE.Stichera01.greekmelody = he.a.m1_gr_GR_cog.heAU.PaneffimoiMartyres.greekname
meVE.Stichera01.melody = he.a.m1_gr_GR_cog.heAU.PaneffimoiMartyres.name
meVE.Stichera01.incipit = "Τὸ ζεῦγος τὸ ἅγιον…"
meVE.Stichera01.text = "Τὸ ζεῦγος τὸ ἅγιον Χριστοῦ, ξυνωρὶς ἡ ἔνθεος, τὸ τῶν Μαρτύρων καλλώπισμα, μέγιστοι Μάρτυρες, οἱ τὴν θείαν ζεύγλην, ἐν βασάνοις στέρξαντες, καὶ μάστιγας τυράννων μὴ πτήξαντες· καὶ νῦν πρεσβεύσατε, δωρηθῆναι ταῖς ψυχαῖς ἡμῶν, τὴν εἰρήνην καὶ τὸ μέγα ἔλεος."

meVE.Stichera02.mode = miscellanea_gr_GR_cog.misc.Mode1
meVE.Stichera02.melody = he.a.m1_gr_GR_cog.heAU.PaneffimoiMartyres.name
meVE.Stichera02.incipit = "Μορφῇ ἀστραπτούσῃ…"
meVE.Stichera02.text = "Μορφῇ ἀστραπτούσῃ ὦ σεμνή, Μαύρα παμμακάριστε, καὶ τῷ φωτὶ τῆς σῆς χάριτος, ὄψεις ἠμαύρωσας, τοῦ δεινοῦ τυράννου, καὶ σπασμοῖς τριχῶν τῶν σῶν, Θεῷ ἀναφανδὸν προσωμίλησας· ἐντεῦθεν ἔφλεξας, τῶν λεβήτων ἐν τοῖς βράσμασι, τοῦ Βελίαρ τὴν κάραν τὴν ἄδικον."

meVE.Stichera03.mode = miscellanea_gr_GR_cog.misc.Mode1
meVE.Stichera03.melody = he.a.m1_gr_GR_cog.heAU.PaneffimoiMartyres.name
meVE.Stichera03.incipit = "Ἰσχύι Τιμόθεε ψυχῆς…"
meVE.Stichera03.text = "Ἰσχύϊ Τιμόθεε ψυχῆς, καὶ καρδίας ὅλης σου, Θεὸν τιμῶν τὸν ὑπέρτιμον, πυρὶ φλεγόμενος, καὶ τοῖς ὀβελίσκοις, εἴληφας φωνῆς Θεοῦ, καρδίας ἐν ὠσὶ τὴν τερπνότητα· καὶ ἐν κρεμάσματι, ἀντιστρόφῳ πρὸς οὐράνιον, δρόμον τρέχων, τὸ στέφος εἰσδέδεξαι."

meVE.Apolytikion1.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.Apolytikion1.greekmelody = he.a.m4_gr_GR_cog.heAU.TachyProkatalave.greekname
meVE.Apolytikion1.melody = he.a.m4_gr_GR_cog.heAU.TachyProkatalave.name
meVE.Apolytikion1.incipit = "Τιμόθεον σήμερον…"
meVE.Apolytikion1.text = "Τιμόθεον σήμερον σὺν τῇ συνάθλῳ πιστοί, συζύγῳ τιμήσωμεν Μαύρᾳ τῇ νύμφῃ Χριστοῦ, τὴν τούτων γεραίροντες, εὔτολμον καρτερίαν. Οὗτοι γὰρ σταυρωθέντες, ἴχνεσι τοῦ σφαγέντος, ἠκολούθησαν πόθῳ, καὶ πάντων τὰς ἁμαρτίας Σταυρῷ προσηλώσαντος."

meVE.Apolytikion2ger.poet = poets_gr_GR_cog.po.Gerasimou
meVE.Apolytikion2ger.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.Apolytikion2ger.greekmelody = he.a.m4_gr_GR_cog.heAU.TachyProkatalave.greekname
meVE.Apolytikion2ger.melody = he.a.m4_gr_GR_cog.heAU.TachyProkatalave.name
meVE.Apolytikion2ger.incipit = "Ὡς ζεῦγος ὁμόζυγον…"
meVE.Apolytikion2ger.text = "Ὡς ζεῦγος ὁμόζυγον, καὶ ξυνωρὶς θαυμαστή, Τιμόθεε πάνσοφε, καὶ Μαύρα νύμφη Χριστοῦ, ἐνθέως ἠθλήσατε· σύμμορφοι γὰρ ὀφθέντες, τῶν παθῶν τοῦ Κυρίου, δόξης ἀκαταλύτου, ἠξιώθητε ἄμφω, πρεσβεύοντες τῷ Σωτῆρι, ὑπὲρ τῶν ψυχῶν ἡμῶν."

meVE.Apolytikion3ger.poet = poets_gr_GR_cog.po.Gerasimou
meVE.Apolytikion3ger.mode = miscellanea_gr_GR_cog.misc.Mode3
meVE.Apolytikion3ger.greekmelody = he.a.m3_gr_GR_cog.heAU.TheiasPisteos.greekname
meVE.Apolytikion3ger.melody = he.a.m3_gr_GR_cog.heAU.TheiasPisteos.name
meVE.Apolytikion3ger.incipit = "Πέτρα ἄῤῥηκτος τῆς Ἐκκλησίας…"
meVE.Apolytikion3ger.text = "Πέτρα ἄῤῥηκτος, τῆς Ἐκκλησίας, ποιμὴν ἄριστος, πόλεως Ἄργους, ἀνεδείχθης Ἱεράρχα πανεύφημε. Ὡς οὖν πιστὸς οἰκονόμος τῆς χάριτος, παντοίων νόσων ἡμᾶς ἐλευθέρωσον, Πέτρε Ὅσιε, Χριστὸν τὸν Θεὸν αἰτούμενος, δωρήσασθαι ἡμῖν τὸ μέγα ἔλεος."

meMA.C1.acrostic.title = "Ὁ Κανὼν τῶν Ἁγίων,"
meMA.C1.acrostic.notice1 = "οὗ ἡ Ἀκροστιχίς·"
meMA.C1.acrostic.text = "Τιμοθέου Μαύρας τε τοὺς πόνους σέβω. Ἰωσήφ."
meMA.C1.title = titles_gr_GR_cog.ti.CanonSaints
meMA.C1.verse = prayers_gr_GR_cog.pr.verse_SaintsM.text

meMA.Ode1C11.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C11.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode1C11.greekmelody = he.h.m6_gr_GR_cog.heHE.OsEnIpeiroPezefsas.greekname
meMA.Ode1C11.melody = he.h.m6_gr_GR_cog.heHE.OsEnIpeiroPezefsas.name
meMA.Ode1C11.incipit = "Τῶν ἱερῶν ἀθλοφόρων τὴν ἱεράν…"
meMA.Ode1C11.text = "Τῶν ἱερῶν ἀθλοφόρων τὴν ἱεράν, καὶ σεπτὴν πανήγυριν, ὁ λαὸς ὁ ἱερός, ἱερῶς τιμήσωμεν αὐτῶν, ταῖς δεήσεσι δεινῶν ὅπως ῥυσθείημεν."

meMA.Ode1C12.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C12.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode1C12.melody = he.h.m6_gr_GR_cog.heHE.OsEnIpeiroPezefsas.name
meMA.Ode1C12.incipit = "Ἱερονίκους στεφάνους παρὰ Χριστοῦ, εἰληφέναι…"
meMA.Ode1C12.text = "Ἱερονίκους στεφάνους παρὰ Χριστοῦ, εἰληφέναι σπεύδοντες, τὸ θανεῖν ὑπὲρ τὸ ζῆν, οἱ γενναῖοι Μάρτυρες σαφῶς, ᾑρετίσαντο· διὸ δόξης ἐπέτυχον."

meMA.Ode1C13.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C13.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode1C13.melody = he.h.m6_gr_GR_cog.heHE.OsEnIpeiroPezefsas.name
meMA.Ode1C13.incipit = "Μὴ δειλιάσαντες πόνους τοὺς τῆς…"
meMA.Ode1C13.text = "Μὴ δειλιάσαντες πόνους τοὺς τῆς σαρκός, μὴ κολάσεις πτήξαντες, μὴ τὸ πῦρ τῶν διωκτῶν, ἀθλοφόροι Μάρτυρες Χριστόν, ἐν σταδίῳ ἀνδρικῶς ἀνεκηρύξατε."

meMA.Ode1C1T.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C1T.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode1C1T.melody = he.h.m6_gr_GR_cog.heHE.OsEnIpeiroPezefsas.name
meMA.Ode1C1T.incipit = "Ὁ ὑπερούσιος Λόγος οὐσιωθείς, ἐξ…"
meMA.Ode1C1T.text = "Ὁ ὑπερούσιος Λόγος οὐσιωθείς, ἐξ ἁγνῶν αἱμάτων σου, ἐθεούργησεν ἡμᾶς, καὶ φθορᾶς ἐῤῥύσατο Ἁγνή· διὰ τοῦτό σε πιστῶς ἀεὶ δοξάζομεν."

meMA.Ode3C11.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C11.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode3C11.greekmelody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.greekname
meMA.Ode3C11.melody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.name
meMA.Ode3C11.incipit = "Θεοῦ τὰς βίβλους τῷ λαῷ…"
meMA.Ode3C11.text = "Θεοῦ τὰς βίβλους τῷ λαῷ, ἱερῶς ὑπανοίγων, καὶ φωτίζων καρδίας, στρατιῶτα τοῦ Χριστοῦ, Τιμόθεε τῷ σεπτῷ μαρτυρίῳ, χαίρων ἐπιβέβηκας."

meMA.Ode3C12.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C12.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode3C12.melody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.name
meMA.Ode3C12.incipit = "Ἐνθέῳ πόθῳ τὴν ψυχήν, πυρπολούμενος…"
meMA.Ode3C12.text = "Ἐνθέῳ πόθῳ τὴν ψυχήν, πυρπολούμενος Μάρτυς, τῷ πυρὶ τῶν βασάνων, προσωμίλεις καρτερῶς· διὸ σε ἡ τοῦ Χριστοῦ, θεία δρόσος, μάκαρ ἐπανέψυχεν."

meMA.Ode3C13.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C13.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode3C13.melody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.name
meMA.Ode3C13.incipit = "Ὁ νοῦς σου νεύσει πρὸς…"
meMA.Ode3C13.text = "Ὁ νοῦς σου νεύσει πρὸς Θεόν, λαμπρυνόμενος Μάρτυς, οὐκ ᾐσθάνετο ὅλως, τῶν σιδήρων τὰς δεινάς, προσψαύσεις διὰ πυρός, γινομένας, ἔνδοξε Τιμόθεε."

meMA.Ode3C1T.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C1T.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode3C1T.melody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.name
meMA.Ode3C1T.incipit = "Ὑπάρχων φύσει ἀπαθής, ὁ Υἱός…"
meMA.Ode3C1T.text = "Ὑπάρχων φύσει ἀπαθής, ὁ Υἱός σου Παρθένε, προσελάβετο σάρκα, ἐξ αἱμάτων σου ἁγνῶν, καὶ διὰ πάθους Σταυροῦ, τῶν παθῶν με, ἄχραντε ἐῤῥύσατο."

meMA.Ode3H.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3H.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode3H.greekmelody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.greekname
meMA.Ode3H.melody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.melody
meMA.Ode3H.incipit = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.incipit
meMA.Ode3H.text = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.text

meMA.Ode3Kathisma1.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode3Kathisma1.greekmelody = he.a.m1_gr_GR_cog.heAU.TonTafonSouSotir.greekname
meMA.Ode3Kathisma1.melody = he.a.m1_gr_GR_cog.heAU.TonTafonSouSotir.name
meMA.Ode3Kathisma1.incipit = "Τιμήσαντες Θεόν, ἀπημαύρωσαν πλάνην, Τιμόθεος…"
meMA.Ode3Kathisma1.text = "Τιμήσαντες Θεόν, ἀπημαύρωσαν πλάνην, Τιμόθεος σαφῶς, καὶ ἡ ἔνδοξος Μαύρα, καὶ πᾶσαν ὑπομείναντες, οἱ πανένδοξοι βάσανον, ἐλαμπρύνθησαν, ὑπὲρ ἀκτῖνας ἡλίου, καὶ γεγόνασι, συλλειτουργοὶ τῶν Ἀγγέλων, οὓς πίστει δοξάσωμεν."

meMA.Ode4C11.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C11.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode4C11.greekmelody = he.h.m6_gr_GR_cog.heHE.ChristosMouDynamis.greekname
meMA.Ode4C11.melody = he.h.m6_gr_GR_cog.heHE.ChristosMouDynamis.name
meMA.Ode4C11.incipit = "Μεγίστην εὔκλειαν, ἐπιποθήσαντες, ἠξιώθητε ταύτης…"
meMA.Ode4C11.text = "Μεγίστην εὔκλειαν, ἐπιποθήσαντες, ἠξιώθητε ταύτης, τέκνα φωτός, θείου χρηματίσαντες, Μεγαλομάρτυρες Χριστοῦ, τῶν Ἀγγέλων ἰσοστάσιοι."

meMA.Ode4C12.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C12.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode4C12.melody = he.h.m6_gr_GR_cog.heHE.ChristosMouDynamis.name
meMA.Ode4C12.incipit = "Ἁγίων τάγμασι, συναριθμούμενος, καὶ εὐήκοα…"
meMA.Ode4C12.text = "Ἁγίων τάγμασι, συναριθμούμενος, καὶ εὐήκοα ὦτα πρὸς θεϊκούς, νόμους ἔχων ἔνδοξε, τὰς διατρήσεις σταθερῶς, ἐκαρτέρησας Τιμόθεε."

meMA.Ode4C13.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C13.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode4C13.melody = he.h.m6_gr_GR_cog.heHE.ChristosMouDynamis.name
meMA.Ode4C13.incipit = "Ὑψοῦντες ξύλῳ σε, οἱ ματαιόφρονες…"
meMA.Ode4C13.text = "Ὑψοῦντες ξύλῳ σε, οἱ ματαιόφρονες, προσβαρύνουσι λίθῳ τὴν κεφαλήν, ἔνδοξε Τιμόθεε· ἀλλʼ ἐδυνάμου σε Χριστός, θείῳ πάθει σεμνυνόμενον."

meMA.Ode4C14.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C14.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode4C14.melody = he.h.m6_gr_GR_cog.heHE.ChristosMouDynamis.name
meMA.Ode4C14.incipit = "Ῥεόντων ἄστατον, φορὰν Μακάριοι, ἑβδελύξασθε…"
meMA.Ode4C14.text = "Ῥεόντων ἄστατον, φορὰν Μακάριοι, ἑβδελύξασθε ὄντως, καὶ ἑαυτούς, πόνοις ἐξεδώκατε, τὰ διʼ αἰῶνος ἀγαθά, διὰ τούτων κληρωσάμενοι."

meMA.Ode4C1T.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C1T.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode4C1T.melody = he.h.m6_gr_GR_cog.heHE.ChristosMouDynamis.name
meMA.Ode4C1T.incipit = "Ἁγνείας σκήνωμα, κατοικητήριον, τοῦ Δεσπότου…"
meMA.Ode4C1T.text = "Ἁγνείας σκήνωμα, κατοικητήριον, τοῦ Δεσπότου τῶν ὅλων, καὶ Προφητῶν, πάντων περιήχημα, θεοχαρίτωτε Ναέ, τοὺς ὑμνοῦντάς σε οἰκτίρησον."

meMA.Ode5C11.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C11.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode5C11.greekmelody = he.h.m6_gr_GR_cog.heHE.ToTheioFengeiSou.greekname
meMA.Ode5C11.melody = he.h.m6_gr_GR_cog.heHE.ToTheioFengeiSou.name
meMA.Ode5C11.incipit = "Συζύγου Μαύρα φωτοειδεῖς, λόγους ἠκροάσω…"
meMA.Ode5C11.text = "Συζύγου Μαύρα φωτοειδεῖς, λόγους ἠκροάσω εὐμενῶς, καὶ τὴν ζοφώδη κακόνοιαν, ἀποῤῥιψαμένη φῶς ἐχρημάτισας, διὰ τοῦ μαρτυρίου θεομακάριστε."

meMA.Ode5C12.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C12.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode5C12.melody = he.h.m6_gr_GR_cog.heHE.ToTheioFengeiSou.name
meMA.Ode5C12.incipit = "Τῷ θείῳ Πνεύματι σεαυτήν, Μαύρα…"
meMA.Ode5C12.text = "Τῷ θείῳ Πνεύματι σεαυτήν, Μαύρα θωρακίσασα σεμνή, πρὸς συμπλοκὴν τοῦ ἀλάστορος, χαίρουσα ἐξῆλθες, καὶ τοῦτον ἔτρεψας, καὶ νίκης τοὺς στεφάνους Μάρτυς ἀπείληφας."

meMA.Ode5C13.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C13.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode5C13.melody = he.h.m6_gr_GR_cog.heHE.ToTheioFengeiSou.name
meMA.Ode5C13.incipit = "Ἕνα σε Κύριον Ἰησοῦν, καθομολογοῦσα…"
meMA.Ode5C13.text = "Ἕνα σε Κύριον Ἰησοῦν, καθομολογοῦσα ἡ Σεμνή, χειρὶ τὸν ἄνθρωπον πλάσαντα, τέμνεται δακτύλους χειρὸς τὴν ἄδικον, ἀνδρείως τιμωρίαν καθυποφέρουσα."

meMA.Ode5C1T.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C1T.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode5C1T.melody = he.h.m6_gr_GR_cog.heHE.ToTheioFengeiSou.name
meMA.Ode5C1T.incipit = "Τὴν καταιγίδα τῶν λογισμῶν, τὰς…"
meMA.Ode5C1T.text = "Τὴν καταιγίδα τῶν λογισμῶν, τὰς ἐπαναστάσεις τῶν παθῶν, τῶν πειρασμῶν τὸ κλυδώνιον, κόπασον Παρθένε θεοχαρίτωτε, καὶ σῶσόν με ὑμνοῦντα τὰ μεγαλεῖα σου."

meMA.Ode6C11.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C11.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode6C11.greekmelody = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.greekname
meMA.Ode6C11.melody = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.name
meMA.Ode6C11.incipit = "Οἱ ἅγιοι Μάρτυρες, τῷ ἱστίῳ…"
meMA.Ode6C11.text = "Οἱ ἅγιοι Μάρτυρες, τῷ ἱστίῳ τοῦ Σταυροῦ, πνευματικῶς πτερούμενοι, τῶν πειρασμῶν τὸ πέλαγος ἀβλαβῶς, ἐν πίστει παρέδραμον, καὶ πρὸς θείους λιμένας ἀνεπαύσαντο."

meMA.Ode6C12.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C12.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode6C12.melody = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.name
meMA.Ode6C12.incipit = "Ὑφάψαντες λέβητα, ἐν αὐτῷ σε…"
meMA.Ode6C12.text = "Ὑφάψαντες λέβητα, ἐν αὐτῷ σε τὸν Χριστόν, ὁμολογοῦσαν ῥίπτουσιν, οἱ δυσσεβεῖς, ἀλλʼ ἔμεινας ἀβλαβής, τὴν δρόσον τοῦ Πνεύματος, εὑραμένη σε Μαύρα ἀναψύχουσαν."

meMA.Ode6C13.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C13.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode6C13.melody = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.name
meMA.Ode6C13.incipit = "Στρεβλούμενοι Μάρτυρες, πάσας στρέβλας τοῦ…"
meMA.Ode6C13.text = "Στρεβλούμενοι Μάρτυρες, πάσας στρέβλας τοῦ ἐχθροῦ, ἀνδρείως διελύσατε, καὶ τῇ ἀγάπῃ τέλεον τοῦ Χριστοῦ, ψυχὰς συνεδήσατε, τῆς σαρκὸς ἀποθέσει παναοίδιμοι."

meMA.Ode6C1T.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C1T.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode6C1T.melody = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.name
meMA.Ode6C1T.incipit = "Πανάγαθε Δέσποινα, τὸν πανάγαθον Θεόν…"
meMA.Ode6C1T.text = "Πανάγαθε Δέσποινα, τὸν πανάγαθον Θεόν, ἡ ἐν σαρκὶ κυήσασα, κεκακωμένην πάθεσι τὴν ἐμήν, καρδίαν ἀγάθυνον, ἵνα πίστει καὶ πόθῳ μεγαλύνω σε."

meMA.Ode6H.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6H.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode6H.greekmelody = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.greekname
meMA.Ode6H.melody = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.melody
meMA.Ode6H.incipit = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.incipit
meMA.Ode6H.text = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.text

meMA.Kontakion1.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Kontakion1.melody = he.a.m4_gr_GR_cog.heAU.OYpsotheisEnToStavro.name
meMA.Kontakion1.incipit = "Τοὺς πολυτρόπους αἰκισμοὺς ἐνεγκόντες, καὶ…"
meMA.Kontakion1.text = "Τοὺς πολυτρόπους αἰκισμοὺς ἐνεγκόντες, καὶ τοὺς στεφάνους ἐκ Θεοῦ εἰληφότες, ὑπὲρ ἡμῶν πρεσβεύσατε πρὸς Κύριον, μνήμην τὴν πανίερον, τὴν ὑμῶν ἐκτελούντων, μέγιστε Τιμόθεε, καὶ ἀοίδιμε Μαύρα, τοῦ εἰρηνεῦσαι πόλιν καὶ λαόν· αὐτός ἐστι γάρ, πιστῶν τὸ κραταίωμα."

meMA.Kontakion2ger.poet = poets_gr_GR_cog.po.Gerasimou
meMA.Kontakion2ger.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Kontakion2ger.melody = he.a.m4_gr_GR_cog.heAU.OYpsotheisEnToStavro.name
meMA.Kontakion2ger.incipit = "Τῷ Κορυφαίῳ τῶν σοφῶν Ἀποστόλων…"
meMA.Kontakion2ger.text = "Τῷ Κορυφαίῳ τῶν σοφῶν Ἀποστόλων, συνωνυμήσας εὐκλεῶς Πάτερ Πέτρε, τῶν τούτου κατετρύφησας ἁγίων δωρεῶν· ζήλῳ γὰρ τῆς πίστεως, εὐσεβῶς διαπρέψας, θαύμασιν ἐκόσμησας, τὸν σὸν ἔνθεον βίον· Ἱερουργὲ τῆς δόξης τοῦ Χριστοῦ, τοὺς σὲ τιμῶντας, κινδύνων διάσῳζε."

meMA.Ode7C11.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C11.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode7C11.greekmelody = he.h.m6_gr_GR_cog.heHE.DrosovolonMenTinKaminon.greekname
meMA.Ode7C11.melody = he.h.m6_gr_GR_cog.heHE.DrosovolonMenTinKaminon.name
meMA.Ode7C11.incipit = "Οὐκ ᾐσθάνου ταῖς λαμπάσι παναοίδιμε…"
meMA.Ode7C11.text = "Οὐκ ᾐσθάνου ταῖς λαμπάσι παναοίδιμε, φλογιζομένη πάντοθεν, πῦρ ἐγκάρδιον, τὴν ἀγάπην, φέρουσα Χριστοῦ, καὶ τούτῳ βοῶσα ἐμμελῶς· Εὐλογητὸς εἶ ὁ Θεός, ὁ τῶν Πατέρων ἡμῶν."

meMA.Ode7C12.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C12.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode7C12.melody = he.h.m6_gr_GR_cog.heHE.DrosovolonMenTinKaminon.name
meMA.Ode7C12.incipit = "Νυσταγμὸν ἀπὸ βλεφάρων ἀπωσάμενος, κακίας…"
meMA.Ode7C12.text = "Νυσταγμὸν ἀπὸ βλεφάρων ἀπωσάμενος, κακίας ὦ Τιμόθεε, ἐκαρτέρησας, ὀφθαλμῶν πηρώσεις τὸν Χριστόν, καρδίας τοῖς ὄμμασιν ὁρῶν, καὶ μελῳδῶν· Εὐλογητὸς εἶ ὁ Θεός, ὁ τῶν Πατέρων ἡμῶν."

meMA.Ode7C13.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C13.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode7C13.melody = he.h.m6_gr_GR_cog.heHE.DrosovolonMenTinKaminon.name
meMA.Ode7C13.incipit = "Οἱ τὰ κάτω παριδόντες θεῖοι…"
meMA.Ode7C13.text = "Οἱ τὰ κάτω παριδόντες θεῖοι Μάρτυρες, τῶν ἄνω δὲ τὴν οἴκησιν, κληρωσάμενοι, ἱεραῖς τιμάσθωσαν ᾠδαίς, βοῶντες ἀπαύστως σὺν ἡμῖν· Εὐλογητὸς εἶ ὁ Θεός, ὁ τῶν Πατέρων ἡμῶν."

meMA.Ode7C1T.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C1T.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode7C1T.melody = he.h.m6_gr_GR_cog.heHE.DrosovolonMenTinKaminon.name
meMA.Ode7C1T.incipit = "Ὑπερύμνητε Παρθένε, ὑπερύμνητον Θεὸν ἀποκυήσασα…"
meMA.Ode7C1T.text = "Ὑπερύμνητε Παρθένε, ὑπερύμνητον Θεὸν ἀποκυήσασα, τοὺς ὑμνοῦντάς σε, κοινωνοὺς ἀνάδειξον φωτός, καὶ ῥῦσαι γεέννης καὶ φλογός, καὶ πάσης βλάβης τοῦ ἐχθροῦ ταῖς ἱκεσίαις σου."

meMA.Ode8C11.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C11.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode8C11.greekmelody = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.greekname
meMA.Ode8C11.melody = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.name
meMA.Ode8C11.incipit = "Συντηροῦντες τοὺς νόμους τοῦ Παντοκράτορος…"
meMA.Ode8C11.text = "Συντηροῦντες τοὺς νόμους τοῦ Παντοκράτορος, ἀνομούντων τῷ κράτει οὐχ ὑπεκύψατε, σέβας πονηροῖς, οὐκ ἐνείματε δαίμοσι, Μάρτυρες γενναῖοι, Κυρίου κληρονόμοι."

meMA.Ode8C12.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C12.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode8C12.melody = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.name
meMA.Ode8C12.incipit = "Συζυγίᾳ ἀρίστῃ σαφῶς συνδούμενοι, τὸν…"
meMA.Ode8C12.text = "Συζυγίᾳ ἀρίστῃ σαφῶς συνδούμενοι, τὸν ζυγὸν τοῦ Κυρίου τὸν ἐλαφρότατον, ᾔρατε ὁμοῦ, ἐπαυχένιον Μάρτυρες, καὶ ταῖς τῶν Μαρτύρων, συνήφθητε ἀγέλαις."

meMA.Ode8C13.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C13.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode8C13.melody = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.name
meMA.Ode8C13.incipit = "Ἐπὶ ξύλου ταθέντες σταυροῦ Πανεύφημοι…"
meMA.Ode8C13.text = "Ἐπὶ ξύλου ταθέντες σταυροῦ Πανεύφημοι, ἐπὶ πλείστας ἡμέρας ἐξεικονίσατε, πάθος τὸ σεπτόν, τοῦ παθόντος βουλήματι, ὃν ὑπερυψοῦμεν, εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8C14.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C14.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode8C14.melody = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.name
meMA.Ode8C14.incipit = "Βιαιότατον ὄντως θάνατον Ἅγιοι, παραστάσει…"
meMA.Ode8C14.text = "Βιαιότατον ὄντως θάνατον Ἅγιοι, παραστάσει τῶν πόνων καθυπεμείνατε, ἄπονον· διό, πρὸς ζωὴν μετετέθητε, ἀνυμνολογοῦντες, Χριστὸν εἰς τοὺς αἰῶνας."

meMA.Ode8C1T.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C1T.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode8C1T.melody = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.name
meMA.Ode8C1T.incipit = "Ὡραιώθης Παρθένε ἀποκυήσασα, ὡραιότατον κάλλει…"
meMA.Ode8C1T.text = "Ὡραιώθης Παρθένε ἀποκυήσασα, ὡραιότατον κάλλει Χριστὸν τὸν Κύριον, θείαις τοὺς πιστούς, ἀγλαΐαις φαιδρύνοντα, ὃν ὑπερυψοῦμεν, εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8H.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8H.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode8H.greekmelody = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.greekname
meMA.Ode8H.melody = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.melody
meMA.Ode8H.incipit = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.incipit
meMA.Ode8H.text = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.text

meMA.Ode9C11.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C11.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode9C11.greekmelody = he.h.m6_gr_GR_cog.heHE.TheonAnthropois.greekname
meMA.Ode9C11.melody = he.h.m6_gr_GR_cog.heHE.TheonAnthropois.name
meMA.Ode9C11.incipit = "Ἰδεῖν τὴν δόξαν κατηξιώθητε, τοῦ…"
meMA.Ode9C11.text = "Ἰδεῖν τὴν δόξαν κατηξιώθητε, τοῦ ἑαυτὸν κενώσαντος διʼ οἶκτον Πανεύφημοι· τοὺς αὐτοῦ γὰρ νόμους ἐφυλάξατε, καὶ κοινωνοὶ τῶν τούτου, παθημάτων γεγόνατε· ὅθεν συνελθόντες, εὐσεβῶς ἡμᾶς γεραίρομεν."

meMA.Ode9C12.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C12.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode9C12.melody = he.h.m6_gr_GR_cog.heHE.TheonAnthropois.name
meMA.Ode9C12.incipit = "Ὁρῶν ἰσάριθμον διηνύσατε, μέτρον Σοφοί…"
meMA.Ode9C12.text = "Ὁρῶν ἰσάριθμον διηνύσατε, μέτρον Σοφοί, νυκτῶν καὶ ἡμερῶν προσκαλούμενοι, τοῦ διʼ οἶκτον Σταυρῷ ὁμιλήσαντος· ὃν καθομολογοῦντες πάντων δεσπόζοντα, τούτου ἐν χερσί, τὰ ἑαυτῶν πνεύματα ἔθεσθε."

meMA.Ode9C13.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C13.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode9C13.melody = he.h.m6_gr_GR_cog.heHE.TheonAnthropois.name
meMA.Ode9C13.incipit = "Στρατὸς Ἀγγέλων χορὸς μακάριος, τῶν…"
meMA.Ode9C13.text = "Στρατὸς Ἀγγέλων χορὸς μακάριος, τῶν Προφητῶν Μαρτύρων Ἀποστόλων Ὁσίων τε, τῇ ἀνόδῳ ὑμῶν ἐπεκρότησε, πάντων δὲ ὁ Δεσπότης, στέφει κατέστεψε, Μάρτυρες ἡμᾶς νικητικῷ ὡς ὑπεράγαθος."

meMA.Ode9C14.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C14.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode9C14.melody = he.h.m6_gr_GR_cog.heHE.TheonAnthropois.name
meMA.Ode9C14.incipit = "Ἡμῖν τοῖς πόθῳ ὑμᾶς γεραίρουσι…"
meMA.Ode9C14.text = "Ἡμῖν τοῖς πόθῳ ὑμᾶς γεραίρουσι, καὶ τὴν σεπτὴν ὑμῶν ἐπιτελοῦσι πανήγυριν Ἀθλοφόροι Χριστοῦ γενναιότατοι, λύσιν ἁμαρτημάτων, βίου διόρθωσιν πάντων τε δεινῶν ἀπαλλαγὴν σοφοὶ αἰτήσασθε."

meMA.Ode9C1T.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C1T.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode9C1T.melody = he.h.m6_gr_GR_cog.heHE.TheonAnthropois.name
meMA.Ode9C1T.incipit = "Φωτὶ τῷ σῷ με Θεοχαρίτωτε…"
meMA.Ode9C1T.text = "Φωτὶ τῷ σῷ με Θεοχαρίτωτε, ἐν γυναιξὶ Παρθένε καταλάμπρυνον δέομαι, καὶ πυρὸς αἰωνίου με λύτρωσαι ὅπως σε μεγαλύνω, ὅπως δοξάζω σε, ὅπως κατὰ χρέος, ἀνυμνῶ τὰ μεγαλεῖά σου."

meMA.Ode9H.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9H.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode9H.greekmelody = he.h.m6_gr_GR_cog.heHE.TheonAnthropois.greekname
meMA.Ode9H.melody = he.h.m6_gr_GR_cog.heHE.TheonAnthropois.melody
