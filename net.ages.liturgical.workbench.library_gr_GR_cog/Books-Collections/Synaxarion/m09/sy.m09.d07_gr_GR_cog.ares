alt1.syMA.commemoration01.narration.p01 = syMA.commemoration05.narration.p01

syMA.lastline = "Ταῖς τῶν σῶν Ἁγίων πρεσβείαις, ὁ Θεός, ἐλέησον ἡμᾶς. Ἀμήν."

