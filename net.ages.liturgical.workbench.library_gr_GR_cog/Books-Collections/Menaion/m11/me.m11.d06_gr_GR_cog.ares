meMA.Ode1C11.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode1C11.greekmelody = he.h.m4_gr_GR_cog.heHE.AnoixoToStomaMou.greekname
meMA.Ode1C11.melody = he.h.m4_gr_GR_cog.heHE.AnoixoToStomaMou.name
meMA.Ode1C11.incipit = "Τῆς πίστεως ἔρεισμα…"
meMA.Ode1C11.text = "Τῆς πίστεως ἔρεισμα, τῆς Ἐκκλησίας διδάσκαλον, καὶ στῦλον ἀκράνδαντον, ὁμολογίας σε, καὶ πολύφωτον, τῆς χάριτος φωστῆρα, καὶ στόμα πυρίπνοον, Παῦλε κηρύττομεν."

meMA.Ode1C12.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode1C12.melody = he.h.m4_gr_GR_cog.heHE.AnoixoToStomaMou.name
meMA.Ode1C12.incipit = "Ὁ Παῦλος ὁ μέγιστος…"
meMA.Ode1C12.text = "Ὁ Παῦλος ὁ μέγιστος, τῆς οἰκουμένης ὁ ἥλιος, τρισμάκαρ προβάλλεται, σὲ Παῦλον δεύτερον, ὡς πῦρ φλέγοντα, εὐτόνως τὰς αἱρέσεις, καὶ πέλεκυν κόπτοντα τὴν ἀθεότητα."

meMA.Ode1C13.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode1C13.melody = he.h.m4_gr_GR_cog.heHE.AnoixoToStomaMou.name
meMA.Ode1C13.incipit = "Νομίμως ἠγώνισαι…"
meMA.Ode1C13.text = "Νομίμως ἠγώνισαι, προκινδυνεύων θεσπέσιε, τοῦ θείου κηρύγματος, ὡς Ἱεράρχης πιστός· καὶ κατέπνιξας, νευραῖς τῶν σῶν δογμάτων, ὡς θῆρα τὸν Ἄρειον τὸν ματαιόφρονα."

meMA.Ode1C1T.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode1C1T.melody = he.h.m4_gr_GR_cog.heHE.AnoixoToStomaMou.name
meMA.Ode1C1T.incipit = "Μαρίαν τὴν ἄχραντον…"
meMA.Ode1C1T.text = "Μαρίαν τὴν ἄχραντον, δεῦτε ὑμνήσωμεν ἅπαντες, τὴν μόνην κοσμήσασαν, τὴν ἀνθρωπότητα, τὴν κυήσασαν, Θεὸν σεσαρκωμένον, Παρθένον τε μείνασαν ἀδιαλώβητον."

meMA.Ode3C11.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C11.greekmelody = he.h.m4_gr_GR_cog.heHE.TousSousYmnologous.greekname
meMA.Ode3C11.melody = he.h.m4_gr_GR_cog.heHE.TousSousYmnologous.name
meMA.Ode3C11.incipit = "Υἱοθετηθεὶς χάριτι…"
meMA.Ode3C11.text = "Υἱοθετηθεὶς χάριτι θείᾳ, τὸν φύσει Υἱὸν Μονογενῆ, εἰς κτίσιν οὐ κατήγαγες, τὸν τῷ Πατρὶ συνάναρχον, Παῦλε τοῖς Παύλου δόγμασι, τοῦ θεοφόρου ἑπόμενος."

meMA.Ode3C12.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C12.melody = he.h.m4_gr_GR_cog.heHE.TousSousYmnologous.name
meMA.Ode3C12.incipit = "Στόματι καὶ γλώσσῃ…"
meMA.Ode3C12.text = "Στόματι καὶ γλώσσῃ καὶ καρδίᾳ, σοφίαν καὶ δύναμιν Θεοῦ, καὶ Λόγον ἐνυπόστατον, σὺ τὸν Χριστὸν ἐκήρυξας, Παῦλε θεόφρον Ὅσιε, τὸν δυσμενῆ φαυλίσας Ἄρειον."

meMA.Ode3C13.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C13.melody = he.h.m4_gr_GR_cog.heHE.TousSousYmnologous.name
meMA.Ode3C13.incipit = "Τῆς ὑπερουσίου ἐξουσίας…"
meMA.Ode3C13.text = "Τῆς ὑπερουσίου ἐξουσίας, τὸ Πνεῦμα τὸ Ἅγιον Θεοῦ, διʼ οὗ θεοποιούμεθα, κρίσει δικαίᾳ πάνσοφε, φύσει Θεὸν ἐδίδαξας, καὶ παντουργὸν καὶ παντοδύναμον."

meMA.Ode3C1T.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C1T.melody = he.h.m4_gr_GR_cog.heHE.TousSousYmnologous.name
meMA.Ode3C1T.incipit = "Ἀνάρχου Γεννήτορος…"
meMA.Ode3C1T.text = "Ἀνάρχου Γεννήτορος ὁ Λόγος, ὁ πάσης ἀνώτερος ἀρχῆς, ἀρχὴν νῦν σωματούμενος, ἐκ σοῦ Ἁγνὴ ἐδέξατο, καὶ ὑπὸ χρόνον γέγονεν, ὁ χρόνων πάντων ὑπέρτερος."

meMA.Ode3H.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3H.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3H.greekmelody = he.h.m4_gr_GR_cog.heHE.TousSousYmnologous.greekname
meMA.Ode3H.melody = he.h.m4_gr_GR_cog.heHE.TousSousYmnologous.melody
meMA.Ode3H.incipit = he.h.m4_gr_GR_cog.heHE.TousSousYmnologous.incipit
meMA.Ode3H.text = he.h.m4_gr_GR_cog.heHE.TousSousYmnologous.text

meMA.Ode3Kathisma1.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3Kathisma1.greekmelody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.greekname
meMA.Ode3Kathisma1.melody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.name
meMA.Ode3Kathisma1.incipit = "Ὡς τοῦ σκεύους ὑπάρχων…"
meMA.Ode3Kathisma1.text = "Ὡς τοῦ σκεύους ὑπάρχων τῆς ἐκλογῆς, καὶ ὁμώνυμος Πάτερ καὶ μιμητής, κινδύνους ὑπέμεινας, καὶ διωγμοὺς ὑπὲρ πίστεως, καὶ ὡς αὐτὸς τὴν Ῥώμην, κατέλαβες Ὅσιε, πανταχοῦ κηρύσσων, Τριάδος τὸ ὁμότιμον· ὅθεν καὶ τὸν δρόμον, ἐν Ἀρμενίᾳ τελέσας, ἀξίως ἀπείληφας, ἐκ Κυρίου τὸν στέφανον, καταισχύνας τὸν Ἄρειον. Πρέσβευε Χριστῷ τῷ Θεῷ, τῶν πταισμάτων ἄφεσιν δωρήσασθαι, τοῖς ἑορτάζουσι πόθῳ, τὴν ἁγίαν μνήμην σου."

meMA.Ode3Kathisma2.mode = miscellanea_gr_GR_cog.misc.Mode3
meMA.Ode3Kathisma2.greekmelody = he.a.m3_gr_GR_cog.heAU.TheiasPisteos.greekname
meMA.Ode3Kathisma2.melody = he.a.m3_gr_GR_cog.heAU.TheiasPisteos.name
meMA.Ode3Kathisma2.incipit = "Δόγμα ἔνθεον…"
meMA.Ode3Kathisma2.text = "Δόγμα ἔνθεον ἐπικρατύνας, ῥῆμα ἄθεον ἐξοστρακίσας, Παῦλε θεόφρον κατῄσχυνας Ἄρειον· τῷ γὰρ Πατρὶ τὸν Υἱὸν ὁμοούσιον, ἀνακηρύξας πιστοὺς ἐβεβαίωσας. Πάτερ Ὅσιε, Χριστὸν τὸν Θεὸν ἱκέτευε, δωρήσασθαι ἡμῖν τὸ μέγα ἔλεος."

