meVE.Stichera01.mode = miscellanea_gr_GR_cog.misc.Mode2
meVE.Stichera01.greekmelody = he.a.m2_gr_GR_cog.heAU.OteEkTouXylou.greekname
meVE.Stichera01.melody = he.a.m2_gr_GR_cog.heAU.OteEkTouXylou.name
meVE.Stichera01.incipit = "Νόμοις πειθαρχῶν τοῦ διʼ ἡμᾶς…"
meVE.Stichera01.text = "Νόμοις πειθαρχῶν τοῦ διʼ ἡμᾶς, σάρκα ἐπὶ γῆς εἰληφότος, καὶ πάθος τίμιον, Μάρτυς ὑπομείναντος, θεομακάριστε, τὴν ἀγάπην ἐτήρησας, τὴν πρὸς τὸν πλησίον, νόμου τὸ κεφάλαιον, οὖσαν καὶ τῶν Προφητῶν· ὅθεν, καὶ μακάριον τέλος, εἴληφας πηγῇ τῆς ἀγάπης, νῦν ὦ Νικηφόρε παριστάμενος."

meVE.Stichera02.mode = miscellanea_gr_GR_cog.misc.Mode2
meVE.Stichera02.melody = he.a.m2_gr_GR_cog.heAU.OteEkTouXylou.name
meVE.Stichera02.incipit = "Κλίνεις τὸν αὐχένα τῷ Θεῷ…"
meVE.Stichera02.text = "Κλίνεις τὸν αὐχένα τῷ Θεῷ, ᾧ τὰ πάντα κλίνει τὸ γόνυ, Μάρτυς θαυμάσιε, κάραν ἐκτεμνόμενος, καὶ χωριζόμενος, τοῦ γενναίου σου σώματος, Χριστῷ δὲ τῇ πάντων, κεφαλῇ ἑνούμενος, καθαρωτάτῳ νοΐ, οὗ νῦν τῷ φωτὶ πλησιάζων, αἴτησαι ἡμῖν Νικηφόρε, πᾶσι φωτισμὸν τοῖς εὐφημοῦσί σε."

meVE.Stichera03.mode = miscellanea_gr_GR_cog.misc.Mode2
meVE.Stichera03.melody = he.a.m2_gr_GR_cog.heAU.OteEkTouXylou.name
meVE.Stichera03.incipit = "Ῥείθροις τῶν αἱμάτων σου Σοφέ…"
meVE.Stichera03.text = "Ῥείθροις τῶν αἱμάτων σου Σοφέ, τῶν ὑπὲρ Χριστοῦ κενωθέντων τὴν γῆν ἡγίασας, πνεύματι δὲ πνεύματα, ἐχαροποίησας, ἀσωμάτων Δυνάμεων, Μαρτύρων ἀγέλας, πάσας δὲ ἐφαίδρυνας, ταύταις ἑνούμενος, Μάκαρ ὡς γενναῖος ὁπλίτης, ὡς ἀκαταγώνιστος Μάρτυς, καὶ ὑπὲρ ἡμῶν Θεοῦ δεόμενος."

meVE.SticTheotokion.mode = miscellanea_gr_GR_cog.misc.Mode2
meVE.SticTheotokion.greekmelody = he.a.m2_gr_GR_cog.heAU.OteEkTouXylou.greekname
meVE.SticTheotokion.melody = he.a.m2_gr_GR_cog.heAU.OteEkTouXylou.name
meVE.SticTheotokion.incipit = "Δέξαι τὴν ἱκέσιον ᾠδήν, τῶν…"
meVE.SticTheotokion.text = "Δέξαι τὴν ἱκέσιον ᾠδήν, τῶν σῶν οἰκετῶν Θεοτόκε, ἣν σοι προσάγομεν, Δέσποινα διάσωσον, ἀπορουμένους ἡμᾶς, καὶ κινδύνων ἐξάγαγε, τὴν ποίμνην σου ταύτην, πίστει σοι προσπίπτουσαν, ἐν τῷ σεπτῷ σου ναῷ· σὺ γὰρ τὸν Σωτῆρα τεκοῦσα, ἔχεις παῤῥησίαν ὡς Μήτηρ, ἐκτενῶς πρεσβεύειν τοῦ σωθῆναι ἡμᾶς."

meVE.SticStavrotheotokion.mode = miscellanea_gr_GR_cog.misc.Mode2
meVE.SticStavrotheotokion.greekmelody = he.a.m2_gr_GR_cog.heAU.OteEkTouXylou.greekname
meVE.SticStavrotheotokion.melody = he.a.m2_gr_GR_cog.heAU.OteEkTouXylou.name
meVE.SticStavrotheotokion.incipit = "Ὅτε ἡ ἀμίαντος Ἀμνάς, ἔβλεψε…"
meVE.SticStavrotheotokion.text = "Ὅτε ἡ ἀμίαντος Ἀμνάς, ἔβλεψε τὸν ἴδιον ἄρνα, ἐπὶ σφαγὴν ὡς κριόν, θέλοντα ἑλκόμενον, θρηνοῦσα ἔλεγεν· Ἀτεκνῶσαι νῦν σπεύδεις με, Χριστὲ τὴν τεκοῦσαν, τί τοῦτο πεποίηκας, ὁ Λυτρωτὴς τοῦ παντός; ὅμως, ἀνυμνῶ καὶ δοξάζω, σοῦ τὴν ὑπὲρ νοῦν τε καὶ λόγον, ἄκραν συγκατάβασιν φιλάνθρωπε."

meVE.SticGlory.poet = poets_gr_GR_cog.po.IoannouMonachou
meVE.SticGlory.mode = miscellanea_gr_GR_cog.misc.Mode6
meVE.SticGlory.melody = ""
meVE.SticGlory.incipit = "Ἔδειξας πᾶσιν ἐμφανῶς, Ἀθλητὰ Νικηφόρε…"
meVE.SticGlory.text = "Ἔδειξας πᾶσιν ἐμφανῶς, Ἀθλητὰ Νικηφόρε, ὅτι τὸν πλησίον ὁ μὴ φιλῶν, οὔτε τὸν Δεσπότην ἀγαπῆσαι δύναται, διόπερ, αὐτὸς μὲν εἰλικρινῶς ἀγαπήσας Σαπρίκιον τὸν ὁμόδουλον, ἐντεῦθεν καὶ πρὸς τὸν θεῖον ἀνεπτερώθης ἔρωτα, καὶ τὴν ψυχήν σου τέθεικας, ὑπὲρ τῆς εἰς Χριστὸν ὁμολογίας καὶ πίστεως. Σαπρίκιος δὲ ὁ δυσώνυμος, ἄσπονδον πρὸς σὲ τὸ μῖσος κτησάμενος, ἀρνητὴς ἐδείχθη καὶ τοῦ Δεσπότου Χριστοῦ, ᾧ νῦν αὐτὸς παρεστώς, καθικέτευε τοῦ σωθῆναι τὰς ψυχὰς ἡμῶν."

meVE.SticTheotokion2.mode = miscellanea_gr_GR_cog.misc.Mode6
meVE.SticTheotokion2.greekmelody = he.a.m6_gr_GR_cog.heAU.OlinApothemenoi.greekname
meVE.SticTheotokion2.melody = he.a.m6_gr_GR_cog.heAU.OlinApothemenoi.name
meVE.SticTheotokion2.incipit = "Τετραυματισμένος με, ταῖς λῃστρικαῖς τῶν…"
meVE.SticTheotokion2.text = "Τετραυματισμένος με, ταῖς λῃστρικαῖς τῶν Δαιμόνων, ἐφόδοις καὶ κείμενον, ὅλον ἀνενέργητον παναμώμητε, ἐν ὁδῷ πάντοτε, τοῦ ἀστάτου βίου, καὶ ἐλέους προσδεόμενον, τάχος ἐπίσκεψαι, οἶνον ἐπιθεῖσα καὶ ἔλαιον, τοῖς ἀνιάτοις μώλωψι, καὶ πρὸς εὐρωστίαν ἀνάγαγε, ὅπως σε δοξάζω, καὶ πόθῳ κατὰ χρέος ἀνυμνῶ, τὰ μεγαλεῖά σου ἄχραντε, Μήτηρ ἀειπάρθενε."

meVE.SticStavrotheotokion2.mode = miscellanea_gr_GR_cog.misc.Mode6
meVE.SticStavrotheotokion2.greekmelody = he.a.m6_gr_GR_cog.heAU.OlinApothemenoi.greekname
meVE.SticStavrotheotokion2.melody = he.a.m6_gr_GR_cog.heAU.OlinApothemenoi.name
meVE.SticStavrotheotokion2.incipit = me.m01.d15_gr_GR_cog.meVE.AposStavrotheotokion.incipit
meVE.SticStavrotheotokion2.text = me.m01.d15_gr_GR_cog.meVE.AposStavrotheotokion.text

meVE.Apolytikion1.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.Apolytikion1.melody = he.a.m4_gr_GR_cog.heAU.TachyProkatalave.name
meVE.Apolytikion1.incipit = "Ἀγάπῃ τοῦ Κτίσαντος, καταυγασθεὶς τὴν…"
meVE.Apolytikion1.text = "Ἀγάπῃ τοῦ Κτίσαντος, καταυγασθεὶς τὴν ψυχήν, τοῦ νόμου τῆς χάριτος, ἐκπληρωτὴς ἀκριβής, ἐμφρόνως γεγένησαι· ὅθεν καὶ τὸν πλησίον, ὡς σαὐτὸν ἀγαπήσας, ἤθλησας Νικηφόρε, καὶ τὸν ὄφιν καθεῖλες· ἐντεῦθεν ἐν ὁμονίᾳ, ἡμᾶς διατήρησον."

meMA.C1.acrostic.title = "Κανὼν τοῦ Μάρτυρος,"
meMA.C1.acrostic.notice1 = "οὗ ἡ Ἀκροστιχίς·"
meMA.C1.acrostic.text = "Κλεινὸν ἐν ἀθλοφόροις Νικηφόρον ᾄσματι μέλπω."
meMA.C1.poet = poets_gr_GR_cog.po.Theofanous
meMA.C1.title = titles_gr_GR_cog.ti.CanonMartyrM
meMA.C1.verse = prayers_gr_GR_cog.pr.verse_SaintM.text

