meVE.Stichera01.mode = misc_gr_GR_cog.Mode4
meVE.Stichera01.melody = he.a.m4_gr_GR_cog.OsGennaionEnMartysi.name
meVE.Stichera01.text = "Ἀββακοὺμ ὁ θαυμάσιος, τὴν ἀκτῖνα τοῦ Πνεύματος, δεδεγμένος γέγονεν, ὅλος ἔνθεος· καὶ τῶν κριτῶν τὴν ἀσέβειαν, καὶ κρίσιν τὴν ἄδικον, θεωρῶν ἀγανακτεῖ, τήν τῆς γνώμης εὐθύτητα, ἐνδεικνύμενος, τοῦ Θεοῦ καὶ Δεσπότου φιλοθέως, καὶ θερμῇ τῇ διανοίᾳ, ὡς δυσχεραίνων καθάπτεται."

meVE.Stichera02.mode = misc_gr_GR_cog.Mode4
meVE.Stichera02.melody = he.a.m4_gr_GR_cog.OsGennaionEnMartysi.name
meVE.Stichera02.text = "Ἐπὶ θείας ἱστάμενος, φυλακῆς ὁ σεβάσμιος, Ἀββακοὺμ ἀκήκοε τὸ μυστήριον, τῆς πρὸς ἡμᾶς παρουσίας σου, Χριστὲ τὸ ἀπόρρητον, καὶ τὸ κήρυγμα τὸ σόν, προφητεύει σαφέστατα, προορώμενος, τοὺς σοφοὺς Ἀποστόλους ὥσπερ ἵππους, τῶν Ἐθνῶν τῶν πολυσπόρων, διαταράσσοντας θάλασσαν."

meVE.Stichera03.mode = misc_gr_GR_cog.Mode4
meVE.Stichera03.melody = he.a.m4_gr_GR_cog.OsGennaionEnMartysi.name
meVE.Stichera03.text = "Ἐν Κυρίῳ γηθόμενος, ἐν Θεῷ τῷ Σωτῆρί σου, θεορρῆμον ἔνδοξε ἀγαλλόμενος, καὶ τῆς ἐκεῖθεν λαμπρότητος, τὴν αἴγλην δεχόμενος, καὶ φωτὶ θεουργικῷ, νοερῶς φωτιζόμενος, τοὺς τὴν μνήμην σου, τὴν πανίερον πίστει ἐκτελοῦντας, πειρασμῶν τε καὶ κινδύνων, ταῖς σαῖς πρεσβείαις ἐξάρπασον."

meVE.SticTheotokion.mode = misc_gr_GR_cog.Mode4
meVE.SticTheotokion.melody = he.a.m4_gr_GR_cog.OsGennaionEnMartysi.name
meVE.SticTheotokion.text = "Ἡ Θεὸν τὸν ἀχώρητον ἐν γαστρί σου χωρήσασα, φιλανθρώπως ἄνθρωπον χρηματίσαντα, καὶ τὸ ἡμέτερον φύραμα, ἐκ σοῦ προσλαβόμενον, καὶ θεώσαντα σαφῶς, μὴ παρίδῃς με Πάναγνε, νῦν θλιβόμενον· ἀλλ' οἰκτείρησον τάχος, καὶ παντοίας, δυσμενείας τε καὶ βλάβης, τοῦ πονηροῦ ἐλευθέρωσον."

meVE.SticStavrotheotokion.mode = misc_gr_GR_cog.Mode4
meVE.SticStavrotheotokion.melody = he.a.m4_gr_GR_cog.OsGennaionEnMartysi.name
meVE.SticStavrotheotokion.text = "Ὡς ἑώρακε Κύριε, ἡ Παρθένος καὶ Μήτηρ σου, ἐν Σταυρῷ κρεμάμενον, ἐξεπλήττετο, καὶ μετὰ θρήνων σοι ἔλεγε· Βαβαὶ τῆς ἀπείρου σου, Πλαστουργέ μου ἀνοχῆς! πῶς ἑκὼν ὡς κατάκριτος, κρίσιν ἄδικον, κατακρίνῃ θανάτου παρὰ δήμου, κατακρίτου καὶ φρικώδη, καθυποφέρεις παθήματα;"

meVE.Apolytikion1.mode = misc_gr_GR_cog.Mode2
meVE.Apolytikion1.melody = he.ga_gr_GR_cog.TouProfitouSou.name
meVE.GeneralApolytikion1.name = "Ἀββακοὺμ"

meVE.Apolytikion2.poet = poets_gr_GR_cog.Gerasimou
meVE.Apolytikion2.mode = misc_gr_GR_cog.Mode4
meVE.Apolytikion2.melody = he.a.m4_gr_GR_cog.TachyProkatalave.name
meVE.Apolytikion2.text = "Ὡς ὄρος προέγραψας, τὴν Θεοτόκον Ἁγνήν, ἐξ ἧς ἡμῖν ἔλαμψεν, ὁ τῶν ἁπάντων Θεός, σαρκὸς ὁμοιώματι. Ὅθεν σε ὡς προφήτην, θεηγόρον τιμῶντες, χάριτος ούρανίου, μετασχεῖν δυσωποῦμεν, πρεσβείαις σου θεοδέκτοις, Ἀββακοὺμ ἔνδοξε."

meVE.Apolytikion3.poet = poets_gr_GR_cog.Gerasimou
meVE.Apolytikion3.mode = misc_gr_GR_cog.Mode4
meVE.Apolytikion3.melody = he.a.m4_gr_GR_cog.TachyProkatalave.name
meVE.Apolytikion3.text = "Ὡς ξένος καὶ πάροικος, τῶν ἐπιγείων τερπνῶν, ζωὴν τὴν ἰσάγγελον, ἐπολιτεύσω σαφῶς, Πατὴρ ἡμῶν Κύριλλε· ὅθεν τὰς οὐρανίους, εἰληφὼς ἀντιδόσεις, πρέσβευε θεοφόρε, τῷ Σωτῆρι τῶν ὅλων, δοθῆναι τοῖς σὲ τιμῶσι, χάριν καὶ ἔλεος."

meMA.C1Acrostic.text = "οὗ ἡ Ἀκροστιχίς· Σὲ τὸν Προφήτην Ἀββακοὺμ μέλπω Μάκαρ."

meMA.C1.title = ""
meMA.C1.verse = ""

meMA.Ode1C11.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode1C11.melody = he.h.m4_gr_GR_cog.AsomaiSoiKyrie.name
meMA.Ode1C11.text = "Σόφισόν με Κύριε ὁ Θεός μου, τὸν σὸν Προφήτην Ἀββακούμ, ὑμνῆσαι τὸν θεῖον, καὶ χάριτί σου λάμπρυνον, ἀγαθὲ τὴν καρδίαν μου."

meMA.Ode1C12.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode1C12.melody = he.h.m4_gr_GR_cog.AsomaiSoiKyrie.name
meMA.Ode1C12.text = "Ἕως τίνος Κύριε ὁ Προφήτης, λέγει βοήσομαι πρὸς σέ, καὶ οὐκ εἰσακούσῃ; ἵνα τὶ δὲ μοι ἔδειξας, τῶν κριτῶν τὴν ἀσέβειαν."

meMA.Ode1C13.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode1C13.melody = he.h.m4_gr_GR_cog.AsomaiSoiKyrie.name
meMA.Ode1C13.text = "Τὸ σεπτὸν μνημόσυνόν σου Προφῆτα, τοὺς ἐκτελοῦντας εὐλαβῶς, ταῖς σαῖς ἱκεσίαις, κινδύνων ἀπολύτρωσαι, καὶ δεινῶν περιστάσεων."

meMA.Ode1C1T.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode1C1T.melody = he.h.m4_gr_GR_cog.AsomaiSoiKyrie.name
meMA.Ode1C1T.text = "Ὁ Θεὸς ὁ Ἅγιος ἐπεφάνη, ἐν τῷ ἐγγίζειν τὸν καιρόν, ἐκ σοῦ Θεοτόκε, γενόμενος ἄνθρωπος, ἵνα σώσῃ τὸν ἄνθρωπον."

meMA.Ode3C11.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode3C11.melody = he.h.m4_gr_GR_cog.ToxonDynatonIsthenise.name
meMA.Ode3C11.text = "Νεύσει πρὸς Θεὸν ὑψούμενος, καὶ φωτοδοσίαις, ταῖς παρ' αὐτοῦ φωτιζόμενος, τῶν μελλόντων προεθεώρεις, Θεοφάντορ τὴν προτύπωσιν."

meMA.Ode3C12.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode3C12.melody = he.h.m4_gr_GR_cog.ToxonDynatonIsthenise.name
meMA.Ode3C12.text = "Πᾶσαν ἀρετὴν ἐξήσκησας, πᾶσαν δὲ κακίαν, ἐκ διανοίας ἐμίσησας, καὶ δικαίως τοὺς ἀδικοῦντας, ἐβδελύξω Παμμακάριστε."

meMA.Ode3C13.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode3C13.melody = he.h.m4_gr_GR_cog.ToxonDynatonIsthenise.name
meMA.Ode3C13.text = "Ῥήσεις τοῦ Ἁγίου Πνεύματος, σὺ ὡς καθαρός, ἐπιστεύθης τρανότατα, προθεσπίζων τῶν ἐσομένων, τὰς ἐκβάσεις Ἀξιάγαστε."

meMA.Ode3C1T.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode3C1T.melody = he.h.m4_gr_GR_cog.ToxonDynatonIsthenise.name
meMA.Ode3C1T.text = "Ὄρος ἀρεταῖς κατάσκιον, ἐξ οὗ ὁ Δεσπότης, δουλοπρεπῶς ἐπεδήμησε, τῆς δουλείας ἐλευθερῶσαι, τοὺς ἀνθρώπους ὤφθης Πάναγνε."

meMA.Ode3H.ode = misc_gr_GR_cog.Ode3
meMA.Ode3H.mode = misc_gr_GR_cog.Mode4
meMA.Ode3H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode3H.text = he.h.m4_gr_GR_cog.ToxonDynatonIsthenise.text

meMA.Ode3Kathisma1.mode = misc_gr_GR_cog.Mode4
meMA.Ode3Kathisma1.melody = he.a.m4_gr_GR_cog.OYpsotheisEnToStavro.name
meMA.Ode3Kathisma1.text = "Ἐπὶ τῆς θείας φυλακῆς ἔστης μάκαρ, καὶ κατενόησας Θεοῦ παρουσίαν, προφητικοῖς ἐν ὄμμασι θεόπνευστε· ὅθεν καὶ ἐβόησας, Ἀββακοὺμ μετὰ φόβου, Κύριε ἀκήκοα, τὴν φρικτὴν ἔλευσίν σου, καὶ ἀνυμνῶ σε σάρκα χοϊκήν, ἐκ τῆς Παρθένου, φορέσαι θελήσαντα."

meMA.Ode3KathismaT.mode = misc_gr_GR_cog.Mode4
meMA.Ode3KathismaT.melody = he.a.m4_gr_GR_cog.OYpsotheisEnToStavro.name
meMA.Ode3KathismaT.text = "Καταφυγὴ τῶν ἐν δεινοῖς ὑπαρχόντων, καταλλαγὴ πρὸς τὸν Θεὸν τῶν πταιόντων, ὑπεραγία Δέσποινα περίσωζε ἡμᾶς, πάσης περιστάσεως, καὶ κακίας ἀνθρώπων, καὶ φρικτῆς κολάσεως, καὶ παθῶν ἀτιμίας, τοὺς ἀδιστάκτῳ πίστει σε ἀεί, προσκαλουμένους, Παρθένε πανύμνητε."

meMA.Ode3KathismaSt.mode = misc_gr_GR_cog.Mode4
meMA.Ode3KathismaSt.melody = he.a.m4_gr_GR_cog.OYpsotheisEnToStavro.name
meMA.Ode3KathismaSt.text = "Ἐν τῷ Σταυρῷ σε καθορῶσα Χριστέ μου, ἡ ἀπειράνδρως σὲ τεκοῦσα ἐβόα, σπαρασσομένη ὄψιν τε καὶ σπλάγχνα μητρικῶς· Οἴμοι ὦ παμφίλτατε! πῶς ἐνέγκω τὸ πάθος; φλέγονται τὰ σπλάγχνα μου, καὶ δεινῆς μοι ῥομφαίας, τραῦμα πικρὸν διέδραμε σαφῶς, ἀλλ' ἀνυμνῶ σου, τὸ εὔσπλαγχνον Κύριε."

meMA.Ode4C11.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode4C11.melody = he.h.m4_gr_GR_cog.TousOuranousIAretiSou.name
meMA.Ode4C11.text = "Φωτοειδὴς δι' ἀρετῆς ὤφθης θεόπνευστε, καὶ τὸ φῶς τὸ ἀνέσπερον ἐμήνυσας, ᾧ νῦν ἐν πίστει κράζομεν· Δόξα τῇ δυνάμει σου Κύριε."

meMA.Ode4C12.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode4C12.melody = he.h.m4_gr_GR_cog.TousOuranousIAretiSou.name
meMA.Ode4C12.text = "Ἡ τοῦ παντὸς φωτιστικὴ χάρις τοῦ Πνεύματος, δαψιλῶς ἐνοικήσασά σοι Πάνσοφε, Προφήτην ἀπετέλεσε, πᾶσι τὸν Σωτῆρα μηνύοντα."

meMA.Ode4C13.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode4C13.melody = he.h.m4_gr_GR_cog.TousOuranousIAretiSou.name
meMA.Ode4C13.text = "Τὴν ἀκοὴν ἐνηχηθεὶς τοῦ Παντοκράτορος, εὐλαβῶς ἐφοβήθης θαυμάσιε, κατανόησας ἔργα δὲ τούτου, ἐν ἐκστάσει γεγένησαι."

meMA.Ode4C1T.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode4C1T.melody = he.h.m4_gr_GR_cog.TousOuranousIAretiSou.name
meMA.Ode4C1T.text = "Ἡ Κιβωτὸς τοῦ μαρτυρίου σε Πανάμωμε, διαγράφει τὸν ἄρτον τῆς ὄντως ζωῆς, τὴν δεξαμένην, Ἄχραντε, Λόγον τῷ Πατρὶ συναΐδιον."

meMA.Ode5C11.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode5C11.melody = he.h.m4_gr_GR_cog.OAnateilasToFos.name
meMA.Ode5C11.text = "Νῦν ἐπαρθέντα ὁρᾷ, ἡ σεπτὴ Ἐκκλησία, τὸν Ἥλιον ἐν ξύλῳ, ὡς προεθέσπισας, Ἀββακοὺμ Ἱερώτατε."

meMA.Ode5C12.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode5C12.melody = he.h.m4_gr_GR_cog.OAnateilasToFos.name
meMA.Ode5C12.text = "Ἀναπτερώσας τὸν νοῦν, ἐπὶ τῆς φυλακῆς σου, Πανόλβιε παρέστης, καὶ ἀπεσκόπευσας, τοῦ Κυρίου τὴν ἔλευσιν."

meMA.Ode5C13.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode5C13.melody = he.h.m4_gr_GR_cog.OAnateilasToFos.name
meMA.Ode5C13.text = "Μεγαλοφώνως σοφέ, ἐπὶ Θεῷ Σωτῆρι, χαρήσομαι ἐβόας, καὶ εὐφρανθήσομαι, Ἀββακοὺμ παμμακάριστε."

meMA.Ode5C1T.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode5C1T.melody = he.h.m4_gr_GR_cog.OAnateilasToFos.name
meMA.Ode5C1T.text = "Βεβαρυμμένος σειραῖς, τῶν πολλῶν μου πταισμάτων, ἐπὶ σὲ καταφεύγω, σῶσόν με Δέσποινα, τῶν πιστῶν τὸ κραταίωμα."

meMA.Ode6C11.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode6C11.melody = he.h.m4_gr_GR_cog.EvoiseProtypon.name
meMA.Ode6C11.text = "Ἀπὸ Θαιμάν, ὁ Θεὸς σαρκωθεὶς ἐπεδήμησεν, ὡς προέφης, παρ' αὐτοῦ τηλαυγῶς φωτιζόμενος, Ἀββακοὺμ τρισμάκαρ, καὶ τὸν κόσμον φωτὶ κατελάμπρυνεν."

meMA.Ode6C12.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode6C12.melody = he.h.m4_gr_GR_cog.EvoiseProtypon.name
meMA.Ode6C12.text = "Καταύγασον, ὁ Θεὸς τὰς ψυχὰς τῶν ὑμνούντων σε, ταῖς πρεσβείαις,, Ἀββακοὺμ τοῦ σεπτοῦ καὶ θεόφρονος, ὁ ἐμπνεύσει θείᾳ, καταλάμψας αὐτοῦ τὴν διάνοιαν."

meMA.Ode6C13.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode6C13.melody = he.h.m4_gr_GR_cog.EvoiseProtypon.name
meMA.Ode6C13.text = "Ὁ φθόγγος σου, καὶ ῥημάτων τῶν θείων ἡ δύναμις, θεηγόρε, διελήλυθε πάντα τὰ πέρατα, τὴν τοῦ Λόγου θείαν, παρουσίαν ἡμῖν προθεσπίζοντα."

meMA.Ode6C1T.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode6C1T.melody = he.h.m4_gr_GR_cog.EvoiseProtypon.name
meMA.Ode6C1T.text = "Ὑπάρχοντα, τῷ Πατρὶ τὸν Υἱὸν ὁμοούσιον, καὶ ἀνάρχως, τῷ Τεκόντι σαφῶς συννοούμενον, ἐπ' ἐσχάτων σάρκα, γεγονότα Παρθένε γεγέννηκας."

meMA.Ode6H.ode = misc_gr_GR_cog.Ode6
meMA.Ode6H.mode = misc_gr_GR_cog.Mode4
meMA.Ode6H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode6H.text = he.h.m4_gr_GR_cog.EvoiseProtypon.text

meMA.Kontakion1.mode = misc_gr_GR_cog.Mode4
meMA.Kontakion1.melody = he.a.m4_gr_GR_cog.TachyProkatalave.name
meMA.Kontakion1.text = "Ὡς ἵππους ἑώρακας τοὺς ἱεροὺς Μαθητάς, θαλάσσας ταράσσοντας, τῆς ἀγνωσίας σαφῶς, καὶ πλάνην βυθίζοντας, δόγμασιν εὐσεβείας, Ἀββακοὺμ θεηγόρε· ὅθεν σε ὡς Προφήτην, ἀληθῆ εὐφημοῦμεν, αἰτούμενοι τοῦ πρεσβεύειν ἐλεηθῆναι ἡμᾶς."

meMA.Synaxarion.text = ""

meMA.Ode7C11.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode7C11.melody = he.h.m4_gr_GR_cog.AvramiaioiPote.name
meMA.Ode7C11.text = "Μεμυημένος σοφέ, τὴν ὑπὲρ λόγον γνῶσιν, καὶ προφητείας τὴν πολύφωτον, λαμπάδα κραυγάζεις νῦν· ὁ τῶν Πατέρων ἡμῶν, Θεὸς εὐλογητὸς εἶ."

meMA.Ode7C12.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode7C12.melody = he.h.m4_gr_GR_cog.AvramiaioiPote.name
meMA.Ode7C12.text = "Μαρμαρυγῆς τριλαμποῦς, καὶ ἀνεκφράστου δόξης, τὴν μετουσίαν σοι δεδώρηται, Θεὸς ὃν ἐδόξασας, ὁ τῶν Πατέρων βοῶν· Θεὸς εὐλογητὸς εἶ."

meMA.Ode7C1T.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode7C1T.melody = he.h.m4_gr_GR_cog.AvramiaioiPote.name
meMA.Ode7C1T.text = "Ἐν γενεαῖς γενεῶν, Μήτηρ Παρθένος ὤφθης, εὐλογημένη ὑπὲρ ἔννοιαν, τὸν Λόγον κυήσασα, σεσαρκωμένον Θεόν, ὑπερφυῶς Παρθένε."

meMA.Ode8C11.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode8C11.melody = he.h.m4_gr_GR_cog.PantaTaErgaTouTheou.name
meMA.Ode8C11.text = "Λελαμπρυσμένος τῷ φωτί, τῷ τρισηλίῳ ἠξιώθης Πανόλβιε, θείων καὶ ὑπερφυῶν θαυμάτων, κραυγάζων· Ὑμνεῖτε τὸν Κύριον, καὶ ὑπερυψοῦτε, εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8C12.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode8C12.melody = he.h.m4_gr_GR_cog.PantaTaErgaTouTheou.name
meMA.Ode8C12.text = "Πίνων χειμάρρουν τῆς τρυφῆς, καὶ τῆς ἀΰλου θεωρίας πληρούμενος, δόξης τε προφητικῆς, καὶ προεδρίας μετέχων, χαίρων ἀνυμνεῖς, τὸν Θεὸν εἰς τοὺς αἰῶνας."

meMA.Ode8C13.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode8C13.melody = he.h.m4_gr_GR_cog.PantaTaErgaTouTheou.name
meMA.Ode8C13.text = "Ὢ τῆς ἐνθέου καὶ σεπτῆς, τῆς τοῦ Προφήτου Ἀββακοὺμ ὡραιότητος! οὗτος γάρ, Ἀγγελικαῖς, συγχορεύων δυνάμεσι, χαίρων ἀνυμνεῖ, τὸν Θεὸν εἰς τους αἰῶνας."

meMA.Ode8C1T.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode8C1T.melody = he.h.m4_gr_GR_cog.PantaTaErgaTouTheou.name
meMA.Ode8C1T.text = "Μήτηρ ἐδείχθης ἀληθῶς, τοῦ πρὸ αἰώνων ἐκ Πατρὸς ἀνατείλαντος· μόνη γὰρ ἀξιωτέρα, γεγένησαι τῶν ἀσωμάτων Νοῶν, εὐλογημένη ἁγνὴ Παρθένε."

meMA.Ode8H.ode = misc_gr_GR_cog.Ode8
meMA.Ode8H.mode = misc_gr_GR_cog.Mode4
meMA.Ode8H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode8H.text = he.h.m4_gr_GR_cog.PantaTaErgaTouTheou.text

meMA.Ode9C11.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode9C11.melody = he.h.m4_gr_GR_cog.OTokosSouAfthoros.name
meMA.Ode9C11.text = "Ἀστράπτει ἡ μνήμη σου Προφῆτα, λαμπρὰν ἀποπέμπουσα τὴν αἴγλην, τῆς σῆς θεηγορίας, καὶ τῆς σῆς μυσταγωγίας καὶ προφητείας, τοῖς σὲ τιμῶσιν, Ἀββακοὺμ μακαριώτατε."

meMA.Ode9C12.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode9C12.melody = he.h.m4_gr_GR_cog.OTokosSouAfthoros.name
meMA.Ode9C12.text = "Κηρύξας τὸν Κύριον τῆς δόξης, καὶ τούτου προειπὼν τὴν παρουσίαν, ἐξ ἁγίας Παρθένου, προφανῶς γεγενημένην καὶ δεδειγμένην, βλέπων εὐφραίνου, Ἀββακοὺμ μακαριώτατε."

meMA.Ode9C13.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode9C13.melody = he.h.m4_gr_GR_cog.OTokosSouAfthoros.name
meMA.Ode9C13.text = "Ἀγάλλονται πάντες οἱ Προφῆται, ἐπὶ τῇ σῇ χαρμοσύνῳ ἡμέρᾳ, γεγηθότες θεόφρον, τῆς χαρᾶς σοι κοινωνοῦντες καὶ θείας δόξης· οἷς συμπρεσβεύων, τοὺς ὑμνοῦντάς σε περίσῳζε."

meMA.Ode9C1T.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode9C1T.melody = he.h.m4_gr_GR_cog.OTokosSouAfthoros.name
meMA.Ode9C1T.text = "Ῥανίσιν Ἁγνὴ τῶν οἰκτιρμῶν σου, ψυχῆς μου ἀπόπλυνον τὸν ῥύπον, καὶ κρουνοὺς μοι δακρύων ἀναβλύζειν, ἀενάως ποίησον Κόρη, τὰς τῶν παθῶν μου, ἀναβλύσεις ἀναστέλλουσα."

meMA.Ode9H.ode = misc_gr_GR_cog.Ode9
meMA.Ode9H.mode = misc_gr_GR_cog.Mode4
meMA.Ode9H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode9H.text = he.h.m4_gr_GR_cog.OTokosSouAfthoros.text

