A_Resource_Whose_Name = oc.m7.d6_gr_GR_cog

ocVE.Stichera1.mode = misc_gr_GR_cog.Mode7
ocVE.Stichera1.melody = he.a.m7_gr_GR_cog.OukEtiKolyometha.incipit
ocVE.Stichera1.incipit = "Ἵνα Θεὸν τὸν ἄνθρωπον ἀπεργάσῃ,"
ocVE.Stichera1.text = "Ἵνα Θεὸν τὸν ἄνθρωπον ἀπεργάσῃ, βροτὸς γέγονας Ὑπεράγαθε, καὶ σταυροῦσαι, δόξα τῇ δόξῃ σου."

ocVE.Stichera2.mode = misc_gr_GR_cog.Mode7
ocVE.Stichera2.melody = he.a.m7_gr_GR_cog.OukEtiKolyometha.incipit
ocVE.Stichera2.incipit = "Ὅτε Σταυρῷ σε δῆμος Ἑβραίων,"
ocVE.Stichera2.text = "Ὅτε Σταυρῷ σε δῆμος Ἑβραίων, Λόγε, κατεδίκασε, γῆ ἐσείετο, καὶ ἡλίου σέλας ἐκρύπτετο."

ocVE.Stichera3.mode = misc_gr_GR_cog.Mode7
ocVE.Stichera3.melody = he.a.m7_gr_GR_cog.OukEtiKolyometha.incipit
ocVE.Stichera3.incipit = "Ὁ ἥλιος ἐσκότασεν Ἐλεῆμον, σταυρουμένου"
ocVE.Stichera3.text = "Ὁ ἥλιος ἐσκότασεν Ἐλεῆμον, σταυρουμένου σου, καὶ ἡ κτίσις συνεκλονεῖτο, κράζουσα· Δόξα σοι."

ocVE.Stichera4.mode = misc_gr_GR_cog.Mode7
ocVE.Stichera4.melody = he.a.m7_gr_GR_cog.SimeronGrigoreiOIoudas.incipit
ocVE.Stichera4.incipit = "Κλῖνόν μοι τὸ φιλάγαθον οὖς"
ocVE.Stichera4.text = "Κλῖνόν μοι τὸ φιλάγαθον οὖς σου, δεομένῳ Πανάμωμε, καὶ μὴ ἀποῤῥίψῃς τοῦ σοῦ με προσώπου, μηδὲ πόῤῥω πέμψῃς τῆς σῆς βοηθείας· δόλῳ γὰρ ὁ δόλιος, σπουδάζει ἀπολέσαι με, παραλόγοις πάθεσι, κινεῖ τὴν καρδίαν μου, σὺ δέ με συντήρησον, ὅλον τῆς αὐτοῦ βλάβης ἀμέτοχον."

ocVE.Stichera5.mode = misc_gr_GR_cog.Mode7
ocVE.Stichera5.melody = he.a.m7_gr_GR_cog.SimeronGrigoreiOIoudas.incipit
ocVE.Stichera5.incipit = "Ὄφελος οὐδαμῶς ἐκ τῆς ὕλης,"
ocVE.Stichera5.text = "Ὄφελος οὐδαμῶς ἐκ τῆς ὕλης, ὦ ψυχή μου γενήσεται, περὶ ἣν σπουδάζεις εἰς μάτην τοῦ βίου, περὶ ἣν ἐκτήκεις σαυτὴν ταῖς φροντίσι, πᾶσαν οὖν τὴν μέριμναν, ἐπίῤῥιψον πρὸς Κύριον, τὸν κρῖναί σε μέλλοντα, καὶ δάκρυσι βόησον· Δέσποτα φιλάνθρωπε, σῶσόν με πρεσβείαις τῆς Τεκούσης σε."

ocVE.Stichera6.mode = misc_gr_GR_cog.Mode7
ocVE.Stichera6.melody = he.a.m7_gr_GR_cog.SimeronGrigoreiOIoudas.incipit
ocVE.Stichera6.incipit = "Ὄνομα εὐλαβείας Παρθένε, ἐμαυτῷ ἔξω"
ocVE.Stichera6.text = "Ὄνομα εὐλαβείας Παρθένε, ἐμαυτῷ ἔξω θέμενος, παίγνιον πορνείας, ἐντὸς χρηματίζω, καὶ ἀλαζονείας, γέλως καθορῶμαι· θηρίοις γὰρ δυσίν, ὡς ἀντιμάχοις, παραδέχομαι ἐξ ἀπροσεξίας· διὰ τοῦτο ῥῦσαί με· σὺ γὰρ ἀγαλλίαμα, πέφυκας ψυχῆς μου καὶ πεποίθησις."

ocVE.SticTheotokion.mode = misc_gr_GR_cog.Mode7
ocVE.SticTheotokion.melody = he.a.m7_gr_GR_cog.SimeronGrigoreiOIoudas.incipit
ocVE.SticTheotokion.incipit = "Ῥήματα ἐν τῷ τόκῳ σου"
ocVE.SticTheotokion.text = "Ῥήματα ἐν τῷ τόκῳ σου Τέκνον, δι'ὧν χαροποιὰ πρῴην ἤκουον, προσεδόκων, χαρᾶς ἐμπλησθῆναι, καὶ χαροποιῆσαι, ψυχὰς τεθλιμμένας, ταῦτα δὲ πρὸς λύπην μετετράπη, τῇ σταυρώσει σου ἐναργῶς τὰ πράγματα· ἡ λύπη γὰρ τήκει με, καὶ θανεῖν προῄρημαι, πληρούσης τῆς λύπης τὴν καρδίαν μου."

ocVE.Aposticha1.mode = misc_gr_GR_cog.Mode7
ocVE.Aposticha1.melody = misc_gr_GR_cog.Automelon
ocVE.Aposticha1.incipit = he.a.m7_gr_GR_cog.OukEtiKolyometha.incipit
ocVE.Aposticha1.text = he.a.m7_gr_GR_cog.OukEtiKolyometha.text

