A_Resource_Whose_Name = oc.m6.d5_gr_GR_cog

ocVE.Stichera1.mode = misc_gr_GR_cog.Mode6
ocVE.Stichera1.melody = he.a.m6_gr_GR_cog.OlinApothemenoi.incipit
ocVE.Stichera1.incipit = "Δύναμιν οὐράνιον, τοῦ Παρακλήτου λαβόντες,"
ocVE.Stichera1.text = "Δύναμιν οὐράνιον, τοῦ Παρακλήτου λαβόντες, τῶν ἐχθρῶν τὰς φάλαγγας, οἱ Χριστοῦ Ἀπόστολοι, ἐτροπώσαντο. Βασιλεῖς ἤλεγξαν, καὶ σοφοὺς ᾔσχυναν, ἰδιῶται καὶ ἀγράμματοι, γυμνοὶ καὶ ἄοπλοι, εὐαγγελικῶς παρετάξαντο, τῷ λόγῳ δὲ τῆς χάριτος, τὰς τερατουργίας εἰργάσαντο, καὶ οὕτω τὰ ἔθνη, ὑπήκοα ποιούμενοι Χριστῷ, ἐν παῤῥησίᾳ πρεσβεύουσιν, ὑπὲρ τῶν ψυχῶν ἡμῶν."

ocVE.Stichera2.mode = misc_gr_GR_cog.Mode6
ocVE.Stichera2.melody = he.a.m6_gr_GR_cog.OlinApothemenoi.incipit
ocVE.Stichera2.incipit = "Σὲ ἐπιποθήσαντες, οἱ εὐκλεεῖς Μαθηταί"
ocVE.Stichera2.text = "Σὲ ἐπιποθήσαντες, οἱ εὐκλεεῖς Μαθηταί σου, καὶ ἀπαρνησάμενοι, ἑαυτοὺς προθύμως σοι, ἠκολούθησαν, καὶ Σταυρὸν ἄραντες, σοῦ αὐτοῦ ὀπίσω, πειθαρχοῦντες τῇ προστάξει σου, τὴν γῆν κατέλιπον, καὶ τοὺς οὐρανοὺς ἐκληρώσαντο, τὰ ἔθνη ἐπιστρέφοντες, εἰς τὴν ἀληθῆ σου ἐπίγνωσιν, καὶ νῦν διὰ πάντων, ὑπήκοοι γενόμενοι ἀεί, ἐν παῤῥησίᾳ πρεσβεύουσιν, ὑπὲρ τῶν ψυχῶν ἡμῶν."

ocVE.Stichera3.mode = misc_gr_GR_cog.Mode6
ocVE.Stichera3.melody = he.a.m6_gr_GR_cog.OlinApothemenoi.incipit
ocVE.Stichera3.incipit = "Ἅπαντα τὸν βίον μου, ἐν"
ocVE.Stichera3.text = "Ἅπαντα τὸν βίον μου, ἐν ἀμελείᾳ διάγων, πρὸς τὸ τέλος ἔφθασα, ἄκαρπος ὁ δείλαιος, Ὑπεράγαθε, πονηρῶν πράξεων, ἐπιφέρων μόνον, νῦν φορτία τὰ δυσβάστακτα, ἅπερ φιλάνθρωπε, σκόρπισον ῥοπῇ τοῦ ἐλέους σου, καὶ δίδου μοι κατάνυξιν, καὶ ἐπιστροφὴν τὴν σωτήριον, θείαις Ἀποστόλων, πρεσβείαις εὐπροσδέκτοις ὁ Θεός, οὓς εἰς πρεσβείαν προσάγω σοι, καὶ τὴν σὲ κυήσασαν."

ocVE.Stichera4.mode = misc_gr_GR_cog.Mode6
ocVE.Stichera4.melody = he.a.m6_gr_GR_cog.TriimerosAnestis.incipit
ocVE.Stichera4.incipit = "Νικόλαε μακάριε, προσπίπτοντά με οἴκτειρον,"
ocVE.Stichera4.text = "Νικόλαε μακάριε, προσπίπτοντά με οἴκτειρον, δέομαί σου, καὶ τὰ ὄμματα σοφέ, φώτισον τῆς ψυχῆς μου, ἵνα καθαρῶς βλέψω, τὸν φωτοδότην καὶ οἰκτίρμονα."

ocVE.Stichera5.mode = misc_gr_GR_cog.Mode6
ocVE.Stichera5.melody = he.a.m6_gr_GR_cog.TriimerosAnestis.incipit
ocVE.Stichera5.incipit = "Ἐχθρῶν ἐπιζητούντων με, κακοποιῆσαι Ἅγιε,"
ocVE.Stichera5.text = "Ἐχθρῶν ἐπιζητούντων με, κακοποιῆσαι Ἅγιε, λύτρωσαί με, παῤῥησίαν πρὸς Θεόν, ὡς ἔχων Ἱεράρχα, καὶ σῶσόν με ἐκ βλάβης, καὶ ἐξ ἀνδρῶν αἱμάτων ῥῦσαί με."

ocVE.Stichera6.mode = misc_gr_GR_cog.Mode6
ocVE.Stichera6.melody = he.a.m6_gr_GR_cog.TriimerosAnestis.incipit
ocVE.Stichera6.incipit = "Λιμένα σε ἀχείμαστον, καὶ τεῖχος"
ocVE.Stichera6.text = "Λιμένα σε ἀχείμαστον, καὶ τεῖχος ἀπροσμάχητον, Ἱεράρχα, νῦν κεκτήμεθα πιστῶς, καὶ πύργον ἀσφαλείας, καὶ θύραν μετανοίας, καὶ ὁδηγὸν πιστόν, καὶ πρόμαχον."

ocVE.SticTheotokion.mode = misc_gr_GR_cog.Mode6
ocVE.SticTheotokion.melody = he.a.m6_gr_GR_cog.TriimerosAnestis.incipit
ocVE.SticTheotokion.incipit = "Μεγίστων δωρημάτων ἡμῖν, ὁ τόκος"
ocVE.SticTheotokion.text = "Μεγίστων δωρημάτων ἡμῖν, ὁ τόκος ὁ ἀλόχευτος, τῆς Παρθένου, πᾶσι γέγονε σαφῶς· Θεὸς γὰρ τὴν φθαρεῖσαν, καινίζει πάλιν φύσιν, καὶ ἀναπλάττει τὸ ἀνθρώπινον."

ocVE.Aposticha1.mode = misc_gr_GR_cog.Mode6
ocVE.Aposticha1.melody = ""
ocVE.Aposticha1.incipit = "Ποτὲ οἱ Μαθηταί σου Χριστέ,"
ocVE.Aposticha1.text = "Ποτὲ οἱ Μαθηταί σου Χριστέ, ἐν πλοίῳ χειμαζόμενοι, ἀνεβόων· Διδάσκαλε, σῶσον ἡμᾶς, ἀπολλύμεθα, καὶ νῦν ἡμεῖς κραυγάζομεν, Φιλάνθρωπε Σωτὴρ ἡμῶν, ῥῦσαι ἡμᾶς ἐκ τῶν ἀναγκῶν ἡμῶν δεόμεθα."

ocVE.Aposticha2.mode = misc_gr_GR_cog.Mode6
ocVE.Aposticha2.melody = ""
ocVE.Aposticha2.incipit = "Κύριε, οἱ Ἀπόστολοι κατέλαβον, ποτὲ"
ocVE.Aposticha2.text = "Κύριε, οἱ Ἀπόστολοι κατέλαβον, ποτὲ μὲν τὸ βάθος τῆς θαλάσσης διὰ δικτύων, ποτὲ δὲ τὸ ὕψος τῆς Βασιλείας διὰ δογμάτων, καὶ τῇ μὲν τέχνῃ, τὸν ἀκατάληπτον βυθὸν ἠρεύνησαν, τῇ δὲ πίστει, τὸν ἀπερίγραπτόν σου κόλπον ἔφθασαν, καὶ τῷ κόσμῳ ἐκήρυξαν τὸν ἄχρονον Υἱόν σου, ὧν ταῖς πρεσβείαις, καὶ πάντων τῶν Ἁγίων, ἐλέησον ἡμᾶς."

ocVE.Aposticha3.mode = misc_gr_GR_cog.Mode6
ocVE.Aposticha3.melody = ""
ocVE.Aposticha3.incipit = "Ὁ Σταυρός σου Κύριε, τοῖς"
ocVE.Aposticha3.text = "Ὁ Σταυρός σου Κύριε, τοῖς Μάρτυσι γέγονεν, ὅπλον ἀήττητον· ἔβλεπον γὰρ τὸν προκείμενον θάνατον, καὶ προβλέποντες τὴν μέλλουσαν ζωήν, τῇ ἐλπίδι τῇ εἰς σὲ ἐνεδυναμοῦντο. Αὐτῶν ταῖς παρακλήσεσιν, ἐλέησον ἡμᾶς."

ocVE.AposTheotokion.mode = misc_gr_GR_cog.Mode6
ocVE.AposTheotokion.melody = ""
ocVE.AposTheotokion.incipit = "Θεοτόκε, σὺ εἶ ἡ ἄμπελος"
ocVE.AposTheotokion.text = "Θεοτόκε, σὺ εἶ ἡ ἄμπελος ἡ ἀληθινή, ἡ βλαστήσασα τὸν καρπὸν τῆς ζωῆς, σὲ ἱκετεύομεν, πρέσβευε Δέσποινα, μετὰ τῶν Ἀποστόλων, καὶ πάντων τῶν Ἁγίων, ἐλεηθῆναι τὰς ψυχὰς ἡμῶν."

ocVE.ApolTheotokionVM.mode = misc_gr_GR_cog.Mode6
ocVE.ApolTheotokionVM.melody = he.a.m6_gr_GR_cog.ElpisTouKosmouAgathi.incipit
ocVE.ApolTheotokionVM.incipit = oc.m6.d5_gr_GR_cog.ocMA.Kathisma23.incipit
ocVE.ApolTheotokionVM.text = oc.m6.d5_gr_GR_cog.ocMA.Kathisma23.text

ocMA.Kathisma11.mode = misc_gr_GR_cog.Mode6
ocMA.Kathisma11.melody = ""
ocMA.Kathisma11.incipit = "Ὡς ἐν μέσῳ τῶν Μαθητῶν"
ocMA.Kathisma11.text = "Ὡς ἐν μέσῳ τῶν Μαθητῶν σου, παρεγένου Σωτήρ, τὴν εἰρήνην διδοὺς αὐτοῖς, ἐλθὲ καὶ μεθ' ἡμῶν καὶ σῶσον ἡμᾶς."

ocMA.Kathisma12.mode = misc_gr_GR_cog.Mode6
ocMA.Kathisma12.melody = he.a.m6_gr_GR_cog.ElpisTouKosmouAgathi.incipit
ocMA.Kathisma12.incipit = "Ὡς φῶς σταλέντες ἐπὶ γῆς,"
ocMA.Kathisma12.text = "Ὡς φῶς σταλέντες ἐπὶ γῆς, ἐφωτίσατε πάντας, σοφοῖς διδάγμασιν ὑμῶν, καὶ ἐν δόγμασι θείοις διώξαντες, πολυθεΐας σκοτασμόν, καὶ λάμψαντες τὸν τῆς Τριάδος φωτισμόν, καὶ σώσαντες λαοὺς πολλούς, ἐκ πλάνης ἀφεγγοῦς, Ἀπόστολοι τοῦ Σωτῆρος."

ocMA.Kathisma13.mode = misc_gr_GR_cog.Mode6
ocMA.Kathisma13.melody = ""
ocMA.Kathisma13.incipit = "Τῆς εὐσπλαγχνίας τὴν πύλην ἄνοιξον"
ocMA.Kathisma13.text = "Τῆς εὐσπλαγχνίας τὴν πύλην ἄνοιξον ἡμῖν, εὐλογημένη Θεοτόκε· ἐλπίζοντες εἰς σέ, μὴ ἀστοχήσωμεν· ῥυσθείημεν διὰ σοῦ τῶν περιστάσεων· σὺ γὰρ εἶ ἡ σωτηρία, τοῦ γένους τῶν Χριστιανῶν."

ocMA.Kathisma21.mode = misc_gr_GR_cog.Mode6
ocMA.Kathisma21.melody = ""
ocMA.Kathisma21.incipit = "Τῶν Ἀποστόλων σου ἡ μνήμη"
ocMA.Kathisma21.text = "Τῶν Ἀποστόλων σου ἡ μνήμη Κύριε, ἀνεδείχθη ὡς Παράδεισος ὁ ἐν Ἐδέμ· ἐν αὐτῇ γὰρ ἀγάλλεται ἡ Οἰκουμένη· διὸ δίδου ἡμῖν ταῖς αὐτῶν ἱκεσίαις, εἰρήνην καὶ τὸ μέγα ἔλεος."

ocMA.Kathisma22.mode = misc_gr_GR_cog.Mode6
ocMA.Kathisma22.melody = ""
ocMA.Kathisma22.incipit = "Ἡ δωδεκὰς τῶν Ἀποστόλων Κύριε,"
ocMA.Kathisma22.text = "Ἡ δωδεκὰς τῶν Ἀποστόλων Κύριε, δωδεκάπυρσος ὡς ἥλιος, ὤφθη τῇ γῇ· δι' αὐτῶν γὰρ ἐμάρανας, πλάνης τὴν βλάβην, δι' αὐτῶν καταυγάζεις ψυχάς, τῶν σῶν δούλων, δι' ὧν καὶ ἡμᾶς, σῶσον εὔσπλαγχνε."

ocMA.Kathisma23.mode = misc_gr_GR_cog.Mode6
ocMA.Kathisma23.melody = he.a.m6_gr_GR_cog.ElpisTouKosmouAgathi.incipit
ocMA.Kathisma23.incipit = "Ἁγία Δέσποινα Ἁγνή, τοῦ Θεοῦ"
ocMA.Kathisma23.text = "Ἁγία Δέσποινα Ἁγνή, τοῦ Θεοῦ ἡμῶν Μήτηρ, ἡ τῶν ἁπάντων ποιητήν, ἀποῤῥήτως τεκοῦσα, ἱκέτευε σὺν Ἀποστόλοις ἱεροῖς, ἑκάστοτε τὴν ἀγαθότητα αὐτοῦ, παθῶν ἡμᾶς λυτρώσασθαι, καὶ ἄφεσιν ἡμῖν, δοῦναι ἁμαρτημάτων."

ocMA.Kathisma31.mode = misc_gr_GR_cog.Mode6
ocMA.Kathisma31.melody = ""
ocMA.Kathisma31.incipit = "Τῇ σαγήνῃ τῆς ἐνθέου πίστεως,"
ocMA.Kathisma31.text = "Τῇ σαγήνῃ τῆς ἐνθέου πίστεως, τὰς διπλόας ἀνατρέψαντες οἱ ἁλιεῖς, ἐκ βυθοῦ ματαιότητος εἵλκυσαν πάντας, καὶ παρέσχον αὐτοὺς τῷ Θεῷ εὐσεβοῦντας, καὶ τοῦτον ἀεὶ μεγαλύνοντας."

ocMA.Kathisma32.mode = misc_gr_GR_cog.Mode6
ocMA.Kathisma32.melody = ""
ocMA.Kathisma32.incipit = "Τῶν Ἁγίων σου ἡ μνήμη"
ocMA.Kathisma32.text = "Τῶν Ἁγίων σου ἡ μνήμη Κύριε, ἀνεδείχθη ὡς παράδεισος ὁ ἐν Ἐδέμ· ἐν αὐτῇ γὰρ ἀγάλλεται πᾶσα ἡ κτίσις. Διὸ παράσχου ἡμῖν, τῇ αὐτῶν παρακλήσει, εἰρήνην καὶ τὸ μέγα ἔλεος."

ocMA.Kathisma33.mode = misc_gr_GR_cog.Mode6
ocMA.Kathisma33.melody = ""
ocMA.Kathisma33.incipit = "Ἐν τῷ Ναῷ ἑστῶτες τῆς"
ocMA.Kathisma33.text = "Ἐν τῷ Ναῷ ἑστῶτες τῆς δόξης σου, ἐν οὐρανῷ ἑστάναι νομίζομεν· Θεοτόκε, πύλη ἐπουράνιε, ἄνοιξον ἡμῖν τὴν θύραν τοῦ ἐλέους σου."

ocMA.C2.acrostic = " οὗ ἡ ἀκροστιχὶς μετὰ τῶν Θεοτοκίων· Ἕκτον μέλισμα, Νικόλαε, προσδέχου. Ἰωσήφ."

ocMA.CA.notice = rubrical_gr_GR_cog.unavailable.octoechos

ocMA.OdeW1C1H.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW1C1H.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW1C1H.incipit = he.h.m6_gr_GR_cog.OsEnIpeiroPezefsas.incipit
ocMA.OdeW1C1H.text = he.h.m6_gr_GR_cog.OsEnIpeiroPezefsas.text

ocMA.OdeW1C11.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW1C11.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C11.melody = he.h.m6_gr_GR_cog.OsEnIpeiroPezefsas.incipit
ocMA.OdeW1C11.incipit = "Φῶς πεφυκότες τοῦ κόσμου φωτιστικαῖς,"
ocMA.OdeW1C11.text = "Φῶς πεφυκότες τοῦ κόσμου φωτιστικαῖς, θεηγόροι λάμψεσι, καταυγάσατε ἡμᾶς, καὶ παθῶν λυτρώσατε νυκτός, καὶ ὁμίχλης πειρασμῶν, θεομακάριστοι."

ocMA.OdeW1C12.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW1C12.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C12.melody = he.h.m6_gr_GR_cog.OsEnIpeiroPezefsas.incipit
ocMA.OdeW1C12.incipit = "Ἄρχοντας γῆν ἐπὶ πᾶσαν Πνεῦμα"
ocMA.OdeW1C12.text = "Ἄρχοντας γῆν ἐπὶ πᾶσαν Πνεῦμα ὑμᾶς, τὸ σεπτὸν κατέστησε, τοῦ Κυρίου Μαθηταί, τοῦ ἀέρος ἄρχοντας· διό, ἐτροπώσασθε πιστούς, ἐκλυτρωσάμενοι."

ocMA.OdeW1C13.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW1C13.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C13.melody = he.h.m6_gr_GR_cog.OsEnIpeiroPezefsas.incipit
ocMA.OdeW1C13.incipit = "Ἐκ τοῦ πυρὸς τοῦ ἀΰλου"
ocMA.OdeW1C13.text = "Ἐκ τοῦ πυρὸς τοῦ ἀΰλου ὡς φωταυγεῖς, ἀναφθέντες ἄνθρακες, τὰ ὑλώδη μου σοφοί, πάθη καταφλέξατε πρὸς φῶς, ἀπαθείας καὶ ζωῆς, καθοδηγοῦντές με."

ocMA.OdeW1C1T.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW1C1T.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C1T.melody = he.h.m6_gr_GR_cog.OsEnIpeiroPezefsas.incipit
ocMA.OdeW1C1T.incipit = "Τῶν Ἀποστόλων πρεσβείαις καὶ Προφητῶν,"
ocMA.OdeW1C1T.text = "Τῶν Ἀποστόλων πρεσβείαις καὶ Προφητῶν, καὶ Μαρτύρων Κύριε, Ἰησοῦ παμβασιλεῦ, καὶ τῆς κυησάσης σε Ἁγνῆς, κατοικτείρησον λαόν, ἡμαρτηκότα σοι."

ocMA.OdeW1C21.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW1C21.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C21.melody = he.h.m6_gr_GR_cog.OAisthitosFarao.incipit
ocMA.OdeW1C21.incipit = "Ἐν οὐρανοῖς κατοικῶν, χαρμονικῶς Πάτερ"
ocMA.OdeW1C21.text = "Ἐν οὐρανοῖς κατοικῶν, χαρμονικῶς Πάτερ σοφέ, ἐπὶ γῆς τῶν ἐν πίστει, συμφώνως ἀνυμνούντων σε, πᾶσαν λύπην καρδίας, πονηρὰν διασκέδασον, τῇ μεσιτείᾳ σου."

ocMA.OdeW1C22.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW1C22.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C22.melody = he.h.m6_gr_GR_cog.OAisthitosFarao.incipit
ocMA.OdeW1C22.incipit = "Κατὰ παθῶν βλαβερῶν, κατὰ ἐχθρῶν"
ocMA.OdeW1C22.text = "Κατὰ παθῶν βλαβερῶν, κατὰ ἐχθρῶν φθοροποιῶν, κατὰ πάσης τοῦ πλάνου, κακίας νῦν ἐνίσχυσον, τοὺς τῇ σκέπῃ σου Πάτερ τῇ ἁγίᾳ Νικόλαε, πίστει προστρέχοντας."

ocMA.OdeW1C23.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW1C23.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C23.melody = he.h.m6_gr_GR_cog.OAisthitosFarao.incipit
ocMA.OdeW1C23.incipit = "Τραυματισθέντας ἡμᾶς, ὑπ' ἀοράτων τοξευτῶν,"
ocMA.OdeW1C23.text = "Τραυματισθέντας ἡμᾶς, ὑπ' ἀοράτων τοξευτῶν, τῇ μοτώσει τῆς σῆς Νικόλαε πρεσβείας, περιποίησαι, ὅπως ὑγιῶς βηματίζωμεν, τὰς τοῦ Κυρίου ὁδούς."

ocMA.OdeW1C2T.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW1C2T.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C2T.melody = he.h.m6_gr_GR_cog.OAisthitosFarao.incipit
ocMA.OdeW1C2T.incipit = "Ὁ συνυπάρχων Πατρί, ὁ συναΐδιος"
ocMA.OdeW1C2T.text = "Ὁ συνυπάρχων Πατρί, ὁ συναΐδιος Υἱός, ἐπ' ἐσχάτων βουληθείς, κατῴκησε τὴν μήτραν σου, εἰς ἡμῶν Θεοτόκε, τῶν πιστῶν ἀναγέννησιν· ὅθεν ὑμνοῦμέν σε."

ocMA.OdeW3C1H.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW3C1H.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW3C1H.incipit = he.h.m6_gr_GR_cog.OukEstinAgios.incipit
ocMA.OdeW3C1H.text = he.h.m6_gr_GR_cog.OukEstinAgios.text

ocMA.OdeW3C11.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW3C11.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C11.melody = he.h.m6_gr_GR_cog.OukEstinAgios.incipit
ocMA.OdeW3C11.incipit = "Τὸ θεῖον Πνεῦμα ταῖς ὑμῶν,"
ocMA.OdeW3C11.text = "Τὸ θεῖον Πνεῦμα ταῖς ὑμῶν, γλώσσαις ἀναχωνεῦσαν, ἐπελεύσει φρικώδει, συντριβέντας πρὸς ζωήν, καινοποιεῖ δι' ὑμῶν, τοὺς ἀνθρώπους ἔνδοξοι Ἀπόστολοι."

ocMA.OdeW3C12.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW3C12.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C12.melody = he.h.m6_gr_GR_cog.OukEstinAgios.incipit
ocMA.OdeW3C12.incipit = "Ἐν γνώσει Λόγε πλημμελῶν, ποίαν"
ocMA.OdeW3C12.text = "Ἐν γνώσει Λόγε πλημμελῶν, ποίαν ἔξω συγγνώμην, ἐν ἡμέρᾳ τῆς δίκης; διὰ τοῦτό σοι βοῶ· Διὰ τῶν σῶν Μαθητῶν, οἴκτειρόν με τὸν αὐτοκατάκριτον."

ocMA.OdeW3C13.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW3C13.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C13.melody = he.h.m6_gr_GR_cog.OukEstinAgios.incipit
ocMA.OdeW3C13.incipit = "Οἱ μιμησάμενοι Χριστοῦ, τὰ σωτήρια"
ocMA.OdeW3C13.text = "Οἱ μιμησάμενοι Χριστοῦ, τὰ σωτήρια πάθη, τῆς ψυχῆς μου τὰ πάθη, τὰ χρόνια καὶ δεινά, ἰάσασθε ἰατροί, πεφυκότες ἔνδοξοι Ἀπόστολοι."

ocMA.OdeW3C1T.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW3C1T.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C1T.melody = he.h.m6_gr_GR_cog.OukEstinAgios.incipit
ocMA.OdeW3C1T.incipit = "Ἡμαρτηκότα με Χριστέ, διὰ τῶν"
ocMA.OdeW3C1T.text = "Ἡμαρτηκότα με Χριστέ, διὰ τῶν Ἀποστόλων, καὶ τῆς σὲ κυησάσης, ἐπιστρέψας ὡς Θεός, ἐλέησον καὶ πυρός, αἰωνίου Σῶτερ ἐλευθέρωσον."

ocMA.OdeW3C21.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW3C21.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C21.melody = he.h.m6_gr_GR_cog.EnTiSterraTisPisteos.incipit
ocMA.OdeW3C21.incipit = "Νίκην σαφῶς, κατὰ τῶν ψυχοφθόρων"
ocMA.OdeW3C21.text = "Νίκην σαφῶς, κατὰ τῶν ψυχοφθόρων ᾖρας παθῶν ἡμῶν Νικόλαε παμμάκαρ· ὑφ' ὧν ἡμᾶς πάντοτε, πορθουμένους χαλεπῶς, σαῖς προσευχαῖς ἀπολύτρωσαι."

ocMA.OdeW3C22.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW3C22.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C22.melody = he.h.m6_gr_GR_cog.EnTiSterraTisPisteos.incipit
ocMA.OdeW3C22.incipit = "Μέλη σαρκός, νεκρώσας ἐγκρατείᾳ, πρὸς"
ocMA.OdeW3C22.text = "Μέλη σαρκός, νεκρώσας ἐγκρατείᾳ, πρὸς τὴν ζωήν, μετέβης τὴν ἀγήρω, ἣν καὶ ἡμᾶς πρέσβευε, ἀπολήψεσθαι σοφέ, χαρμονικῶς εὐφημοῦντάς σε."

ocMA.OdeW3C23.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW3C23.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C23.melody = he.h.m6_gr_GR_cog.EnTiSterraTisPisteos.incipit
ocMA.OdeW3C23.incipit = "Ἐπισκοπῶν, ἡμᾶς ἐξ οὐρανίου περιωπῆς,"
ocMA.OdeW3C23.text = "Ἐπισκοπῶν, ἡμᾶς ἐξ οὐρανίου περιωπῆς, μὴ παύσῃ Ἱεράρχα, ἀποσοβῶν ἅπαντα, τὰ τοῦ βίου δυσχερῆ, σαῖς πρὸς Θεὸν παρακλήσεσι."

ocMA.OdeW3C2T.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW3C2T.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C2T.melody = he.h.m6_gr_GR_cog.EnTiSterraTisPisteos.incipit
ocMA.OdeW3C2T.incipit = "Λύσιν ἡμῖν, Ἁγνὴ ἁμαρτημάτων, τὸν"
ocMA.OdeW3C2T.text = "Λύσιν ἡμῖν, Ἁγνὴ ἁμαρτημάτων, τὸν Λυτρωτήν, δοθῆναι ἐκδυσώπει, τὸν σκοτασμὸν Ἄχραντε, τῶν ψυχῶν ἡμῶν ἀεί, ταῖς σαῖς πρεσβείαις σκεδάζουσα."

ocMA.OdeW4C1H.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW4C1H.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW4C1H.incipit = he.h.m6_gr_GR_cog.ChristosMouDynamis.incipit
ocMA.OdeW4C1H.text = he.h.m6_gr_GR_cog.ChristosMouDynamis.text

ocMA.OdeW4C11.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW4C11.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C11.melody = he.h.m6_gr_GR_cog.ChristosMouDynamis.incipit
ocMA.OdeW4C11.incipit = "Λιμένες εὔδιοι, λιμένες ἄκλυστοι, γαληνόν"
ocMA.OdeW4C11.text = "Λιμένες εὔδιοι, λιμένες ἄκλυστοι, γαληνόν με, πρὸς ὅρμον ἀληθινῆς, μετανοίας σώσατε, ἁμαρτημάτων χαλεπῶν, τρικυμίᾳ χειμαζόμενον."

ocMA.OdeW4C12.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW4C12.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C12.melody = he.h.m6_gr_GR_cog.ChristosMouDynamis.incipit
ocMA.OdeW4C12.incipit = "Ψυχή μου στέναξον, ψυχή μου"
ocMA.OdeW4C12.text = "Ψυχή μου στέναξον, ψυχή μου δάκρυσον, ταπεινή μου καρδία, τῷ Ἀγαθῷ, ὁλικῶς κολλήθητι, ὅπως μελλούσης σε φλογός, καὶ κολάσεως λυτρώσηται."

ocMA.OdeW4C13.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW4C13.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C13.melody = he.h.m6_gr_GR_cog.ChristosMouDynamis.incipit
ocMA.OdeW4C13.incipit = "Σωτῆρα Κύριον, σωτῆρες πέλοντες, τῶν"
ocMA.OdeW4C13.text = "Σωτῆρα Κύριον, σωτῆρες πέλοντες, τῶν βροτῶν, δυσωπεῖτε, ὅπως ἡμᾶς, σώσῃ ὡς φιλάνθρωπος, πειρατηρίων χαλεπῶν, παμμακάριστοι Ἀπόστολοι."

ocMA.OdeW4C1T.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW4C1T.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C1T.melody = he.h.m6_gr_GR_cog.ChristosMouDynamis.incipit
ocMA.OdeW4C1T.incipit = "Παρθένε ἄχραντε, Παρθένε ἄμωμε, μετὰ"
ocMA.OdeW4C1T.text = "Παρθένε ἄχραντε, Παρθένε ἄμωμε, μετὰ τῶν Ἀποστόλων ὑπὲρ ἡμῶν, ἱκεσίαν ποίησον, ὅπως ῥυσθείη μὲν δεινῶν, πειρασμῶν καὶ περιστάσεων."

ocMA.OdeW4C21.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW4C21.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C21.melody = he.h.m6_gr_GR_cog.AkikoaKyrieTinAkoin.incipit
ocMA.OdeW4C21.incipit = "Ἰάτρευσον Ἅγιε, τὴν πληγωθεῖσαν ὑπὸ"
ocMA.OdeW4C21.text = "Ἰάτρευσον Ἅγιε, τὴν πληγωθεῖσαν ὑπὸ τοῦ πλάνου, ἀδιόρθωτον καρδίαν μου, ἱκετεύων, τὸν ἀγαθοδότην Θεὸν ἡμῶν."

ocMA.OdeW4C22.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW4C22.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C22.melody = he.h.m6_gr_GR_cog.AkikoaKyrieTinAkoin.incipit
ocMA.OdeW4C22.incipit = "Συνέτριψας ξόανα, τῆς ἀπωλείας, Χριστοῦ"
ocMA.OdeW4C22.text = "Συνέτριψας ξόανα, τῆς ἀπωλείας, Χριστοῦ θεράπον· ὅθεν πίστει ἱκετεύω σε, τοῦ νοός μου κάκιστα, ἰνδάλματα μείωσον."

ocMA.OdeW4C23.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW4C23.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C23.melody = he.h.m6_gr_GR_cog.AkikoaKyrieTinAkoin.incipit
ocMA.OdeW4C23.incipit = "Μυρίπνοος γέγονας, δικαιοσύνης ἔργοις θεόφρον,"
ocMA.OdeW4C23.text = "Μυρίπνοος γέγονας, δικαιοσύνης ἔργοις θεόφρον, καὶ τῶν Μύρων κατεκόσμησας, τὴν καθέδραν, Πάτερ Ἱεράρχα Νικόλαε."

ocMA.OdeW4C2T.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW4C2T.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C2T.melody = he.h.m6_gr_GR_cog.AkikoaKyrieTinAkoin.incipit
ocMA.OdeW4C2T.incipit = "Ἀνύμφευτε Δέσποινα, εὐλογημένη Θεοκυῆτορ, τοὺς"
ocMA.OdeW4C2T.text = "Ἀνύμφευτε Δέσποινα, εὐλογημένη Θεοκυῆτορ, τοὺς ὑμνοῦντάς σε περίσῳζε, πάσης βλάβης, μόνη τῶν ἀνθρώπων βοήθεια."

ocMA.OdeW5C1H.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW5C1H.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW5C1H.incipit = he.h.m6_gr_GR_cog.ToTheioFengeiSou.incipit
ocMA.OdeW5C1H.text = he.h.m6_gr_GR_cog.ToTheioFengeiSou.text

ocMA.OdeW5C11.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW5C11.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C11.melody = he.h.m6_gr_GR_cog.ToTheioFengeiSou.incipit
ocMA.OdeW5C11.incipit = "Ὁδὸν τὴν φέρουσαν εἰς ζωήν,"
ocMA.OdeW5C11.text = "Ὁδὸν τὴν φέρουσαν εἰς ζωήν, διὰ στενωτάτης ἀγωγῆς, καὶ πειρασμῶν τε καὶ θλίψεων, Μαθηταὶ τοῦ Λόγου, σαφῶς ὁδεύσαντες, τὰς τρίβους τῆς ψυχῆς μου ἐξομαλίσατε."

ocMA.OdeW5C12.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW5C12.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C12.melody = he.h.m6_gr_GR_cog.ToTheioFengeiSou.incipit
ocMA.OdeW5C12.incipit = "Λύχνοι τῆς θείας ἀνατολῆς, ῥύσασθε"
ocMA.OdeW5C12.text = "Λύχνοι τῆς θείας ἀνατολῆς, ῥύσασθε ἐκ σκότους με παθῶν, καὶ ἡδονῶν ἀμαυρότητος, καὶ πειρατηρίων καὶ περιστάσεων, καὶ ζάλης καὶ κινδύνων, Χριστοῦ Ἀπόστολοι."

ocMA.OdeW5C13.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW5C13.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C13.melody = he.h.m6_gr_GR_cog.ToTheioFengeiSou.incipit
ocMA.OdeW5C13.incipit = "Τόμοι τοῦ Λόγου θεοειδεῖς, βίβλῳ"
ocMA.OdeW5C13.text = "Τόμοι τοῦ Λόγου θεοειδεῖς, βίβλῳ με γραφῆναι τῆς ζωῆς, ἐκδυσωπεῖτε Ἀπόστολοι, τῶν ἁμαρτιῶν μου τὸ χαλεπώτατον, ῥηγνύντες γραμματεῖον, ταῖς προσευχαῖς ὑμῶν."

ocMA.OdeW5C1T.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW5C1T.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C1T.melody = he.h.m6_gr_GR_cog.ToTheioFengeiSou.incipit
ocMA.OdeW5C1T.incipit = "Ἡ σωτηρία μου καὶ ἐλπίς,"
ocMA.OdeW5C1T.text = "Ἡ σωτηρία μου καὶ ἐλπίς, σῶσόν με ἐλπίζοντα εἰς σέ, πάντων ἐλπὶς ἀκαταίσχυντε, ταῖς πρὸς τὸν Δεσπότην σου παρακλήσεσι, τῶν θείων Μαθητῶν τε, μόνη Πανύμνητε."

ocMA.OdeW5C21.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW5C21.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C21.melody = he.h.m6_gr_GR_cog.OToFosAnatellon.incipit
ocMA.OdeW5C21.incipit = "Νυσταγμῷ ἀμελείας κρατούμενον, πρόθυμόν με"
ocMA.OdeW5C21.text = "Νυσταγμῷ ἀμελείας κρατούμενον, πρόθυμόν με ἀπέργασαι, ἱεραῖς μεσιτείαις σου Νικόλαε."

ocMA.OdeW5C22.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW5C22.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C22.melody = he.h.m6_gr_GR_cog.OToFosAnatellon.incipit
ocMA.OdeW5C22.incipit = "Ἰησοῦν τὸν Σωτῆρα ἱκέτευε, Ἱεράρχα"
ocMA.OdeW5C22.text = "Ἰησοῦν τὸν Σωτῆρα ἱκέτευε, Ἱεράρχα Νικόλαε, αἰωνίου κολάσεώς με ῥύσασθαι."

ocMA.OdeW5C23.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW5C23.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C23.melody = he.h.m6_gr_GR_cog.OToFosAnatellon.incipit
ocMA.OdeW5C23.incipit = "Κοσμικῶν με σκανδάλων διάσωσον, καὶ"
ocMA.OdeW5C23.text = "Κοσμικῶν με σκανδάλων διάσωσον, καὶ δαιμόνων κακώσεως, Ἱεράρχα Νικόλαε πρεσβείαις σου."

ocMA.OdeW5C2T.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW5C2T.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C2T.melody = he.h.m6_gr_GR_cog.OToFosAnatellon.incipit
ocMA.OdeW5C2T.incipit = "Ὁ τὸ εἶναι ἡμῖν παρεχόμενος,"
ocMA.OdeW5C2T.text = "Ὁ τὸ εἶναι ἡμῖν παρεχόμενος, σοῦ τὴν μήτραν κατῴκησε Θεοτόκε, ὃν αἴτησαι σωθῆναι ἡμᾶς."

ocMA.OdeW6C1H.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW6C1H.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW6C1H.incipit = he.h.m6_gr_GR_cog.TouViouTinThalassan.incipit
ocMA.OdeW6C1H.text = he.h.m6_gr_GR_cog.TouViouTinThalassan.text

ocMA.OdeW6C11.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW6C11.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C11.melody = he.h.m6_gr_GR_cog.TouViouTinThalassan.incipit
ocMA.OdeW6C11.incipit = "Ὡς πλάκες θεόγραφοι, τὰς τοῦ"
ocMA.OdeW6C11.text = "Ὡς πλάκες θεόγραφοι, τὰς τοῦ Πνεύματος σαφῶς, νομοθεσίας ἔνδοξοι, ἐν ταῖς ψυχαῖς ἐσχήκατε ἀψευδῶς, δι' ὧν τὸν ἐν γράμματι, κατηργήσατε νόμον, κόσμον σώσαντες."

ocMA.OdeW6C12.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW6C12.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C12.melody = he.h.m6_gr_GR_cog.TouViouTinThalassan.incipit
ocMA.OdeW6C12.incipit = "Ψυχὴ ἁμαρτάνουσα, συνηθείᾳ τε κακῇ,"
ocMA.OdeW6C12.text = "Ψυχὴ ἁμαρτάνουσα, συνηθείᾳ τε κακῇ, φρενοβλαβῶς δουλεύουσα, ἐπιστροφῇ ἐπίστρεψον καὶ Θεῷ, οἰκτίρμονι βόησον· Διὰ τῶν Ἀποστόλων, Λόγε σῶσόν με."

ocMA.OdeW6C13.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW6C13.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C13.melody = he.h.m6_gr_GR_cog.TouViouTinThalassan.incipit
ocMA.OdeW6C13.incipit = "Προβάτοις με σύνταξον, δεξιοῖς ἐν"
ocMA.OdeW6C13.text = "Προβάτοις με σύνταξον, δεξιοῖς ἐν τῇ φρικτῇ, καὶ φοβερᾷ ἐλεύσει σου, τῶν ἐκλεκτῶν προβάτων σου προσευχαῖς, τῶν θείων Κηρύκων σου, ὁ τυθείς, ὑπὲρ πάντων Ἀμνὸς ἄκακος."

ocMA.OdeW6C1T.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW6C1T.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C1T.melody = he.h.m6_gr_GR_cog.TouViouTinThalassan.incipit
ocMA.OdeW6C1T.incipit = "Χορὸς ἱκετεύει σε, Ἀσωμάτων λειτουργῶν,"
ocMA.OdeW6C1T.text = "Χορὸς ἱκετεύει σε, Ἀσωμάτων λειτουργῶν, Υἱὲ Θεοῦ προάναρχε, ἐκδυσωπεῖ σε δῆμος τῶν Μαθητῶν, διὰ τῆς Τεκούσης σε, τῷ λαῷ σου παράσχου, τὰ ἐλέη σου."

ocMA.OdeW6C21.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW6C21.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C21.melody = he.h.m6_gr_GR_cog.ToKiteiKatapotheis.incipit
ocMA.OdeW6C21.incipit = "Λαμπτῆρά σε νοητόν, ἔχουσα ἡ"
ocMA.OdeW6C21.text = "Λαμπτῆρά σε νοητόν, ἔχουσα ἡ κτίσις Νικόλαε, φωτίζεται ἀκτῖσι, τῶν ἀπείρων θαυμασίων σου."

ocMA.OdeW6C22.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW6C22.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C22.melody = he.h.m6_gr_GR_cog.ToKiteiKatapotheis.incipit
ocMA.OdeW6C22.incipit = "Ἀνύστακτε πρεσβευτά, τῶν σοὶ προστρεχόντων"
ocMA.OdeW6C22.text = "Ἀνύστακτε πρεσβευτά, τῶν σοὶ προστρεχόντων Νικόλαε, ὕπνου χαλεπωτάτου, ῥαθυμίας με ἀπάλλαξον."

ocMA.OdeW6C23.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW6C23.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C23.melody = he.h.m6_gr_GR_cog.ToKiteiKatapotheis.incipit
ocMA.OdeW6C23.incipit = "Ἐῤῥύσω πάλαι σοφέ, μέλλοντας ἀδίκως"
ocMA.OdeW6C23.text = "Ἐῤῥύσω πάλαι σοφέ, μέλλοντας ἀδίκως τεθνήξεσθαι· ὡσαύτως οὖν με ῥῦσαι, συμβαινουσῶν παραπτώσεων."

ocMA.OdeW6C2T.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW6C2T.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C2T.melody = he.h.m6_gr_GR_cog.ToKiteiKatapotheis.incipit
ocMA.OdeW6C2T.incipit = "Παρθένε μόνη ἁγνή, κεχαριτωμένη βοήθει"
ocMA.OdeW6C2T.text = "Παρθένε μόνη ἁγνή, κεχαριτωμένη βοήθει μοι, ἀεὶ χειμαζομένῳ, ταῖς τοῦ βίου περιστάσεσι."

ocMA.OdeW7C1H.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW7C1H.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW7C1H.incipit = he.h.m6_gr_GR_cog.DrosovolonMenTinKaminon.incipit
ocMA.OdeW7C1H.text = he.h.m6_gr_GR_cog.DrosovolonMenTinKaminon.text

ocMA.OdeW7C11.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW7C11.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C11.melody = he.h.m6_gr_GR_cog.DrosovolonMenTinKaminon.incipit
ocMA.OdeW7C11.incipit = "Τῷ ἀγκίστρῳ τῷ τοῦ λόγου"
ocMA.OdeW7C11.text = "Τῷ ἀγκίστρῳ τῷ τοῦ λόγου ἀνελκύσαντες, βυθοῦ τῆς πλάνης ἅπαντας, προσηγάγετε τῷ τυθέντι, Λόγῳ δι' ἡμᾶς, Ἀπόστολοι μέλποντες αὐτῷ· Εὐλογητὸς εἶ ὁ Θεός, ὁ τῶν πατέρων ἡμῶν."

ocMA.OdeW7C12.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW7C12.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C12.melody = he.h.m6_gr_GR_cog.DrosovolonMenTinKaminon.incipit
ocMA.OdeW7C12.incipit = "Τῷ φανέντι ὕλῃ σώματος Ἀπόστολοι,"
ocMA.OdeW7C12.text = "Τῷ φανέντι ὕλῃ σώματος Ἀπόστολοι, ἀμέσως ὁμιλήσαντες, ἐφωτίσθητε φωτοδότοις, λάμψεσιν αὐτοῦ· διό με φωτίσατε βοᾶν· Εὐλογητὸς εἶ ὁ Θεός, ὁ τῶν πατέρων ἡμῶν."

ocMA.OdeW7C13.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW7C13.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C13.melody = he.h.m6_gr_GR_cog.DrosovolonMenTinKaminon.incipit
ocMA.OdeW7C13.incipit = "Τὸν ποιμένα τὸν καλὸν ἐκδυσωπήσατε,"
ocMA.OdeW7C13.text = "Τὸν ποιμένα τὸν καλὸν ἐκδυσωπήσατε, εἰς ὄρη με πλανώμενον, παραβάσεως ἐκζητῆσαι, θεῖοι Μαθηταί, καὶ σῶσαι βοῶντα ἐμμελῶς· Εὐλογητὸς εἶ ὁ Θεός, ὁ τῶν πατέρων ἡμῶν."

ocMA.OdeW7C1T.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW7C1T.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C1T.melody = he.h.m6_gr_GR_cog.DrosovolonMenTinKaminon.incipit
ocMA.OdeW7C1T.incipit = "Σὺν Προφήταις Ἀποστόλοις τε καὶ"
ocMA.OdeW7C1T.text = "Σὺν Προφήταις Ἀποστόλοις τε καὶ Μάρτυσι, Παρθένε, ὃν ἐκύησας, καθικέτευε λυτρωθῆναι, πάντας τῶν δεινῶν, τοὺς φόβῳ κραυγάζοντας αὐτῷ· Εὐλογητὸς εἶ ὁ Θεος, ὁ τῶν πατέρων ἡμῶν."

ocMA.OdeW7C21.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW7C21.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C21.melody = he.h.m6_gr_GR_cog.OTonOsionSouPaidon.incipit
ocMA.OdeW7C21.incipit = "Ῥώμῃ τοῦ Θεοῦ παμμάκαρ, αἱρετιζόντων"
ocMA.OdeW7C21.text = "Ῥώμῃ τοῦ Θεοῦ παμμάκαρ, αἱρετιζόντων στῖφος, κατεπάτησας, καὶ τῆς τούτων ἀπάτης, τὸ ποίμνιον διέσωσας."

ocMA.OdeW7C22.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW7C22.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C22.melody = he.h.m6_gr_GR_cog.OTonOsionSouPaidon.incipit
ocMA.OdeW7C22.incipit = "Ὁ ἐποφθεὶς τοῖς πλωτῆρσι, καὶ"
ocMA.OdeW7C22.text = "Ὁ ἐποφθεὶς τοῖς πλωτῆρσι, καὶ τὴν πόλιν σου θρέψας, κινδυνεύουσαν, ψυχοφθόρου λιμοῦ με, εὐχαῖς σου Πάτερ λύτρωσαι."

ocMA.OdeW7C23.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW7C23.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C23.melody = he.h.m6_gr_GR_cog.OTonOsionSouPaidon.incipit
ocMA.OdeW7C23.incipit = "Στήριξον Πάτερ εὐχαῖς σου, τοὺς"
ocMA.OdeW7C23.text = "Στήριξον Πάτερ εὐχαῖς σου, τοὺς σὲ προσκαλουμένους εἰς βοήθειαν, κατευνάζων δαιμόνων, τὸ πονηρὸν κλυδώνιον."

ocMA.OdeW7C2T.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW7C2T.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C2T.melody = he.h.m6_gr_GR_cog.OTonOsionSouPaidon.incipit
ocMA.OdeW7C2T.incipit = "Δέσποινα πάντων ἀνθρώπων, ἁγία Θεοτόκε,"
ocMA.OdeW7C2T.text = "Δέσποινα πάντων ἀνθρώπων, ἁγία Θεοτόκε, τῆς δουλείας με τῶν παθῶν, σαῖς πρεσβείαις, τελείως ἐλευθέρωσον."

ocMA.OdeW8C1H.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW8C1H.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW8C1H.incipit = he.h.m6_gr_GR_cog.EkFlogosToisOsiois.incipit
ocMA.OdeW8C1H.text = he.h.m6_gr_GR_cog.EkFlogosToisOsiois.text

ocMA.OdeW8C11.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW8C11.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C11.melody = he.h.m6_gr_GR_cog.EkFlogosToisOsiois.incipit
ocMA.OdeW8C11.incipit = "Χερσωθείσας καρδίας κατενεώσατε, τῷ ἀρότρῳ"
ocMA.OdeW8C11.text = "Χερσωθείσας καρδίας κατενεώσατε, τῷ ἀρότρῳ τοῦ λόγου, ἐγκατασπείραντες, δόγμα τὸ σεπτόν, στάχυν ἑκατοστεύοντα, τοὺς διασωθέντας, ἐδρέψασθε θεόπται."

ocMA.OdeW8C12.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW8C12.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C12.melody = he.h.m6_gr_GR_cog.EkFlogosToisOsiois.incipit
ocMA.OdeW8C12.incipit = "Πρεσβειῶν τῇ δρεπάνῃ ἐπικαθάρατε, τὰ"
ocMA.OdeW8C12.text = "Πρεσβειῶν τῇ δρεπάνῃ ἐπικαθάρατε, τὰ ζιζάνια πάντα τῆς διανοίας μου, θεῖοι γεωργοί, τοῦ Σωτῆρος Ἀπόστολοι, ὅπως σωτηρίας, καρποφορήσω στάχυν."

ocMA.OdeW8C13.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW8C13.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C13.melody = he.h.m6_gr_GR_cog.EkFlogosToisOsiois.incipit
ocMA.OdeW8C13.incipit = "Τοῦ ἐχθροῦ τὴν κακίαν κατακοιμίσαντες,"
ocMA.OdeW8C13.text = "Τοῦ ἐχθροῦ τὴν κακίαν κατακοιμίσαντες, ἐκοιμήθητε ὕπνον δικαίοις πρέποντα, ἄγρυπνοι ἡμῶν, πρεσβευταὶ χρηματίσαντες, Μαθηταὶ τοῦ Λόγου, εἰς πάντας τοὺς αἰῶνας."

ocMA.OdeW8C1T.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW8C1T.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C1T.melody = he.h.m6_gr_GR_cog.EkFlogosToisOsiois.incipit
ocMA.OdeW8C1T.incipit = "Τοῦ πυρὸς τοῦ ἀσβέστου ῥῦσαί"
ocMA.OdeW8C1T.text = "Τοῦ πυρὸς τοῦ ἀσβέστου ῥῦσαί με Κύριε, Προφητῶν καὶ Μαρτύρων καὶ Ἀποστόλων σου, καὶ τῆς ἐν γαστρί, χωρησάσης ἀῤῥήτως σε, Κόρης ἀπειράνδρου, πρεσβείαις εὐπροσδέκτοις."

ocMA.OdeW8C21.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW8C21.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C21.melody = he.h.m6_gr_GR_cog.OiOsioiSouPaides.incipit
ocMA.OdeW8C21.incipit = "Ἐν ὕψει πολιτείας φαεινοτάτης, ἀναδραμὼν"
ocMA.OdeW8C21.text = "Ἐν ὕψει πολιτείας φαεινοτάτης, ἀναδραμὼν ὥσπερ γνώριμος, πάσῃ τῇ οἰκουμένῃ θαυματουργίαις, θεοφόρε ποικίλαις, φωτίζεις τοὺς ἐν σκότει Νικόλαε."

ocMA.OdeW8C22.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW8C22.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C22.melody = he.h.m6_gr_GR_cog.OiOsioiSouPaides.incipit
ocMA.OdeW8C22.incipit = "Χαρίτωσον ἐνθέως ἐπαγρυπνοῦντας, τοὺς σὲ"
ocMA.OdeW8C22.text = "Χαρίτωσον ἐνθέως ἐπαγρυπνοῦντας, τοὺς σὲ τιμῶντας Νικόλαε, κατακοίμισον τὴν κακίαν, ἐχθραινόντων ἀνθρώπων, ἡμῖν ἐπεμβαινόντων Νικόλαε."

ocMA.OdeW8C23.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW8C23.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C23.melody = he.h.m6_gr_GR_cog.OiOsioiSouPaides.incipit
ocMA.OdeW8C23.incipit = "Ὁ πάσας τὰς παγίδας τοῦ"
ocMA.OdeW8C23.text = "Ὁ πάσας τὰς παγίδας τοῦ διαβόλου, σθένει συντρίψας τοῦ Πνεύματος, ἐκ τούτων ῥῦσαί με σαῖς πρεσβείαις, κυβερνῶν τὴν ζωήν μου, θεόφρον Ἱεράρχα Νικόλαε."

ocMA.OdeW8C2T.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW8C2T.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C2T.melody = he.h.m6_gr_GR_cog.OiOsioiSouPaides.incipit
ocMA.OdeW8C2T.incipit = "Ὑμνοῦμέν σε Παρθένε Θεοκυῆτορ, δι'"
ocMA.OdeW8C2T.text = "Ὑμνοῦμέν σε Παρθένε Θεοκυῆτορ, δι' ἧς ἀρὰ ἀπεστείρωται, καὶ εὐλογία πᾶσιν ἐπῆλθε, τοῖς εἰδόσι σε μόνην, Μητέρα τοῦ Θεοῦ ἡμῶν ἄνανδρον."

ocMA.OdeW9C1H.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW9C1H.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW9C1H.incipit = he.h.m6_gr_GR_cog.TheonAnthropois.incipit
ocMA.OdeW9C1H.text = he.h.m6_gr_GR_cog.TheonAnthropois.text

ocMA.OdeW9C11.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW9C11.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C11.melody = he.h.m6_gr_GR_cog.TheonAnthropois.incipit
ocMA.OdeW9C11.incipit = "Θεὸν ἀνθρώποις προσομιλήσαντα, καὶ τὸ"
ocMA.OdeW9C11.text = "Θεὸν ἀνθρώποις προσομιλήσαντα, καὶ τὸ θνητὸν ἑνώσει ἀσυγκρίτῳ θεώσαντα, εἰς τὸν σύμπαντα κόσμον κηρύξαντες, πάντας ἀθέου πλάνης, ἠλευθερώσατε· ὅθεν μακαρίζεσθε ἀεί, θεῖοι Ἀπόστολοι."

ocMA.OdeW9C12.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW9C12.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C12.melody = he.h.m6_gr_GR_cog.TheonAnthropois.incipit
ocMA.OdeW9C12.incipit = "Χριστοῦ τὸ πάθος ἐξεικονίζοντες, διὰ"
ocMA.OdeW9C12.text = "Χριστοῦ τὸ πάθος ἐξεικονίζοντες, διὰ παθῶν τοῦ σώματος ποικίλων Ἀπόστολοι, τῆς σαρκός μου τὰ πάθη νεκρώσατε, καὶ τὴν ἐξασθενοῦσαν, καὶ κινδυνεύουσαν, καὶ νενεκρωμένην μου ψυχὴν ζωοποιήσατε."

ocMA.OdeW9C13.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW9C13.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C13.melody = he.h.m6_gr_GR_cog.TheonAnthropois.incipit
ocMA.OdeW9C13.incipit = "Ὁδὸς γενοῦ μοι καθοδηγοῦσά με,"
ocMA.OdeW9C13.text = "Ὁδὸς γενοῦ μοι καθοδηγοῦσά με, πρὸς ἀπλανῆ ὁδὸν τῶν ἐντολῶν σου Φιλάνθρωπε, ἁμαρτίας πεσόντι εἰς βάραθρα, καὶ περιπλανωμένῳ, καὶ ἀποκλίναντι, ἐπὶ τὰς ὁδούς τὰς πονηράς, καὶ κινδυνεύοντι."

ocMA.OdeW9C1T.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW9C1T.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C1T.melody = he.h.m6_gr_GR_cog.TheonAnthropois.incipit
ocMA.OdeW9C1T.incipit = "Χοροὶ Ἀγγέλων καὶ Ἀρχαγγέλων τε,"
ocMA.OdeW9C1T.text = "Χοροὶ Ἀγγέλων καὶ Ἀρχαγγέλων τε, Ἐξουσιών, Δυνάμεις, καὶ Ἀρχαί, Κυριότητες, Χερουβεὶμ καὶ Σεραφείμ, καὶ θρόνοι Χριστέ, δῆμος τῶν Ἀποστόλων, σὺν τῇ Τεκούσῃ σε, μόνε δυσωποῦσι Βασιλεῦ. Σῶσον τοὺς δούλους σου."

ocMA.OdeW9C21.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW9C21.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C21.melody = he.h.m6_gr_GR_cog.IToChaireDiAngelou.incipit
ocMA.OdeW9C21.incipit = "Ἱερέων, καλλονὴ θαυμάτων βρύσις, ἐκδυσώπει,"
ocMA.OdeW9C21.text = "Ἱερέων, καλλονὴ θαυμάτων βρύσις, ἐκδυσώπει, Θεὸν τὸν φιλάνθρωπον, ἁμαρτημάτων, εὑρεῖν ἡμᾶς ἄφεσιν."

ocMA.OdeW9C22.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW9C22.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C22.melody = he.h.m6_gr_GR_cog.IToChaireDiAngelou.incipit
ocMA.OdeW9C22.incipit = "Ὥσπερ πάλαι, τοὺς δεσμώτας ἐλυτρώσω,"
ocMA.OdeW9C22.text = "Ὥσπερ πάλαι, τοὺς δεσμώτας ἐλυτρώσω, τοῦ θανάτου, ἡμᾶς οὕτω λύτρωσαι, πειρατηρίων, τοῦ βίου Νικόλαε."

ocMA.OdeW9C23.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW9C23.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C23.melody = he.h.m6_gr_GR_cog.IToChaireDiAngelou.incipit
ocMA.OdeW9C23.incipit = "Σὺ ἐν Μύροις, τῆς Λυκίας"
ocMA.OdeW9C23.text = "Σὺ ἐν Μύροις, τῆς Λυκίας τεθαμμένος, μύρα βρύεις, πάντοτε Νικόλαε, τὴν δυσωδίαν, τῶν παθῶν διώκοντα."

ocMA.OdeW9C24.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW9C24.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C24.melody = he.h.m6_gr_GR_cog.IToChaireDiAngelou.incipit
ocMA.OdeW9C24.incipit = "Ἡ τοῦ Κτίστου, παρουσία ἐπὶ"
ocMA.OdeW9C24.text = "Ἡ τοῦ Κτίστου, παρουσία ἐπὶ θύραις, μὴ ἀμέλει, ψυχὴ ἀλλὰ βόησον· Ταῖς Νικολάου, Χριστὲ εὐχαῖς σῶσόν με."

ocMA.OdeW9C2T.mode = misc_gr_GR_cog.Mode6
ocMA.OdeW9C2T.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C2T.melody = he.h.m6_gr_GR_cog.IToChaireDiAngelou.incipit
ocMA.OdeW9C2T.incipit = "Φῶς τεκοῦσα, τῆς καρδίας μου"
ocMA.OdeW9C2T.text = "Φῶς τεκοῦσα, τῆς καρδίας μου τὰς κόρας, ἀβλεπτούσας, Πανύμνητε φώτισον, ἵνα ὑμνῶ σε, διὰ σοῦ σῳζόμενος."

ocMA.Aposticha1.mode = misc_gr_GR_cog.Mode6
ocMA.Aposticha1.melody = ""
ocMA.Aposticha1.incipit = oc.m6.d5_gr_GR_cog.ocVE.Aposticha1.incipit
ocMA.Aposticha1.text = oc.m6.d5_gr_GR_cog.ocVE.Aposticha1.text

ocMA.Aposticha2.mode = misc_gr_GR_cog.Mode6
ocMA.Aposticha2.melody = ""
ocMA.Aposticha2.incipit = oc.m6.d5_gr_GR_cog.ocVE.Aposticha2.incipit
ocMA.Aposticha2.text = oc.m6.d5_gr_GR_cog.ocVE.Aposticha2.text

ocMA.Aposticha3.mode = misc_gr_GR_cog.Mode6
ocMA.Aposticha3.melody = ""
ocMA.Aposticha3.incipit = "Τοὺς Ἁγίους ἐθαυμάστωσε, καὶ Ἁγίας"
ocMA.Aposticha3.text = "Τοὺς Ἁγίους ἐθαυμάστωσε, καὶ Ἁγίας ὁ Θεὸς ἡμῶν· ἀγαλλιᾶσθε καὶ εὐφραίνεσθε, πάντες οἱ δοῦλοι αὐτοῦ· ὑμῖν γὰρ ἡτοίμασε τὸν στέφανον, καὶ τὴν Βασιλείαν αὐτοῦ, ἀλλ' αἰτοῦμεν, καὶ ἡμῶν μὴ ἐπιλάθησθε."

ocMA.AposTheotokion.mode = misc_gr_GR_cog.Mode6
ocMA.AposTheotokion.melody = he.a.m6_gr_GR_cog.TriimerosAnestis.incipit
ocMA.AposTheotokion.incipit = "Μεγάλων χαρισμάτων ἁγνή, Παρθένε Θεομῆτορ"
ocMA.AposTheotokion.text = "Μεγάλων χαρισμάτων ἁγνή, Παρθένε Θεομῆτορ σεμνή, ἠξιώθης, ὅτι ἔτεκες σαρκί, τὸν ἕνα τῆς Τριάδος, Χριστὸν τὸν ζωοδότην, εἰς σωτηρίαν τῶν ψυχῶν ἡμῶν."

ocMA.ApolTheotokion.mode = misc_gr_GR_cog.Mode6
ocMA.ApolTheotokion.melody = ""
ocMA.ApolTheotokion.incipit = oc.m6.d5_gr_GR_cog.ocMA.AposTheotokion.incipit
ocMA.ApolTheotokion.text = oc.m6.d5_gr_GR_cog.ocMA.AposTheotokion.text

ocLI.Beatitudes1.mode = misc_gr_GR_cog.Mode6
ocLI.Beatitudes1.melody = he.h.m6_gr_GR_cog.IToChaireDiAngelou.incipit
ocLI.Beatitudes1.incipit = oc.m6.d1_gr_GR_cog.ocLI.Beatitudes1.incipit
ocLI.Beatitudes1.text = oc.m6.d1_gr_GR_cog.ocLI.Beatitudes1.text

ocLI.Beatitudes2.mode = misc_gr_GR_cog.Mode6
ocLI.Beatitudes2.melody = he.h.m6_gr_GR_cog.IToChaireDiAngelou.incipit
ocLI.Beatitudes2.incipit = "Ὡς νεφέλαι, ἐπωμβρίσατε τὸ ὕδωρ,"
ocLI.Beatitudes2.text = "Ὡς νεφέλαι, ἐπωμβρίσατε τὸ ὕδωρ, ἐν τῷ κόσμῳ, τῆς ἐνθέου γνώσεως, πεφωτισμένοι, Κυρίου Ἀπόστολοι."

ocLI.Beatitudes3.mode = misc_gr_GR_cog.Mode6
ocLI.Beatitudes3.melody = he.h.m6_gr_GR_cog.IToChaireDiAngelou.incipit
ocLI.Beatitudes3.incipit = "Ὡς ἀκτῖνες, μυστικαὶ διαδραμόντες, εἰς"
ocLI.Beatitudes3.text = "Ὡς ἀκτῖνες, μυστικαὶ διαδραμόντες, εἰς τὸν κόσμον Ἀπόστολοι ἔνδοξοι, ἀνθρώπων γένος, πίστει ἐφωτίσατε."

