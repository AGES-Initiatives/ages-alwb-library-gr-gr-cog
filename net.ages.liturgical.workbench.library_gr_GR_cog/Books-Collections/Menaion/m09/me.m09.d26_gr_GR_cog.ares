meMA.Kathisma11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Kathisma11.greekmelody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.greekname
meMA.Kathisma11.melody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.name
meMA.Kathisma11.incipit = "Σὺ υἱὸς ἐπεκλήθης θείας βροντῆς…"
meMA.Kathisma11.text = "Σὺ υἱὸς ἐπεκλήθης θείας βροντῆς, ὡς τὰ ὦτα κωφεύσας τῶν δυσσεβῶν, καὶ σάλπιγγος ἡδύτερον, διηχήσας ὦ πάνσοφε, εἰς εὐθείας καρδίας τοῦ Λόγου τὴν σάρκωσιν, καὶ ὡς γνήσιος φίλος, τῷ στήθει ἀνέπεσας· ὅθεν ἑξαντλήσας, τὸ τῆς γνώσεως βάθος, τοῖς πᾶσιν ἐκήρυξας, τοῦ Πατρὸς τὸν συνάναρχον, Ἰωάννη Ἀπόστολε· πρέσβευε Χριστῷ τῷ Θεῷ, τῶν πταισμάτων ἄφεσιν δωρήσασθαι, τοῖς ἑορτάζουσι πόθῳ, τὴν ἁγίαν μνήμην σου."

meMA.Kathisma1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Kathisma1T.melody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.melody
meMA.Kathisma1T.incipit = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.incipit
meMA.Kathisma1T.text = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.text

meMA.Kathisma21.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Kathisma21.greekmelody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.greekname
meMA.Kathisma21.melody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.name
meMA.Kathisma21.incipit = "Τὸν βυθὸν τῆς ἁλείας καταλιπών…"
meMA.Kathisma21.text = "Τὸν βυθὸν τῆς ἁλείας καταλιπών, τοῦ Σταυροῦ τῷ καλάμῳ πάντα σαφῶς, τὰ ἔθνη ἐζώγρησας, ὡς ἰχθύας Πανεύφημε· καὶ γὰρ καθὼς σοι ἔφη, Χριστὸς ἀναδέδειξαι, ἁλιεὺς ἀνθρώπων, ζωγρῶν πρὸς εὐσεβειαν· ὅθεν κατασπείρας, Θεοῦ Λόγου τὴν γνῶσιν, τὴν Πάτμον καὶ τὴν Ἔφεσον, ἐκαρπώσω τοῖς λόγοις σου, Θεολόγε Ἀπόστολε· πρέσβευε Χριστῷ τῷ Θεῷ, τῶν πταισμάτων ἄφεσιν δωρήσασθαι, τοῖς ἑορτάζουσι πόθῳ, τὴν ἁγίαν μνήμην σου."

meMA.Kathisma2T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Kathisma2T.melody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.name
meMA.Kathisma2T.incipit = "Ἐνθυμοῦμαι τὴν κρίσιν καὶ δειλιῶ…"
meMA.Kathisma2T.text = "Ἐνθυμοῦμαι τὴν κρίσιν καὶ δειλιῶ, ἔργα πράξας αἰσχύνης ὁ ταπεινός, ἄξια καὶ δέομαι, Θεοτόκε πανύμνητε· Πρὶν ἢ νὺξ με φθάσῃ, θανάτου ἐπίστρεψον, πρὸς τὴν τῆς μετανοίας, ὁδὸν ὁδηγοῦσα με·, ἵνα εὐχαρίστως, προσκυνῶν ἀνυμνῶ σου, τὴν ἄμετρον δύναμιν, καὶ τὴν θείαν ἀντίληψιν, Παναγία Θεόνυμφε, πρεσβεύουσα Χριστῷ τῷ Θεῷ, ὑπὲρ οὗ αἰτοῦμαι σε, δοθῆναί μοι, ἱλασμὸν ἁμαρτιῶν, καὶ τὸ μέγα ἔλεος."

meMA.Kathisma31.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Kathisma31.greekmelody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.greekname
meMA.Kathisma31.melody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.name
