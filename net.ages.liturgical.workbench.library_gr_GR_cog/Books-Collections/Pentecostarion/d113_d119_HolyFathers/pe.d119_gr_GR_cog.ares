peMA.Ode1C11.ode = miscellanea_gr_GR_cog.misc.Ode1
peMA.Ode1C11.mode = miscellanea_gr_GR_cog.misc.Mode6
peMA.Ode1C11.greekmelody = he.h.m6_gr_GR_cog.heHE.OsEnIpeiroPezefsas.greekname
peMA.Ode1C11.melody = he.h.m6_gr_GR_cog.heHE.OsEnIpeiroPezefsas.name
peMA.Ode1C11.incipit = "Ψυχῶν πασῶν ὁ Δεσπότης καὶ…"
peMA.Ode1C11.text = "Ψυχῶν πασῶν ὁ Δεσπότης καὶ ποιητής, καὶ κριτὴς καὶ Κύριος, οὗπερ πάντων τῶν ἐν γῇ, ἐν χειρὶ τὰ πέρατα, αὐτός, οὓς μετέστησας πιστοὺς ἐν σοὶ ἀνάπαυσον."

peMA.Ode1C12.ode = miscellanea_gr_GR_cog.misc.Ode1
peMA.Ode1C12.mode = miscellanea_gr_GR_cog.misc.Mode6
peMA.Ode1C12.melody = he.h.m6_gr_GR_cog.heHE.OsEnIpeiroPezefsas.name
peMA.Ode1C12.incipit = "Ὑπὲρ παντὸς κοιμηθέντων γένους βροτῶν…"
peMA.Ode1C12.text = "Ὑπὲρ παντὸς κοιμηθέντων γένους βροτῶν, ἡλικίας Δέσποτα, ἀξιώματος ὁμοῦ, καὶ μεγέθους, ἅπαντες θερμῶς, δυσωποῦμεν, ὅπως σώσῃς οὓς μετέστησας."

peMA.Ode1C13.ode = miscellanea_gr_GR_cog.misc.Ode1
peMA.Ode1C13.mode = miscellanea_gr_GR_cog.misc.Mode6
peMA.Ode1C13.melody = he.h.m6_gr_GR_cog.heHE.OsEnIpeiroPezefsas.name
peMA.Ode1C13.incipit = "Χειρὶ ἀχράντῳ σου Λόγε ὁ…"
peMA.Ode1C13.text = "Χειρὶ ἀχράντῳ σου Λόγε ὁ ἀπὸ γῆς, πλαστουργήσας μόνος με, καὶ ψυχώσας ὡς Θεός, ζωηρᾷ ἐμπνεύσει σου, αὐτός, οὓς ἐντεῦθεν προσελάβου σῶσον εὔσπλαγχνε."

peMA.Ode1C1T.ode = miscellanea_gr_GR_cog.misc.Ode1
peMA.Ode1C1T.mode = miscellanea_gr_GR_cog.misc.Mode6
peMA.Ode1C1T.melody = he.h.m6_gr_GR_cog.heHE.OsEnIpeiroPezefsas.name
peMA.Ode1C1T.incipit = "Ἁγνὴ Παρθένε τοῦ κόσμου καταφυγή…"
peMA.Ode1C1T.text = "Ἁγνὴ Παρθένε τοῦ κόσμου καταφυγή, καὶ στεῤῥὰ ὑπέρμαχε, ἱκεσίαις σου θερμαῖς, ὑπὲρ πάντων πρέσβευε τῶν σέ, κεκτημένων ἐν ἀνάγκαις τεῖχος ἄῤῥηκτον."

peMA.Ode3C1H.ode = miscellanea_gr_GR_cog.misc.Ode3
peMA.Ode3C1H.mode = miscellanea_gr_GR_cog.misc.Mode6
peMA.Ode3C1H.greekmelody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.greekname
peMA.Ode3C1H.melody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.melody
peMA.Ode3C1H.incipit = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.incipit
peMA.Ode3C1H.text = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.text

peMA.Ode3C11.ode = miscellanea_gr_GR_cog.misc.Ode3
peMA.Ode3C11.mode = miscellanea_gr_GR_cog.misc.Mode6
peMA.Ode3C11.greekmelody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.greekname
peMA.Ode3C11.melody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.name
peMA.Ode3C11.incipit = "Ἰσχὺϊ δόξης θεϊκῆς, ὁ τῆς…"
peMA.Ode3C11.text = "Ἰσχὺϊ δόξης θεϊκῆς, ὁ τῆς γῆς ἐξ ἐσχάτων, ὡς νεφέλαις ἀνάγων, τῶν πιστῶν πρὸς σὲ ψυχάς, οὓς ἐκ περάτων Χριστέ, προσελάβου δούλους σου ἀνάπαυσον."

peMA.Ode3C12.ode = miscellanea_gr_GR_cog.misc.Ode3
peMA.Ode3C12.mode = miscellanea_gr_GR_cog.misc.Mode6
peMA.Ode3C12.melody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.name
peMA.Ode3C12.incipit = "Σὺ μόνος εἶ ὁ καθιστῶν…"
peMA.Ode3C12.text = "Σὺ μόνος εἶ ὁ καθιστῶν, βασιλεῖς καὶ δυνάστας, καὶ κριτὰς σὺν τοπάρχαις· ὅθεν πάντων ὡς Θεός, αὐτὸς ἐν κρίσει τῇ σῇ, ῥῦσαι τούτους Σῶτερ τῆς κολάσεως."

peMA.Ode3C13.ode = miscellanea_gr_GR_cog.misc.Ode3
peMA.Ode3C13.mode = miscellanea_gr_GR_cog.misc.Mode6
peMA.Ode3C13.melody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.name
peMA.Ode3C13.incipit = "Ἀπάσης γῆς ὁ ποιητής, σὺν…"
peMA.Ode3C13.text = "Ἀπάσης γῆς ὁ ποιητής, σὺν πρεσβύταις παρθένοις, καὶ σὺν νέοις ἐφήβοις, οὓς προσελάβου Χριστέ, τῆς αἰωνίου χαρᾶς, καὶ τρυφῆς σου τούτους καταξίωσον."

peMA.Ode3C1T.ode = miscellanea_gr_GR_cog.misc.Ode3
peMA.Ode3C1T.mode = miscellanea_gr_GR_cog.misc.Mode6
peMA.Ode3C1T.melody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.name
peMA.Ode3C1T.incipit = "Ἐλπὶς τοῦ κόσμου κραταιά, Δέσποινα…"
peMA.Ode3C1T.text = "Ἐλπὶς τοῦ κόσμου κραταιά, Δέσποινα Θεοτόκε, μὴ ἀπώσῃ τοὺς πόθῳ, σοὶ προστρέχοντας ἀεί, ἀλλʼ ἱκεσίαις ταῖς σαῖς, πάσης ῥῦσαι ζάλης τοὺς τιμῶντάς σε."

peMA.Ode3Kathisma1.mode = miscellanea_gr_GR_cog.misc.Mode8
peMA.Ode3Kathisma1.greekmelody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.greekname
peMA.Ode3Kathisma1.melody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.name
peMA.Ode3Kathisma1.incipit = "Ὁ τὰ πάντα ποιήσας νεύματι…"
peMA.Ode3Kathisma1.text = "Ὁ τὰ πάντα ποιήσας νεύματι σῷ, τοὺς ἐν πίστει θανέντας Χριστιανούς, πατέρας προπάτορας, πάππους δὲ καὶ προπάππους τε, ἀδελφοὺς καὶ φίλους, πλουσίους καὶ πένητας, βασιλεῖς καὶ ἄρχοντας, ὁμοῦ καὶ μονάζοντας, ὅπου τῶν Δικαίων καὶ Ἁγίων σου πλήθη, ἁπάντων ἡ οἴκησις, δυσωποῦμεν ἀνάπαυσον, ὡς Θεὸς εὐδιάλλακτος, Δέσποτα Χριστὲ Βασιλεῦ, τῶν πταισμάτων ἄφεσιν παρέχων αὐτοῖς, ὧν ἐξήμαρτον πάντες οἱ δοῦλοί σου."

peMA.Ode3Kathisma2.mode = miscellanea_gr_GR_cog.misc.Mode8
peMA.Ode3Kathisma2.greekmelody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.greekname
peMA.Ode3Kathisma2.melody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.name
