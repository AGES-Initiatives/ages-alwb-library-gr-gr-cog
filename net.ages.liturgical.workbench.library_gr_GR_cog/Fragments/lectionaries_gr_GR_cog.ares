lecAP.title1.text = "Ο ΑΠΟΣΤΟΛΟΣ"
lecAP.title2.text = "ΒΙΒΛΙΟΝ ΑΠΟΣΤΟΛΙΚΩΝ ΠΕΡΙΚΟΠΩΝ"
lecAP.title3.text = "Προκείμενα τοῦ Ἀποστόλου"
lecAP.title4.text = "καὶ"
lecAP.title5.text = "Ἀλληλουϊάρια τοῦ Εὐαγγελίου"

lecAP.R01 = "ἐν αἶς οὐ συμπίπτει ἑορτή."
lecAP.R02 = "Ψαλλόμενα καθ' ὅλην τὴν ἑβδομάδα ἐν αἶς ἡμέραις οὐκ ἔστιν ἑορτή."
lecAP.R03 = "Προκείμενον καὶ Ἀλληλούϊα τῆς ἡμέρας."
lecAP.R04 = "Προκείμενον καὶ Ἀλληλούϊα τοῦ ἤχου τῆς Κυριακῆς."
lecAP.R05 = "Ἐν τῇ Λειτουργίᾳ τὰ παρόντα ἀντίφωνα·"

lecAP.mc.R014 = "Ἕτερος εἰς κοιμηθέντας."
lecAP.mc.R021 = "Ἕτερος τῶν Ὁσίων."
lecAP.mc.R028 = "Ἕτερος τοῦ Ἁγίου Θεοδώρου Τύρωνος."
lecAP.mc.R050 = "Ἕτερος τοῦ Ὁσίου Ἰωάννου."
lecAP.mc.R056 = "Ἕτερος τῆς Θεοτόκου."
lecAP.mc.R057 = "Ἕτερος τῆς Ὁσίας Μαρίας."
lecAP.mc.R071a = "Ἀντὶ τοῦ Ἀλληλούϊα τὸν παρὀντα στίχον."
lecAP.mc.R071b = "Τὸν αὐτὸν ψάλλομεν καὶ ἐν ἑνὶ ἑκάστῳ τῶν ἑπομένων Στίχων, λεγομένων παρὰ τοῦ Ἀναγνώστου χύμα."
lecAP.mc.R120 = "ψαλλόμενα ἐν τῇ Λειτουργία τῆς τοῦ Πάσχα Κυριακῆς, καὶ καθ' ὅλην τὴν Διακαινήσιμον Ἑβδομάδα."

lecAP.me.m07.dHF.R01 = "Κατὰ τὴν δεκατητρίτην τοῦ παρόντος Μηνός, εἰ τύχοι ἐν Κυριακῇ, ἢ τῇ πρώτῃ μετ' αὐτὴν ἐρχομένῃ, ψάλλεται ἡ Ἀκολουθία τῶν τῆς Οἰκουμενικῆς Τετάρτης Συνόδου Ἁγίων Πατέρων, ἐν ᾗ ἀναγινώσκεται ὁ παρών Ἀπόστολος."
lecAP.me.m10.dHF.R01 = "Κατὰ τὴν ἑνδεκάτην τοῦ παρόντος Μηνός, εἰ τύχοι ἐν Κυριακῇ, ἢ τῇ πρώτῃ μετ' αὐτὴν ἐρχομένῃ, ψάλλεται ἡ Ἀκολουθία τῶν τῆς Οἰκουμενικῆς Ἑβδόμης Συνόδου Τριακοσίων πεντήκοντα Ἁγίων Πατέρων, ἐν ᾗ ἀναγινώσκεται ὁ παρών Ἀπόστολος."
lecAP.me.m12.dSunFF.R01 = "Κατὰ τὴν ἑνδεκάτην τοῦ παρόντος Μηνός, εἰ τύχοι ἐν Κυριακῇ, ἢ τῇ πρώτῃ μετ' αὐτὴν ἐρχομένῃ, μνείαν ποιούμεθα τῶν ἁγίων Προπατόρων τοῦ Κυρίου ἡμῶν Ἰησοῦ Χριστοῦ, ἐν ᾗ ἀναγινώσκεται ὁ παρὼν Ἀπόστολος."
lecAP.me.m12.dSunBE.R01 = "(Ἀπὸ τῆς 30 Δεκεμβρίου ἕως 5 Ἰανουαρίου)"
lecAP.me.m12.dSunBE.R02 = ""

