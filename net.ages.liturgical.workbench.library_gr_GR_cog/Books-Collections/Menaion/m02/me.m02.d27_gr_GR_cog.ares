meVE.Stichera01.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.Stichera01.greekmelody = he.a.m4_gr_GR_cog.heAU.OsGennaionEnMartysi.greekname
meVE.Stichera01.melody = he.a.m4_gr_GR_cog.heAU.OsGennaionEnMartysi.name
meVE.Stichera01.incipit = "Κατʼ εἰκόνα γενόμενος, τοῦ Θεοῦ…"
meVE.Stichera01.text = "Κατʼ εἰκόνα γενόμενος, τοῦ Θεοῦ καὶ ὁμοίωσιν, κατʼ ἀρχὴν τῆς πλάσεως Παμμακάριστε, τὸ τῆς εἰκόνος ἀξίωμα, τηρεῖν διεσπούδασας, λογισμῷ πανευσεβεῖ, καὶ νοός καθαρότητι, καὶ ἁγνότητι, καὶ παθῶν ἐγκρατείᾳ καὶ τηρήσει, τῶν Χριστοῦ διαταγμάτων, καὶ εὐσεβείᾳ Πανόλβιε."

meVE.Stichera02.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.Stichera02.melody = he.a.m4_gr_GR_cog.heAU.OsGennaionEnMartysi.name
meVE.Stichera02.incipit = "Τῇ ἀσκήσει τὸ πρότερον, προσλαβὼν…"
meVE.Stichera02.text = "Τῇ ἀσκήσει τὸ πρότερον, προσλαβὼν καὶ τὴν ἄθλησιν, θεοφρόνως ὕστερον, εὐηρέστησας, διʼ ἀμφοτέρων τῷ Κτίσαντι, τῷ μόνῳ τὴν κάθαρσιν, ἀπαιτοῦντι παρʼ ἡμῶν, καὶ ψυχῆς τὴν εὐγένειαν· οὗ τὴν ἔνσαρκον, παρουσίαν δοξάζων προσεκύνεις, τὴν εἰκόνα τὴν ἁγίαν, τῆς κατʼ αὐτὸν ἀνθρωπότητος."

meVE.Stichera03.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.Stichera03.melody = he.a.m4_gr_GR_cog.heAU.OsGennaionEnMartysi.name
meVE.Stichera03.incipit = "Τοὺς τοῦ Λόγου τὴν σάρκωσιν…"
meVE.Stichera03.text = "Τοὺς τοῦ Λόγου τὴν σάρκωσιν, δυσσεβῶς ἀθετήσαντας, διελέγχων ὅσιε, διετέλεσας, διὰ μαστίγων ἐτάσεως, πάσης τε κακώσεως, καὶ δεσμῶν καὶ φυλακῆς, βεβαιῶν τὴν ἀλήθειαν· ὅθεν γέγονας, τῆς Χριστοῦ βασιλείας κληρονόμος, καὶ χαρᾶς ἀνεκλαλήτου, καὶ ἀϊδίου ἐλλάμψεως."

meVE.SticTheotokion.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.SticTheotokion.greekmelody = he.a.m4_gr_GR_cog.heAU.OsGennaionEnMartysi.greekname
meVE.SticTheotokion.melody = he.a.m4_gr_GR_cog.heAU.OsGennaionEnMartysi.name
meVE.SticTheotokion.incipit = oc.m4.d5_gr_GR_cog.ocVE.SticTheotokion.incipit
meVE.SticTheotokion.text = oc.m4.d5_gr_GR_cog.ocVE.SticTheotokion.text

meVE.SticStavrotheotokion.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.SticStavrotheotokion.greekmelody = he.a.m4_gr_GR_cog.heAU.OsGennaionEnMartysi.greekname
meVE.SticStavrotheotokion.melody = he.a.m4_gr_GR_cog.heAU.OsGennaionEnMartysi.name
meVE.SticStavrotheotokion.incipit = "Ἐν Σταυρῷ ὡς ἑώρακε, καθηλούμενον…"
meVE.SticStavrotheotokion.text = "Ἐν Σταυρῷ ὡς ἑώρακε, καθηλούμενον Κύριε, ἡ ἀμνὰς καὶ Μήτηρ σου ἐξεπλήττετο, καί, Τί τὸ ὅραμα, ἔκραζεν· Υἱὲ ποθεινότατε; ταῦτά σοι ὁ ἀπειθής, δῆμος ἀνταποδίδωσιν, ὁ παράνομος, ὁ πολλῶν σου θαυμάτων ἀπολαύσας; Ἀλλὰ δόξα τῇ ἀῤῥήτῳ, συγκαταβάσει σου Δέσποτα."

meVE.Apolytikion1ger.poet = poets_gr_GR_cog.po.Gerasimou
meVE.Apolytikion1ger.mode = miscellanea_gr_GR_cog.misc.Mode1
meVE.Apolytikion1ger.melody = he.a.m1_gr_GR_cog.heAU.TisErimouPolitis.name
meVE.Apolytikion1ger.incipit = "Φερωνύμως προκόπτων ἐν ἀσκήσει Προκόπιε…"
meVE.Apolytikion1ger.text = "Φερωνύμως προκόπτων ἐν ἀσκήσει Προκόπιε, ἤρθης ἐκ δυνάμεως Πάτερ, πρὸς ἀθλήσεως ἔλλαμψιν· Χριστοῦ γὰρ τὴν Εἰκόνα προσκυνῶν, Μαρτύρων ἀνεδείχθης κοινωνός· μεθʼ ὧν πρέσβευε παμμάκαρ διαπαντός, ὑπὲρ τῶν ἐκβοώντων σοι· δόξα τῷ δεδωκότι σοι ἰσχύν, δόξα τῷ σὲ στεφανώσαντι, δόξα τῷ ἐνεργοῦντι διὰ σοῦ, πᾶσιν ἰάματα."

meMA.C1.acrostic.title = "Κανὼν τοῦ Ὁσίου,"
meMA.C1.acrostic.notice1 = "ἔχων Ἀκροστιχίδα·"
meMA.C1.acrostic.text = "Τοὺς σοὺς ἀγῶνας εὐπρεπῶς μέλπω μάκαρ."
meMA.C1.poet = poets_gr_GR_cog.po.Theofanous
meMA.C1.title = titles_gr_GR_cog.ti.CanonDevoutM
