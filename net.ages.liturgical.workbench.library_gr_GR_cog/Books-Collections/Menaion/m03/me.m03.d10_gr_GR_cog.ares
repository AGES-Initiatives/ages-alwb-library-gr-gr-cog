meMA.Ode6C1H.incipit = he.h.m4_gr_GR_cog.heHE.EvoiseProtypon.incipit
meMA.Ode6C1H.text = he.h.m4_gr_GR_cog.heHE.EvoiseProtypon.text

meMA.Ode6C11.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C11.greekmelody = he.h.m4_gr_GR_cog.heHE.EvoiseProtypon.greekname
meMA.Ode6C11.melody = he.h.m4_gr_GR_cog.heHE.EvoiseProtypon.name
meMA.Ode6C11.incipit = "Ἑξάριθμος, Ἀθλοφόρων χορὸς ἡμῖν ἔλαμψεν…"
meMA.Ode6C11.text = "Ἑξάριθμος, Ἀθλοφόρων χορὸς ἡμῖν ἔλαμψεν, ὡς ἀστέρων, Ἐκκλησίας σεπτῷ στερεώματι, τοὺς πιστοὺς φωτίζων, καὶ τῆς πλάνης σκεδάζων σκοτόμαιναν."

meMA.Ode6C12.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C12.melody = he.h.m4_gr_GR_cog.heHE.EvoiseProtypon.name
meMA.Ode6C12.incipit = "Τὸ πέλαγος, τῶν βασάνων διήλθετε…"
meMA.Ode6C12.text = "Τὸ πέλαγος, τῶν βασάνων διήλθετε Μάρτυρες, ἀκυμάντως, καὶ τῆς ἄνω γαλήνης ἐτύχετε, τῶν πιστῶν λιμένες, γεγονότες καὶ πρέσβεις θερμότατοι."

meMA.Ode6C13.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C13.melody = he.h.m4_gr_GR_cog.heHE.EvoiseProtypon.name
meMA.Ode6C13.incipit = "Αἰνέσωμεν, σὺν Ἀνέκτῳ Κοδρᾶτον τὸν…"
meMA.Ode6C13.text = "Αἰνέσωμεν, σὺν Ἀνέκτῳ Κοδρᾶτον τὸν ἔνδοξον, καὶ σὺν Παύλῳ, Κυπριανὸν Διονύσιον Κρήσκεντα, τῆς Χριστοῦ ἀμπέλου, τὰ καλὰ καὶ κατάκαρπα κλήματα."

meMA.Ode6C1T.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C1T.melody = he.h.m4_gr_GR_cog.heHE.EvoiseProtypon.name
meMA.Ode6C1T.incipit = "Οὐράνωσας, γεωθεῖσαν τὴν φύσιν Πανάμωμε…"
meMA.Ode6C1T.text = "Οὐράνωσας, γεωθεῖσαν τὴν φύσιν Πανάμωμε, τῶν ἀνθρώπων, καὶ φθαρεῖσαν αὐτὴν ἐθεούργησας· διὰ τοῦτο πάντες, ἀσιγήτοις φωναῖς σὲ δοξάζομεν."

meMA.Kontakion1ger.poet = poets_gr_GR_cog.po.Gerasimou
meMA.Kontakion1ger.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Kontakion1ger.melody = he.a.m4_gr_GR_cog.heAU.EpefanisSimeron.name
meMA.Kontakion1ger.incipit = "Ὡς λαμπὰς ἑξάφωτος ἐν τῇ…"
meMA.Kontakion1ger.text = "Ὡς λαμπὰς ἑξάφωτος ἐν τῇ Κορίνθῳ, καρτερῶς ἀθλήσαντες, ὤφθητε Μάρτυρες σοφοί, φωταγωγοῦντες ταῖς χάρισι, τῆς Ἐκκλησίας ἀπαύστως τὸ πλήρωμα."

meMA.Ode7C1H.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C1H.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C1H.greekmelody = he.h.m4_gr_GR_cog.heHE.ODiasosasEnPyri.greekname
meMA.Ode7C1H.melody = he.h.m4_gr_GR_cog.heHE.ODiasosasEnPyri.melody
meMA.Ode7C1H.incipit = he.h.m4_gr_GR_cog.heHE.ODiasosasEnPyri.incipit
meMA.Ode7C1H.text = he.h.m4_gr_GR_cog.heHE.ODiasosasEnPyri.text

meMA.Ode7C11.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C11.greekmelody = he.h.m4_gr_GR_cog.heHE.ODiasosasEnPyri.greekname
meMA.Ode7C11.melody = he.h.m4_gr_GR_cog.heHE.ODiasosasEnPyri.name
meMA.Ode7C11.incipit = "Ἐν τῷ πυρὶ τῶν αἰκισμῶν…"
meMA.Ode7C11.text = "Ἐν τῷ πυρὶ τῶν αἰκισμῶν, ἄνωθεν δεχόμενοι δρόσον, ὑπομονῆς σὺν τοῖς Παισίν, οἱ γενναῖοι ἐκραύγαζον Μάρτυρες· Ὑπερύμνητε, Κύριε, ὁ Θεὸς ὁ τῶν Πατέρων εὐλογητός εἶ."

meMA.Ode7C12.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C12.melody = he.h.m4_gr_GR_cog.heHE.ODiasosasEnPyri.name
meMA.Ode7C12.incipit = "Τῇ ἐπομβρίᾳ τοῦ σεπτοῦ, αἵματος…"
meMA.Ode7C12.text = "Τῇ ἐπομβρίᾳ τοῦ σεπτοῦ, αἵματος ὑμῶν Ἀθλοφόροι, πέτρα ἡ ἄνικμος τὸ πρίν, ἀνεστόμωται ῥεῖθρα προχέουσα, ἰαμάτων, τοῖς μέλπουσιν· ὁ Θεὸς ὁ τῶν Πατέρων εὐλογητός εἶ."

meMA.Ode7C13.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C13.melody = he.h.m4_gr_GR_cog.heHE.ODiasosasEnPyri.name
meMA.Ode7C13.incipit = "Ὡς μυροθήκη νοητή, μύρα ἰαμάτων…"
meMA.Ode7C13.text = "Ὡς μυροθήκη νοητή, μύρα ἰαμάτων Κοδρᾶτε, ἡ τῶν λειψάνων σου σορός, ἀναβλύζει παθῶν ἀπελαύνουσα, τὸ δυσῶδες καὶ φλέγουσα, τῶν δαιμόνων παρατάξεις θείᾳ προνοίᾳ."

meMA.Ode7C1T.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C1T.melody = he.h.m4_gr_GR_cog.heHE.ODiasosasEnPyri.name
meMA.Ode7C1T.incipit = "Εὐλογημένος ὁ καρπός, τῆς εὐλογημένης…"
meMA.Ode7C1T.text = "Εὐλογημένος ὁ καρπός, τῆς εὐλογημένης γαστρός σου, ὃν εὐλογοῦσιν οὐρανῶν αἱ Δυνάμεις, βροτῶν τε συστήματα, ὁ ἡμᾶς λυτρωσάμενος, τῆς κατάρας τῆς ἀρχαίας εὐλογητὸς εἶ."

meMA.Ode8C1H.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C1H.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C1H.greekmelody = he.h.m4_gr_GR_cog.heHE.LytrotaTouPantos.greekname
meMA.Ode8C1H.melody = he.h.m4_gr_GR_cog.heHE.LytrotaTouPantos.melody
meMA.Ode8C1H.incipit = he.h.m4_gr_GR_cog.heHE.LytrotaTouPantos.incipit
meMA.Ode8C1H.text = he.h.m4_gr_GR_cog.heHE.LytrotaTouPantos.text

meMA.Ode8C11.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C11.greekmelody = he.h.m4_gr_GR_cog.heHE.LytrotaTouPantos.greekname
meMA.Ode8C11.melody = he.h.m4_gr_GR_cog.heHE.LytrotaTouPantos.name
meMA.Ode8C11.incipit = "Λαμπρυνθεὶς ταῖς τῶν ἄθλων λαμπρότησι…"
meMA.Ode8C11.text = "Λαμπρυνθεὶς ταῖς τῶν ἄθλων λαμπρότησι, στεφηφόρος Κυρίῳ παρίστασαι, σὺν τοῖς συνάθλοις ἔνδοξε, ἀναμέλπων Κοδρᾶτε· Πάντα τὰ ἔργα εὐλογεῖτε ὑμνεῖτε τὸν Κύριον."

meMA.Ode8C12.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C12.melody = he.h.m4_gr_GR_cog.heHE.LytrotaTouPantos.name
meMA.Ode8C12.incipit = "Ἀρετῶν τῷ φωτὶ καλλυνόμενος, ἐπὶ…"
meMA.Ode8C12.text = "Ἀρετῶν τῷ φωτὶ καλλυνόμενος, ἐπὶ τέλει Μαρτύρων λαμπρότητα, κατεκληρώσω πάνσοφε, ἀναμέλπων Κοδρᾶτε· Πάντα τὰ ἔργα, εὐλογεῖτε ὑμνεῖτε τὸν Κύριον."

meMA.Ode8C13.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C13.melody = he.h.m4_gr_GR_cog.heHE.LytrotaTouPantos.name
meMA.Ode8C13.incipit = "Προτειχίσματα Κόρινθος κέκτηται, τῶν Ἁγίων…"
meMA.Ode8C13.text = "Προτειχίσματα Κόρινθος κέκτηται, τῶν Ἁγίων τε τίμια λείψανα, καὶ ἰατρεῖον ἄμισθον, τὸν ναόν, ἔνθα πίστει, πᾷς ὁ προστρέχων, ἀλγεινῶν καὶ παθῶν ἀπαλλάττεται."

meMA.Ode8C1T.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C1T.melody = he.h.m4_gr_GR_cog.heHE.LytrotaTouPantos.name
meMA.Ode8C1T.incipit = "Λυτρωθέντες ἀρᾶς τῆς Προμήτορος, διὰ…"
meMA.Ode8C1T.text = "Λυτρωθέντες ἀρᾶς τῆς Προμήτορος, διὰ σοῦ τῆς Ἁγνῆς Θεομήτορος, εὐλογημένη Δέσποινα, εὐλογοῦμέν σε πίστει, καὶ μελῳδοῦμεν· Εὐλογεῖτε τὰ ἔργα τὸν Κύριον."

meMA.Ode9C1H.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C1H.mode = miscellanea_gr_GR_cog.misc.Mode4
