meMA.Ode3C11.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C11.greekmelody = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.greekname
meMA.Ode3C11.melody = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.name
meMA.Ode3C11.incipit = "Στεῤῥότητι λογισμοῦ, τῆς παρανόμου καὶ…"
meMA.Ode3C11.text = "Στεῤῥότητι λογισμοῦ, τῆς παρανόμου καὶ δεινῆς ἤνεγκας, τὴν χαλεπὴν βάσανον, φρουρὰς ὦ Ματρῶνα πανόλβιε."

meMA.Ode3C12.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C12.melody = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.name
meMA.Ode3C12.incipit = "Μαστίγων ὀδυνηρῶν, αἰσθανομένη καρτερῶς ἔφερες…"
meMA.Ode3C12.text = "Μαστίγων ὀδυνηρῶν, αἰσθανομένη καρτερῶς ἔφερες, ψηλαφητοῦ σκότους δέ, θείαν λαμπηδόνα ἀντείληφας."

meMA.Ode3C13.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C13.melody = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.name
meMA.Ode3C13.incipit = "Αἱμάτων σου οἱ κρουνοί, τῆς…"
meMA.Ode3C13.text = "Αἱμάτων σου οἱ κρουνοί, τῆς ἀσεβείας τὴν πυρὰν ἔσβεσαν, τοὺς δὲ πιστοὺς ἤρδευσαν, τῆς θεοσεβείας τοῖς νάμασι."

meMA.Ode3C1T.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C1T.melody = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.name
meMA.Ode3C1T.incipit = "Τὴν πλάκα, τὴν Κιβωτόν, τὴν…"
meMA.Ode3C1T.text = "Τὴν πλάκα, τὴν Κιβωτόν, τὴν δεξαμένην ἐν γαστρὶ Πάναγνε, Λόγον Θεοῦ τὸν ἄναρχον, θεηγορικῶς εὐφημοῦμέν σε."

meMA.Ode3Kathisma1.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3Kathisma1.greekmelody = he.a.m4_gr_GR_cog.heAU.TachyProkatalave.greekname
meMA.Ode3Kathisma1.melody = he.a.m4_gr_GR_cog.heAU.TachyProkatalave.name
meMA.Ode3Kathisma1.incipit = "Ἐπέστη φιλέορτοι, λαμπρὰ πανήγυρις, ἡ…"
meMA.Ode3Kathisma1.text = "Ἐπέστη φιλέορτοι, λαμπρὰ πανήγυρις, ἡ μνήμη ἡ ἔνδοξος, τῆς Ἀθληφόρου Χριστοῦ, εὐφραίνουσα τὰ σύμπαντα, λάμπουσα ταῖς ἀκτῖσι, τῶν θαυμάτων ἐν κόσμῳ, φέρουσα τοῖς ἀνθρώποις, τὴν ἀέναον χάριν. Αὐτῆς ταῖς ἱκεσίαις Σωτήρ, σῶσον τὸν κόσμον σου."

meMA.Ode3KathismaT.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3KathismaT.melody = he.a.m4_gr_GR_cog.heAU.TachyProkatalave.name
meMA.Ode3KathismaT.incipit = "Ἐκαίνισας Ἄχραντε, τῷ θείῳ τόκῳ…"
meMA.Ode3KathismaT.text = "Ἐκαίνισας Ἄχραντε, τῷ θείῳ τόκῳ σου, φθαρεῖσαν τοῖς πάθεσι, τῶν γηγενῶν τὴν θνητήν, οὐσίαν καὶ ἤγειρας, πάντας ἐκ τοῦ θανάτου, πρὸς ζωὴν ἀφθαρσίας· ὅθεν σε κατὰ χρέος, μακαρίζομεν πάντες, Παρθένε δεδοξασμένη, ὡς προεφήτευσας."

meMA.Ode3KathismaSt.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3KathismaSt.melody = he.a.m4_gr_GR_cog.heAU.TachyProkatalave.name
