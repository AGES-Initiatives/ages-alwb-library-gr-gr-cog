meDA.commemoration.first = "Μνήμη τοῦ Ἁγίου Πορφυρίου, Ἐπισκόπου Γάζης"
meDA.commemoration.first_short = "Μνήμη τοῦ Ἁγίου Πορφυρίου, Ἐπισκόπου Γάζης"
