peMA.PostGospelIdiomelon.mode = misc_gr_GR_cog.Mode6
peMA.PostGospelIdiomelon.melody = misc_gr_GR_cog.Idiomelon
peMA.PostGospelIdiomelon.incipit = "Σήμερον ἐν οὐρανοῖς…"
peMA.PostGospelIdiomelon.text = "Σήμερον ἐν οὐρανοῖς, αἱ ἄνω Δυνάμεις, τὴν ἡμετέραν φύσιν θεώμεναι, θαυμάζουσαι τοῦ ξένου τρόπου τὴν ἄνοδον, διηπόρουν ἀλλήλαις λέγουσαι· Τίς οὗτος ὁ παραγενόμενος; Βλέπουσαι δὲ τὸν οἰκεῖον Δεσπότην, τὰς οὐρανίους πύλας αἴρειν διεκελεύοντο. Μεθʼ ὧν ἀπαύστως ὑμνοῦμέν σε, τὸν μετὰ σαρκὸς ἐκεῖθεν πάλιν ἐρχόμενον, ὡς κριτήν τῶν ἁπάντων, καὶ Θεὸν παντοδύναμον."

peMA.C1.acrostic.title = "Κανὼν τοῦ Ἤχου πλ. αʹ."
peMA.C1.poet = poets_gr_GR_cog.IoannouMonachou
peMA.C1.title = titles_gr_GR_cog.KanonATisEortis
peMA.C1.verse = prayers_gr_GR_cog.verse_God.text

peMA.C2.acrostic.title = "Ὁ Κανὼν του Ἤχου δʹ,"
peMA.C2.acrostic.notice1 = "οὗ ἡ Ἀκροστιχὶς κατ ἀλφάβητον. Ἐν δὲ τῇ ηʹ καὶ θʹ Ὠδῇ,"
peMA.C2.acrostic.text = "ᾨδὴ Ἰωσήφ."
