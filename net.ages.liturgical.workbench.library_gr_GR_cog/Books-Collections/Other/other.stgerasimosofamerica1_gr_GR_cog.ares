hoCA.couplet1.title = "Δίστιχον."
hoCA.couplet1.verse1 = "Φύλαττε, Γεράσιμε, Παναγιώτην,"
hoCA.couplet1.verse2 = "σὺν ὑμνηταῖς σου, Χαραλάμπης κραυγάζει."
