A_Resource_Whose_Name = he.a.m4_gr_GR_cog

he.title = "Αὐτόμελα τοῦ Δʹ Ἦχου"

DiaXylouOAdam.mode = miscellanea_gr_GR_cog.misc.Mode4
DiaXylouOAdam.name = "Διὰ ξύλου ὁ Ἀδάμ."
DiaXylouOAdam.incipit = "Διὰ ξύλου ὁ Ἀδάμ…"
DiaXylouOAdam.text = "Διὰ ξύλου ὁ Ἀδάμ, Παραδείσου γέγονεν ἄποικος, διὰ ξύλου δὲ σταυροῦ, ὁ Λῃστὴς Παράδεισον ᾤκησεν. Ὁ μὲν γὰρ γευσάμενος ἐντολὴν ἠθέτησε τοῦ ποιήσαντος. Ὁ δὲ συσταυρούμενος, Θεὸν ὡμολόγησε τὸν κρυπτόμενον. Μνήσθητι καὶ ἡμῶν Σωτήρ, ἐν τῇ Βασιλείᾳ σου."

EdokasSimeiosin.mode = miscellanea_gr_GR_cog.misc.Mode4
EdokasSimeiosin.name = "Ἔδωκας σημείωσιν."
EdokasSimeiosin.incipit = "Ἔδωκας σημείωσιν…"
EdokasSimeiosin.text = "Ἔδωκας σημείωσιν, τοῖς φοβουμένοις σε Κύριε, τὸν Σταυρόν σου τὸν τίμιον, διʼ οὗ ἐθριάμβευσας, τὰς ἀρχὰς τοῦ σκότους, καὶ τὰς ἐξουσίας, καὶ ἐπανήγαγες ἡμᾶς, εἰς τὴν ἀρχαίαν μακαριότητα· διό σου τὴν φιλάνθρωπον, οἰκονομίαν δοξάζομεν, Ἰησοῦ παντοδύναμε, ὁ Σωτὴρ τῶν ψυχῶν ἡμῶν."

EpefanisSimeron.mode = miscellanea_gr_GR_cog.misc.Mode4
EpefanisSimeron.name = "Ἐπεφάνης σήμερον."
EpefanisSimeron.incipit = "Ἐπεφάνης σήμερον…"
EpefanisSimeron.text = "Ἐπεφάνης σήμερον τῇ οἰκουμένῃ, καὶ τὸ φῶς σου Κύριε, ἐσημειώθη ἐφʼ ἡμᾶς, ἐν ἐπιγνώσει ὑμνοῦντάς σε. Ἦλθες ἐφάνης τὸ Φῶς τὸ ἀπρόσιτον."

IthelonDakrysinExaleipsai.mode = miscellanea_gr_GR_cog.misc.Mode4
IthelonDakrysinExaleipsai.name = "Ἤθελον δάκρυσιν ἐξαλεῖψαι."
IthelonDakrysinExaleipsai.incipit = "Ἤθελον δάκρυσιν ἐξαλεῖψαι…"
IthelonDakrysinExaleipsai.text = "Ἤθελον δάκρυσιν ἐξαλεῖψαι, τῶν ἐμῶν πταισμάτων Κύριε τὸ χειρόγραφον, καὶ τὸ ὑπόλοιπον τῆς ζωῆς μου, διὰ μετανοίας εὐαρεστῆσαί σοι, ἀλλʼ ὁ ἐχθρὸς ἀπατᾷ με, καὶ πολεμεῖ τὴν ψυχήν μου. Κύριε, πρὶν εἰς τέλος ἀπόλωμαι, σῶσόν με."

KateplagiIosif.mode = miscellanea_gr_GR_cog.misc.Mode4
KateplagiIosif.name = "Κατεπλάγη Ἰωσήφ."
KateplagiIosif.incipit = "Κατεπλάγη Ἰωσήφ…"
KateplagiIosif.text = "Κατεπλάγη Ἰωσήφ, τὸ ὑπὲρ φύσιν θεωρῶν, καὶ ἐλάμβανεν εἰς νοῦν, τὸν ἐπὶ πόκον ὑετόν, ἐν τῇ ἀσπόρῳ συλλήψει σου Θεοτόκε. Βάτον ἐν πυρὶ ἀκατάφλεκτον, ῥάβδον Ἀαρὼν τὴν βλαστήσασαν, καὶ μαρτυρῶν ὁ μνήστωρ σου καὶ φύλαξ, τοῖς ἱερεῦσιν ἐκραύγαζε· Παρθένος τίκτει, καὶ μετὰ τόκον, πάλιν μένει Παρθένος."

KyrieAnelthonEnToStavro.mode = miscellanea_gr_GR_cog.misc.Mode4
KyrieAnelthonEnToStavro.name = "Κύριε, ἀνελθὼν ἐν τῷ Σταυρῷ."
KyrieAnelthonEnToStavro.incipit = "Κύριε, ἀνελθὼν ἐν τῷ Σταυρῷ…"
KyrieAnelthonEnToStavro.text = "Κύριε, ἀνελθὼν ἐν τῷ Σταυρῷ, τὴν προγονικὴν ἡμῶν κατάραν ἐξήλειψας καὶ κατελθὼν ἐν τῷ ᾍδῃ, τοὺς ἀπʼ αἰῶνος δεσμίους ἠλευθέρωσας, ἀφθαρσίας δωρούμενος τῶν ἀνθρώπων τῷ γένει· διὰ τοῦτο ὑμνοῦντες, δοξάζομεν, τὴν ζωοποιὸν καὶ σωτήριόν σου Ἔγερσιν."

OExYpsistouKlitheis.mode = miscellanea_gr_GR_cog.misc.Mode4
OExYpsistouKlitheis.name = "Ὁ ἐξ ὑψίστου κληθείς."
OExYpsistouKlitheis.incipit = "Ὁ ἐξ ὑψίστου κληθείς…"
OExYpsistouKlitheis.text = "Ὁ ἐξ ὑψίστου κληθείς, οὐκ ἀπʼ ἀνθρώπων, ὅτε τὸ ἐπίγειον σκότος ἠμαύρωσε, τοὺς ὀφθαλμούς σου τοῦ σώματος, τῆς ἀσεβείας, δημοσιεῦον τὴν σκυθρωπότητα, τότε τὸ οὐράνιον, φῶς περιήστραψε, σῆς διανοίας τὰ ὄμματα, τῆς εὐσεβείας ἀνακαλύπτον τὴν ὡραιότητα· ὅθεν ἐπέγνως τὸν ἐξάγοντα, φῶς ἐκ σκότους Χριστὸν τὸν Θεὸν ἡμῶν, ὂν ἱκέτευε σῶσαι, καὶ φωτίσαι τὰς ψυχὰς ἡμῶν."

OYpsotheisEnToStavro.mode = miscellanea_gr_GR_cog.misc.Mode4
OYpsotheisEnToStavro.name = "Ὁ ὑψωθεὶς ἐν τῷ Σταυρῷ."
OYpsotheisEnToStavro.incipit = "Ὁ ὑψωθεὶς ἐν τῷ Σταυρῷ…"
OYpsotheisEnToStavro.text = "Ὁ ὑψωθεὶς ἐν τῷ Σταυρῷ ἑκουσίως, τῇ ἐπωνύμῳ σου καινῇ πολιτείᾳ τοὺς οἰκτιρμούς σου δώρησαι, Χριστὲ ὁ Θεός· εὔφρανον ἐν τῇ δυνάμει σου τοὺς πιστοὺς βασιλεῖς ἡμῶν, νίκας χορηγῶν αὐτοῖς κατὰ τῶν πολεμίων· τὴν συμμαχίαν ἔχοιεν τὴν σήν, ὅπλον εἰρήνης, ἀήττητον τρόπαιον."

OsGennaionEnMartysi.mode = miscellanea_gr_GR_cog.misc.Mode4
OsGennaionEnMartysi.name = "Ὡς γενναῖον ἐν Μάρτυσι."
OsGennaionEnMartysi.incipit = "Ὡς γενναῖον ἐν Μάρτυσι…"
OsGennaionEnMartysi.text = "Ὡς γενναῖον ἐν Μάρτυσιν, ἀθλοφόρε Γεώργιε, συνελθόντες σήμερον, εὐφημοῦμέν σε, ὅτι τὸν δρόμον τετέλεκας, τὴν πίστιν τετήρηκας, καὶ ἐδέξω ἐκ Θεοῦ, τὸν τῆς νίκης σου στέφανον, ὂν ἱκέτευε, ἐκ φθορᾶς καὶ κινδύνων λυτρωθῆναι, τοὺς ἐν πίστει ἐκτελοῦντας, τὴν ἀεισέβαστον μνήμην σου."

TachyProkatalave.mode = miscellanea_gr_GR_cog.misc.Mode4
TachyProkatalave.name = "Ταχὺ προκατάλαβε."
TachyProkatalave.incipit = "Ταχὺ προκατάλαβε…"
TachyProkatalave.text = "Ταχὺ προκατάλαβε, πρὶν δουλωθῆναι ἡμᾶς, ἐχθροῖς βλασφημοῦσί σε, καὶ ἀπειλοῦσιν ἡμῖν, Χριστὲ ὁ Θεὸς ἡμῶν· ἄνελε τῷ Σταυρῷ σου, τοὺς ἡμᾶς πολεμοῦντας· γνώτωσαν πῶς ἰσχύει, Ὀρθοδόξων ἡ πίστις, πρεσβείαις τῆς Θεοτόκου, μόνε φιλάνθρωπε."
