meMA.Exaposteilarion1.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Exaposteilarion1.greekmelody = he.a.m2_gr_GR_cog.heAU.ToisMathitaisSynelthomen.greekname
meMA.Exaposteilarion1.melody = he.a.m2_gr_GR_cog.heAU.ToisMathitaisSynelthomen.name
meMA.Exaposteilarion1.incipit = "Ἀνδρέας ὁ πανεύφημος, στρατηλάτης καὶ…"
meMA.Exaposteilarion1.text = "Ἀνδρέας ὁ πανεύφημος, στρατηλάτης καὶ Μάρτυς, Σταυροῦ τῇ θείᾳ χάριτι, θράσος τύραννον εἷλε, καὶ στέφος ἤρατο δόξης· τούτῳ δὲ συνεκλάμπει, Μαρτύρων δῆμος ἔνθεος, φάλαγξ τροπαιοφόρος, συνασπισμός, ἀῤῥαγὴς ἀήττητος· ὧν τὴν μνήμην, φαιδρῶς πανηγυρίζοντες, τὸν Χριστὸν ἀνυμνοῦμεν."

meMA.Exaposteilarion2.mode = miscellanea_gr_GR_cog.misc.Mode3
meMA.Exaposteilarion2.greekmelody = he.a.m3_gr_GR_cog.heAU.OOuranonToisAstrois.greekname
meMA.Exaposteilarion2.melody = he.a.m3_gr_GR_cog.heAU.OOuranonToisAstrois.name
