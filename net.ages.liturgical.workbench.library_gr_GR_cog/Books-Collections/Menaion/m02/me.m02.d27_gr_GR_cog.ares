meMA.Ode3C1H.melody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.melody
meMA.Ode3C1H.incipit = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.incipit
meMA.Ode3C1H.text = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.text

meMA.Ode3C11.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C11.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode3C11.greekmelody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.greekname
meMA.Ode3C11.melody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.name
meMA.Ode3C11.incipit = "Σοῦ ἡ θεόληπτος ψυχή, τῷ…"
meMA.Ode3C11.text = "Σοῦ ἡ θεόληπτος ψυχὴ τῷ τῆς πίστεως ζήλῳ, πυρπολουμένη μάκαρ, τῶν ἀνόμων τὴν πικράν, μανίαν καὶ τὸν θυμόν, Θεοφόρε κραταιῶς διέλυσεν."

meMA.Ode3C12.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C12.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode3C12.melody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.name
meMA.Ode3C12.incipit = "Οἰκείοις αἵμασι βαφείς, τῶν Μαρτύρων…"
meMA.Ode3C12.text = "Οἰκείοις αἵμασι βαφεὶς τῶν Μαρτύρων τοὺς ἄθλους, ἐμιμήσω τρισμάκαρ, αἰκισμοὺς τῶν ἀσεβῶν, ἀνδρειοτάτῃ ψυχῇ, ὑπομείνας καρτερικῶς Προκόπιε."

meMA.Ode3C13.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C13.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode3C13.melody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.name
meMA.Ode3C13.incipit = "Ὑπέστης Ὅσιε διπλοῦς, τοὺς ἀγῶνας…"
meMA.Ode3C13.text = "Ὑπέστης Ὅσιε διπλοῦς τοὺς ἀγῶνας ἐμφρόνως, τὰς αἱρέσεις ἐλέγχων, καὶ λεόντων τὰς ὁρμάς, ἀνδρείως ὑπενεγκών, θεοφρόνως πάνσοφε Προκόπιε."

meMA.Ode3C1T.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C1T.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode3C1T.melody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.name
meMA.Ode3C1T.incipit = "Σοφία Λόγος τοῦ Πατρός, ὁ…"
meMA.Ode3C1T.text = "Σοφία Λόγος τοῦ Πατρὸς ὁ πρὸ πάντων αἰώνων, ἐπʼ ἐσχάτων τῶν χρόνων, ἀποῤῥήτως σαρκωθείς, ἐξ ἀπειράνδρου Μητρός, Θεοτόκον ταύτην ἀπειργάσατο."

meMA.Ode3Kathisma1.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode3Kathisma1.greekmelody = he.a.m1_gr_GR_cog.heAU.TouLithouSfragisthentos.greekname
meMA.Ode3Kathisma1.melody = he.a.m1_gr_GR_cog.heAU.TouLithouSfragisthentos.name
meMA.Ode3Kathisma1.incipit = "Ἀσώματος ἐν βίῳ ἐγνωρίσθης Πατὴρ…"
meMA.Ode3Kathisma1.text = "Ἀσώματος ἐν βίῳ, ἐγνωρίσθης Πατὴρ ἡμῶν, καὶ ἔνσαρκος Ἀγγέλοις, ἀνεδείχθης συνδίαιτος, τῷ κόσμῳ κατὰ Παῦλον σταυρωθείς, τῷ σοὶ ἐσταυρωμένῳ ἀληθῶς, καὶ εἰς οὐράνιον οἰκῶν, τῇ διανοίᾳ χῶρον ὁ γήϊνος. Δόξα τῷ εὐδοκήσαντι ἐν σοί· δόξα τῷ γνωρισθέντι σοι· δόξα τῷ ἐνεργοῦντι διὰ σοῦ πᾶσιν ἰάματα."

meMA.Ode3KathismaT.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode3KathismaT.melody = he.a.m1_gr_GR_cog.heAU.TouLithouSfragisthentos.name
meMA.Ode3KathismaT.incipit = me.m02.d13_gr_GR_cog.meMA.Ode3KathismaT.incipit
meMA.Ode3KathismaT.text = me.m02.d13_gr_GR_cog.meMA.Ode3KathismaT.text

meMA.Ode3KathismaSt.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode3KathismaSt.melody = he.a.m1_gr_GR_cog.heAU.TouLithouSfragisthentos.name
meMA.Ode3KathismaSt.incipit = "Σταυρῷ σου προσπαγέντος, ὑπὸ τῶν…"
meMA.Ode3KathismaSt.text = "Σταυρῷ σου προσπαγέντος, ὑπὸ τῶν παρανόμων, καὶ στρατιωτῶν Σῶτερ λόγχῃ, τὴν πλευρὰν ὀρυγέντος, ἡ Πάναγνος ὠδύρετο πικρῶς, τὰ σπλάγχνα κοπτομένη μητρικῶς, καὶ τὸ πολύ σου καὶ φρικτόν, τῆς ἀνοχῆς ἐξίστατο βοῶσα· Δόξα τῇ πρὸς ἀνθρώπους σου στοργῇ· δόξα σου τῇ χρηστότητι· δόξα τῷ ἐν τῷ θανάτω σου βροτοὺς ἀθανατίζοντι."

meMA.Ode4C1H.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C1H.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode4C1H.greekmelody = he.h.m6_gr_GR_cog.heHE.ChristosMouDynamis.greekname
meMA.Ode4C1H.melody = he.h.m6_gr_GR_cog.heHE.ChristosMouDynamis.melody
meMA.Ode4C1H.incipit = he.h.m6_gr_GR_cog.heHE.ChristosMouDynamis.incipit
meMA.Ode4C1H.text = he.h.m6_gr_GR_cog.heHE.ChristosMouDynamis.text

meMA.Ode4C11.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C11.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode4C11.greekmelody = he.h.m6_gr_GR_cog.heHE.ChristosMouDynamis.greekname
meMA.Ode4C11.melody = he.h.m6_gr_GR_cog.heHE.ChristosMouDynamis.name
meMA.Ode4C11.incipit = "Ἀρδεύων Ὅσιε, δακρύων χεύμασι, τὴν…"
meMA.Ode4C11.text = "Ἀρδεύων Ὅσιε δακρύων χεύμασι, τὴν καρδίαν σου μάκαρ τῶν ἀρετῶν, σπόρον ἐγεώργησας, ἐκ διανοίας καθαρᾶς, ὁσιώτατε Προκόπιε."

meMA.Ode4C12.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C12.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode4C12.melody = he.h.m6_gr_GR_cog.heHE.ChristosMouDynamis.name
meMA.Ode4C12.incipit = "Γεῶδες φρόνημα, τῷ θείῳ Πνεύματι…"
meMA.Ode4C12.text = "Γεῶδες φρόνημα τῷ θείῳ Πνεύματι, καθυπέταξας Πάτερ ἀσκητικόν, βίον ἀνελόμενος καὶ συνταττόμενος χοροῖς, τῶν Μαρτύρων παμμακάριστε."

meMA.Ode4C13.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C13.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode4C13.melody = he.h.m6_gr_GR_cog.heHE.ChristosMouDynamis.name
meMA.Ode4C13.incipit = "Ὡς μάρτυς ἔνθεος, καὶ ἀληθέστατος…"
meMA.Ode4C13.text = "Ὡς μάρτυς ἔνθεος καὶ ἀληθέστατος, ὑπὲρ τῆς εὐσεβείας αἱρετικήν, ἤλεγξας ἀπόνοιαν· τῆς γὰρ μανίας ἀληθῶς, τῶν ἀθέων κατεφρόνησας."

meMA.Ode4C1T.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C1T.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode4C1T.melody = he.h.m6_gr_GR_cog.heHE.ChristosMouDynamis.name
meMA.Ode4C1T.incipit = "Νομίμων ἄνευθεν…"
meMA.Ode4C1T.text = "Νομίμων ἄνευθεν θεσμῶν Πανάμωμε, συλλαβοῦσα τὸν Κτίστην δίχα φθορᾶς, τοῦτον ἀπεγέννησας καὶ Θεοτόκος ἀληθῶς, καὶ κυρίως ἐχρημάτισας."

meMA.Ode5C1H.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C1H.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode5C1H.greekmelody = he.h.m6_gr_GR_cog.heHE.ToTheioFengeiSou.greekname
