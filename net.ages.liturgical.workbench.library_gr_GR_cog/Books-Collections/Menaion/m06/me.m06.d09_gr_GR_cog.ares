meVE.GeneralApolytikion1.name = "Κύριλλε" // name of saint for General Apolytikia

meMA.C1.acrostic.title = "Ὁ Κανὼν τοῦ Ἁγίου,"
meMA.C1.acrostic.notice1 = "οὗ ἡ Ἀκροστιχίς ἄνευ τῶν Θεοτοκίων·"
meMA.C1.acrostic.text = "Θεωριῶν Κύριλλος ἐνθέων λύρα."
