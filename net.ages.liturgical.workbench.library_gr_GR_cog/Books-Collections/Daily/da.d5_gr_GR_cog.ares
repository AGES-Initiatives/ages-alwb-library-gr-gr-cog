daMA.Photagogic1Ending.text = "πρεσβείαις τῶν Ἀποστόλων, καὶ σῶσόν με."
daMA.Photagogic2Ending.text = "πρεσβείαις τοῦ Ἱεράρχου, καὶ σῶσόν με."
daMA.Photagogic3Ending.text = prayers_gr_GR_cog.fePresveiaisTisTheotokou

daMA.Photagogic1Beginning.text = "Πρεσβείαις, Κύριε, τῶν Ἀποστόλων,"
daMA.Photagogic2Beginning.text = "Πρεσβείαις, Κύριε, τοῦ Ἱεράρχου,"
