alt1.meDA.commemoration.full = "Μνήμη τοῦ ἐν Ἁγίοις Πατρὸς ἡμῶν Νεκταρίου, Μητροπολίτου Πενταπόλεως, τοῦ Θαυματουργοῦ." // full title of day
alt1.meDA.commemoration.first_short = "Μνήμη τοῦ Ἁγίου Νεκταρίου Πενταπόλεως"
alt1.meDA.commemoration.first_title = titles_gr_GR_cog.ti.SaintM
