meVE.Apolytikion1ger.poet = poets_gr_GR_cog.po.Gerasimou
meVE.Apolytikion1ger.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.Apolytikion1ger.greekmelody = he.a.m4_gr_GR_cog.heAU.TachyProkatalave.greekname
meVE.Apolytikion1ger.melody = he.a.m4_gr_GR_cog.heAU.TachyProkatalave.name
meVE.Apolytikion1ger.incipit = "Σοφίας τοῦ Πνεύματος, καταυγασθεὶς τῷ…"
meVE.Apolytikion1ger.text = "Σοφίας τοῦ Πνεύματος, καταυγασθεὶς τῷ φωτί, τὸν λόγον ἐτράνωσας, τῆς εὐσεβείας ἡμῖν, Εὐσέβιε ἔνδοξε· σὺ γὰρ ἱεραρχήσας, εὐσεβῶς τῇ Τριάδι, ἤθλησας θεοφρόνως, καὶ τὴν πλάνην καθεῖλες. Καὶ νῦν Πάτερ δυσώπησον, σώζεσθαι ἅπαντας."

meVE.MM.GeneralApolytikion2.text = properties_gr_GR_cog.media.key

meMA.C1.acrostic.title = "Ὁ Κανὼν τοῦ Ἁγίου,"
meMA.C1.acrostic.notice1 = "οὗ ἡ Ἀκροστιχίς·"
meMA.C1.acrostic.text = "Τῆς εὐσεβείας τὸν φερώνυμον σέβω. Ἰωσήφ."
meMA.C1.title = titles_gr_GR_cog.ti.CanonSaintM
meMA.C1.verse = prayers_gr_GR_cog.pr.verse_SaintM.text

meMA.Ode1C11.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C11.greekmelody = he.h.m8_gr_GR_cog.heHE.IKekommeni.greekname
meMA.Ode1C11.melody = he.h.m8_gr_GR_cog.heHE.IKekommeni.name
meMA.Ode1C11.incipit = "Τῷ ἀκροτάτῳ φωτὶ ἐλλαμπόμενος…"
meMA.Ode1C11.text = "Τῷ ἀκροτάτῳ φωτὶ ἐλλαμπόμενος, καὶ Ἱερέων χοροῖς, μάκαρ συνταττόμενος, καὶ ταῖς μαρτυρικαῖς εὐκλείαις ἐγκοσμούμενος, τοὺς πίστει ἑορτάζοντας τὴν σήν, Εὐσέβιε μακάριε, μνήμην περιφύλαττε, Χριστὸν ἐξιλεούμενος."

meMA.Ode1C12.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C12.melody = he.h.m8_gr_GR_cog.heHE.IKekommeni.name
meMA.Ode1C12.incipit = "Ἤδη τῷ πόθῳ Χριστοῦ…"
meMA.Ode1C12.text = "Ἤδη τῷ πόθῳ Χριστοῦ πυρπολούμενος, ἀπὸ νεότητος σήν, σάρκα καθυπέταξας, παμμάκαρ τῆς ψυχῆς ταῖς ἱεραῖς δυνάμεσι, καὶ σκεῦος ἱερὸν ἀποδειχθείς, λαὸν ἐν ὁσιότητι, Ὅσιε ἐποίμανας, χρισθεὶς μύρῳ τῆς χάριτος."

meMA.Ode1C13.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C13.melody = he.h.m8_gr_GR_cog.heHE.IKekommeni.name
meMA.Ode1C13.incipit = "Σὲ ποιμενάρχην τὸ Πνεῦμα…"
meMA.Ode1C13.text = "Σὲ ποιμενάρχην τὸ Πνεῦμα προβάλλεται, αἱρετιζόντων ἀχλύν, λόγοις σελασφόροις σου, σκεδάζοντα σοφέ, καὶ προφανῶς δεικνύοντα, τὴν τρίβον τὴν ὀρθήν, τοῖς ἐν αὐτῇ ὁσίως ἐπιβαίνουσι, μέγιστε Εὐσέβιε, φωστὴρ τῶν καρδιῶν ἡμῶν."

meMA.Ode1C1T.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C1T.melody = he.h.m8_gr_GR_cog.heHE.IKekommeni.name
meMA.Ode1C1T.incipit = "Εὐλογημένη Θεὸν ἡ κυήσασα…"
meMA.Ode1C1T.text = "Εὐλογημένη Θεὸν ἡ κυήσασα, τοὺς εὐλογοῦντάς σε νῦν, Ἄχραντε εὐλόγησον, καὶ πρὸς τὰς ἀγαθὰς εἰσόδους χειραγώγησον, καὶ ἔμπλησον ἐνθέων ἀρετῶν, καὶ ψάλλειν ἐνδυνάμωσον· Τῷ Κυρίῳ ᾄσωμεν, ἐνδόξως γὰρ δεδόξασται."

meMA.Ode3C11.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C11.greekmelody = he.h.m8_gr_GR_cog.heHE.TonFovonSouKyrie.greekname
meMA.Ode3C11.melody = he.h.m8_gr_GR_cog.heHE.TonFovonSouKyrie.name
meMA.Ode3C11.incipit = "Ὑπῆρξας πρᾷος καὶ ἀνεξίκακος…"
meMA.Ode3C11.text = "Ὑπῆρξας πρᾷος καὶ ἀνεξίκακος, ταπεινόφρων καὶ μέτριος, καὶ ἀγάπης θείας ἔμπλεως, Μαρτύρων ἐπὶ τέλει, κοσμηθεὶς στεφάνῳ."

meMA.Ode3C12.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C12.melody = he.h.m8_gr_GR_cog.heHE.TonFovonSouKyrie.name
meMA.Ode3C12.incipit = "Στολαῖς ταῖς ἐξ αἵματος…"
meMA.Ode3C12.text = "Στολαῖς ταῖς ἐξ αἵματος Εὐσέβιε, ἱερώτατε λαμπόμενος, εἰς τὰ Ἅγια ἀνέδραμες, φαιδρῶς τὰ τῶν Ἁγίων, δόξῃ ἐστεμμένος."

meMA.Ode3C13.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C13.melody = he.h.m8_gr_GR_cog.heHE.TonFovonSouKyrie.name
meMA.Ode3C13.incipit = "Ἐλέγχων Ἀρείου…"
meMA.Ode3C13.text = "Ἐλέγχων Ἀρείου τοὺς ὁμόφρονας, διωγμοὺς Μάκαρ ὑπήνεγκας, βιαιότατόν τε θάνατον· διὸ τῆς ἀθανάτου, δόξης ἠξιώθης."

meMA.Ode3C1T.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C1T.melody = he.h.m8_gr_GR_cog.heHE.TonFovonSouKyrie.name
meMA.Ode3C1T.incipit = "Βουλήσει Πατρὸς τὸν Λόγον…"
meMA.Ode3C1T.text = "Βουλήσει Πατρὸς τὸν Λόγον τέτοκας, ἐπελεύσει Πνεύματος, Παναγία Μητροπάρθενε, Ἀρχιερέων δόξα· ὅθεν σε ὑμνοῦμεν."

meMA.Ode3H.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3H.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3H.greekmelody = he.h.m8_gr_GR_cog.heHE.TonFovonSouKyrie.greekname
meMA.Ode3H.melody = he.h.m8_gr_GR_cog.heHE.TonFovonSouKyrie.melody
meMA.Ode3H.incipit = he.h.m8_gr_GR_cog.heHE.TonFovonSouKyrie.incipit
meMA.Ode3H.text = he.h.m8_gr_GR_cog.heHE.TonFovonSouKyrie.text

meMA.Ode3Kathisma1.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3Kathisma1.greekmelody = he.a.m4_gr_GR_cog.heAU.TachyProkatalave.greekname
meMA.Ode3Kathisma1.melody = he.a.m4_gr_GR_cog.heAU.TachyProkatalave.name
meMA.Ode3Kathisma1.incipit = "Φαιδρύνας τὸν βίον σου…"
meMA.Ode3Kathisma1.text = "Φαιδρύνας τὸν βίον σου, τῶν ἀρετῶν καλλοναῖς, Ποιμὴν ἀνηγόρευσαι, καὶ Ἱεράρχης σεπτός, παμμάκαρ Εὐσέβιε· αἵματι δὲ Μαρτύρων, σεαυτὸν ὡραΐσας, ἄνω πρὸς τὰς ἀΰλους, μετετέθης χορείας· μεθʼ ὧν ὑπὲρ ἡμῶν τὸν Χριστὸν Πάτερ ἱκέτευε."

meMA.Ode3KathismaT.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3KathismaT.melody = he.a.m4_gr_GR_cog.heAU.TachyProkatalave.name
meMA.Ode3KathismaT.incipit = "Ἐλπὶς ἀκαταίσχυντε…"
meMA.Ode3KathismaT.text = "Ἐλπὶς ἀκαταίσχυντε, τῶν πεποιθότων εἰς σέ, ἡ μόνη κυήσασα, ὑπερφυῶς ἐν σαρκί, Χριστὸν τὸν Θεὸν ἡμῶν· τοῦτον σὺν τοῖς Ἁγίοις, Ἀποστόλοις δυσώπει, δοῦναι τῇ οἰκουμένῃ, ἱλασμὸν καὶ εἰρήνην, καὶ πᾶσιν ἡμῖν πρὸ τέλους, βίου διόρθωσιν."

meMA.Ode3KathismaSt.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3KathismaSt.melody = he.a.m4_gr_GR_cog.heAU.TachyProkatalave.name
meMA.Ode3KathismaSt.incipit = oc.m4.d6_gr_GR_cog.ocMA.Kathisma23.incipit
meMA.Ode3KathismaSt.text = oc.m4.d6_gr_GR_cog.ocMA.Kathisma23.text

meMA.Ode4C11.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C11.greekmelody = he.h.m8_gr_GR_cog.heHE.ExOrousKataskiou.greekname
meMA.Ode4C11.melody = he.h.m8_gr_GR_cog.heHE.ExOrousKataskiou.name
meMA.Ode4C11.incipit = "Ἔστησας σοφέ…"
meMA.Ode4C11.text = "Ἔστησας σοφέ, τῆς πλάνης σηπεδόνα ἅλατι τῶν σῶν, νοστίμων διδαγμάτων, καὶ τοὺς νοσοῦντας αἱρέσει κακοπιστίας, μάκαρ ὑγείωσας Εὐσέβιε."

meMA.Ode4C12.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C12.melody = he.h.m8_gr_GR_cog.heHE.ExOrousKataskiou.name
meMA.Ode4C12.incipit = "Ἴθυνας λαόν…"
meMA.Ode4C12.text = "Ἴθυνας λαόν, πρὸς ὅρμον σωτηρίας, ἀρειανικήν, βυθίσας ἀπιστίαν, τῇ τῶν αἱμάτων πλημμύρᾳ ποιμὴν καὶ μάρτυς, Ἀρχιερέων ἐγκαλλώπισμα."

meMA.Ode4C13.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C13.melody = he.h.m8_gr_GR_cog.heHE.ExOrousKataskiou.name
meMA.Ode4C13.incipit = "Ἄστρον φαεινόν…"
meMA.Ode4C13.text = "Ἄστρον φαεινόν, ἀνέτειλας ἐν κόσμῳ, φέγγει ἱερῶν, δογμάτων καταυγάζων, τὰς διανοίας παμμάκαρ τῶν ὀρθοδόξων· ὅθεν πιστῶς σε μακαρίζομεν."

meMA.Ode4C1T.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C1T.melody = he.h.m8_gr_GR_cog.heHE.ExOrousKataskiou.name
meMA.Ode4C1T.incipit = "Στόματι Ἁγνή…"
meMA.Ode4C1T.text = "Στόματι Ἁγνή, καὶ γλώσσῃ καὶ καρδίᾳ καθομολογῶ, τοῦ Κτίστου σε Μητέρα, καὶ δυσωπῶ σε· Καταύγασον τὴν ψυχήν μου, ἐσκοτισμένην πλημμελήμασιν."

meMA.Ode5C11.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C11.greekmelody = he.h.m8_gr_GR_cog.heHE.OEkNyktosAgnoias.greekname
meMA.Ode5C11.melody = he.h.m8_gr_GR_cog.heHE.OEkNyktosAgnoias.name
meMA.Ode5C11.incipit = "Ταῖς διδαχαῖς σου Μάκαρ…"
meMA.Ode5C11.text = "Ταῖς διδαχαῖς σου Μάκαρ, τῶν ὀρθοδόξων στηρίζων τὸ φρόνημα, ὑπὲρ τῆς εὐσεβείας, τὰς ὑπερορίας ὑπήνεγκας."

meMA.Ode5C12.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C12.melody = he.h.m8_gr_GR_cog.heHE.OEkNyktosAgnoias.name
meMA.Ode5C12.incipit = "Ὀρθοδοξίας στήλην…"
meMA.Ode5C12.text = "Ὀρθοδοξίας στήλην, καὶ ἰαμάτων πηγήν σε γινώσκομεν, πάνσοφε Ἱεράρχα, καὶ τῆς Ἐκκλησίας ἑδραίωμα."

meMA.Ode5C13.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C13.melody = he.h.m8_gr_GR_cog.heHE.OEkNyktosAgnoias.name
meMA.Ode5C13.incipit = "Νομοθεσίαις θείαις…"
meMA.Ode5C13.text = "Νομοθεσίαις θείαις, τοὺς ἀνομοῦντας στηρίζων ἐν χάριτι, ὑπὸ χειρὸς ἀνόμου, ἄδικον ὑπέμεινας θάνατον."

meMA.Ode5C1T.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C1T.melody = he.h.m8_gr_GR_cog.heHE.OEkNyktosAgnoias.name
meMA.Ode5C1T.incipit = "Φωτιστικὴ λυχνία…"
meMA.Ode5C1T.text = "Φωτιστικὴ λυχνία, Θεοκυῆτορ τὸ θεῖον λαμπάδιον, φέρουσα ἐγνωρίσθης, πάντα καταυγάζον τὰ πέρατα."

meMA.Ode6C11.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C11.greekmelody = he.h.m8_gr_GR_cog.heHE.TinDeisinEkcheo.greekname
meMA.Ode6C11.melody = he.h.m8_gr_GR_cog.heHE.TinDeisinEkcheo.name
meMA.Ode6C11.incipit = "Ἐκήρυξας, ὁμοούσιον Λόγον…"
meMA.Ode6C11.text = "Ἐκήρυξας, ὁμοούσιον Λόγον, καὶ συνάναρχον Πατρὶ τὸν Σωτῆρα, τῆς βδελυρᾶς, τοῦ Ἀρείου μανίας, ἐξαφανίσας τὸ μάταιον φρόνημα, Εὐσέβιε τῶν εὐσεβῶν, ἀῤῥαγὲς καὶ στεῤῥὸν περιτείχισμα."

meMA.Ode6C12.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C12.melody = he.h.m8_gr_GR_cog.heHE.TinDeisinEkcheo.name
meMA.Ode6C12.incipit = "Ῥαντίσματι, τῶν σεπτῶν…"
meMA.Ode6C12.text = "Ῥαντίσματι, τῶν σεπτῶν σου αἱμάτων, πορφυρίδα σεαυτῷ ἐπιχρώσας, βασιλικήν, προσεχώρησας Πάτερ, μέτʼ εὐφροσύνης εἰς θεῖα βασίλεια, παρίστασθαι τῷ Βασιλεῖ, τῶν αἰώνων παμμάκαρ Εὐσέβιε."

meMA.Ode6C13.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C13.melody = he.h.m8_gr_GR_cog.heHE.TinDeisinEkcheo.name
meMA.Ode6C13.incipit = "Ὡς ῥόδον, ὡς εὐωδέστατον…"
meMA.Ode6C13.text = "Ὡς ῥόδον, ὡς εὐωδέστατον κρίνον, ὡς παράδεισος Θεοῦ ἀνεδείχθης, μέσον ζωῆς, κεκτημένος τὸ ξύλον, τὸν φυτουργὸν τῶν ἁπάντων καὶ Κύριον, Εὐσέβιε καὶ νῦν ἡμᾶς, εὐωδίας πληροῖς θείου Πνεύματος."

meMA.Ode6C1T.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C1T.melody = he.h.m8_gr_GR_cog.heHE.TinDeisinEkcheo.name
meMA.Ode6C1T.incipit = "Νοήσαντες, θεηγόροι…"
meMA.Ode6C1T.text = "Νοήσαντες, θεηγόροι Προφῆται, μυστηρίου σου τὸ ἄπειρον βάθος, τοῦτο σαφῶς, προεδήλωσαν Κόρη, διʼ αἰνιγμάτων καὶ θείων προῤῥήσεων· τὸν Λόγον γὰρ τὸν τοῦ Πατρός, ὑπὲρ λόγον Ἁγνὴ ἐσωμάτωσας."

meMA.Ode6H.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6H.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6H.greekmelody = he.h.m8_gr_GR_cog.heHE.TinDeisinEkcheo.greekname
meMA.Ode6H.melody = he.h.m8_gr_GR_cog.heHE.TinDeisinEkcheo.melody
meMA.Ode6H.incipit = he.h.m8_gr_GR_cog.heHE.TinDeisinEkcheo.incipit
meMA.Ode6H.text = he.h.m8_gr_GR_cog.heHE.TinDeisinEkcheo.text

meMA.Ode7C11.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode7C11.greekmelody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.greekname
meMA.Ode7C11.melody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.name
meMA.Ode7C11.incipit = "Ὕμνοις σχολάζων Ἱεράρχα…"
meMA.Ode7C11.text = "Ὕμνοις σχολάζων Ἱεράρχα, καὶ δεήσεσιν, ἐκοίμισας τὰ πάθη, καὶ ἡμῶν τῶν πιστῶν, ἄγρυπνος ὤφθης φύλαξ, τῶν εὐσεβῶς ὑμνούντων σε, καὶ τελούντων σου τὴν μνήμην."

meMA.Ode7C12.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode7C12.melody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.name
meMA.Ode7C12.incipit = "Μάρτυς δειχθεὶς καὶ Ἱεράρχης…"
meMA.Ode7C12.text = "Μάρτυς δειχθεὶς καὶ Ἱεράρχης, τὰ οὐράνια, ἐνθέως ἐκληρώσω, καὶ τῷ θρόνῳ βοῶν, παρίστασαι τοῦ Κτίστου· Εὐλογητὸς εἶ Κύριε, ὁ Θεὸς εἰς τοὺς αἰῶνας."

meMA.Ode7C13.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode7C13.melody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.name
meMA.Ode7C13.incipit = "Ὄρθρος ἀνέτειλας κηρύττων…"
meMA.Ode7C13.text = "Ὄρθρος ἀνέτειλας κηρύττων, τὸν ἐκλάμψαντα, Πατρὸς πρὸ ἑωσφόρου, Ἰησοῦν τὸν Χριστόν, φωστὴρ τῆς Ἐκκλησίας, Ἱεραρχῶν ἀγλάϊσμα, ἀθλοφόρων ὡραιότης."

meMA.Ode7C1T.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode7C1T.melody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.name
meMA.Ode7C1T.incipit = "Νόμους τῆς φύσεως Παρθένε…"
meMA.Ode7C1T.text = "Νόμους τῆς φύσεως Παρθένε, τῇ κυήσει σου, καινίζεις παραδόξως· καὶ φθαρέντας ἡμᾶς, κακίστῃ παραβάσει, ἀναχωνεύεις· ὅθεν σε, κατὰ χρέος εὐφημοῦμεν."

meMA.Ode8C11.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C11.greekmelody = he.h.m8_gr_GR_cog.heHE.OiTheorrimonesPaides.greekname
meMA.Ode8C11.melody = he.h.m8_gr_GR_cog.heHE.OiTheorrimonesPaides.name
meMA.Ode8C11.incipit = "Σὺν Ἱεράρχαις καὶ Μάρτυσι…"
meMA.Ode8C11.text = "Σὺν Ἱεράρχαις καὶ Μάρτυσι δυσώπει, ὡς Ἱεράρχης καὶ Μάρτυς ἱεροκήρυξ Εὐσέβιε, τὸν Σωτῆρα τῶν ὅλων, εὑρεῖν ἡμᾶς ἔλεος."

meMA.Ode8C12.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C12.melody = he.h.m8_gr_GR_cog.heHE.OiTheorrimonesPaides.name
meMA.Ode8C12.incipit = "Ἐν ἀγαθῇ συνειδήσει…"
meMA.Ode8C12.text = "Ἐν ἀγαθῇ συνειδήσει διαπρέπων, καὶ βακτηρίᾳ τῶν λόγων, τὸ ἱερὸν νέμων ποίμνιον, τῆς αἱρέσεως θῆρας, Παμμάκαρ ἀπήλασας."

meMA.Ode8C13.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C13.melody = he.h.m8_gr_GR_cog.heHE.OiTheorrimonesPaides.name
meMA.Ode8C13.incipit = "Βεβαρυμένους τῇ μέθῃ…"
meMA.Ode8C13.text = "Βεβαρυμένους τῇ μέθῃ τῆς ἀπάτης, τοὺς αἱρετίζοντας μάτην, σὺ διελέγχων Εὐσέβιε, ὑπʼ αὐτῶν ἀνῃρέθης, ἀθλήσας στεῤῥότατα."

meMA.Ode8C1T.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C1T.melody = he.h.m8_gr_GR_cog.heHE.OiTheorrimonesPaides.name
meMA.Ode8C1T.incipit = "Ὡς ἀνωτέρα τῶν θείων…"
meMA.Ode8C1T.text = "Ὡς ἀνωτέρα τῶν θείων ὑψωμάτων, ὕψιστον ἔτεκες Λόγον, τὸν ἀπὸ τῆς γῆς ἀνυψώσαντα, τὴν πεσοῦσαν οὐσίαν, ἀνθρώπων Πανύμνητε."

meMA.Ode8H.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8H.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8H.greekmelody = he.h.m8_gr_GR_cog.heHE.OiTheorrimonesPaides.greekname
meMA.Ode8H.melody = he.h.m8_gr_GR_cog.heHE.OiTheorrimonesPaides.melody
meMA.Ode8H.incipit = he.h.m8_gr_GR_cog.heHE.OiTheorrimonesPaides.incipit
meMA.Ode8H.text = he.h.m8_gr_GR_cog.heHE.OiTheorrimonesPaides.text

meMA.Ode9C11.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode9C11.greekmelody = he.h.m8_gr_GR_cog.heHE.KyriosTheotokon.greekname
meMA.Ode9C11.melody = he.h.m8_gr_GR_cog.heHE.KyriosTheotokon.name
meMA.Ode9C11.incipit = "Ἰδεῖν κατηξιώθης…"
meMA.Ode9C11.text = "Ἰδεῖν κατηξιώθης, Πάτερ τὰ μακρόθεν, τῇ ἱερᾷ σου ψυχῇ ἐλπιζόμενα, καὶ πρὸς ἀνέσπερον φέγγος, χαίρων ἐσκήνωσας."

meMA.Ode9C12.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode9C12.melody = he.h.m8_gr_GR_cog.heHE.KyriosTheotokon.name
meMA.Ode9C12.incipit = "Ὡραῖος ἀπεφάνθης…"
meMA.Ode9C12.text = "Ὡραῖος ἀπεφάνθης, ταῖς θεηγορίαις, καὶ ἀγαθαῖς ἐργασίαις Εὐσέβιε, καὶ ταῖς παντίμοις τῶν ἄθλων, μάκαρ λαμπρότησι."

meMA.Ode9C13.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode9C13.melody = he.h.m8_gr_GR_cog.heHE.KyriosTheotokon.name
meMA.Ode9C13.incipit = "Στησώμεθα χορείαν…"
meMA.Ode9C13.text = "Στησώμεθα χορείαν, ἐν ἀγγαλλιάσει, τὸν ἱερὸν εὐφημοῦντες Εὐσέβιον, τῆς εὐσεβείας τῷ κάλλει ἀγλαϊζόμενον."

meMA.Ode9C14.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C14.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode9C14.melody = he.h.m8_gr_GR_cog.heHE.KyriosTheotokon.name
meMA.Ode9C14.incipit = "Ἡ θήκη σου μυρίζει…"
meMA.Ode9C14.text = "Ἡ θήκη σου μυρίζει, χάριν ἰαμάτων, ἡ δὲ ἁγία σου μνήμη Εὐσέβιε, καταφωτίζει τὸν κόσμον, ὡς μέγας ἥλιος."

meMA.Ode9C1T.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode9C1T.melody = he.h.m8_gr_GR_cog.heHE.KyriosTheotokon.name
meMA.Ode9C1T.incipit = "Φιλάγαθε Παρθένε…"
meMA.Ode9C1T.text = "Φιλάγαθε Παρθένε, τὴν κεκακωμένην, τῇ ἁμαρτίᾳ ψυχήν μου ἀγάθυνον, ἡ τὸν πανάγαθον Λόγον, σαρκὶ κυήσασα."

meMA.Ode9H.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9H.mode = miscellanea_gr_GR_cog.misc.Mode8
