ocVE.Aposticha2.mode = misc_gr_GR_cog.Mode3
ocVE.Aposticha2.melody = ""
ocVE.Aposticha2.incipit = oc.m3.d4_gr_GR_cog.ocVE.Aposticha3.incipit
ocVE.Aposticha2.text = oc.m3.d4_gr_GR_cog.ocVE.Aposticha3.text

ocVE.Aposticha3.mode = misc_gr_GR_cog.Mode3
ocVE.Aposticha3.melody = ""
ocVE.Aposticha3.incipit = "Οἱ Μάρτυρές σου Κύριε, πίστει"
ocVE.Aposticha3.text = "Οἱ Μάρτυρές σου Κύριε, πίστει στηριχθέντες, ἐλπίδι βεβαιωθέντες, τῇ ἀγάπῃ τοῦ Σταυροῦ σου, ψυχικῶς ἑνωθέντες, τοῦ ἐχθροῦ τὴν τυραννίδα ὤλεσαν, καὶ τυχόντες τῶν στεφάνων, μετὰ τῶν Ἀσωμάτων πρεσβεύουσιν, ὑπὲρ τῶν ψυχῶν ἡμῶν."

ocVE.Aposticha4.mode = misc_gr_GR_cog.Mode3
ocVE.Aposticha4.melody = ""
ocVE.Aposticha4.incipit = "Πάντα ματαιότης τὰ ἀνθρώπινα, ὅσα"
ocVE.Aposticha4.text = "Πάντα ματαιότης τὰ ἀνθρώπινα, ὅσα οὐχ ὑπάρχει μετὰ θάνατον, οὐ παραμένει ὁ πλοῦτος, οὐ συνοδεύει ἡ δόξα· ἐπελθὼν γὰρ ὁ θάνατος, ταῦτα πάντα ἐξηφάνισται. Διὸ Χριστῷ τῷ ἀθανάτῳ Βασιλεῖ βοήσωμεν· Τοὺς μεταστάντας ἐξ ἡμῶν ἀνάπαυσον, ἔνθα πάντων ἐστίν, εὐφραινομένων ἡ κατοικία."

ocVE.AposTheotokion.mode = misc_gr_GR_cog.Mode3
ocVE.AposTheotokion.melody = ""
ocVE.AposTheotokion.incipit = "Ἐν γυναιξὶν ἁγία Θεοτόκε, Μήτηρ"
ocVE.AposTheotokion.text = "Ἐν γυναιξὶν ἁγία Θεοτόκε, Μήτηρ ἀνύμφευτε, πρέσβευε ὃν ἔτεκες, Βασιλέα καὶ Θεόν, ἵνα σώσῃ ἡμᾶς ὡς φιλάνθρωπος."

