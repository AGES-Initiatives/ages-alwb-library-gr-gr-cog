meVE.SticStavrotheotokion.incipit = oc.m6.d6_gr_GR_cog.ocVE.AposTheotokion.incipit
meVE.SticStavrotheotokion.text = oc.m6.d6_gr_GR_cog.ocVE.AposTheotokion.text

meVE.Apolytikion1.poet = poets_gr_GR_cog.Gerasimou
meVE.Apolytikion1.mode = misc_gr_GR_cog.Mode3
meVE.Apolytikion1.melody = he.a.m3_gr_GR_cog.TheiasPisteos.name
meVE.Apolytikion1.incipit = "Βίον ἔνθεον διαδραμόντες, Χριστοῦ ὤφθητε…"
meVE.Apolytikion1.text = "Βίον ἔνθεον διαδραμόντες, Χριστοῦ ὤφθητε, συγκληρονόμοι, διʼ ἀθλήσεως ἐχθρὸν τροπωσάμενοι, Κάρπε σοφὲ καὶ κλεινὲ Ἀγαθόδωρε, Ἀγαθονίκη καὶ Πάπυλε ἔνδοξε. Θεῖοι Μάρτυρες, Χριστῷ τῷ Θεῷ πρεσβεύσατε, δωρήσασθαι ἡμῖν τὸ μέγα ἔλεος."

