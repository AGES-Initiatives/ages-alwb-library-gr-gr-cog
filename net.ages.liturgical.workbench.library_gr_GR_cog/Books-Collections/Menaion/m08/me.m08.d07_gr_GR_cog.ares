meMegalynarion.text = ""

alt1.meDA.commemoration.first_short = "Μνήμη τοῦ Ὁσίου Θεοδοσίου τοῦ Νέου"
alt1.meMegalynarion.text = "Χαίροις θεῖον βλάστημα Ἀθηνῶν, καὶ τῆς Ἀργολίδος ἀντιλήπτωρ καὶ ἀρωγός· χαίροις ὁ βλυστάνων, ἰάματα ποικίλα, τοῖς σὲ ὑμνολογοῦσιν, ὦ Θεοδόσιε."
