meMA.Ode3KathismaSt.incipit = oc.m8.d4_gr_GR_cog.ocMA.Kathisma13.incipit
meMA.Ode3KathismaSt.text = oc.m8.d4_gr_GR_cog.ocMA.Kathisma13.text

meMA.Ode4C1H.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C1H.mode = misc_gr_GR_cog.Mode8
meMA.Ode4C1H.melody = he.h.m8_gr_GR_cog.EisakikoaKyrie.melody
meMA.Ode4C1H.incipit = he.h.m8_gr_GR_cog.EisakikoaKyrie.incipit
meMA.Ode4C1H.text = he.h.m8_gr_GR_cog.EisakikoaKyrie.text

meMA.Ode4C11.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C11.mode = misc_gr_GR_cog.Mode8
meMA.Ode4C11.melody = he.h.m8_gr_GR_cog.EisakikoaKyrie.name
meMA.Ode4C11.incipit = "Οἱ τοῦ σώματος ἔρωτες, ἔρωτι…"
meMA.Ode4C11.text = "Οἱ τοῦ σώματος ἔρωτες, ἔρωτι τῷ θείῳ ὑποχωρήσαντες, καταγώγια τοῦ Πνεύματος, τὰς Χριστοῦ Παρθένους ἀπειργάσαντο."

meMA.Ode4C12.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C12.mode = misc_gr_GR_cog.Mode8
meMA.Ode4C12.melody = he.h.m8_gr_GR_cog.EisakikoaKyrie.name
meMA.Ode4C12.incipit = "Συμφωνίᾳ θεόφρονι, τοῖς ὑπεναντίοις ἀντιταξάμεναι…"
meMA.Ode4C12.text = "Συμφωνίᾳ θεόφρονι, τοῖς ὑπεναντίοις ἀντιταξάμεναι, νίκην θείαν ἀπηνέγκασθε, καὶ στεφάνων θείων ἠξιώθητε."

meMA.Ode4C13.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C13.mode = misc_gr_GR_cog.Mode8
meMA.Ode4C13.melody = he.h.m8_gr_GR_cog.EisakikoaKyrie.name
meMA.Ode4C13.incipit = "Ὁ χορὸς ὁ μακάριος, τῶν…"
meMA.Ode4C13.text = "Ὁ χορὸς ὁ μακάριος, τῶν ἀειπαρθένων πυρὶ ὀπτώμενος, εὐωδίᾳ προσενήνεκται, τῷ ἐκ τῆς Παρθένου ἀνατείλαντι."

meMA.Ode4C14.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C14.mode = misc_gr_GR_cog.Mode8
meMA.Ode4C14.melody = he.h.m8_gr_GR_cog.EisakikoaKyrie.name
meMA.Ode4C14.incipit = "Ἱερῶς ὥσπερ ἄκακοι, ἤγεσθε ἀμνάδες…"
meMA.Ode4C14.text = "Ἱερῶς ὥσπερ ἄκακοι, ἤγεσθε ἀμνάδες θύεσθαι πάνσεμνοι, οὐκ ἐρίζουσαι, οὐ κράζουσαι, ἀλλὰ προαιρέσει θανατούμεναι."

meMA.Ode4C1T.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C1T.mode = misc_gr_GR_cog.Mode8
meMA.Ode4C1T.melody = he.h.m8_gr_GR_cog.EisakikoaKyrie.name
meMA.Ode4C1T.incipit = "Σωματούμενον Κύριον, ἔτεκες Κυρία πάσης…"
meMA.Ode4C1T.text = "Σωματούμενον Κύριον, ἔτεκες Κυρία πάσης τῆς κτίσεως, ὃν ἱκέτευε ῥυσθῆναί με, ἐχθροῦ Παναγία κυριεύοντος."

meMA.Ode5C1H.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C1H.mode = misc_gr_GR_cog.Mode8
meMA.Ode5C1H.melody = he.h.m8_gr_GR_cog.FotisonImas.melody
meMA.Ode5C1H.incipit = he.h.m8_gr_GR_cog.FotisonImas.incipit
meMA.Ode5C1H.text = he.h.m8_gr_GR_cog.FotisonImas.text

meMA.Ode5C11.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C11.mode = misc_gr_GR_cog.Mode8
meMA.Ode5C11.melody = he.h.m8_gr_GR_cog.FotisonImas.name
meMA.Ode5C11.incipit = "Ὤρθρισαν ὁμοῦ, πρὸς Χριστὸν τὸ…"
meMA.Ode5C11.text = "Ὤρθρισαν ὁμοῦ, πρὸς Χριστὸν τὸ φῶς τὸ ἄδυτον, αἱ Παρθένοι αἱ θεόφρονες, καὶ ὑπὲρ φέγγος, τοῦ ἡλίου κατηυγάσθησαν."

meMA.Ode5C12.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C12.mode = misc_gr_GR_cog.Mode8
meMA.Ode5C12.melody = he.h.m8_gr_GR_cog.FotisonImas.name
meMA.Ode5C12.incipit = "Μέλη ἀνδρικῶς, ἐκδεδώκατε πρὸς βάσανα…"
meMA.Ode5C12.text = "Μέλη ἀνδρικῶς, ἐκδεδώκατε πρὸς βάσανα, βασανίσασαι τὸν τύραννον, τῇ καρτερίᾳ, ἀξιάγαστοι τῆς πίστεως."

meMA.Ode5C13.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C13.mode = misc_gr_GR_cog.Mode8
meMA.Ode5C13.melody = he.h.m8_gr_GR_cog.FotisonImas.name
meMA.Ode5C13.incipit = "Ἄνθος ἱερόν, τῶν Μαρτύρων ἀνεδείχθητε…"
meMA.Ode5C13.text = "Ἄνθος ἱερόν, τῶν Μαρτύρων ἀνεδείχθητε, εὐωδίαν θείας γνώσεως, τῇ Ἐκκλησίᾳ διαπνέουσαι πανεύφημοι."

meMA.Ode5C14.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C14.mode = misc_gr_GR_cog.Mode8
meMA.Ode5C14.melody = he.h.m8_gr_GR_cog.FotisonImas.name
meMA.Ode5C14.incipit = "Ῥήμασιν ὑμῶν, ὁ μεγάλαυχος πεφίμωται…"
meMA.Ode5C14.text = "Ῥήμασιν ὑμῶν, ὁ μεγάλαυχος πεφίμωται, καρτερίᾳ δὲ νενίκηται, καὶ ἀφανείᾳ, παρεπέμφθη παναοίδιμοι."

meMA.Ode5C1T.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C1T.mode = misc_gr_GR_cog.Mode8
meMA.Ode5C1T.melody = he.h.m8_gr_GR_cog.FotisonImas.name
meMA.Ode5C1T.incipit = "Τέλος μοι χρηστόν, προσγενέσθαι Κόρη…"
meMA.Ode5C1T.text = "Τέλος μοι χρηστόν, προσγενέσθαι Κόρη πρέσβευε, ὅπως φύγω τὰς μενούσας με, ἀτελευτήτους, τιμωρίας καὶ σωθήσομαι."

meMA.Ode6C1H.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C1H.mode = misc_gr_GR_cog.Mode8
meMA.Ode6C1H.melody = he.h.m8_gr_GR_cog.TinDeisinEkcheo.melody
meMA.Ode6C1H.incipit = he.h.m8_gr_GR_cog.TinDeisinEkcheo.incipit
meMA.Ode6C1H.text = he.h.m8_gr_GR_cog.TinDeisinEkcheo.text

meMA.Ode6C11.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C11.mode = misc_gr_GR_cog.Mode8
meMA.Ode6C11.melody = he.h.m8_gr_GR_cog.TinDeisinEkcheo.name
meMA.Ode6C11.incipit = "Ὑπέμειναν τῆς σαρκὸς τὰς ὀδύνας…"
meMA.Ode6C11.text = "Ὑπέμειναν τῆς σαρκὸς τὰς ὀδύνας, καὶ πυρὸς τὰς τιμωρίας ἀνδρείως, αἱ τοῦ Χριστοῦ, ἀθληφόροι καὶ νύμφαι, ἡ Χιονία Ἀγάπη Εἰρήνη τε, καὶ στέφανον τὸν εὐπρεπῆ, καὶ τὴν ἄῤῥητον δόξαν ἀπέλαβον."

meMA.Ode6C12.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C12.mode = misc_gr_GR_cog.Mode8
meMA.Ode6C12.melody = he.h.m8_gr_GR_cog.TinDeisinEkcheo.name
meMA.Ode6C12.incipit = "Συρόμεναι ἐπὶ γῆς ὥσπερ λίθοι…"
meMA.Ode6C12.text = "Συρόμεναι ἐπὶ γῆς ὥσπερ λίθοι, κατεστρέψατε Παρθένοι τῆς πλάνης, οἰκοδομήν, πᾶσαν σθένει τοῦ Λόγου, καὶ πρὸς ναὸν ἐνεχθεῖσαι τὸν ἅγιον, παρίστασθε περιχαρῶς, τῷ δεσπόζοντι πάσης τῆς Κτίσεως."

meMA.Ode6C13.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C13.mode = misc_gr_GR_cog.Mode8
meMA.Ode6C13.melody = he.h.m8_gr_GR_cog.TinDeisinEkcheo.name
meMA.Ode6C13.incipit = "Ἰάματα ἡ σορὸς Χιονίας, καὶ…"
meMA.Ode6C13.text = "Ἰάματα ἡ σορὸς Χιονίας, καὶ Εἰρήνης καὶ Ἀγάπης πηγάζει, ἐκ τῶν πηγῶν, ἀρδομένη πλουσίως, τοῦ Παρακλήτου, καὶ παύει νοσήματα, ποτίζει δὲ τῶν εὐσεβῶς, προσιόντων καρδίας ἐν χάριτι."

meMA.Ode6C1T.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C1T.mode = misc_gr_GR_cog.Mode8
meMA.Ode6C1T.melody = he.h.m8_gr_GR_cog.TinDeisinEkcheo.name
meMA.Ode6C1T.incipit = "Νεκρώσεως δερματίνους χιτῶνας, ὁ Ἀδὰμ…"
meMA.Ode6C1T.text = "Νεκρώσεως δερματίνους χιτῶνας, ὁ Ἀδὰμ ἀποβαλὼν τῇ σαρκώσει, τοῦ σοῦ Υἱοῦ, Θεοτόκε Παρθένε, καταστολὴν θείας δόξης ἐνδέδυται, δοξάζων σε περιχαρῶς, ὡς Μητέρα Θεοῦ παναμώμητον."

meMA.Kontakion1.poet = poets_gr_GR_cog.Gerasimou
meMA.Kontakion1.mode = misc_gr_GR_cog.Mode3
meMA.Kontakion1.melody = he.a.m3_gr_GR_cog.IParthenosSimeron.name
meMA.Kontakion1.incipit = "Παρθενίας ἔσοπτρα, φωτοειδῆ πεφυκυῖαι, νοερῶς…"
meMA.Kontakion1.text = "Παρθενίας ἔσοπτρα, φωτοειδῆ πεφυκυῖαι, νοερῶς ἠστράψατε, ἀθλητικὰς λαμπηδόνας, πᾶσαν μέν, τὴν Ἐκκλησίαν ἀγλαϊζούσας, νύκτα δέ, τῶν νοσημάτων ἀπελαυνούσας, Χιονία καὶ Ἀγάπη, σὺν τῇ Εἰρήνῃ, Χριστοῦ κειμήλια."

meMA.Ode7C1H.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C1H.mode = misc_gr_GR_cog.Mode8
meMA.Ode7C1H.melody = he.h.m8_gr_GR_cog.PaidesEvraion.melody
meMA.Ode7C1H.incipit = he.h.m8_gr_GR_cog.PaidesEvraion.incipit
meMA.Ode7C1H.text = he.h.m8_gr_GR_cog.PaidesEvraion.text

meMA.Ode7C11.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C11.mode = misc_gr_GR_cog.Mode8
meMA.Ode7C11.melody = he.h.m8_gr_GR_cog.PaidesEvraion.name
meMA.Ode7C11.incipit = "Θῆρας ἀγρίους ἐπιόντας, οὐ παφλάζουσαν…"
meMA.Ode7C11.text = "Θῆρας ἀγρίους ἐπιόντας, οὐ παφλάζουσαν ὁρμὴν πυρὸς ἀστέκτου, οὐ μελῶν ἐκτομάς, οὐ πόνους πολυτρόπους, αἱ τοῦ Χριστοῦ νεάνιδες, ἐδειλίασαν ἐμφρόνως"

meMA.Ode7C12.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C12.mode = misc_gr_GR_cog.Mode8
meMA.Ode7C12.melody = he.h.m8_gr_GR_cog.PaidesEvraion.name
meMA.Ode7C12.incipit = "Ἔχουσαι ὄμματα καρδίας, πρὸς τὸν…"
meMA.Ode7C12.text = "Ἔχουσαι ὄμματα καρδίας, πρὸς τὸν σῴζοντα Θεὸν καὶ Βασιλέα, αἱ Ἀμνάδες Χριστοῦ, ἐχώρησαν πρὸς πᾶσαν, τοῦ δυσμενοῦς παράταξιν, καὶ ἐνίκησαν ἀνδρείως."

meMA.Ode7C13.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C13.mode = misc_gr_GR_cog.Mode8
meMA.Ode7C13.melody = he.h.m8_gr_GR_cog.PaidesEvraion.name
meMA.Ode7C13.incipit = "Ἵλεως ἔσο τοῖς σοῖς δούλοις…"
meMA.Ode7C13.text = "Ἵλεως ἔσο τοῖς σοῖς δούλοις, Ὑπεράγαθε πρεσβείαις Χιονίας, καὶ Εἰρήνης σεμνῆς, καὶ τῆς σεπτῆς Ἀγάπης, ἁμαρτιῶν συγχώρησιν, δι' αὐτῶν ἡμῖν παρέχων."

meMA.Ode7C14.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C14.mode = misc_gr_GR_cog.Mode8
meMA.Ode7C14.melody = he.h.m8_gr_GR_cog.PaidesEvraion.name
meMA.Ode7C14.incipit = "Ὅλον τὸν νοῦν πρὸς τὸν…"
meMA.Ode7C14.text = "Ὅλον τὸν νοῦν πρὸς τὸν νυμφίον, ἀνατείνασαι Χριστὸν αἱ Ἀθληφόροι ὡς τρυφὰς τὰς πικράς, ἡγήσαντο βασάνους· Εὐλογητὸς εἶ κράζουσαι, ὁ Θεὸς εἰς τοὺς αἰῶνας."

meMA.Ode7C1T.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C1T.mode = misc_gr_GR_cog.Mode8
meMA.Ode7C1T.melody = he.h.m8_gr_GR_cog.PaidesEvraion.name
meMA.Ode7C1T.incipit = "Νέκρωσον Ἄχραντε τὴν ζῶσαν, καὶ…"
meMA.Ode7C1T.text = "Νέκρωσον Ἄχραντε τὴν ζῶσαν, καὶ νεκροῦσαν τὴν ψυχήν μου ἁμαρτίαν, καὶ ζωῆς μετασχεῖν, ἀξίωσόν με θείας, ἡ τὴν χαρὰν κυήσασα, καὶ νεκρώσασα τὸν ὄφιν."

meMA.Ode8C1H.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C1H.mode = misc_gr_GR_cog.Mode8
meMA.Ode8C1H.melody = he.h.m8_gr_GR_cog.OiTheorrimonesPaides.melody
meMA.Ode8C1H.incipit = he.h.m8_gr_GR_cog.OiTheorrimonesPaides.incipit
meMA.Ode8C1H.text = he.h.m8_gr_GR_cog.OiTheorrimonesPaides.text

meMA.Ode8C11.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C11.mode = misc_gr_GR_cog.Mode8
meMA.Ode8C11.melody = he.h.m8_gr_GR_cog.OiTheorrimonesPaides.name
meMA.Ode8C11.incipit = "Μεμυημέναι τὰ θεῖα παραδόξως, ἐν…"
meMA.Ode8C11.text = "Μεμυημέναι τὰ θεῖα παραδόξως, ἐν τῷ καιρῷ τῶν ἀγώνων, γνώμην ἀνδρείαν ἀνέλαβον, τοῦ Χριστοῦ αἱ Ἀμνάδες, στεῤῥῶς ἐναθλήσασαι."

meMA.Ode8C12.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C12.mode = misc_gr_GR_cog.Mode8
meMA.Ode8C12.melody = he.h.m8_gr_GR_cog.OiTheorrimonesPaides.name
meMA.Ode8C12.incipit = "Ἐσκοτισμένος τῇ μέθῃ ὁ διώκτης…"
meMA.Ode8C12.text = "Ἐσκοτισμένος τῇ μέθῃ ὁ διώκτης, τῆς ζοφερᾶς ἁμαρτίας, γέλωτ' ὀφλήσας ἐφαίνετο, αἱ Χριστοῦ δὲ Παρθένοι, ὡς φῶς ἀπεδείκνυντο."

meMA.Ode8C13.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C13.mode = misc_gr_GR_cog.Mode8
meMA.Ode8C13.melody = he.h.m8_gr_GR_cog.OiTheorrimonesPaides.name
meMA.Ode8C13.incipit = "Λύρα φωνοῦσα προφρόνως θεῖον μέλος…"
meMA.Ode8C13.text = "Λύρα φωνοῦσα προφρόνως θεῖον μέλος, ὁμολογίας ἐνθέου, καὶ μαρτυρίας καὶ πίστεως, ἀνεδείχθητε ὄντως, παρθένοι Νεάνιδες."

meMA.Ode8C14.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C14.mode = misc_gr_GR_cog.Mode8
meMA.Ode8C14.melody = he.h.m8_gr_GR_cog.OiTheorrimonesPaides.name
meMA.Ode8C14.incipit = "Ὀχυρωθεῖσαι τῷ πόθῳ τοῦ Δεσπότου…"
meMA.Ode8C14.text = "Ὀχυρωθεῖσαι τῷ πόθῳ τοῦ Δεσπότου, τὰ ὀχυρώματα πάντα, τοῦ διαβόλου καθείλετε, Ἀθληφόροι Κυρίου, καὶ νῦν μακαρίζεσθε."

meMA.Ode8C1T.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C1T.mode = misc_gr_GR_cog.Mode8
meMA.Ode8C1T.melody = he.h.m8_gr_GR_cog.OiTheorrimonesPaides.name
meMA.Ode8C1T.incipit = "Σύμβολα φέρουσα πάλαι τοῦ σοῦ…"
meMA.Ode8C1T.text = "Σύμβολα φέρουσα πάλαι τοῦ σοῦ τόκου, τοὺς νεανίας οὐ φλέγει, Παρθενομῆτορ ἡ κάμινος, ὡς οὐδὲ τὴν νηδύν σου, τὸ πῦρ τῆς Θεότητος."

meMA.Ode9C1H.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C1H.mode = misc_gr_GR_cog.Mode8
meMA.Ode9C1H.melody = he.h.m8_gr_GR_cog.KyriosTheotokon.melody
meMA.Ode9C1H.incipit = he.h.m8_gr_GR_cog.KyriosTheotokon.incipit
meMA.Ode9C1H.text = he.h.m8_gr_GR_cog.KyriosTheotokon.text

meMA.Ode9C11.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C11.mode = misc_gr_GR_cog.Mode8
meMA.Ode9C11.melody = he.h.m8_gr_GR_cog.KyriosTheotokon.name
meMA.Ode9C11.incipit = "Ἰθύνθητε Παρθένοι, θείους πρὸς λιμένας…"
meMA.Ode9C11.text = "Ἰθύνθητε Παρθένοι, θείους πρὸς λιμένας, τῇ κυβερνήσει Χριστοῦ τοῦ Θεοῦ ἡμῶν, τῶν πειρασμῶν παρελθοῦσαι ἄγρια κύματα."

meMA.Ode9C12.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C12.mode = misc_gr_GR_cog.Mode8
meMA.Ode9C12.melody = he.h.m8_gr_GR_cog.KyriosTheotokon.name
meMA.Ode9C12.incipit = "Ὡς ἄστρα ὡς λαμπάδες, φαίνουσαι…"
meMA.Ode9C12.text = "Ὡς ἄστρα ὡς λαμπάδες, φαίνουσαι Εἰρήνη, σὺν Χιονίᾳ Ἀγάπῃ φωτίζουσι, τῆς Ἐκκλησιας πλουσίως ἅπαν τὸ πλήρωμα."

meMA.Ode9C13.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C13.mode = misc_gr_GR_cog.Mode8
meMA.Ode9C13.melody = he.h.m8_gr_GR_cog.KyriosTheotokon.name
meMA.Ode9C13.incipit = "Συνήφθητε χορείαις, ταῖς τῶν Ἀσωμάτων…"
meMA.Ode9C13.text = "Συνήφθητε χορείαις, ταῖς τῶν Ἀσωμάτων, τοὺς ἀσωμάτους ἐχθροὺς μετὰ σώματος, περιφανῶς, Ἀθληφόροι καταπαλαίσασαι."

meMA.Ode9C14.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C14.mode = misc_gr_GR_cog.Mode8
meMA.Ode9C14.melody = he.h.m8_gr_GR_cog.KyriosTheotokon.name
meMA.Ode9C14.incipit = "Ἡλίου λαμπροτέρα, φαίνουσα ἡ μνήμη…"
meMA.Ode9C14.text = "Ἡλίου λαμπροτέρα, φαίνουσα ἡ μνήμη, ἡμῶν φωτίζει πιστῶν ἅπαν πλήρωμα, ἣν ἐτησίως τιμῶμεν Παρθενομάρτυρες."

meMA.Ode9C1T.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C1T.mode = misc_gr_GR_cog.Mode8
meMA.Ode9C1T.melody = he.h.m8_gr_GR_cog.KyriosTheotokon.name
meMA.Ode9C1T.incipit = "Φιλάγαθε Παρθένε, τὴν κεκακωμένην, τῇ…"
meMA.Ode9C1T.text = "Φιλάγαθε Παρθένε, τὴν κεκακωμένην, τῇ ἁμαρτίᾳ ψυχήν μου ἀγάθυνον, ἡ τὸν πανάγαθον Λόγον ἀποκυήσασα."

