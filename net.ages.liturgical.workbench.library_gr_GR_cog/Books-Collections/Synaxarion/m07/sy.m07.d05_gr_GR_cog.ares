S02.couplet1.title = 
S02.couplet1.verse1 = "Ἄρδων ἐλαίῳ Λαμπαδὸς τὴν λαμπάδα "
S02.couplet1.verse2 = "Ἕτοιμος ἐγγίσαντος ἦν τοῦ Νυμφίου."
