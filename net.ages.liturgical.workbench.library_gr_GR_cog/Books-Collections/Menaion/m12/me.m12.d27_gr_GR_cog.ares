meMA.Ode1C11.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C11.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode1C11.melody = he.h.m5_gr_GR_cog.heHE.IpponKaiAnavatin.name
meMA.Ode1C11.incipit = "Δεῦτε Στέφανον ὕμνοις, τὸν πρωτομάρτυρα…"
meMA.Ode1C11.text = "Δεῦτε Στέφανον ὕμνοις, τὸν πρωτομάρτυρα, τῆς Χριστοῦ Ἐκκλησίας, ἐνδόξως στεφανώσωμεν, καὶ θείᾳ στεφόμενοι, τοῦ Στεφάνου χάριτι, ἐπινίκιον ὕμνον ᾄσωμεν."

meMA.Ode1C12.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C12.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode1C12.melody = he.h.m5_gr_GR_cog.heHE.IpponKaiAnavatin.name
meMA.Ode1C12.incipit = "Φέρων τοῦ Διδασκάλου, τὸ πρᾶον…"
meMA.Ode1C12.text = "Φέρων τοῦ Διδασκάλου, τὸ πρᾶον φρόνημα, καὶ τὴν ἐκ τῆς ἀγάπης, ὑψοποιὸν ταπείνωσιν, ἀξίως ὁ Στέφανος, Διακόνων πρόκριτος, καὶ προστάτης χηρῶν γεγένηται."

meMA.Ode1C13.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C13.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode1C13.melody = he.h.m5_gr_GR_cog.heHE.IpponKaiAnavatin.name
meMA.Ode1C13.incipit = "Σὺ πρὸς ἐπικουρίαν, τῶν Ἀποστόλων…"
meMA.Ode1C13.text = "Σὺ πρὸς ἐπικουρίαν, τῶν Ἀποστόλων Χριστοῦ, ἐπαξίως ἐκλήθης, καὶ ὡς πιστὸς Διάκονος, φερώνυμε Στέφανε, χρηματίσας ἔνθα Χριστός, διʼ αἵματος μεταβέβηκας."

meMA.Ode1C1T.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C1T.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode1C1T.melody = he.h.m5_gr_GR_cog.heHE.IpponKaiAnavatin.name
meMA.Ode1C1T.incipit = "Σὺ μὲν ὑπὲρ ἀνθρώπων, Χριστὲ…"
meMA.Ode1C1T.text = "Σὺ μὲν ὑπὲρ ἀνθρώπων, Χριστὲ τὸν ἄνθρωπον, ἐκ Παρθένου φορέσας, ὡς βρέφος ἐσπαργάνωσαι· ὁ σὸς δὲ χωννύμενος, Πρωτομάρτυς λίθων βολαῖς, τὸν ἄνθρωπον ἀποδύεται."

meMA.Ode1C21.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C21.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode1C21.melody = he.h.m4_gr_GR_cog.heHE.ThalassisToErythraion.name
meMA.Ode1C21.incipit = "Τὸν τίτλον τῆς σῆς τιμίας…"
meMA.Ode1C21.text = "Τὸν τίτλον τῆς σῆς τιμίας ὄψεως, ἰδόντα παραχωρεῖ, τὰ Χερουβὶμ τοῦ ξύλου τῆς ζωῆς· ἡ φλογίνη ῥομφαία δέ, πανευλαβῶς τὰ νῶτά σοι, δίδωσι πάνσοφε Θεόδωρε."

meMA.Ode1C22.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C22.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode1C22.melody = he.h.m4_gr_GR_cog.heHE.ThalassisToErythraion.name
meMA.Ode1C22.incipit = "Ὁ τίτλος τῇ κορυφῇ ἐπέκειτο…"
meMA.Ode1C22.text = "Ὁ τίτλος τῇ κορυφῇ ἐπέκειτο, τοῦ Βασιλέως Χριστοῦ, ἐγεγγραμμένος ξύλῳ τοῦ Σταυροῦ, ὁ σὸς τίτλος δὲ Πάνσοφε, ὄψει τῇ σῇ κεκόλαπται, κεχαραγμένος εὐπρεπέστατα."

meMA.Ode1C23.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C23.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode1C23.melody = he.h.m4_gr_GR_cog.heHE.ThalassisToErythraion.name
meMA.Ode1C23.incipit = "Ὑψώθη ἐπὶ Σταυροῦ μὲν Κύριος…"
meMA.Ode1C23.text = "Ὑψώθη ἐπὶ Σταυροῦ μὲν Κύριος, καὶ διενύγη πλευράν, σὺ δὲ ταθεὶς ἐν καμίνῳ καρτερῶς, τὰς κεντήσεις ὑπέμεινας, ὥσπερ κριὸς ἐπίσημος, Ἱερομύστα σφραγιζόμενος."

meMA.Ode1C24.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C24.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode1C24.melody = he.h.m4_gr_GR_cog.heHE.ThalassisToErythraion.name
meMA.Ode1C24.incipit = "Σοφίᾳ παντοδαπῇ κοσμούμενος, Πάτερ Θεόδωρε…"
meMA.Ode1C24.text = "Σοφίᾳ παντοδαπῇ κοσμούμενος, Πάτερ Θεόδωρε, τὸν πρωτοστάτην ἤλεγξας σαφῶς, τῆς κακίστης αἱρέσεως, ὑπομονῇ τῶν ἔργων δέ, τούτου τὰς φρένας κατεπάτησας."

meMA.Ode1C2T.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C2T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode1C2T.melody = he.h.m4_gr_GR_cog.heHE.ThalassisToErythraion.name
meMA.Ode1C2T.incipit = "Σοφίας τῆς ὑπερσόφου γέγονας, δοχεῖον…"
meMA.Ode1C2T.text = "Σοφίας τῆς ὑπερσόφου γέγονας, δοχεῖον Πάναγνε, Θεογεννῆτορ μόνη τῶν πιστῶν, σωτηρία καὶ ὕμνησις· σὺ γὰρ ἡμῖν γεγέννηκας, τὴν πάντων θείαν ἀπολύτρωσιν."

meMA.Ode3C11.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C11.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode3C11.melody = he.h.m5_gr_GR_cog.heHE.OPixasEpOudenos.name
meMA.Ode3C11.incipit = "Νομίμως ὡς Χριστοῦ, στρατιώτης ὁ…"
meMA.Ode3C11.text = "Νομίμως ὡς Χριστοῦ, στρατιώτης ὁ Στέφανος, πρὸς τοὺς θεοκτόνους παραταξάμενος, τὸ ἀκαταγώνιστον αὐτοῦ, σθένος ἐνδεδυμένος, τῶν παρανόμων ἐθριάμβευσεν, ἅπασαν τὴν βλάσφημον αἵρεσιν."

meMA.Ode3C12.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C12.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode3C12.melody = he.h.m5_gr_GR_cog.heHE.OPixasEpOudenos.name
meMA.Ode3C12.incipit = "Τῷ ζήλῳ τῆς Χριστοῦ, πυρωθεὶς…"
meMA.Ode3C12.text = "Τῷ ζήλῳ τῆς Χριστοῦ, πυρωθεὶς ἀγαπήσεως, καὶ πρὸς τοὺς ἀγῶνας ἀποδυσάμενος, πίστεως καὶ Πνεύματος πλησθείς, θείου ὁ Πρωτομάρτυς, τῶν Ἁλιέων ἐβεβαίωσε, πᾶσι τὸ θεόκριτον κήρυγμα."

meMA.Ode3C13.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C13.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode3C13.melody = he.h.m5_gr_GR_cog.heHE.OPixasEpOudenos.name
meMA.Ode3C13.incipit = "Ὡς κήρυξ τῆς κεκρυμμένης, ὄντως…"
meMA.Ode3C13.text = "Ὡς κήρυξ τῆς κεκρυμμένης, ὄντως καὶ θείας ζωῆς, τῆς παρούσης ὥς περ ἐπιλαθόμενος, ἔργῳ διαδείκνυσι σαφῶς, πᾶσιν ὁ Πρωτομάρτυς, τροπαιοφόρον τὴν ἀλήθειαν, ἄριστα προκρίνας τὸν θάνατον."

meMA.Ode3C1T.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C1T.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode3C1T.melody = he.h.m5_gr_GR_cog.heHE.OPixasEpOudenos.name
meMA.Ode3C1T.incipit = "Τοῦ βίου διʼ ἡμᾶς, ὁ…"
meMA.Ode3C1T.text = "Τοῦ βίου διʼ ἡμᾶς, ὁ Δεσπότης τὸ σπήλαιον, ὑπεισῆλθε τρόπῳ συγκαταβάσεως, ὁ δὲ τῶν Μαρτύρων ἀρχηγός, Στέφανος τὴν σπιλάδα, τὴν ἀνθρωπίνην ὑπεξέρχεται, πόθῳ τοῦ Δεσπότου βαλλόμενος."

meMA.Ode3C21.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C21.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C21.melody = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.name
meMA.Ode3C21.incipit = "Οὐκ ἔχων ὅ,τι ταῖς σαῖς…"
meMA.Ode3C21.text = "Οὐκ ἔχων ὅ,τι ταῖς σαῖς, διδασκαλίαις ἀντειπεῖν Ὅσιε, ἐπʼ αἰκισμοὺς στρέφεται, ὁ βαρβαρικῆς βίας ἔμπλεως."

meMA.Ode3C22.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C22.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C22.melody = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.name
meMA.Ode3C22.incipit = "Ὑπέστης καρτερικῶς, τὰς ἐπὶ νῶτον…"
meMA.Ode3C22.text = "Ὑπέστης καρτερικῶς, τὰς ἐπὶ νῶτον καὶ στηθῶν μάστιγας, διὰ Χριστὸν θεόπνευστε, καὶ τὰ ἐπὶ κόῤῥης ῥαπίσματα."

meMA.Ode3C23.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C23.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C23.melody = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.name
meMA.Ode3C23.incipit = "Σοφίᾳ πνευματικῇ, καὶ προθυμίᾳ λογισμοῦ…"
meMA.Ode3C23.text = "Σοφίᾳ πνευματικῇ, καὶ προθυμίᾳ λογισμοῦ ᾔσχυνας, τὸν δυσσεβῆ Λέοντα, τοῖς ἱερωτάτοις σου δόγμασιν."

meMA.Ode3C24.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C24.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C24.melody = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.name
meMA.Ode3C24.incipit = "Ἀθραύστοις ὑπομοναῖς, καὶ καρτερίᾳ τῶν…"
meMA.Ode3C24.text = "Ἀθραύστοις ὑπομοναῖς, καὶ καρτερίᾳ τῶν δεινῶν ἔθραυσας, τὴν μανικὴν Ὅσιε, βδέλλαν τῇ μαχαίρᾳ τοῦ Πνεύματος."

meMA.Ode3C2T.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C2T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C2T.melody = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.name
meMA.Ode3C2T.incipit = "Δαυῒδ καθὰ κιβωτόν, ἐν Βηθλεὲμ…"
meMA.Ode3C2T.text = "Δαυῒδ καθὰ κιβωτόν, ἐν Βηθλεὲμ προφητικοῖς ὄμμασι, Μήτηρ Θεοῦ βλέπει σε, βρέφος τὸν ὑπέρθεον φέρουσαν."

meMA.Ode3Kathisma1.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode3Kathisma1.melody = he.a.m1_gr_GR_cog.heAU.TonTafonSouSotir.name
meMA.Ode3Kathisma1.incipit = "Ἀπόστολε Χριστοῦ, Διακόνων ὁ πρῶτος…"
meMA.Ode3Kathisma1.text = "Ἀπόστολε Χριστοῦ, Διακόνων ὁ πρῶτος, Πρωτόαθλε σοφέ, τῶν Μαρτύρων ἀκρότης, ὁ κόσμου τὰ πέρατα, ἁγιάσας τοῖς ἄθλοις σου, καὶ τοῖς θαύμασι, ψυχὰς ἀνθρώπων λαμπρύνας, τους τιμῶντάς σε, ῥῦσαι παντοίων κινδύνων, πανεύφημε Στέφανε."

meMA.Ode3Kathisma2.mode = miscellanea_gr_GR_cog.misc.Mode3
meMA.Ode3Kathisma2.melody = he.a.m3_gr_GR_cog.heAU.TheiasPisteos.name
meMA.Ode3Kathisma2.incipit = "Στόμα πέφηνας τῆς παῤῥησίας, θράσος…"
meMA.Ode3Kathisma2.text = "Στόμα πέφηνας τῆς παῤῥησίας, θράσος τύραννον, μεγαλοφρόνως, καταισχύνας καρτερίᾳ τῶν ἄθλων σου· ἐκκεντηθεὶς γὰρ τὰς ὄψεις Θεόδωρε, διὰ Χριστοῦ τὴν εἰκόνα τὴν πάνσεπτον, ἐβεβαίωσας, τιμᾶν καὶ δοξάζειν ἅπαντας, αἰτούμενος ἡμῖν τὸ μέγα ἔλεος."

meMA.Ode3Kathisma3.mode = miscellanea_gr_GR_cog.misc.Mode3
meMA.Ode3Kathisma3.melody = he.a.m3_gr_GR_cog.heAU.TinOraiotita.name
meMA.Ode3Kathisma3.incipit = "Θαῦμα παράδοξον, γέγονε σήμερον· ὁ…"
meMA.Ode3Kathisma3.text = "Θαῦμα παράδοξον, γέγονε σήμερον· ὁ γὰρ Σωτὴρ ἡμῶν, ἐν τῷ Σπηλαίῳ σαρκί, ὤφθη ἐκ Παρθένου διʼ ἡμᾶς, καθὼς αὐτὸς ἐπίσταται. Μάγοι μετὰ δώρων δέ, ὡς Βασιλεῖ προσεκύνησαν. Ποιμένες μέτʼ Ἀγγέλων τε, ἐδοξολόγουν αὐτόν· μεθʼ ὧν καὶ ἡμεῖς βοῶμεν αὐτῷ. Δόξα τῷ διʼ ἡμᾶς ἐνανθρωπήσαντι."

meMA.Ode4C11.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C11.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode4C11.melody = he.h.m5_gr_GR_cog.heHE.TinTheianEnnoisas.name
meMA.Ode4C11.incipit = "Κατήγορος κριτής τε καὶ ἄδικος…"
meMA.Ode4C11.text = "Κατήγορος κριτής τε καὶ ἄδικος, ὁ βροτοκτόνος καὶ πατήρ, τοῦ ψεύδους Στέφανε δείκνυται, ἀλλʼ ἑαυτὸν ἀνατρέπει, τοὺς βρόχους καθʼ ἑαυτοῦ τεκτηνάμενος."

meMA.Ode4C12.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C12.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode4C12.melody = he.h.m5_gr_GR_cog.heHE.TinTheianEnnoisas.name
meMA.Ode4C12.incipit = "Ὁ μέγας τῆς ἀθλήσεως πρόβολος…"
meMA.Ode4C12.text = "Ὁ μέγας τῆς ἀθλήσεως πρόβολος, ἀντʼ οὐδενὸς τοῦ δυσμενοῦς, τυράννου κρίνας τὴν ἔφοδον, καταπαλαίει ἀνδρείως, ἐντέχνως ὑποβὰς τὸν ἀντίπαλον."

meMA.Ode4C13.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C13.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode4C13.melody = he.h.m5_gr_GR_cog.heHE.TinTheianEnnoisas.name
meMA.Ode4C13.incipit = "Ὡς πρῶτος τῶν Μαρτύρων ὁ…"
meMA.Ode4C13.text = "Ὡς πρῶτος τῶν Μαρτύρων ὁ Στέφανος, ὁδοποιήσας ἐν ἡμῖν, τοῦ Μαρτυρίου τὴν εἴσοδον, στεφανηφόρος χορεύει, τῷ στέφει τῶν Μαρτύρων στεφόμενος."

meMA.Ode4C1T.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C1T.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode4C1T.melody = he.h.m5_gr_GR_cog.heHE.TinTheianEnnoisas.name
meMA.Ode4C1T.incipit = "Ἐκλάμψας ἐκ Παρθένου ὡς Ἥλιος…"
meMA.Ode4C1T.text = "Ἐκλάμψας ἐκ Παρθένου ὡς Ἥλιος, συνανατέλλουσαν Χριστέ, ὡς Ἑωσφόρον τὴν ἔνδοξον, τοῦ Πρωτομάρτυρος μνήμην, τῇ σῇ καταγλαΐζεις φαιδρότητι."

meMA.Ode4C21.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C21.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C21.melody = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.name
meMA.Ode4C21.incipit = "Ἐφηπλωμένος σανίδι κατεκεντήθης, ὡς προσκυνῶν…"
meMA.Ode4C21.text = "Ἐφηπλωμένος σανίδι κατεκεντήθης, ὡς προσκυνῶν Θεόδωρε, Χριστοῦ τὴν εἰκόνα, καὶ τῆς Θεομήτορος, Θρᾳκὸς ἀγριότητι, καὶ βαρβαρικῇ ἰταμότητι."

meMA.Ode4C22.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C22.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C22.melody = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.name
meMA.Ode4C22.incipit = "Λειτουργήσας ἐκ νεότητος τῷ Δεσπότῃ…"
meMA.Ode4C22.text = "Λειτουργήσας ἐκ νεότητος τῷ Δεσπότῃ, καὶ πρὸς αὐτὸν ἀνένδοτον τὴν ἄφεσιν ἔχων, Μάκαρ διʼ ἀσκήσεως, Μαρτύρων ἐν αἵματι, αὖθις ἐκοσμήθης φαιδρότατα."

meMA.Ode4C23.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C23.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C23.melody = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.name
meMA.Ode4C23.incipit = "Φαεινότατος ὡς ἥλιος Θεοφόρε, ἀνατολῆς…"
meMA.Ode4C23.text = "Φαεινότατος ὡς ἥλιος Θεοφόρε, ἀνατολῆς ἐξώρμησας, ἀκτῖνας ἐκπέμπων, τῆς ὁμολογίας σου, καὶ τῆς μεγαλόφρονος, καὶ γενναιοτάτης ἐνστάσεως."

meMA.Ode4C2T.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C2T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C2T.melody = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.name
meMA.Ode4C2T.incipit = "Εὐλογημένος ὁ ἐρχόμενος Θεὸς Λόγος…"
meMA.Ode4C2T.text = "Εὐλογημένος ὁ ἐρχόμενος Θεὸς Λόγος, ἐν Βηθλεὲμ ὡς εὔσπλαγχνος, ἐξ ἀπειρογάμου, Πάναγνε κοιλίας σου, ὃν νῦν ἐκδυσώπησον, σῶσαι τοὺς πρὸς σὲ καταφεύγοντας."

meMA.Ode5C11.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C11.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode5C11.melody = he.h.m5_gr_GR_cog.heHE.OAnavallomenos.name
meMA.Ode5C11.incipit = "Ἔλαμψεν ὡς Ἄγγελος, ὁ Πρωτομάρτυς…"
meMA.Ode5C11.text = "Ἔλαμψεν ὡς Ἄγγελος, ὁ Πρωτομάρτυς σου, τῶν οὐρανίων Χριστὲ ἁψίδων, γεγονὼς ὑπέρτερος, καὶ δόξης ἀῤῥήτου, ἐμφορηθεὶς Φιλάνθρωπε."

meMA.Ode5C12.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C12.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode5C12.melody = he.h.m5_gr_GR_cog.heHE.OAnavallomenos.name
meMA.Ode5C12.incipit = "Ὡς ἀκαταγώνιστος, τοῦ Πρωτομάρτυρος, τοῦ…"
meMA.Ode5C12.text = "Ὡς ἀκαταγώνιστος, τοῦ Πρωτομάρτυρος, τοῦ θείου ζήλου ἡ παῤῥησία· καὶ γὰρ μέχρις αἵματος, πρὸς τοὺς θεοκτόνους, ἀνδρείως παρετάξατο,"

meMA.Ode5C13.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C13.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode5C13.melody = he.h.m5_gr_GR_cog.heHE.OAnavallomenos.name
meMA.Ode5C13.incipit = "Θεηγόρου στόματος, νιφάσιν ἔβαλε, τοὺς…"
meMA.Ode5C13.text = "Θεηγόρου στόματος, νιφάσιν ἔβαλε, τοὺς μιαιφόνους ὁ Πρωτομάρτυς, ὑπʼ αὐτῶν ἀπείροις δέ, τῶν λίθων νιφάσιν, ὡς νικητὴς ἐστέφετο."

meMA.Ode5C1T.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C1T.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode5C1T.melody = he.h.m5_gr_GR_cog.heHE.OAnavallomenos.name
meMA.Ode5C1T.incipit = "Τὸν ἐπιδημήσαντα, ἐξ ἄπειράνδρου Μητρός…"
meMA.Ode5C1T.text = "Τὸν ἐπιδημήσαντα, ἐξ ἄπειράνδρου Μητρός, ὁ Πρωτομάρτυς ἐν ἀκινήτῳ, τοῦ Πατρὸς Θεότητι, ἑστῶτα καὶ δόξῃ, ἐν οὐρανοῖς τεθέαται."

meMA.Ode5C21.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C21.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C21.melody = he.h.m4_gr_GR_cog.heHE.SyKyrieMouFos.name
meMA.Ode5C21.incipit = "Γῆν ξένην κατοικῶν, ξενιτείας ἐπ'…"
meMA.Ode5C21.text = "Γῆν ξένην κατοικῶν, ξενιτείας ἐπʼ ἄλγεσιν, ἀλγήματα ἐξορίας, καὶ φρουρὰς ὠμοτάτας, ὑπέστης καρτερόψυχε."

meMA.Ode5C22.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C22.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C22.melody = he.h.m4_gr_GR_cog.heHE.SyKyrieMouFos.name
meMA.Ode5C22.incipit = "Νῦν χαίρεις ἀληθῶς, νικηφόρον στεφάνωμα…"
meMA.Ode5C22.text = "Νῦν χαίρεις ἀληθῶς, νικηφόρον στεφάνωμα, δεξάμενος ἐν ἡμέρᾳ τῇ φαιδρᾷ καὶ φωσφόρῳ τοῦ θείου Πρωτομάρτυρος."

meMA.Ode5C23.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C23.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C23.melody = he.h.m4_gr_GR_cog.heHE.SyKyrieMouFos.name
meMA.Ode5C23.incipit = "Ἣν ἔσχες ἐκ παιδός, εὐσεβῶς…"
meMA.Ode5C23.text = "Ἣν ἔσχες ἐκ παιδός, εὐσεβῶς ὑπόθεσιν, ἐτήρησας μέχρι τέλους, ἀπερίτρεπτος μείνας, ἀήττητε Θεόδωρε."

meMA.Ode5C2T.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C2T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C2T.melody = he.h.m4_gr_GR_cog.heHE.SyKyrieMouFos.name
meMA.Ode5C2T.incipit = "Σὲ ὅπλον ἀῤῥαγές, κατʼ ἐχθρῶν…"
meMA.Ode5C2T.text = "Σὲ ὅπλον ἀῤῥαγές, κατʼ ἐχθρῶν προβαλλόμεθα, σὲ ἄγκυραν καὶ ἐλπίδα, τῆς ἡμῶν σωτηρίας, Θεόνυμφε κεκτήμεθα."

meMA.Ode6C11.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C11.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode6C11.melody = he.h.m5_gr_GR_cog.heHE.MainomeninKlydoni.name
meMA.Ode6C11.incipit = "Ὁ κλεινὸς ταξίαρχος τῶν Μαρτύρων…"
meMA.Ode6C11.text = "Ὁ κλεινὸς ταξίαρχος τῶν Μαρτύρων, Στέφανος πιστοί, τοὺς θεσμοὺς τῆς φύσεως τῇ χάριτι, ἐκβεβηκώς, τῇ θείᾳ δόξῃ αὐγάζεται."

meMA.Ode6C12.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C12.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode6C12.melody = he.h.m5_gr_GR_cog.heHE.MainomeninKlydoni.name
meMA.Ode6C12.incipit = "Μιμητὴς πανάριστος χρηματίσας, Δέσποτα Χριστέ…"
meMA.Ode6C12.text = "Μιμητὴς πανάριστος χρηματίσας, Δέσποτα Χριστέ, τοῦ τιμίου πάθους σου ὁ Στέφανος, τοὺς φονευτάς, διʼ εὐλογίας ἀμύνεται."

meMA.Ode6C13.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C13.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode6C13.melody = he.h.m5_gr_GR_cog.heHE.MainomeninKlydoni.name
meMA.Ode6C13.incipit = "Μιαιφόνου πράξεως, ἀμετόχους, φύλαττε Χριστέ…"
meMA.Ode6C13.text = "Μιαιφόνου πράξεως, ἀμετόχους, φύλαττε Χριστέ, καὶ τοῦ Πρωτομάρτυρος ἀξίωσον, τοὺς ὑμνητάς, τῆς κληρουχίας ὡς εὔσπλαγχνος."

meMA.Ode6C1T.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C1T.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode6C1T.melody = he.h.m5_gr_GR_cog.heHE.MainomeninKlydoni.name
meMA.Ode6C1T.incipit = "Παγκοσμίου γέγονε σωτηρίας, Δέσποτα Χριστέ…"
meMA.Ode6C1T.text = "Παγκοσμίου γέγονε σωτηρίας, Δέσποτα Χριστέ, ἀπαρχὴ ὁ τόκος σου, καὶ Μάρτυσι, θεοπρεποῦς, ὁμολογίας ὑπόθεσις."

meMA.Ode6C21.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C21.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C21.melody = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.name
meMA.Ode6C21.incipit = "Ἱδρῶτι, τῶν ἀγώνων εἰσέτι σταζόμενος…"
meMA.Ode6C21.text = "Ἱδρῶτι, τῶν ἀγώνων εἰσέτι σταζόμενος, Ἀγωνοθέτῃ παρέστης, τῷ δικαίᾳ κρίσει, πᾶσαν τὴν Κτίσιν, κυβερνῶντι, θεοφόρε παμμάκαρ Θεόδωρε."

meMA.Ode6C22.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C22.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C22.melody = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.name
meMA.Ode6C22.incipit = "Οὐδὲν σε, τῆς εἰς Χριστὸν…"
meMA.Ode6C22.text = "Οὐδὲν σε, τῆς εἰς Χριστὸν ἀγάπης ἐχώρισεν, οὐκ αἰκιζόμενον σῶμα, οὐ θανάτου ψῆφος ἀπειλουμένη, οὐ ζημία, τῶν προσόντων θεόφρον Θεόδωρε."

meMA.Ode6C23.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C23.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C23.melody = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.name
meMA.Ode6C23.incipit = "Υἱὸς ὤν, καὶ φωτὸς τῆς…"
meMA.Ode6C23.text = "Υἱὸς ὤν, καὶ φωτὸς τῆς ἡμέρας Θεόδωρε, πλήρης φωτὸς μετετέθης, πρὸς τὸ φῶς τὸ θεῖον αὐξανομένης, τῆς ἡμέρας, καὶ νυκτὸς μειουμένης θεόληπτε."

meMA.Ode6C2T.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C2T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C2T.melody = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.name
meMA.Ode6C2T.incipit = "Σὲ μόνην, τῶν ἀκανθῶν ἐν…"
meMA.Ode6C2T.text = "Σὲ μόνην, τῶν ἀκανθῶν ἐν μέσῳ εὑράμενος ὡς καθαρώτατον κρίνον, καὶ κοιλάδων ἄνθος ὦ Θεομῆτορ, ὁ νυμφίος, ἐκ τῆς γαστρός σου Λόγος προέρχεται."

meMA.Kontakion1.mode = miscellanea_gr_GR_cog.misc.Mode3
meMA.Kontakion1.melody = he.a.m3_gr_GR_cog.heAU.IParthenosSimeron.name
meMA.Kontakion1.incipit = "Ὁ Δεσπότης χθὲς ἡμῖν, διὰ…"
meMA.Kontakion1.text = "Ὁ Δεσπότης χθὲς ἡμῖν, διὰ σαρκὸς ἐπεδήμει, καὶ ὁ δοῦλος σήμερον, ἀπὸ σαρκὸς ἐξεδήμει· χθὲς μὲν γάρ, ὁ Βασιλεύων σαρκὶ ἐτέχθη, σήμερον δέ, ὁ οἰκέτης λιθοβολεῖται, διʼ αὐτὸν καὶ τελειοῦται, ὁ Πρωτομάρτυς καὶ θεῖος Στέφανος."

meMA.Oikos1.text = "Ὡς ἀστὴρ φαεινὸς σήμερον συνεξέλαμψε, τῇ Γεννήσει Χριστοῦ, ὁ Πρωτομάρτυς Στέφανος, ἀστράπτων καὶ φωτίζων τὰ πέρατα ἅπαντα, τῶν Ἰουδαίων μόνον ἠμαύρωσε τὴν πᾶσαν δυσσέβειαν, σοφίας λόγοις τούτους διελέγξας, ἀπὸ τῶν Γραφῶν διαλεγόμενος, καὶ πείθων τούτους, τὸν γεννηθέντα ἐκ τῆς Παρθένου Ἰησοῦν, Υἱὸν αὐτόν εἶναι Θεοῦ, κατῄσχυνε τούτων τὴν ἀσεβῆ κακουργίαν, ὁ Πρωτομάρτυς καὶ θεῖος Στέφανος."

meMA.Ode7C11.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C11.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode7C11.melody = he.h.m5_gr_GR_cog.heHE.OYperypsoumenos.name
meMA.Ode7C11.incipit = "Ὡς ποικίλοις ἄνθεσι, καὶ ὡραίοις…"
meMA.Ode7C11.text = "Ὡς ποικίλοις ἄνθεσι, καὶ ὡραίοις Στέφανε, τοῖς λίθοις κοσμούμενος, σαυτὸν προσενήνοχας, Χριστῷ τῷ Ζωοδότῃ, μελῳδῶν· Εὐλογητὸς εἶ."

meMA.Ode7C12.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C12.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode7C12.melody = he.h.m5_gr_GR_cog.heHE.OYperypsoumenos.name
meMA.Ode7C12.incipit = "Ὡς ἀκαταγώνιστος, ἡ τοῦ Σαύλου…"
meMA.Ode7C12.text = "Ὡς ἀκαταγώνιστος, ἡ τοῦ Σαύλου ἔνστασις, πορθοῦντος τὸ πρότερον, Χριστοῦ τοὺς θεράποντας, προσάγοντος δὲ ἔθνη, νῦν Χριστῷ εἰς τοὺς αἰῶνας."

meMA.Ode7C13.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C13.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode7C13.melody = he.h.m5_gr_GR_cog.heHE.OYperypsoumenos.name
meMA.Ode7C13.incipit = "Τοὺς ὀδόντας βρύχοντες, φονικῶς οἱ…"
meMA.Ode7C13.text = "Τοὺς ὀδόντας βρύχοντες, φονικῶς οἱ ἄνομοι, ὡς θῆρες ἁρπάσαντες, ἀνήρουν τὸν Στέφανον, ἐνθέως μελῳδοῦντα· Ὁ Θεὸς εὐλογητὸς εἶ."

meMA.Ode7C14.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C14.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode7C14.melody = he.h.m5_gr_GR_cog.heHE.OYperypsoumenos.name
meMA.Ode7C14.incipit = "Ἐπαρθεὶς τῷ Πνεύματι, τῷ ἁγίῳ…"
meMA.Ode7C14.text = "Ἐπαρθεὶς τῷ Πνεύματι, τῷ ἁγίῳ Στέφανε, ἀῤῥήτως τεθέασαι, Υἱὸν σὺν Γεννήτορι, κραυγάζων τῇ Τριάδι· Ὁ Θεὸς εὐλογητὸς εἶ."

meMA.Ode7C1T.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C1T.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode7C1T.melody = he.h.m5_gr_GR_cog.heHE.OYperypsoumenos.name
meMA.Ode7C1T.incipit = "Σοὶ τῷ ἀνατείλαντι, ἐκ Παρθένου…"
meMA.Ode7C1T.text = "Σοὶ τῷ ἀνατείλαντι, ἐκ Παρθένου θείας ἁγνῆς, προσήνεκται ἔμψυχος, ὡς Βασιλεῖ Στέφανος, ἐνθέως μελῳδῶν σοι· ὁ Θεὸς εὐλογητὸς εἶ."

meMA.Ode7C21.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C21.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C21.melody = he.h.m4_gr_GR_cog.heHE.EnTiKaminoAvramiaioi.name
meMA.Ode7C21.incipit = "Πεπυρσευμένος, τῷ ὀρθοδόξῳ ζήλῳ Πάνσοφε…"
meMA.Ode7C21.text = "Πεπυρσευμένος, τῷ ὀρθοδόξῳ ζήλῳ Πάνσοφε, πλάνην μιαρᾶς αἱρέσεως καὶ δεινῆς, θαρσαλέως ἐξηκόντισας· Εὐλογημένος εἶ, ὁ Θεός μου κραυγάζων καὶ Κύριος."

meMA.Ode7C22.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C22.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C22.melody = he.h.m4_gr_GR_cog.heHE.EnTiKaminoAvramiaioi.name
meMA.Ode7C22.incipit = "Λόγον σοφίας, λόγον δογμάτων, λόγον…"
meMA.Ode7C22.text = "Λόγον σοφίας, λόγον δογμάτων, λόγον γνώσεως, λόγον διδαγμάτων, Πάτερ πανευσεβῶν, Θεὸς Λόγος σοι δεδώρηται· Εὐλογημένος εἶ, ὁ Θεός μου βοῶντι καὶ Κύριος."

meMA.Ode7C23.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C23.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C23.melody = he.h.m4_gr_GR_cog.heHE.EnTiKaminoAvramiaioi.name
meMA.Ode7C23.incipit = "Ἐξανατείλας, ἐκ τῆς Ἑῴας ὥσπερ…"
meMA.Ode7C23.text = "Ἐξανατείλας, ἐκ τῆς Ἑῴας ὥσπερ ἥλιος, αἴγλην εὐσεβείας ἔσπειρας ἐπὶ γῆς, τῆς εἰκόνος τὴν προσκύνησιν, Χριστοῦ φωτίζουσαν, καὶ διδάσκουσαν Πάτερ Θεόδωρε."

meMA.Ode7C2T.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C2T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C2T.melody = he.h.m4_gr_GR_cog.heHE.EnTiKaminoAvramiaioi.name
meMA.Ode7C2T.incipit = "Κεκοσμημένη, δεδοξασμένη ὑπερλάμπουσα, φέγγει Παρθενίας…"
meMA.Ode7C2T.text = "Κεκοσμημένη, δεδοξασμένη ὑπερλάμπουσα, φέγγει Παρθενίας Μήτηρ Θεοῦ, μετὰ σώματος ἐγέννησας, καὶ ἐσπαργάνωσας, τὸν ὁμίχλῃ τὴν γῆν σπαργανώσαντα."

meMA.Ode8C11.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C11.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode8C11.melody = he.h.m5_gr_GR_cog.heHE.SoiToPantourgo.name
meMA.Ode8C11.incipit = "Δεῦτε νοητῶς, τῇ τοῦ Στεφάνου…"
meMA.Ode8C11.text = "Δεῦτε νοητῶς, τῇ τοῦ Στεφάνου δόξῃ, λαμπόμενοι μέλψωμεν, τῷ σαρκωθέντι Θεῷ. Πάντα τὰ ἔργα, τὸν Κύριον ὑμνεῖτε, καὶ ὑπερυψοῦτε, εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8C12.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C12.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode8C12.melody = he.h.m5_gr_GR_cog.heHE.SoiToPantourgo.name
meMA.Ode8C12.incipit = "Ὤφθη σοι Χριστός, ἐν τῇ…"
meMA.Ode8C12.text = "Ὤφθη σοι Χριστός, ἐν τῇ Πατρῴα δόξῃ, τῶν ἄθλων μηνύων σοι, σαφῶς τὰ ἔπαθλα· ὅθεν ἐβόας· Τὸν Κύριον ὑμνεῖτε, καὶ ὑπερυψοῦτε, εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8C13.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C13.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode8C13.melody = he.h.m5_gr_GR_cog.heHE.SoiToPantourgo.name
meMA.Ode8C13.incipit = "Σοὶ ὡς νικητῇ, τῆς παρανόμου…"
meMA.Ode8C13.text = "Σοὶ ὡς νικητῇ, τῆς παρανόμου πλάνης, ἀθλήσεως στέφανος, ἐπλάκη Στέφανε· ὅθεν ἐβόας· Τὸν Κύριον ὑμνεῖτε, καὶ ὑπερυψοῦτε εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8C14.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C14.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode8C14.melody = he.h.m5_gr_GR_cog.heHE.SoiToPantourgo.name
meMA.Ode8C14.incipit = "Ἔπλεξεν ἡμῖν, ὁ τοῦ Δεσπότου…"
meMA.Ode8C14.text = "Ἔπλεξεν ἡμῖν, ὁ τοῦ Δεσπότου τόκος, χορείαν ἡ μνήμη τε, τοῦ Πρωτομάρτυρος· ὅθεν ἀπαύστως, τὸν Κύριον ὑμνοῦμεν, καὶ ὑπερυψοῦμεν, εἰς πάντας τους αἰῶνας."

meMA.Ode8C1T.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C1T.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode8C1T.melody = he.h.m5_gr_GR_cog.heHE.SoiToPantourgo.name
meMA.Ode8C1T.incipit = "Σὲ τὴν ὑπὲρ νοῦν, θεανδρικῶς…"
meMA.Ode8C1T.text = "Σὲ τὴν ὑπὲρ νοῦν, θεανδρικῶς τῷ λόγῳ, τεκοῦσαν τὸν Κύριον, καὶ παρθενεύουσαν, πάντα τὰ ἔργα· Παρθένε εὐλογοῦμεν, καὶ ὑπερυψοῦμεν εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8C21.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C21.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C21.melody = he.h.m4_gr_GR_cog.heHE.CheirasEkpetasasDaniil.name
meMA.Ode8C21.incipit = "Ὡς λέων Παμμάκαρ πεποιθώς, ψυχῆς…"
meMA.Ode8C21.text = "Ὡς λέων Παμμάκαρ πεποιθώς, ψυχῆς στεῤῥότητι, οὐκ ἐδειλίασας, τὸν τῆς αἱρέσεως πρόβολον, τῆς μανίας τὸν ἐπώνυμον, ὃν καταπτήξας τοῖς λαοῖς, χαίρων ἐκραύγαζες· Εὐλογεῖτε, πάντα τὰ ἔργα Κυρίου τὸν Κύριον."

meMA.Ode8C22.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C22.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C22.melody = he.h.m4_gr_GR_cog.heHE.CheirasEkpetasasDaniil.name
meMA.Ode8C22.incipit = "Κοσμήσας ἀνδρείᾳ τὸν θυμόν, τῇ…"
meMA.Ode8C22.text = "Κοσμήσας ἀνδρείᾳ τὸν θυμόν, τῇ σωφροσύνῃ δέ, Μάκαρ τὴν ἔφεσιν, τὸν νοῦν φρονήσεως ἔμπλεων, κεκτημένος διετέλεσας, δικαιοσύνῃ δὲ τὰς σὰς φρένας ἐῤῥύθμισας, ἀνακράζων· Πάντα τὰ ἔργα ὑμνεῖτε τὸν Κύριον."

meMA.Ode8C23.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C23.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C23.melody = he.h.m4_gr_GR_cog.heHE.CheirasEkpetasasDaniil.name
meMA.Ode8C23.incipit = "Ῥαγδαίως φερόμενον τὸν ῥοῦν, τὸν…"
meMA.Ode8C23.text = "Ῥαγδαίως φερόμενον τὸν ῥοῦν, τὸν τῆς αἱρέσεως Πάτερ ἐξήρανας, τοῖς σοῖς συντάγμασιν Ὅσιε, καὶ πανσόφοις ἀποδείξεσι, τὰς μηχανὰς τῶν ἀσεβῶν, καταστρεψάμενος, καὶ κραυγάζων· Πάντα τὰ ἔργα ὑμνεῖτε τὸν Κύριον."

meMA.Ode8C2T.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C2T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C2T.melody = he.h.m4_gr_GR_cog.heHE.CheirasEkpetasasDaniil.name
meMA.Ode8C2T.incipit = "Ὁ πλάσας τὴν φύσιν κατ'…"
meMA.Ode8C2T.text = "Ὁ πλάσας τὴν φύσιν κατʼ ἀρχάς, τὴν τῶν ἀνθρώπων Θεός, ταύτην ἐνδύεται, ἐκ τῆς ἀχράντου σου Πάναγνε, καὶ ἁγίας γαστρὸς σήμερον, ἐν Βηθλεὲμ προφητικῶς, ἀποτικτόμενος· ὅθεν πάντες, σὲ εὐλογοῦμεν Μαρία Θεόνυμφε."

meMA.Ode9C11.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C11.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode9C11.melody = he.h.m5_gr_GR_cog.heHE.IsaiaChoreve.name
meMA.Ode9C11.incipit = "Τῶν ἐπαίνων Στέφανε, ὑπερέβης ἅπαντα…"
meMA.Ode9C11.text = "Τῶν ἐπαίνων Στέφανε, ὑπερέβης ἅπαντα θεσμόν, καὶ φέρεις κατὰ παντός, λόγου ἀψευδῶς, σὺ τὰ νικητήρια· ἀδυνατεῖ, βρότειος γὰρ νοῦς, πλέξασθαι στέφανον, ἐγκωμίων σοι ἐπάξιον."

meMA.Ode9C12.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C12.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode9C12.melody = he.h.m5_gr_GR_cog.heHE.IsaiaChoreve.name
meMA.Ode9C12.incipit = "Ὢ τῆς μακαρίας σου, ἧς…"
meMA.Ode9C12.text = "Ὢ τῆς μακαρίας σου, ἧς ἐφθέγξω, Στέφανε φωνῆς! Μὴ στήσῃς τοῖς φονευταῖς, Δέσποτα βοῶν, Χριστὲ τὸ ἀγνόημα· ἀλλʼ ὡς Θεὸς καὶ Δημιουργός, δέξαι τὸ πνεῦμά μου, ὥσπερ θῦμα εὐωδέστατον."

meMA.Ode9C13.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C13.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode9C13.melody = he.h.m5_gr_GR_cog.heHE.IsaiaChoreve.name
meMA.Ode9C13.incipit = "Σύ τῶν πόνων ἔπαθλον, ἀνεδήσω…"
meMA.Ode9C13.text = "Σύ τῶν πόνων ἔπαθλον, ἀνεδήσω τὸ νικητικόν, ἐκ Παντοκρατορικῆς, στέφος δεξιᾶς, καὶ νῦν Παμμακάριστε, παρεστηκὼς τῷ Παμβασιλεῖ, δόξῃ καὶ χάριτι, τοὺς ἀνυμνοῦντάς σε περίσῳζε."

meMA.Ode9C1T.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C1T.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode9C1T.melody = he.h.m5_gr_GR_cog.heHE.IsaiaChoreve.name
meMA.Ode9C1T.incipit = "Χρονικῆς ὑπάρξεως, ἐκ Παρθένου, εἴληφεν…"
meMA.Ode9C1T.text = "Χρονικῆς ὑπάρξεως, ἐκ Παρθένου, εἴληφεν ἀρχήν, σαρκὶ ὁ ἐκ τοῦ Πατρὸς ἄχρονος Υἱός· διὸ ἀναιρούμενος, ὁ μιμητής, Στέφανος Χριστοῦ, νῦν ἐκληρώσατο, ἀπαρχήν τῆς ἀϊδίου ζωῆς."

meMA.Ode9C21.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C21.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode9C21.melody = he.h.m4_gr_GR_cog.heHE.LithosAcheirotmitos.name
meMA.Ode9C21.incipit = "Τῶ θείῳ βαλλόμενος πόθῳ, τῶν…"
meMA.Ode9C21.text = "Τῶ θείῳ βαλλόμενος πόθῳ, τῶν διωγμῶν τὰς παραστάσεις, ῥᾷον ὑποφέρεις θεόφρον, καὶ τοῦ διώκτου τὴν ἀγριότητα, καταβαλὼν ἀπείληφας, τὸν τῶν ἀγώνων Πάτερ στέφανον."

meMA.Ode9C22.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C22.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode9C22.melody = he.h.m4_gr_GR_cog.heHE.LithosAcheirotmitos.name
meMA.Ode9C22.incipit = "Ὄλβον ἐπουράνιον εὗρες, καὶ διαμένουσαν…"
meMA.Ode9C22.text = "Ὄλβον ἐπουράνιον εὗρες, καὶ διαμένουσαν οἰκίαν, ἀντὶ τῆς μακρᾶς ἐξορίας, καὶ ξενιτείας καὶ μεταβάσεως, καὶ νῦν τρυφᾷς λαβόμενος, τῆς αἰωνίου ἀπολαύσεως."

meMA.Ode9C23.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C23.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode9C23.melody = he.h.m4_gr_GR_cog.heHE.LithosAcheirotmitos.name
meMA.Ode9C23.incipit = "Ὕμνον τῷ Δεσπότῃ προσφέρων, Μάκαρ…"
meMA.Ode9C23.text = "Ὕμνον τῷ Δεσπότῃ προσφέρων, Μάκαρ δυσώπησον ἀπαύστως, ὑπὲρ τῆς ἐμῆς ἀσθενείας, καὶ συσκηνίας καὶ ἀδελφότητος, ὅπως ὁμοῦ βιώσαντες, τύχωμεν ἅμα τῆς θεώσεως."

meMA.Ode9C2T.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C2T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode9C2T.melody = he.h.m4_gr_GR_cog.heHE.LithosAcheirotmitos.name
meMA.Ode9C2T.incipit = "Σὲ τὴν ὑπὲρ νοῦν τε…"
meMA.Ode9C2T.text = "Σὲ τὴν ὑπὲρ νοῦν τε καὶ λόγον, ἐν Βηθλεὲμ τὸν Θεὸν Λόγον, σήμερον τεκοῦσαν Παρθένε, καὶ ἐν σπαργάνοις τοῦτον εἱλήσασαν, ὡς ἀληθῶς Θεόπαιδα, καὶ Θεοτόκον μεγαλύνομεν."

meMA.Exaposteilarion1.mode = miscellanea_gr_GR_cog.misc.Mode3
meMA.Exaposteilarion1.melody = he.a.m3_gr_GR_cog.heAU.EpeskepsatoImas.name
meMA.Exaposteilarion1.incipit = "Στέφανος σοι ὡς Βασιλεῖ, ἔμψυχος…"
meMA.Exaposteilarion1.text = "Στέφανος σοι ὡς Βασιλεῖ, ἔμψυχος προσενήνεκται, νῦν τῆς σαρκὸς ἐκδημήσας, τῷ ἐνδημήσαντι σαρκί, Θεῷ τῷ παντοκράτορι, διʼ ὃν καὶ τὸν ἀγῶνα, ἐνδόξως ἐτέλεσεν."

meMA.Exaposteilarion2.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Exaposteilarion2.melody = he.a.m2_gr_GR_cog.heAU.ToisMathitaisSynelthomen.name
meMA.Exaposteilarion2.incipit = "Ὁ πλήρης θείας χάριτος, καὶ…"
meMA.Exaposteilarion2.text = "Ὁ πλήρης θείας χάριτος, καὶ δυνάμεως ὄντως, τερατουργῶν διήλεγχε, θεοκτόνων τὰ στίφη, διακενῆς μελετῶντα, ὑψηγόρῳ δὲ γλώττῃ, θεολογῶν ὁ Στέφανος, τοῦ Πατρός δεξιόθεν, τὸν Ἰησοῦν, καθορῶ νῦν ἔφησεν ἑστηκότα, πρὸς ὃν λιθολευστούμενος στεφηφόρος ἀνῆλθεν."

meMA.Exaposteilarion3.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Exaposteilarion3.melody = he.a.m2_gr_GR_cog.heAU.ToisMathitaisSynelthomen.name
meMA.Exaposteilarion3.incipit = "Ὁ τῷ Πατρὶ καὶ Πνεύματι…"
meMA.Exaposteilarion3.text = "Ὁ τῷ Πατρὶ καὶ Πνεύματι, συναΐδιος Λόγος, καὶ συμφυὴς καὶ σύνθρονος, ἐκ Παρθένου γεννᾶται, ἐν Βηθλεὲμ νῦν ὡς βρέφος· ὃν Θεὸν καὶ Σωτῆρα, ὁ Πρωτομάρτυς Στέφανος, ἀριδήλως κηρύξας, ὑπὸ χειρῶν, μιαιφόνων χαίρων λιθολευστεῖται, καὶ στεφηφόρος ἄνεισι, πρὸς αὐτὸν μετὰ δόξης."

meMA.Lauds1.poet = poets_gr_GR_cog.po.Kyprianou
meMA.Lauds1.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Lauds1.melody = miscellanea_gr_GR_cog.misc.Idiomelon
meMA.Lauds1.incipit = "Ἀθλοφορικὸν στέφανον, τῷ Πρωτάθλῳ οἱ…"
meMA.Lauds1.text = "Ἀθλοφορικὸν στέφανον, τῷ Πρωτάθλῳ οἱ πιστοὶ πλέξωμεν, ἐκ λογικῶν ἀνθέων· αὐτὸς γὰρ τὴν τῶν Μαρτύρων προητοίμασεν ὁδόν, καὶ ἐν χαρᾷ ἀνεβόα· Ἰδοὺ τοὺς οὐρανούς ἀνεῳγμένους θεωρῶ, καὶ τὸν Υἱὸν τοῦ Θεοῦ, ἐκ δεξιῶν ἑστῶτα τοῦ ἀοράτου Πατρός."

