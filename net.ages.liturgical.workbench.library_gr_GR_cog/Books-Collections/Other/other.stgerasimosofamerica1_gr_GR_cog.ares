hoCA.title2 = "ΕΙΣ ΤΟΝ ΕΝ OΣΙΟΙΣ ΠΑΤΕΡΑ ΗΜΩΝ"
hoCA.title3 = "ΓΕΡΑΣΙΜΟΝ"
hoCA.title4 = "ΕΠΙΣΚΟΠΟΝ ΑΒΥΔΟΥ"
