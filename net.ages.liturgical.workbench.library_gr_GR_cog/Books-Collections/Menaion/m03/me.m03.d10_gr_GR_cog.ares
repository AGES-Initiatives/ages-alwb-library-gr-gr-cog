meMA.Ode3C1H.incipit = he.h.m4_gr_GR_cog.heHE.OtiSteiraEteken.incipit
meMA.Ode3C1H.text = he.h.m4_gr_GR_cog.heHE.OtiSteiraEteken.text

meMA.Ode3C11.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C11.greekmelody = he.h.m4_gr_GR_cog.heHE.OtiSteiraEteken.greekname
meMA.Ode3C11.melody = he.h.m4_gr_GR_cog.heHE.OtiSteiraEteken.name
meMA.Ode3C11.incipit = "Οἱ στεῤῥοὶ ἀδάμαντες, τυμπανιζόμενοι ξίφει…"
meMA.Ode3C11.text = "Οἱ στεῤῥοὶ ἀδάμαντες, τυμπανιζόμενοι ξίφει, καὶ ἀλγεινῶν ἰδέαις, προσομιλοῦντες καρτερῶς, τῷ θαυμαστῷ Θεῷ ἡμῶν ἐκραυγαζον· Ἅγιος εἶ Κύριε."

meMA.Ode3C12.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C12.melody = he.h.m4_gr_GR_cog.heHE.OtiSteiraEteken.name
meMA.Ode3C12.incipit = "Τῇ σοφίᾳ Κοδρᾶτος, τῇ θεοδότῳ…"
meMA.Ode3C12.text = "Τῇ σοφίᾳ Κοδρᾶτος, τῇ θεοδότῳ κατήργει, Ἑλληνικῆς σοφίας ἐρεσχελίας ἐναθλῶν, καὶ τοῖς οἰκείοις αἵμασιν, ἐν Πνεύματι θείῳ καλλυνόμενος."

meMA.Ode3C13.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C13.melody = he.h.m4_gr_GR_cog.heHE.OtiSteiraEteken.name
meMA.Ode3C13.incipit = "Σὺν Κοδράτῳ Ἄνεκτον, καὶ σὺν…"
meMA.Ode3C13.text = "Σὺν Κοδράτῳ Ἄνεκτον, καὶ σὺν τῷ Παύλῳ Κρήσκεντα, Κυπριανὸν τὸν θεῖον, καὶ Διονύσιον ὁμοῦ, εὐσεβοφρόνως μέλψωμεν, τοῖς ᾄσμασι, πίστει ἐναθλήσαντας."

meMA.Ode3C1T.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C1T.melody = he.h.m4_gr_GR_cog.heHE.OtiSteiraEteken.name
meMA.Ode3C1T.incipit = "Νοῦς οὐδὲ οὐράνιος, τὴν ὑπὲρ…"
meMA.Ode3C1T.text = "Νοῦς οὐδὲ οὐράνιος, τὴν ὑπὲρ νοῦν σου λοχείαν, διερμηνεῦσαι Κόρη δύναται· Νοῦ γὰρ τοῦ πρώτου ἐν γαστρί, Λόγον Ἁγνὴ συνέλαβες, τὸν τὰ πάντα λόγῳ συστησάμενον."

meMA.Ode3Kathisma1.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3Kathisma1.greekmelody = he.a.m4_gr_GR_cog.heAU.OYpsotheisEnToStavro.greekname
meMA.Ode3Kathisma1.melody = he.a.m4_gr_GR_cog.heAU.OYpsotheisEnToStavro.name
meMA.Ode3Kathisma1.incipit = "Τῇ καρτερίᾳ τῶν δεινῶν θανατώσας…"
meMA.Ode3Kathisma1.text = "Τῇ καρτερίᾳ τῶν δεινῶν θανατώσας, τὸν ὑπερήφανον ἐχθρὸν Ἀθλοφόρε, τὸν διὰ ξίφους θάνατον ὑπέμεινας, ἅμα τοῖς συνάθλοις σου, τοῖς πανσόφοις Κοδρᾶτε· ὅθεν ἑορτάζομεν, τὴν σεπτὴν ὑμῶν μνήμην, ἀναβοῶντες· Μνήσθητε ἡμῶν, πρὸς τὸν Δεσπότην, ἀήττητοι Μάρτυρες."

meMA.Ode3KathismaT.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3KathismaT.melody = he.a.m4_gr_GR_cog.heAU.OYpsotheisEnToStavro.name
meMA.Ode3KathismaT.incipit = ho.ho23_gr_GR_cog.hoCA.Apolytikion2.incipit
meMA.Ode3KathismaT.text = ho.ho23_gr_GR_cog.hoCA.Apolytikion2.text

meMA.Ode3KathismaSt.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3KathismaSt.melody = he.a.m4_gr_GR_cog.heAU.OYpsotheisEnToStavro.name
meMA.Ode3KathismaSt.incipit = "Τὸν ἐξ ἀνάρχου τοῦ Πατρὸς…"
meMA.Ode3KathismaSt.text = "Τὸν ἐξ ἀνάρχου τοῦ Πατρὸς γεννηθέντα, ἡ ἐπʼ ἐσχάτων σε σαρκὶ τετοκυῖα, ἐπὶ Σταυροῦ κρεμάμενον ὁρῶσα Χριστέ· Οἴμοι ποθεινότατε Ἰησοῦ! ἀνεβόα, πῶς ὁ δοξαζόμενος, ὡς Θεὸς ὑπʼ Ἀγγέλων, ὑπὸ ἀνόμων νῦν βροτῶν Υἱέ, θέλων σταυροῦσαι; Ὑμνῶ σε μακρόθυμε."

meMA.Ode4C1H.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C1H.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C1H.greekmelody = he.h.m4_gr_GR_cog.heHE.OKathimenosEnDoxi.greekname
meMA.Ode4C1H.melody = he.h.m4_gr_GR_cog.heHE.OKathimenosEnDoxi.melody
meMA.Ode4C1H.incipit = he.h.m4_gr_GR_cog.heHE.OKathimenosEnDoxi.incipit
meMA.Ode4C1H.text = he.h.m4_gr_GR_cog.heHE.OKathimenosEnDoxi.text

meMA.Ode4C11.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C11.greekmelody = he.h.m4_gr_GR_cog.heHE.OKathimenosEnDoxi.greekname
meMA.Ode4C11.melody = he.h.m4_gr_GR_cog.heHE.OKathimenosEnDoxi.name
meMA.Ode4C11.incipit = "Τῆς σοφίας σου τῷ λόγῳ…"
meMA.Ode4C11.text = "Τῆς σοφίας σου τῷ λόγῳ, τοὺς ἀσόφους κατῄσχυνας, καὶ τῇ παραινέσει, τῶν θεοπειθῶν διδαγμάτων σου Μάρτυς Κοδρᾶτε ζωγρήσας θεῖον, Ἄνεκτον, τῷ Δεσπότῃ σου, καρτερικὸν φέρεις Μάρτυρα."

meMA.Ode4C12.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C12.melody = he.h.m4_gr_GR_cog.heHE.OKathimenosEnDoxi.name
meMA.Ode4C12.incipit = "Ἐκ σπαργάνων ᾠκειώθης, τῷ τὰ…"
meMA.Ode4C12.text = "Ἐκ σπαργάνων ᾠκειώθης, τῷ τὰ πάντα ποιήσαντι, ἐκ παιδὸς τελείου, φρόνημα ἀνδρὸς ἐπιδέδειξαι, καὶ τὴν σοφίαν ποθήσας, ὅλος γέγονας, καθαρώτατον, Μάρτυς Χριστοῦ ἐνδιαίτημα."

meMA.Ode4C13.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C13.melody = he.h.m4_gr_GR_cog.heHE.OKathimenosEnDoxi.name
meMA.Ode4C13.incipit = "Ἡ ἐξάφωτος λυχνία, τῶν Μαρτύρων…"
meMA.Ode4C13.text = "Ἡ ἐξάφωτος λυχνία, τῶν Μαρτύρων σου Κύριε, μυστικῷ ἐλαίῳ, ἐπαρδευομένη ἐμείωσε, πολυθεΐας τὴν νύκτα καὶ κατηύγασε, τοὺς κραυγάζοντας· Δόξα Χριστὲ τῇ δυνάμει σου."

meMA.Ode4C1T.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C1T.melody = he.h.m4_gr_GR_cog.heHE.OKathimenosEnDoxi.name
meMA.Ode4C1T.incipit = "Ἡ ἁγία Θεοτόκος, ἣν οἰκῆσαι…"
meMA.Ode4C1T.text = "Ἡ ἁγία Θεοτόκος, ἣν οἰκῆσαι ηὐδόκησεν ὡς εὐώδη οἶκον, Λόγος τοῦ Πατρὸς ὁμοούσιος, οὐ κατεφλέχθη τὴν μήτραν, οὐκ ὠδίνησε· καὶ γὰρ τέτοκεν, Ἐμμανουὴλ τὸν Θεάνθρωπον."

meMA.Ode5C1H.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C1H.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C1H.greekmelody = he.h.m4_gr_GR_cog.heHE.TonFotismonSouKyrie.greekname
meMA.Ode5C1H.melody = he.h.m4_gr_GR_cog.heHE.TonFotismonSouKyrie.melody
meMA.Ode5C1H.incipit = he.h.m4_gr_GR_cog.heHE.TonFotismonSouKyrie.incipit
meMA.Ode5C1H.text = he.h.m4_gr_GR_cog.heHE.TonFotismonSouKyrie.text

meMA.Ode5C11.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C11.greekmelody = he.h.m4_gr_GR_cog.heHE.TonFotismonSouKyrie.greekname
meMA.Ode5C11.melody = he.h.m4_gr_GR_cog.heHE.TonFotismonSouKyrie.name
meMA.Ode5C11.incipit = "Τῷ φωτισμῷ τῆς γνώσεως, ἀγνοίας…"
meMA.Ode5C11.text = "Τῷ φωτισμῷ τῆς γνώσεως, ἀγνοίας τὴν ἀχλύν, ἀποδιώκων Κυρίῳ προσάγεις, δῆμον Ἀθλητῶν σοφὲ Κοδρᾶτε, σὺν αὐτοῖς στεφανούμενος."

meMA.Ode5C12.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C12.melody = he.h.m4_gr_GR_cog.heHE.TonFotismonSouKyrie.name
meMA.Ode5C12.incipit = "Ἀσκητικῶς τὸ πρότερον, νικήσας τῶν…"
meMA.Ode5C12.text = "Ἀσκητικῶς τὸ πρότερον, νικήσας τῶν παθῶν, ἐπαναστάσεις τὸ δεύτερον, εἷλες κράτος δυσσεβῶν, Κοδρᾶτε Μάρτυς, ἐναθλήσας στεῤῥότατα."

meMA.Ode5C13.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C13.melody = he.h.m4_gr_GR_cog.heHE.TonFotismonSouKyrie.name
meMA.Ode5C13.incipit = "Ἀναστομοῦται ἄνικμος, ἡ πέτρα προχοαῖς…"
meMA.Ode5C13.text = "Ἀναστομοῦται ἄνικμος, ἡ πέτρα προχοαῖς, σεπτῶν αἱμάτων καὶ ῥεῖθρον διαυγές, δίδωσι, πιστοὺς καθαγιάζον, θεϊκαῖς ἐπινεύσεσι."

meMA.Ode5C1T.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C1T.melody = he.h.m4_gr_GR_cog.heHE.TonFotismonSouKyrie.name
meMA.Ode5C1T.incipit = "Ὁ πλαστουργήσας ἄχραντε, τὴν Εὔαν…"
meMA.Ode5C1T.text = "Ὁ πλαστουργήσας ἄχραντε, τὴν Εὔαν ἐκ πλευρᾶς, σοῦ ἐκ νηδύος Ἁγνὴ πλαστουργεῖται, σῶσαι τὸν Ἀδὰμ διʼ εὐσπλαγχνίαν, βουληθεὶς ὡς φιλάνθρωπος."

meMA.Ode6C1H.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C1H.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C1H.greekmelody = he.h.m4_gr_GR_cog.heHE.EvoiseProtypon.greekname
