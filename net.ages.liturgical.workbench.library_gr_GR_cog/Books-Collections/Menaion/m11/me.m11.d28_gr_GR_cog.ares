meMA.Ode1C11.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C11.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode1C11.greekmelody = he.h.m6_gr_GR_cog.heHE.OsEnIpeiroPezefsas.greekname
meMA.Ode1C11.melody = he.h.m6_gr_GR_cog.heHE.OsEnIpeiroPezefsas.name
meMA.Ode1C11.incipit = "Χαρμονικῶς διοδεύσας…"
meMA.Ode1C11.text = "Χαρμονικῶς διοδεύσας σὺ τὴν στενήν, τρίβον τῆς ἀσκήσεως, μαρτυρίου πλατυσμῷ, δυσμενῶν ἐστένωσας ὁρμήν, καὶ εὐρύχωρον ζωὴν Μάκαρ κεκλήρωσαι."

meMA.Ode1C12.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C12.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode1C12.melody = he.h.m6_gr_GR_cog.heHE.OsEnIpeiroPezefsas.name
meMA.Ode1C12.incipit = "Ῥίζα σε ἄκαρπος…"
meMA.Ode1C12.text = "Ῥίζα σε ἄκαρπος πρώην καρπογονεῖ, Ἄννης ἡ ὁμώνυμος, ὡς τὸν πάλαι Σαμουήλ, καὶ δοτὸν σε δίδωσι Θεῷ, τὴν τοῦ βίου σου Σοφὲ χάριν σημαίνουσα."

meMA.Ode1C13.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C13.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode1C13.melody = he.h.m6_gr_GR_cog.heHE.OsEnIpeiroPezefsas.name
meMA.Ode1C13.incipit = "Ἱερωτάτως τῷ θείῳ…"
meMA.Ode1C13.text = "Ἱερωτάτως τῷ θείῳ τῶν Μοναστῶν, προσχωρήσας τάγματι, ὡς φαιδρότατος ἀστήρ, ἀρεταῖς ἐξέλαμψας πιστούς, καταυγάζων μυστικῶς, Ὅσιε Στέφανε."

meMA.Ode1C14.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C14.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode1C14.melody = he.h.m6_gr_GR_cog.heHE.OsEnIpeiroPezefsas.name
meMA.Ode1C14.incipit = "Στενοχωρήσας τὸ σῶμα…"
meMA.Ode1C14.text = "Στενοχωρήσας τὸ σῶμα τῷ συγκλεισμῷ, τῆς στενῆς οἰκήσεως, ἀνεπτέρωσας τὸν νοῦν, πρὸς τὰ ἐπουράνια Σοφέ, ἀναβάσει καλῶς προσπλατυνόμενος."

meMA.Ode1C1T.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C1T.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode1C1T.melody = he.h.m6_gr_GR_cog.heHE.OsEnIpeiroPezefsas.name
meMA.Ode1C1T.incipit = "Τὴν ἀδιόδευτον πύλην…"
meMA.Ode1C1T.text = "Τὴν ἀδιόδευτον πύλην τὸ καθαρόν, τῆς ἁγνείας τέμενος, τὴν καλὴν ἐν γυναιξί, τὴν ἁγίαν Δέσποιναν φωναῖς, ἱεραῖς ὁ Ἱερὸς λαὸς δοξάσωμεν."

meMA.Ode1C21.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C21.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode1C21.greekmelody = he.h.m6_gr_GR_cog.heHE.OsEnIpeiroPezefsas.greekname
meMA.Ode1C21.melody = he.h.m6_gr_GR_cog.heHE.OsEnIpeiroPezefsas.name
meMA.Ode1C21.incipit = "Ἐκ τῶν σκανδάλων τοῦ βίου…"
meMA.Ode1C21.text = "Ἐκ τῶν σκανδάλων τοῦ βίου καὶ τῶν παθῶν, τῶν παρενοχλούντων με, εἰρηνεύσας μου τὸν νοῦν, Ἀθλητὰ Εἰρήναρχε τὴν σήν, εὐφημῆσαι γαληνῶς μνήμην ἀξίωσον."

meMA.Ode1C22.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C22.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode1C22.melody = he.h.m6_gr_GR_cog.heHE.OsEnIpeiroPezefsas.name
meMA.Ode1C22.incipit = "Ἱερονίκους στεφάνους ὡς Ἀθλητής, εἰληφὼς…"
meMA.Ode1C22.text = "Ἱερονίκους στεφάνους ὡς Ἀθλητής, εἰληφὼς Εἰρήναρχε, μετετέθης ἐκ τῆς γῆς, πρὸς εἰρήνην ἄμαχον καὶ φῶς, καὶ ζωὴν ἀληθινὴν μακαριώτατε."

meMA.Ode1C23.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C23.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode1C23.melody = he.h.m6_gr_GR_cog.heHE.OsEnIpeiroPezefsas.name
meMA.Ode1C23.incipit = "Ῥείθροις αἱμάτων τὴν φλόγα τῶν…"
meMA.Ode1C23.text = "Ῥείθροις αἱμάτων τὴν φλόγα τῶν διωκτῶν, κατασβέσας ἤρδευσας, διανοίας τῶν πιστῶν, ἀναθάλλειν ἔρωτα ζωῆς, αἰωνίου εὐσεβῶς Μάρτυς Εἰρήναρχε."

meMA.Ode1C2T.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C2T.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode1C2T.melody = he.h.m6_gr_GR_cog.heHE.OsEnIpeiroPezefsas.name
meMA.Ode1C2T.incipit = "Ἡ τὸν ἀχώρητον Λόγον τῇ…"
meMA.Ode1C2T.text = "Ἡ τὸν ἀχώρητον Λόγον τῇ σῇ γαστρί, χωρηθέντα τέξασα, θεοχώρητε σκηνή, τῆς στενοχωρούσης με δεινῶς, τρικυμίας τῶν κακῶν Ἁγνὴ ἀπάλλαξον."

meMA.Ode3C11.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C11.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode3C11.greekmelody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.greekname
meMA.Ode3C11.melody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.name
meMA.Ode3C11.incipit = "Ὁ νοῦς σου νεύσει πρὸς…"
meMA.Ode3C11.text = "Ὁ νοῦς σου νεύσει πρὸς Θεόν, καλλυνόμενος Πάτερ, ὡραιότατος ὤφθη, καὶ χαρίσματος παντός, ἀνάπλεως ἀληθῶς, καὶ τῆς θείας, μέτοχος λαμπρότητος."

meMA.Ode3C12.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C12.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode3C12.melody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.name
meMA.Ode3C12.incipit = "Σεπτὴν εἰκόνα τοῦ Χριστοῦ, καὶ…"
meMA.Ode3C12.text = "Σεπτὴν εἰκόνα τοῦ Χριστοῦ, καὶ τῆς τοῦτον Τεκούσης, ἀσπαζόμενος Μάκαρ, βασιλέως δυσσεβοῦς, τὸ δόγμα τὸ δυσσεβές, ἑβδελύξω, σθένει θείου Πνεύματος."

meMA.Ode3C13.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C13.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode3C13.melody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.name
meMA.Ode3C13.incipit = "Σιδήρῳ σε τὸν σιδηρᾶν, κεκτημένον…"
meMA.Ode3C13.text = "Σιδήρῳ σε τὸν σιδηρᾶν, κεκτημένον καρδίαν, ὁ παράφρων δεσμεύσας, παραπέμπει φυλακαῖς, ὡς φύλακα τῶν Πιστῶν, διδαγμάτων, Στέφανε πανόλβιε."

meMA.Ode3C14.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C14.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode3C14.melody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.name
meMA.Ode3C14.incipit = "Τοὺς σοὺς ὡραίους προφανῶς, ἔσχες…"
meMA.Ode3C14.text = "Τοὺς σοὺς ὡραίους προφανῶς, ἔσχες Ὅσιε πόδας, μαρτυρίου πρὸς τρίβους, βηματίζοντας λαμπρῶς, καὶ κεφαλὰς δυσμενῶν, γενναιόφρον, Στέφανε συνθλάττοντας."

meMA.Ode3C1T.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C1T.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode3C1T.melody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.name
meMA.Ode3C1T.incipit = "Ἐκ σοῦ τῆς μόνης καθαρᾶς…"
meMA.Ode3C1T.text = "Ἐκ σοῦ τῆς μόνης καθαρᾶς, ὁ ὑπέρθεος Λόγος, ἐσαρκώθη ὡς οἶδε, καὶ διέσωσεν ἡμᾶς, τοὺς προσκυνοῦντας αὐτοῦ, τὴν διʼ οἶκτον, θείαν συγκατάβασιν."

meMA.Ode3C21.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C21.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode3C21.greekmelody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.greekname
meMA.Ode3C21.melody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.name
meMA.Ode3C21.incipit = "Νευροῦσαι σθένει θεϊκῷ, καὶ χωρεῖς…"
meMA.Ode3C21.text = "Νευροῦσαι σθένει θεϊκῷ, καὶ χωρεῖς πρὸς ἀγῶνας, θαρσαλέως παμμάκαρ, ὑπομένων αἰκισμούς, καὶ στρέβλας τῶν δυσμενῶν, ἀθλοφόρε, Μάρτυς γενναιότατε."

meMA.Ode3C22.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C22.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode3C22.melody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.name
meMA.Ode3C22.incipit = "Ἰσχὺν σοι δίδωσι Χριστός, Ἀθλοθέτης…"
meMA.Ode3C22.text = "Ἰσχὺν σοι δίδωσι Χριστός, Ἀθλοθέτης ὁ μέγας, διολέσαι τὴν πλάνην, καὶ αἰσχῦναι τὸν ἐχθρόν, καυχώμενον ἀναιδῶς, καὶ εἰκαίως, Μάρτυς φρυαττόμενον."

meMA.Ode3C23.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C23.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode3C23.melody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.name
meMA.Ode3C23.incipit = "Καθεῖλε πόνοις ἀνδρικοῖς, τὸν ἀρχέκακον…"
meMA.Ode3C23.text = "Καθεῖλε πόνοις ἀνδρικοῖς, τὸν ἀρχέκακον ὄφιν, ὁ χορὸς τῶν Ἁγίων, ἐναθλήσας κραταιῶς, καὶ μάστιγας καὶ δεσμά, ὑπομείνας, σθένει θείου Πνεύματος."

meMA.Ode3C2T.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C2T.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode3C2T.melody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.name
meMA.Ode3C2T.incipit = "Ἣν εἶδε πάλαι Ἰακώβ, ἀπὸ…"
meMA.Ode3C2T.text = "Ἣν εἶδε πάλαι Ἰακώβ, ἀπὸ γῆς τεταμένην, πρὸς οὐράνιον ὕψος, θείαν κλίμακα σαφῶς, Μαρίαν τὸ καθαρόν, τοῦ Δεσπότου, μέλψωμεν παλάτιον."

meMA.Ode3H.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3H.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode3H.greekmelody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.greekname
meMA.Ode3H.melody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.melody
meMA.Ode3H.incipit = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.incipit
meMA.Ode3H.text = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.text

meMA.Ode3Kathisma1.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3Kathisma1.greekmelody = he.a.m4_gr_GR_cog.heAU.OYpsotheisEnToStavro.greekname
meMA.Ode3Kathisma1.melody = he.a.m4_gr_GR_cog.heAU.OYpsotheisEnToStavro.name
meMA.Ode3Kathisma1.incipit = "Τῶν Μοναστῶν ὑπογραμμὸς…"
meMA.Ode3Kathisma1.text = "Τῶν Μοναστῶν ὑπογραμμὸς ἀνεδείχθης, τῶν Ἀθλητῶν καλλωπισμὸς ἀνεφάνης, διʼ ἀμφοτέρων Στέφανε κοσμούμενος· ὅθεν ἀξιάγαστε, καὶ διπλοῦς τοὺς στεφάνους, ἔλαβες ἀσκήσεως, καὶ ἀθλήσεως Πάτερ. Ἀλλʼ ἐκτενῶς Χριστὸν ὑπὲρ ἡμῶν, τῶν σὲ ὑμνούντων, ἱκέτευε Στέφανε."

meMA.Ode3Kathisma2.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3Kathisma2.greekmelody = he.a.m4_gr_GR_cog.heAU.OYpsotheisEnToStavro.greekname
meMA.Ode3Kathisma2.melody = he.a.m4_gr_GR_cog.heAU.OYpsotheisEnToStavro.name
meMA.Ode3Kathisma2.incipit = "Γῆ καὶ ὑγρὰ τοὺς καρτερούς…"
meMA.Ode3Kathisma2.text = "Γῆ καὶ ὑγρὰ τοὺς καρτερούς σου ἀγῶνας, κατεμερίσθη Ἀθλητὰ γενναιόφρον, διʼ ὧν τῆς πλάνης ἔλυσας τὸ φρύαγμα, καὶ τὴν ὑπερκόσμιον, μετὰ πάντων Μαρτύρων, δόξαν αἰωνίζουσαν, ἐκληρώσω παμμάκαρ. Ἀλλʼ ὑπὲρ πάντων πρέσβευε Χριστῷ, τῶν σὲ τιμώντων, τρισμάκαρ Εἰρήναρχε."

meMA.Ode3KathismaT.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3KathismaT.melody = he.a.m4_gr_GR_cog.heAU.OYpsotheisEnToStavro.name
meMA.Ode3KathismaT.incipit = "Καταφυγὴ τῶν ἐν δεινοῖς…"
meMA.Ode3KathismaT.text = "Καταφυγὴ τῶν ἐν δεινοῖς ὑπαρχόντων, καταλλαγὴ πρὸς τὸν Θεὸν τῶν πταιόντων, Ὑπεραγία Δέσποινα περίσῳζε ἡμᾶς, πάσης περιστάσεως, καὶ κακίας ἀνθρώπων, καὶ φρικτῆς κολάσεως, καὶ παθῶν ἀτιμίας, τοὺς ἀδιστάκτῳ πίστει σε ἀεί, προσκαλουμένους, Παρθένε πανύμνητε."

meMA.Ode3KathismaSt.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3KathismaSt.melody = he.a.m4_gr_GR_cog.heAU.OYpsotheisEnToStavro.name
meMA.Ode3KathismaSt.incipit = "Ἐν τῷ Σταυρῷ παρισταμένη…"
meMA.Ode3KathismaSt.text = "Ἐν τῷ Σταυρῷ παρισταμένη Παρθένε, τῷ τοῦ Υἱοῦ σου καὶ Θεοῦ ἀνεβόας, ὀδυνηρῶς στενάζουσα, δακρύουσα θερμῶς· Οἴμοι, ὦ γλυκύτατε! πῶς ἀνδρῶν παρανόμων, φέρεις τὰ λακτίσματα, καὶ τὰς τρώσεις τῶν ἥλων, καὶ τῶν κακούργων θάνατον σαφῶς, σῶσαι τὸ γένος, βροτῶν προμηθούμενος;"

meMA.Ode4C11.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C11.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode4C11.greekmelody = he.h.m6_gr_GR_cog.heHE.ChristosMouDynamis.greekname
meMA.Ode4C11.melody = he.h.m6_gr_GR_cog.heHE.ChristosMouDynamis.name
meMA.Ode4C11.incipit = "Φωτὶ τοῦ Πνεύματος…"
meMA.Ode4C11.text = "Φωτὶ τοῦ Πνεύματος, καταλαμπόμενος, διʼ ἐντεύξεως θείας, τυφλοῖς τὸ φῶς, Πάτερ ἐχορήγησας, τὸν σὸν Δεσπότην καὶ Θεόν, μιμησάμενος Θεόπνευστε."

meMA.Ode4C12.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C12.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode4C12.melody = he.h.m6_gr_GR_cog.heHE.ChristosMouDynamis.name
meMA.Ode4C12.incipit = "Ἐφαίνου πόῤῥωθεν…"
meMA.Ode4C12.text = "Ἐφαίνου πόῤῥωθεν, τοῖς θαλαττεύουσι, κυβερνῶν τούτους μάκαρ πρὸς γαληνόν, ὅρμον θείᾳ χάριτι, προσκαλουμένους σου πιστῶς, Πάτερ Στέφανε τὸ ὄνομα."

meMA.Ode4C13.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C13.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode4C13.melody = he.h.m6_gr_GR_cog.heHE.ChristosMouDynamis.name
meMA.Ode4C13.incipit = "Ἱερουργούμενος…"
meMA.Ode4C13.text = "Ἱερουργούμενος, καθάπερ σφάγιον, ἱερὸν προσηνέχθης τῷ διὰ σέ, τεθυμένῳ Στέφανε, καὶ πρωτοτόκων ἐν σκηναῖς, ἀνεπαύσω εὐφραινόμενος."

meMA.Ode4C14.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C14.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode4C14.melody = he.h.m6_gr_GR_cog.heHE.ChristosMouDynamis.name
meMA.Ode4C14.incipit = "Σεπτῷ σου ῥήματι…"
meMA.Ode4C14.text = "Σεπτῷ σου ῥήματι, ὁ πρὶν ἡμίξηρος, ὑγιὴς ὅλος ὤφθη καταπλαγείς, Μάρτυς τὴν δοθεῖσάν σοι, χάριν ἐξ ὕψους δαψιλῶς, εἰς ἀνθρώπων περιποίησιν."

meMA.Ode4C1T.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C1T.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode4C1T.melody = he.h.m6_gr_GR_cog.heHE.ChristosMouDynamis.name
meMA.Ode4C1T.incipit = "Ἐν σοὶ Πανάμωμε…"
meMA.Ode4C1T.text = "Ἐν σοὶ Πανάμωμε, τῆς σωτηρίας μου, τὰς ἐλπίδας ἐθέμην, ἐπὶ τὴν σήν, σκέπην καταπέφευγα, γενοῦ μοι Κόρη βοηθός, τῶν δεινῶν ἐξαιρουμένη με."

meMA.Ode4C21.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C21.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode4C21.greekmelody = he.h.m6_gr_GR_cog.heHE.ChristosMouDynamis.greekname
meMA.Ode4C21.melody = he.h.m6_gr_GR_cog.heHE.ChristosMouDynamis.name
meMA.Ode4C21.incipit = "Νομίμως ἔδραμες, νομίμως ἤθλησας, καὶ…"
meMA.Ode4C21.text = "Νομίμως ἔδραμες, νομίμως ἤθλησας, καὶ νομίμως ἐστέφθης τῷ τοῦ Θεοῦ, νόμῳ κρατυνόμενος, καὶ ἀπαράτρωτον σαυτόν, συντηρῶν Μακαριώτατε."

meMA.Ode4C22.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C22.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode4C22.melody = he.h.m6_gr_GR_cog.heHE.ChristosMouDynamis.name
meMA.Ode4C22.incipit = "Μὴ στέργων ἔνδοξε, τῇ πλάνῃ…"
meMA.Ode4C22.text = "Μὴ στέργων ἔνδοξε, τῇ πλάνῃ νήχεσθαι, τῇ θεογνωσίᾳ ὡς νουνεχής, χαίρων προσκεχώρηκας, φῶς νοερὸν ἀναλαβών, καὶ φωστὴρ ἀποδεικνύμενος."

meMA.Ode4C23.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C23.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode4C23.melody = he.h.m6_gr_GR_cog.heHE.ChristosMouDynamis.name
meMA.Ode4C23.incipit = "Οὐδόλως ἔκρυψε, βυθὸς δεξάμενος, σὲ…"
meMA.Ode4C23.text = "Οὐδόλως ἔκρυψε, βυθὸς δεξάμενος, σὲ πανόλβιε Μάρτυς ἐπεγνωκώς, θείων παθημάτων σε, παναληθέστατον Χριστοῦ, ὄντα Μάρτυρα Εἰρήναρχε."

meMA.Ode4C24.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C24.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode4C24.melody = he.h.m6_gr_GR_cog.heHE.ChristosMouDynamis.name
meMA.Ode4C24.incipit = "Ἱερουργούμεναι, καὶ προθυόμεναι, καὶ ξεόμεναι…"
meMA.Ode4C24.text = "Ἱερουργούμεναι, καὶ προθυόμεναι, καὶ ξεόμεναι ἅμα καὶ τῷ πυρί, πάντοθεν φλεγόμεναι, οὐκ ἐξηρνήσασθε Χριστόν, Ἀθλοφόροι ἀξιΰμνητοι."

meMA.Ode4C2T.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C2T.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode4C2T.melody = he.h.m6_gr_GR_cog.heHE.ChristosMouDynamis.name
meMA.Ode4C2T.incipit = "Παρθένος ἔμεινας, μετὰ τὴν κύησιν…"
meMA.Ode4C2T.text = "Παρθένος ἔμεινας, μετὰ τὴν κύησιν, ὥσπερ ἦς πρὸ τοῦ τόκου, ὅτι Θεόν, Λόγον ἀπεκύησας, τὸν λυτρωσάμενον ἡμᾶς, μεσιτείᾳ σου Πανάμωμε."

meMA.Ode5C11.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C11.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode5C11.greekmelody = he.h.m6_gr_GR_cog.heHE.ToTheioFengeiSou.greekname
meMA.Ode5C11.melody = he.h.m6_gr_GR_cog.heHE.ToTheioFengeiSou.name
meMA.Ode5C11.incipit = "Μαρτυρικαῖς ἔστεψε τιμαῖς…"
meMA.Ode5C11.text = "Μαρτυρικαῖς ἔστεψε τιμαῖς, τοὺς ἀσκητικούς σου ἀληθῶς καμάτους Ὅσιε Στέφανε, ὁ ἀγωνοθέτης μόνος Θεὸς ἡμῶν, κατὰ τῶν παλαμναίων ἐνδυναμώσας σε."

meMA.Ode5C12.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C12.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode5C12.melody = he.h.m6_gr_GR_cog.heHE.ToTheioFengeiSou.name
meMA.Ode5C12.incipit = "Ἀγωνιζόμενος εὐκλεῶς…"
meMA.Ode5C12.text = "Ἀγωνιζόμενος εὐκλεῶς, δῆμον συμμαρτύρων ταῖς εἱρκταῖς, προσομιλῶν Μάρτυς εὕρηκας· σὲ γὰρ ὡς ἀστέρες ἄδυτον ἥλιον, κυκλοῦντες ἐπὶ πλεῖον κατελαμπρύνοντο."

meMA.Ode5C13.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C13.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode5C13.melody = he.h.m6_gr_GR_cog.heHE.ToTheioFengeiSou.name
meMA.Ode5C13.incipit = "Ῥήμασι θείοις τοὺς ἱερούς…"
meMA.Ode5C13.text = "Ῥήμασι θείοις τοὺς ἱερούς, Ὁμολογητὰς καὶ μιμητάς, θείων παθῶν μακαρίζομεν, τοὺς τριακοσίους καὶ τεσσαράκοντα, καὶ δύο τοὺς ἀθέους καταπαλαίσαντας."

meMA.Ode5C14.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C14.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode5C14.melody = he.h.m6_gr_GR_cog.heHE.ToTheioFengeiSou.name
meMA.Ode5C14.incipit = "Τριχῶν ἀφαίρεσιν παικτικῶς…"
meMA.Ode5C14.text = "Τριχῶν ἀφαίρεσιν παικτικῶς, ὤτων ἐκκοπήν τε καὶ χειρῶν, μελῶν τε θείων κατάφλεξιν, ὑπὲρ τῆς εἰκόνος Χριστοῦ ὑπήνεγκεν, ὁ δῆμος τῶν Ὁσίων ὁ εὐκλεέστατος."

meMA.Ode5C1T.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C1T.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode5C1T.melody = he.h.m6_gr_GR_cog.heHE.ToTheioFengeiSou.name
meMA.Ode5C1T.incipit = "Ὑπεραγία ἡ τὸν Θεόν…"
meMA.Ode5C1T.text = "Ὑπεραγία ἡ τὸν Θεόν, μόνη ἐπὶ γῆς ὡς ἀληθῶς, τὸν ὑπεράγιον τέξασα τοὺς σὲ Θεοτόκον ἀεὶ κηρύττοντας, ἁγίασον καὶ σῶσον τῇ μεσιτείᾳ σου."

meMA.Ode5C21.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C21.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode5C21.greekmelody = he.h.m6_gr_GR_cog.heHE.ToTheioFengeiSou.greekname
meMA.Ode5C21.melody = he.h.m6_gr_GR_cog.heHE.ToTheioFengeiSou.name
meMA.Ode5C21.incipit = "Ῥομφαίᾳ τέτρωται ὁ ἐχθρός, τῆς…"
meMA.Ode5C21.text = "Ῥομφαίᾳ τέτρωται ὁ ἐχθρός, τῆς ὑπομονῆς τῆς καρτερᾶς, τοῦ γενναιόφρονος Μάρτυρος, καὶ ταῖς τούτου πτέρναις καθυποτέτακται, καὶ γέλως καθορᾶται καὶ καταισχύνεται."

meMA.Ode5C22.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C22.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode5C22.melody = he.h.m6_gr_GR_cog.heHE.ToTheioFengeiSou.name
meMA.Ode5C22.incipit = "Ὁ ποῦς σου ἔστη προφητικῶς…"
meMA.Ode5C22.text = "Ὁ ποῦς σου ἔστη προφητικῶς, Μάρτυς ἐν εὐθύτητι σοφέ, ἀπεριτρέπτῳ φρονήματι, καθυποσκελίζων τὸν πολυμήχανον, ταῖς τρίβοις μαρτυρίου καλλωπιζόμενος."

meMA.Ode5C23.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C23.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode5C23.melody = he.h.m6_gr_GR_cog.heHE.ToTheioFengeiSou.name
meMA.Ode5C23.incipit = "Σὺ τὴν κακίαν τῶν διωκτῶν…"
meMA.Ode5C23.text = "Σὺ τὴν κακίαν τῶν διωκτῶν, ἄκακος ἀκέραιος ὀφθείς, Μάρτυς ἀπώσω Εἰρήναρχε, καὶ τῇ τελειώσει σου τῇ διʼ αἵματος, στεφάνοις ἀφθαρσίας κατεστεφάνωσαι."

meMA.Ode5C2T.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C2T.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode5C2T.melody = he.h.m6_gr_GR_cog.heHE.ToTheioFengeiSou.name
meMA.Ode5C2T.incipit = "Νοήσας πόῤῥωθεν μυστικῶς, ὁ τῶν…"
meMA.Ode5C2T.text = "Νοήσας πόῤῥωθεν μυστικῶς, ὁ τῶν Προφητῶν θεολαμπής, χορὸς Παρθένε Θεόνυμφε, θείου τοκετοῦ σου βάθος ἀπόῤῥητον, συμβόλοις ἱεροῖς σε προδιεχάραξε."

meMA.Ode6C11.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C11.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode6C11.greekmelody = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.greekname
meMA.Ode6C11.melody = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.name
meMA.Ode6C11.incipit = "Ῥημάτων σου δυνάμιν, ὁ κακίας…"
meMA.Ode6C11.text = "Ῥημάτων σου δυνάμιν, ὁ κακίας εὑρετής, ὑπενεγκεῖν οὐκ ἴσχυσεν· ὅθεν δεσμοῖς σε Στέφανε καὶ ποιναῖς, θανάτῳ βιαίῳ τε, ὠμοτάτως ὁ πλάνος παραδέδωκεν."

meMA.Ode6C12.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C12.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode6C12.melody = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.name
meMA.Ode6C12.incipit = "Ὡς ἄρτος ὀπτώμενος, τῷ πυρὶ…"
meMA.Ode6C12.text = "Ὡς ἄρτος ὀπτώμενος, τῷ πυρὶ καὶ ἐκ ποδῶν ἀνηλεῶς κρεμάμενος, ὡλοκαυτώθης Παῦλε καὶ τῷ Θεῷ, θυσία προσήνεξαι, συναυλίας Μαρτύρων ἀξιούμενος."

meMA.Ode6C13.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C13.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode6C13.melody = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.name
meMA.Ode6C13.incipit = "Νομίμως ἐνήθλησαν, συγκλεισθέντες καὶ πνιγμῷ…"
meMA.Ode6C13.text = "Νομίμως ἐνήθλησαν, συγκλεισθέντες καὶ πνιγμῷ, τὸν βίον καταλύσαντες, ἐν τῇ Ἐφέσῳ Ὅσιοι Μονασταί, ὀκτὼ καὶ τριάκοντα, οὓς ἐν πίστει καὶ πόθῳ μακαρίσωμεν."

meMA.Ode6C14.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C14.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode6C14.melody = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.name
meMA.Ode6C14.incipit = "Στεῤῥῶς τῷ δικάζοντι, ἀντετάξω καὶ…"
meMA.Ode6C14.text = "Στεῤῥῶς τῷ δικάζοντι, ἀντετάξω καὶ πληγαῖς, καταξανθεὶς τὸ σῶμά σου, θανατωθῆναι εἵλου διὰ Χριστόν, τὸν μόνον ἀθάνατον, ἀξιάγαστε Πέτρε μεγαλώνυμε."

meMA.Ode6C1T.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C1T.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode6C1T.melody = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.name
meMA.Ode6C1T.incipit = "Τὴν μόνην πανύμνητον, τὴν καλὴν…"
meMA.Ode6C1T.text = "Τὴν μόνην πανύμνητον, τὴν καλὴν ἐν γυναιξί, τὴν τοῦ Θεοῦ λοχεύτριαν, Χριστιανῶν τὸ τεῖχος τὸ ἀῤῥαγές, τὴν ἄχραντον Δέσποιναν, ἀδιστάκτῳ καρδία μακαρίσωμεν."

meMA.Ode6C21.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C21.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode6C21.greekmelody = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.greekname
meMA.Ode6C21.melody = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.name
meMA.Ode6C21.incipit = "Ἐπέβης τοῖς ὕδασι, κυβερνώμενος χερσί…"
meMA.Ode6C21.text = "Ἐπέβης τοῖς ὕδασι, κυβερνώμενος χερσί, ζωαρχικῇ Εἰρήναρχε, καὶ διεσώθης τούτοις τῶν διωκτῶν, βυθίσας σεβάσματα, προσευχῇ ἐπιμόνῳ Ἱερώτατε."

meMA.Ode6C22.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C22.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode6C22.melody = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.name
meMA.Ode6C22.incipit = "Μικρῶν ἀντωνούμεναι, τὰ μεγάλα ἑαυτάς…"
meMA.Ode6C22.text = "Μικρῶν ἀντωνούμεναι, τὰ μεγάλα ἑαυτάς, τοῖς αἰκισμοῖς ἐξέδωκαν, αἱ ἱεραὶ Γυναῖκες τὸν πτερνιστήν, τῆς Εὔας ἐν πνεύματι, θανατώσασαι ὄφιν τὸν ἀρχέκακον."

meMA.Ode6C23.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C23.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode6C23.melody = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.name
meMA.Ode6C23.incipit = "Οἱ παῖδες οἱ ἅγιοι, ἱερεῖα…"
meMA.Ode6C23.text = "Οἱ παῖδες οἱ ἅγιοι, ἱερεῖα καθαρά, καὶ προσφοραὶ ὁλόκληροι, τῷ τυθέντι ὡς θῦμα ἀληθινῷ, ἀμνῷ προσηνέχθησαν, εἰς ὀσμὴν εὐωδίας ἀγαλλόμενοι."

meMA.Ode6C2T.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C2T.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode6C2T.melody = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.name
meMA.Ode6C2T.incipit = "Ἰδοὺ νῦν σεσάρκωται, ἐξ αἱμάτων…"
meMA.Ode6C2T.text = "Ἰδοὺ νῦν σεσάρκωται, ἐξ αἱμάτων σου ἀγνῶν, Παρθενομῆτορ Κύριος, καὶ τοῖς βροτοῖς ἑνοῦται δίχα τροπῆς, διʼ ἄφατον ἔλεος, τὴν ἡμῶν σωτηρίαν ἐργαζόμενος."

meMA.Ode6H.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6H.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode6H.greekmelody = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.greekname
meMA.Ode6H.melody = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.melody
meMA.Ode6H.incipit = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.incipit
meMA.Ode6H.text = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.text

meMA.Kontakion1.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Kontakion1.melody = he.a.m4_gr_GR_cog.heAU.EpefanisSimeron.name
meMA.Kontakion1.incipit = "Ἑορτάζει σήμερον, ἡ Ἐκκλησία, ἑορτὴν…"
meMA.Kontakion1.text = "Ἑορτάζει σήμερον, ἡ Ἐκκλησία, ἑορτὴν εὐφρόσυνον, ἐν τῇ σῇ μνήμῃ καὶ πιστῶς, ἀνευφημοῦσα κραυγάζει σοι· Στέφανε θεῖε, ὁσίων τὸ καυχημα."

meMA.Oikos1.text = "Εἰς πᾶσαν γῆν ὡς ἀληθῶς, διέδραμεν ὁ φθόγγος τῶν σῶν κατορθωμάτων, σοφὲ Ὁσιομάρτυς, ὧν περ εἰργάσω θαυμαστῶς· ὅθεν δυσωπῶ σε, παῤῥησίαν πρὸς Θεὸν ὡς κεκτημένος Ὅσιε, ἱκέτευε τοῦ δοθῆναί μοι λόγον ἐπάξιον, τοῦ ἀνευφημῆσαι τοὺς ἀγῶνας, οὓς ὑπέστης ἐξ ὁρατῶν ἐχθρῶν καὶ νοουμένων· ὃς πρὶν ἀσκητικῶς καθεῖλες, ἀπάσας τὰς κινήσεις τῆς σαρκὸς ἀπονεκρώσας, ἀθλήσει δὲ νῦν τὸν τύραννον ἐτροπώσω, Ὁσίων τὸ καύχημα."

meMA.Ode7C11.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C11.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode7C11.greekmelody = he.h.m6_gr_GR_cog.heHE.DrosovolonMenTinKaminon.greekname
meMA.Ode7C11.melody = he.h.m6_gr_GR_cog.heHE.DrosovolonMenTinKaminon.name
meMA.Ode7C11.incipit = "Ἐμηνύθη σοι τὸ τέλος τὸ…"
meMA.Ode7C11.text = "Ἐμηνύθη σοι τὸ τέλος τὸ μακάριον, παντουργικῷ βουλήματι· ὅθεν πλείονι ἐπιδοὺς ἀσκήσει σεαυτόν, ἐκ δόξης εἰς δόξαν προχωρεῖς, ἀποκτανθεὶς διὰ Χριστόν, τὸν ἐπὶ πάντων Θεόν."

meMA.Ode7C12.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C12.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode7C12.melody = he.h.m6_gr_GR_cog.heHE.DrosovolonMenTinKaminon.name
meMA.Ode7C12.incipit = "Φερωνύμως τῷ πρωτάθλῳ συνδεδόξασαι· καὶ…"
meMA.Ode7C12.text = "Φερωνύμως τῷ πρωτάθλῳ συνδεδόξασαι· καὶ γὰρ λιθολευστούμενος, καὶ συρόμενος καὶ τυπτόμενος ἀνηλεῶς, αἱμάτων ἐπλήρωσας τὴν γῆν, καὶ τὴν ψυχήν σου τῷ Θεῷ, χαίρων παρέδωκας."

meMA.Ode7C13.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C13.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode7C13.melody = he.h.m6_gr_GR_cog.heHE.DrosovolonMenTinKaminon.name
meMA.Ode7C13.incipit = "Ἐν πλατείαις μάρτυς Στέφανε συρόμενος…"
meMA.Ode7C13.text = "Ἐν πλατείαις μάρτυς Στέφανε συρόμενος, ἀνηλεῶς τῆς πόλεως, ἐξωμάλισας μαρτυρίου τρίβον τοῖς πιστοῖς, ἐν ᾗ ἐπιβαίνοντες στεῤῥῶς, πρὸς τὴν οὐράνιον σαφῶς, πόλιν κατήντησαν."

meMA.Ode7C1T.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C1T.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode7C1T.melody = he.h.m6_gr_GR_cog.heHE.DrosovolonMenTinKaminon.name
meMA.Ode7C1T.incipit = "Ἱερώτατοι προφῆται προεδήλωσαν, Ἁγνὴ ἐμφαντικώτατα…"
meMA.Ode7C1T.text = "Ἱερώτατοι προφῆται προεδήλωσαν, Ἁγνὴ ἐμφαντικώτατα, τὸ μυστήριον τῆς σεπτῆς λοχείας σου, καὶ νῦν ἡμῖν τὰς ἐκβάσεις τηλαυγῶς κατανοοῦντες εὐσεβῶς, σὲ μακαρίζομεν."

meMA.Ode7C21.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C21.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode7C21.greekmelody = he.h.m6_gr_GR_cog.heHE.DrosovolonMenTinKaminon.greekname
meMA.Ode7C21.melody = he.h.m6_gr_GR_cog.heHE.DrosovolonMenTinKaminon.name
meMA.Ode7C21.incipit = "Συμπαθείας τῆς πρὸς σάρκα διαζεύξας…"
meMA.Ode7C21.text = "Συμπαθείας τῆς πρὸς σάρκα διαζεύξας σου, τὸν νοῦν Μακαριώτατε, φλογιζόμενος καὶ πυρὶ καιόμενος σοφέ, τῶν Παίδων ἐβόας τὴν ᾠδήν· Εὐλογητὸς εἶ ὁ Θεός, ὁ τῶν Πατέρων ἡμῶν."

meMA.Ode7C22.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C22.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode7C22.melody = he.h.m6_gr_GR_cog.heHE.DrosovolonMenTinKaminon.name
meMA.Ode7C22.incipit = "Μεμακάρισται τῶν παίδων θεῖος ὅμιλος…"
meMA.Ode7C22.text = "Μεμακάρισται τῶν παίδων θεῖος ὅμιλος, καὶ γυναικῶν ἑπτάριθμος, χορὸς ἅγιος, τοὺς ξεσμούς, τὸ πῦρ, τοὺς αἰκισμούς, στεῤῥῶς ὑπομείναντες ὁμοῦ, καὶ οὐρανίων ἀγαθῶν καταξιούμενοι."

meMA.Ode7C23.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C23.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode7C23.melody = he.h.m6_gr_GR_cog.heHE.DrosovolonMenTinKaminon.name
meMA.Ode7C23.incipit = "Απελαύνονται ἐκ μόνης σου προσκλήσεως…"
meMA.Ode7C23.text = "Απελαύνονται ἐκ μόνης σου προσκλήσεως, τῆς πονηρίας πνεύματα, ἐγνωκότα σε Αθλητὴν ἀήττητον Χριστοῦ, Εἰρήναρχε μέλποντα θερμῶς· Εὐλογητὸς εἶ ὁ Θεός, ὁ τῶν Πατέρων ἡμῶν."

meMA.Ode7C2T.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C2T.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode7C2T.melody = he.h.m6_gr_GR_cog.heHE.DrosovolonMenTinKaminon.name
meMA.Ode7C2T.incipit = "Ῥαθυμίᾳ με κρατούμενον διέγειρον, πρὸς…"
meMA.Ode7C2T.text = "Ῥαθυμίᾳ με κρατούμενον διέγειρον, πρὸς ἐργασίαν Ἄχραντε θείων πράξεων, ἐνισχύουσά με κατʼ ἐχθρῶν, ἀεὶ πολεμούντων με δεινῶς, καὶ ἐναντίοις λογισμοῖς, δελεαζόντων με."

meMA.Ode8C11.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C11.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode8C11.greekmelody = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.greekname
meMA.Ode8C11.melody = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.name
meMA.Ode8C11.incipit = "Μανικώτατοι ὥσπερ θῆρες συνέλαβον…"
meMA.Ode8C11.text = "Μανικώτατοι ὥσπερ θῆρες συνέλαβον, τοῦ Χριστοῦ τὸ ἀρνίον, παίοντες, κτείνοντες, καὶ μετὰ ἀνδρῶν ἀνομούντων συνθάπτοντες, οἱ παρανομίᾳ, συζῶντες εἰς αἰῶνας."

meMA.Ode8C12.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C12.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode8C12.melody = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.name
meMA.Ode8C12.incipit = "Ἀπηνῶς σου τὴν κάραν ἐχθρὸς…"
meMA.Ode8C12.text = "Ἀπηνῶς σου τὴν κάραν ἐχθρὸς συνέτριψεν, ἣν Θεὸς ἐν ὑψίστοις στέφει κατέστεψε, τῷ νικητικῷ, Στεφανῖτα πολύαθλε, κλέος τῶν Μαρτύρων, καὶ πάντων τῶν Ὁσίων."

meMA.Ode8C13.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C13.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode8C13.melody = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.name
meMA.Ode8C13.incipit = "Καρτερῶς, τῷ διώκτῃ ἀντιταττόμενος…"
meMA.Ode8C13.text = "Καρτερῶς, τῷ διώκτῃ ἀντιταττόμενος, ὁ θεόφρων Ἀνδρέας πληγαῖς συγκόπτεται, καὶ ἀνηλεῶς, θανατοῦται ὁ δίκαιος, μέλπων τῷ Δεσπότῃ, Χριστῷ εἰς τοὺς αἰῶνας."

meMA.Ode8C14.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C14.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode8C14.melody = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.name
meMA.Ode8C14.incipit = "Ἁγιώτατος οἶκος ἤδη γενόμενος…"
meMA.Ode8C14.text = "Ἁγιώτατος οἶκος ἤδη γενόμενος, τὸν ἐν πᾶσιν Ἁγίοις ἀναπαυόμενον, ἔσχες ἐν τῇ σῇ, ἐνοικοῦντα πολύαθλε, Στέφανε καρδίᾳ εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8C1T.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C1T.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode8C1T.melody = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.name
meMA.Ode8C1T.incipit = "Ῥαντισμῷ θείου λύθρου θεοχαρίτωτε…"
meMA.Ode8C1T.text = "Ῥαντισμῷ θείου λύθρου θεοχαρίτωτε, ἐκ πλευρᾶς κενωθέντος, τοῦ σοῦ Υἱοῦ καὶ Θεοῦ, τὸν ἐκ πονηρᾶς, ἁμαρτίας προσόντα μοι, ῥύπον δυσωπῶ σε, ἀπόπλυνον Παρθένε."

meMA.Ode8C21.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C21.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode8C21.greekmelody = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.greekname
meMA.Ode8C21.melody = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.name
meMA.Ode8C21.incipit = "Τῶν αἱμάτων τῷ λύθρῳ ἐπισταζόμενος…"
meMA.Ode8C21.text = "Τῶν αἱμάτων τῷ λύθρῳ ἐπισταζόμενος, καὶ μαστίγων τῷ κάλλει καλλωπιζόμενος, τῷ Νικοποιῷ, παραστὰς ὦ Εἰρήναρχε, νίκης τὰ βραβεῖα, πρὸς τούτῳ ἐκομίσω."

meMA.Ode8C22.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C22.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode8C22.melody = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.name
meMA.Ode8C22.incipit = "Ὑψωθεὶς τῇ ἀγάπῃ τοῦ Παντοκράτορος…"
meMA.Ode8C22.text = "Ὑψωθεὶς τῇ ἀγάπῃ τοῦ Παντοκράτορος, διωκτῶν ἐπηρμένην ὀφρὺν κατέαξας, καὶ εἰδωλικόν, ἐταπείνωσας φρύαγμα, Μάρτυς ἀθλοφόρε, Εἰρήναρχε παμμάκαρ."

meMA.Ode8C23.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C23.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode8C23.melody = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.name
meMA.Ode8C23.incipit = "Συμπαθείας με θείας Μάρτυς ἀξίωσον…"
meMA.Ode8C23.text = "Συμπαθείας με θείας Μάρτυς ἀξίωσον, πειρασμῶν καὶ κινδύνων ἀπολυτρούμενος, πίστει σε θερμῇ, τὸν τιμῶντα Εἰρήναρχε, καὶ ὑπὸ τὴν σκέπην, τὴν σὴν προσπεφευγότα."

meMA.Ode8C2T.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C2T.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode8C2T.melody = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.name
meMA.Ode8C2T.incipit = "Χαῖρε ἅγιον ὄρος καὶ θεοβάδιστον…"
meMA.Ode8C2T.text = "Χαῖρε ἅγιον ὄρος καὶ θεοβάδιστον, ἀποῤῥήτων πραγμάτων χαῖρε φανέρωσις, ἄκουσμα φρικτόν, δυσθεώρητον ὅραμα, χαῖρε τῶν πεσόντων, ἀνάκλησις Παρθένε."

meMA.Ode8H.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8H.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode8H.greekmelody = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.greekname
