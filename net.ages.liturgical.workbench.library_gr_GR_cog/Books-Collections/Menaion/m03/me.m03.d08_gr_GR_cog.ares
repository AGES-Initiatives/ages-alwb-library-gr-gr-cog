meMA.Ode5C11.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C11.greekmelody = he.h.m8_gr_GR_cog.heHE.OrthrizontesVoomenSoi.greekname
meMA.Ode5C11.melody = he.h.m8_gr_GR_cog.heHE.OrthrizontesVoomenSoi.name
meMA.Ode5C11.incipit = "Αἱ χεῖρές σου ὁσίως θεόπνευστε…"
meMA.Ode5C11.text = "Αἱ χεῖρές σου ὁσίως θεόπνευστε, πρὸς τὸν Θεόν, Πάτερ ἐπαιρόμεναι, τοὺς δυσσεβεῖς ἐτροπώσαντο."

meMA.Ode5C12.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C12.melody = he.h.m8_gr_GR_cog.heHE.OrthrizontesVoomenSoi.name
meMA.Ode5C12.incipit = "Κρατήσας τῆς χειρός σου ὁ…"
meMA.Ode5C12.text = "Κρατήσας τῆς χειρός σου ὁ Κύριος Θεοφόρε, Πάτερ καθωδήγησε, πρὸς οὐρανίαν ἀπόλαυσιν."

meMA.Ode5C13.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C13.melody = he.h.m8_gr_GR_cog.heHE.OrthrizontesVoomenSoi.name
meMA.Ode5C13.incipit = "Τὴν τρίβον τὴν στενὴν Θεοφύλακτε…"
meMA.Ode5C13.text = "Τὴν τρίβον τὴν στενὴν Θεοφύλακτε, διαπερῶν, πρὸς πλάτος κατήντησας, τοῦ Παραδείσου θεόπνευστε."

meMA.Ode5C1T.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C1T.melody = he.h.m8_gr_GR_cog.heHE.OrthrizontesVoomenSoi.name
meMA.Ode5C1T.incipit = "Οἱ μή σε Θεοτόκον γινώσκοντες…"
meMA.Ode5C1T.text = "Οἱ μή σε Θεοτόκον γινώσκοντες, Θεομῆτορ, φῶς οὐ μὴ θεάσωνται, τὸ γεννηθὲν ἐκ σοῦ πάναγνε."

meMA.Ode6C1H.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C1H.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C1H.greekmelody = he.h.m8_gr_GR_cog.heHE.ChitonaMoiParaschou.greekname
meMA.Ode6C1H.melody = he.h.m8_gr_GR_cog.heHE.ChitonaMoiParaschou.melody
meMA.Ode6C1H.incipit = he.h.m8_gr_GR_cog.heHE.ChitonaMoiParaschou.incipit
meMA.Ode6C1H.text = he.h.m8_gr_GR_cog.heHE.ChitonaMoiParaschou.text

meMA.Ode6C11.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C11.greekmelody = he.h.m8_gr_GR_cog.heHE.ChitonaMoiParaschou.greekname
meMA.Ode6C11.melody = he.h.m8_gr_GR_cog.heHE.ChitonaMoiParaschou.name
meMA.Ode6C11.incipit = "Νεκρώσας τὰς αἰσθήσεις ἐγκρατῶς, παθῶν…"
meMA.Ode6C11.text = "Νεκρώσας τὰς αἰσθήσεις ἐγκρατῶς, παθῶν ἐβασίλευσας, καὶ νῦν Θεοφύλακτε, ἀπαθείας ἐντρυφᾷς ταῖς λαμπρότησιν."

meMA.Ode6C12.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C12.melody = he.h.m8_gr_GR_cog.heHE.ChitonaMoiParaschou.name
meMA.Ode6C12.incipit = "Εἰκόνα τοῦ Σωτῆρος προσκυνεῖν, τοὺς…"
meMA.Ode6C12.text = "Εἰκόνα τοῦ Σωτῆρος προσκυνεῖν, τοὺς πάντας ἐδίδαξας, ἔργοις καὶ δόγμασι, διαπρέψας ἐμφανῶς Θεοφύλακτε."

meMA.Ode6C13.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C13.melody = he.h.m8_gr_GR_cog.heHE.ChitonaMoiParaschou.name
meMA.Ode6C13.incipit = "Νεφέλην σε τὸν ὄμβρον τῆς…"
meMA.Ode6C13.text = "Νεφέλην σε τὸν ὄμβρον τῆς ζωῆς, ὁ Χριστὸς ἀνέδειξε, Πάτερ πηγάζουσαν, Ἱεράρχα τοῦ Χριστοῦ Θεοφύλακτε."

meMA.Ode6C1T.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C1T.melody = he.h.m8_gr_GR_cog.heHE.ChitonaMoiParaschou.name
meMA.Ode6C1T.incipit = "Ὡραῖος παρὰ πάντας τοὺς βροτούς…"
meMA.Ode6C1T.text = "Ὡραῖος παρὰ πάντας τοὺς βροτούς, ὁ Υἱός σου Πάναγνε, κάλλει Θεότητος, εἰ καὶ σάρκα διʼ ἡμᾶς ἀνελάβετο."

meMA.Kontakion1ger.poet = poets_gr_GR_cog.po.Gerasimou
meMA.Kontakion1ger.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Kontakion1ger.melody = he.a.m2_gr_GR_cog.heAU.TousAsfaleis.name
meMA.Kontakion1ger.incipit = "Ὡς ἀῤῥαγῆ, Ὀρθοδοξίας πρόμαχον, καὶ…"
meMA.Kontakion1ger.text = "Ὡς ἀῤῥαγῆ, Ὀρθοδοξίας πρόμαχον, καὶ ἰσχυρόν, κακοδοξίας ἔλεγχον, εὐφημοῦμεν καὶ βοῶμέν σοι, Ἱερομύστα Θεοφύλακτε. Ἐκ πάσης ἐπηρείας διαφύλαττε, τοὺς πίστει ἑορτάζοντας τὴν μνήμην σου, πρεσβεύων σωθῆναι τὰς ψυχὰς ἡμῶν."

meMA.Ode7C1H.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C1H.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode7C1H.greekmelody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.greekname
meMA.Ode7C1H.melody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.melody
meMA.Ode7C1H.incipit = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.incipit
meMA.Ode7C1H.text = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.text

meMA.Ode7C11.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode7C11.greekmelody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.greekname
meMA.Ode7C11.melody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.name
meMA.Ode7C11.incipit = "Δρόμον τὸν θεῖον ἐκτελέσας, καὶ…"
meMA.Ode7C11.text = "Δρόμον τὸν θεῖον ἐκτελέσας, καὶ τὴν πίστιν σου τηρήσας μέχρι τέλους, τῶν σῶν πόνων Σοφέ, τὸν στέφανον ἐκτήσω, ἀναβοῶν μακάριε· ὁ Θεὸς εὐλογητὸς εἶ."

meMA.Ode7C12.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode7C12.melody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.name
meMA.Ode7C12.incipit = "Ἅρματι θείων ἀρετῶν σου, ἐποχούμενος…"
meMA.Ode7C12.text = "Ἅρματι θείων ἀρετῶν σου, ἐποχούμενος πρὸς ὕψος ἀνηνέχθης, τῷ Θεῷ δὲ τανῦν παρέστηκας κραυγάζων· Εὐλογητὸς εἶ Κύριε, ὁ Θεὸς εἰς τοὺς αἰῶνας."

meMA.Ode7C13.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode7C13.melody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.name
meMA.Ode7C13.incipit = "Ἴδεν ὁ πλάσας κατὰ μόνας…"
meMA.Ode7C13.text = "Ἴδεν ὁ πλάσας κατὰ μόνας, τὰς καρδίας τῶν ἀνθρώπων Θεοφόρε, τῆς καρδίας τῆς σῆς, τὸ ἔνθεον καὶ θρόνοις, διδασκαλίας ἵδρυσε, Θεοφύλακτε τρισμάκαρ."

meMA.Ode7C1T.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode7C1T.melody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.name
meMA.Ode7C1T.incipit = "Σοὶ νῦν προσφεύγω Θεοτόκε, καὶ…"
meMA.Ode7C1T.text = "Σοὶ νῦν προσφεύγω Θεοτόκε, καὶ τῇ σκέπῃ σου καὶ θείᾳ προστασίᾳ, λυτρωθῆναι δεινῶν, δεόμενος πταισμάτων, εὐλογημένη πάναγνε, ἡ Θεὸν σαρκὶ τεκοῦσα."

meMA.Ode8C1H.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C1H.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C1H.greekmelody = he.h.m8_gr_GR_cog.heHE.EptaplasiosKaminon.greekname
meMA.Ode8C1H.melody = he.h.m8_gr_GR_cog.heHE.EptaplasiosKaminon.melody
meMA.Ode8C1H.incipit = he.h.m8_gr_GR_cog.heHE.EptaplasiosKaminon.incipit
meMA.Ode8C1H.text = he.h.m8_gr_GR_cog.heHE.EptaplasiosKaminon.text

meMA.Ode8C11.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C11.greekmelody = he.h.m8_gr_GR_cog.heHE.EptaplasiosKaminon.greekname
meMA.Ode8C11.melody = he.h.m8_gr_GR_cog.heHE.EptaplasiosKaminon.name
meMA.Ode8C11.incipit = "Εὐφραινομένων ἔνθα νῦν, κατοικία κατῴκησας…"
meMA.Ode8C11.text = "Εὐφραινομένων ἔνθα νῦν, κατοικία κατῴκησας, Ὁμολογητὰ τῆς ἀληθείας πρόμαχε, καὶ φῶς σοι ἀνέτειλεν, οἷα δικαίω πάνσοφε, καὶ τοὺς τῶν Μαρτύρων ἠθληκότι ἀγῶνας, μεθʼ ὧν νῦν ἀνακράζεις· Ἱερεῖς εὐλογεῖτε, λαὸς ὑπερυψοῦτε, Χριστὸν εἰς τοὺς αἰῶνας."

meMA.Ode8C12.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C12.melody = he.h.m8_gr_GR_cog.heHE.EptaplasiosKaminon.name
meMA.Ode8C12.incipit = "Ὑπομονῆς τὸν στέφανον, ἐκομίσω θεόληπτε…"
meMA.Ode8C12.text = "Ὑπομονῆς τὸν στέφανον, ἐκομίσω θεόληπτε· τὰς ὑπερορίας γὰρ πικρῶς κατῴκησας, μισθὸν ἐκδεχόμενος, ἀπολαβεῖν τῶν ἄθλων σου, καὶ τῆς ἀγαθῆς οὐ διεψεύσθης ἐλπίδος, Χριστῷ ἀνακραυγάζων· Ἱερεῖς εὐλογεῖτε, λαὸς ὑπερυψοῦτε, εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8C13.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C13.melody = he.h.m8_gr_GR_cog.heHE.EptaplasiosKaminon.name
meMA.Ode8C13.incipit = "Φωτοφανῶς ἡ μνήμη σου, θεοφόρε…"
meMA.Ode8C13.text = "Φωτοφανῶς ἡ μνήμη σου, θεοφόρε τελεῖται νῦν, τῆς ὁμολογίας φωτισμοῦ πυρσεύουσα· ἁγίαν εἰκόνα γάρ, τὴν τοῦ Χριστοῦ ἐτίμησας, καὶ τῆς Θεοτόκου, τῶν Ἁγίων τε πάντων, μεθʼ ὧν νῦν ἀναμέλπεις· Ἱερεῖς εὐλογεῖτε, λαὸς ὑπερυψοῦτε, Χριστὸν εἰς τοὺς αἰῶνας."

meMA.Ode8C1T.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C1T.melody = he.h.m8_gr_GR_cog.heHE.EptaplasiosKaminon.name
meMA.Ode8C1T.incipit = "Ἡ Θεομήτωρ πάναγνε, τῆς ψυχῆς…"
meMA.Ode8C1T.text = "Ἡ Θεομήτωρ πάναγνε, τῆς ψυχῆς μου τὰ τραύματα καὶ τῆς ἁμαρτίας τὰς οὐλὰς ἐξάλειψον, Θεὸν ἡ κυήσασα, τῶν ἐπὶ πάντων Κόρη ἁγνή, ἐκ παρθενικῆς ἀπειρογάμου νηδύος, ὃν Παῖδες εὐλογοῦσιν, Ἱερεῖς ἀνυμνοῦσι, λαοὶ ὑπερυψοῦσιν, εἰς πάντας τοὺς αἰῶνας."

meMA.Ode9C1H.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C1H.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode9C1H.greekmelody = he.h.m8_gr_GR_cog.heHE.KyriosTheotokon.greekname
meMA.Ode9C1H.melody = he.h.m8_gr_GR_cog.heHE.KyriosTheotokon.melody
meMA.Ode9C1H.incipit = he.h.m8_gr_GR_cog.heHE.KyriosTheotokon.incipit
meMA.Ode9C1H.text = he.h.m8_gr_GR_cog.heHE.KyriosTheotokon.text

meMA.Ode9C11.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode9C11.greekmelody = he.h.m8_gr_GR_cog.heHE.KyriosTheotokon.greekname
meMA.Ode9C11.melody = he.h.m8_gr_GR_cog.heHE.KyriosTheotokon.name
meMA.Ode9C11.incipit = "Μεγάλων χαρισμάτων, ὄντως ἠξιώθης, ὑπὲρ…"
meMA.Ode9C11.text = "Μεγάλων χαρισμάτων, ὄντως ἠξιώθης, ὑπὲρ Χριστοῦ κινδυνεύσας φρονήματι, καρτερικῷ θεοφόρε θεομακάριστε."

meMA.Ode9C12.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode9C12.melody = he.h.m8_gr_GR_cog.heHE.KyriosTheotokon.name
meMA.Ode9C12.incipit = "Ἡμᾶς τοὺς σὲ ὑμνοῦντας, Μάκαρ…"
meMA.Ode9C12.text = "Ἡμᾶς τοὺς σὲ ὑμνοῦντας, Μάκαρ ἐποπτεύεις, ἐκ πειρασμῶν καὶ κινδύνων λυτρούμενος, καὶ τῶν αἱρέσεων θραύων τὰ μηχανήματα."

meMA.Ode9C13.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode9C13.melody = he.h.m8_gr_GR_cog.heHE.KyriosTheotokon.name
meMA.Ode9C13.incipit = "Σωφρόνως καὶ δικαίως, ὡς Ἱεροφάντωρ…"
meMA.Ode9C13.text = "Σωφρόνως καὶ δικαίως, ὡς Ἱεροφάντωρ, πολιτευσάμενος Μάρτυς ἐγένου λαμπρός, τὴν σὴν πορφύραν ὑφάνας ἐκ τῶν αἱμάτων σου."

meMA.Ode9C1T.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode9C1T.melody = he.h.m8_gr_GR_cog.heHE.KyriosTheotokon.name
meMA.Ode9C1T.incipit = "Ὡς ὄρθρος τοῖς ἐν σκότει…"
meMA.Ode9C1T.text = "Ὡς ὄρθρος τοῖς ἐν σκότει, καὶ πεπλανημένοις, δικαιοσύνης τὸν Ἥλιον ἔχουσα, ἐν ταῖς ἀγκάλαις Παρθένε Χριστὸν ἀνέτειλας."

