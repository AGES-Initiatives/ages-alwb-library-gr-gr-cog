alt1.meVE.Apolytikion1.mode = miscellanea_gr_GR_cog.misc.Mode3
alt1.meVE.Apolytikion1.greekmelody = he.a.m3_gr_GR_cog.heAU.TheiasPisteos.greekname
alt1.meVE.Apolytikion1.melody = he.a.m3_gr_GR_cog.heAU.TheiasPisteos.name
alt1.meVE.Apolytikion1.incipit = "Θείας πίστεως, διδασκαλίᾳ, κατεκόσμησας, τὴν…"
alt1.meVE.Apolytikion1.text = "Θείας πίστεως, διδασκαλίᾳ, κατεκόσμησας, τὴν Ἐκκλησίαν, ζηλωτὴς τῶν Ἀποστόλων γενόμενος· καὶ κατασπείρας τὰ θεῖα διδάγματα, μαρτυρικῶς τὸν ἀγῶνα ἐτέλεσας. Κοσμᾶ ἔνδοξε, Χριστὸν τὸν Θεὸν ἱκέτευε, δωρήσασθαι ἡμῖν τὸ μέγα ἔλεος."

alt1.meMA.Kathisma11.mode = miscellanea_gr_GR_cog.misc.Mode1
alt1.meMA.Kathisma11.greekmelody = he.a.m1_gr_GR_cog.heAU.TonTafonSouSotir.greekname
alt1.meMA.Kathisma11.melody = he.a.m1_gr_GR_cog.heAU.TonTafonSouSotir.name
alt1.meMA.Kathisma11.incipit = "Ὡς ἔλαβες σοφέ, οὐρανόθεν τὴν…"
alt1.meMA.Kathisma11.text = "Ὡς ἔλαβες σοφέ, οὐρανόθεν τὴν χάριν, ἐξῆλθες ἐν σπουδῇ, ἐκ τοῦ Ἄθω κηρύττων, ὡς ἄλλος Ἀπόστολος, εὐσεβείας διδάγματα. Ὅθεν σήμερον, τὴν φωτοφόρον σου μνήμην, ἑορτάζοντες, τοὺς σοὺς ἀγῶνας τιμῶμεν, Κοσμᾶ Ἰσαπόστολε."

alt1.meMA.Kathisma1T.mode = miscellanea_gr_GR_cog.misc.Mode1
alt1.meMA.Kathisma1T.greekmelody = he.a.m1_gr_GR_cog.heAU.TonTafonSouSotir.greekname
alt1.meMA.Kathisma1T.melody = he.a.m1_gr_GR_cog.heAU.TonTafonSouSotir.name
alt1.meMA.Kathisma1T.incipit = "Κυήσασα σαρκί, τὸν Ὑπέρθεον Λόγον…"
alt1.meMA.Kathisma1T.text = "Κυήσασα σαρκί, τὸν Ὑπέρθεον Λόγον, ἀτρέπτως ὑπὲρ νοῦν, διὰ πλῆθος ἐλέους, ἠφάνισας Πανάχραντε, τὴν ἀρὰν τῆς Προμήτορος, καὶ ἐπήγασας, τὴν εὐλογίαν τῷ κόσμῳ· ὅθεν Δέσποινα, ὡς σωτηρίας σε πύλην, συμφώνως δοξάζομεν."

alt1.meMA.Kathisma21.mode = miscellanea_gr_GR_cog.misc.Mode4
alt1.meMA.Kathisma21.greekmelody = he.a.m4_gr_GR_cog.heAU.OYpsotheisEnToStavro.greekname
alt1.meMA.Kathisma21.melody = he.a.m4_gr_GR_cog.heAU.OYpsotheisEnToStavro.name
alt1.meMA.Kathisma21.incipit = "Εὐαγγελίου τῆς εἰρήνης ἐκφάντωρ, καὶ…"
alt1.meMA.Kathisma21.text = "Εὐαγγελίου τῆς εἰρήνης ἐκφάντωρ, καὶ εὐσεβείας ἀληθὴς φυτοκόμος, τοῖς τῆς δουλείας χρόνοις ὤφθης ἔνδοξε· πᾶσι γὰρ διέσπειρας, τὸν τῆς πίστεως λόγον, ἤθη δὲ ἐκάθηρας, καὶ ψυχῶν καταστάσεις, καὶ τοὺς ἀνθρώπους ἴθυνας Κοσμᾶ, πρὸς ἀληθείας, ὁδὸν τὴν σωτήριον."

alt1.meMA.Kathisma2T.mode = miscellanea_gr_GR_cog.misc.Mode4
alt1.meMA.Kathisma2T.greekmelody = he.a.m4_gr_GR_cog.heAU.OYpsotheisEnToStavro.greekname
alt1.meMA.Kathisma2T.melody = he.a.m4_gr_GR_cog.heAU.OYpsotheisEnToStavro.name
alt1.meMA.Kathisma2T.incipit = "Ἀνερμηνεύτως Θεοτόκε τεκοῦσα, τὸν πᾶσι…"
alt1.meMA.Kathisma2T.text = "Ἀνερμηνεύτως Θεοτόκε τεκοῦσα, τὸν πᾶσι Κόρη τὴν ζωὴν χορηγοῦντα, καὶ μετὰ τόκον μείνασα πανάφθορος, πάσης φθορᾶς λύτρωσαι, τὴν ῥυπῶσαν ψυχήν μου, καὶ τὸν νοῦν μου τείχισον, οὐρανίαις ἐννοίαις, ἵνα σωφρόνως ζήσας ἐπὶ γῆς, τῆς αἰωνίου, ζωῆς ὀφθῶ μέτοχος."

alt1.meMA.Kathisma31.mode = miscellanea_gr_GR_cog.misc.Mode4
alt1.meMA.Kathisma31.greekmelody = he.a.m4_gr_GR_cog.heAU.TachyProkatalave.greekname
alt1.meMA.Kathisma31.melody = he.a.m4_gr_GR_cog.heAU.TachyProkatalave.name
alt1.meMA.Kathisma31.incipit = "Τὴν θείαν ἐπίπνοιαν, τοῦ Παρακλήτου…"
alt1.meMA.Kathisma31.text = "Τὴν θείαν ἐπίπνοιαν, τοῦ Παρακλήτου σοφέ, ὑψόθεν δεξάμενος, τῇ καθαρᾷ σου ψυχῇ, ὡς ἄλλος Ἀπόστολος, πόλεις τε καὶ χωρία, τῆς Ἑλλάδος διῆλθες, ἅπασι καταγγέλλων, σωτηρίας τὸν λόγον, καὶ ἤθλησας νομίμως, Κοσμᾶ θεομακάριστε."

alt1.meMA.Kathisma3T.mode = miscellanea_gr_GR_cog.misc.Mode4
alt1.meMA.Kathisma3T.greekmelody = he.a.m4_gr_GR_cog.heAU.TachyProkatalave.greekname
alt1.meMA.Kathisma3T.melody = he.a.m4_gr_GR_cog.heAU.TachyProkatalave.name
alt1.meMA.Kathisma3T.incipit = "Ἀφράστως κυήσασα, τὸν Ποιητὴν τοῦ…"
alt1.meMA.Kathisma3T.text = "Ἀφράστως κυήσασα, τὸν Ποιητὴν τοῦ παντός, διέσωσας Ἄχραντε, ἐκ τῆς ἀρχαίας ἀρᾶς, τοὺς πίστει δοξάζοντας, χείλεσι καὶ καρδίᾳ, τὸν σὸν ἄῤῥητον τόκον, Δέσποινα Θεοτόκε, ἱλαστήριον κόσμου· διὸ κἀμοὶ παράσχου, τὴν θείαν σου ἀντίληψιν."

alt1.meMA.PostGospelGlory.mode = miscellanea_gr_GR_cog.misc.Mode2
alt1.meMA.PostGospelGlory.melody = ""
alt1.meMA.PostGospelGlory.incipit = ho.ho03_gr_GR_cog.hoMA.PostGospelTeacher.incipit
alt1.meMA.PostGospelGlory.text = ho.ho03_gr_GR_cog.hoMA.PostGospelTeacher.text

alt1.meMA.PostGospelBoth.mode = miscellanea_gr_GR_cog.misc.Mode2
alt1.meMA.PostGospelBoth.melody = ""
alt1.meMA.PostGospelBoth.incipit = ho.ho03_gr_GR_cog.hoMA.PostGospelTheotokos.incipit
alt1.meMA.PostGospelBoth.text = ho.ho03_gr_GR_cog.hoMA.PostGospelTheotokos.text

alt1.meMA.PostGospelIdiomelon.mode = miscellanea_gr_GR_cog.misc.Mode6
alt1.meMA.PostGospelIdiomelon.melody = ""
alt1.meMA.PostGospelIdiomelon.incipit = "Διδασκαλίας τῷ φωτί, ὡς ἥλιος…"
alt1.meMA.PostGospelIdiomelon.text = "Διδασκαλίας τῷ φωτί, ὡς ἥλιος ἀνατείλας, ἐν χρόνοις δουλείας ζοφερᾶς, εὐσεβείας αἴγλην παρέσχες, ταῖς καρδίαις τῶν πιστῶν, ἀμαυρωθείσαις ἀπάτῃ τῆς πλάνης· στόμα γὰρ θεῖον ὤφθης, καὶ ἔργον Εὐαγγελιστοῦ ἀνεδέξω, ἐν περιουσίᾳ σοφίας κρείττονος, καὶ γνώσει θειοτέρᾳ. Ἀλλʼ ὦ Ἰσαπόστολε Κοσμᾶ, τῷ Ζωοδότῃ Λόγῳ πρέσβευε, ὑπὲρ τῶν τιμώντων σε."

alt1.meMA.C1.acrostic.title = "Κανὼν τοῦ Ἁγίου,"
alt1.meMA.C1.acrostic.notice1 = "οὗ ἡ Ἀκροστιχίς·"
alt1.meMA.C1.acrostic.text = "Κοσμᾶ, σοὶ ἔνθεον ᾄδω μέλος. Γερασίμου."
alt1.meMA.C1.title = titles_gr_GR_cog.ti.CanonSaintM
alt1.meMA.C1.verse = prayers_gr_GR_cog.pr.verse_SaintM.text

alt1.meMA.Ode1C11.ode = miscellanea_gr_GR_cog.misc.Ode1
alt1.meMA.Ode1C11.mode = miscellanea_gr_GR_cog.misc.Mode4
alt1.meMA.Ode1C11.greekmelody = he.h.m4_gr_GR_cog.heHE.AnoixoToStomaMou.greekname
alt1.meMA.Ode1C11.melody = he.h.m4_gr_GR_cog.heHE.AnoixoToStomaMou.name
alt1.meMA.Ode1C11.incipit = "Καταύγασον Κύριε, τὴν σκοτισθεῖσαν καρδίαν…"
alt1.meMA.Ode1C11.text = "Καταύγασον Κύριε, τὴν σκοτισθεῖσαν καρδίαν μου, φωτὶ τῆς σῆς χάριτος, ὅπως ὑμνήσω Χριστέ, θείοις ᾄσμασι, Κοσμᾶν τὸν θεηγόρον, τὸν πᾶσι κηρύξαντα, λόγον τὸν ἔνθεον."

alt1.meMA.Ode1C12.ode = miscellanea_gr_GR_cog.misc.Ode1
alt1.meMA.Ode1C12.mode = miscellanea_gr_GR_cog.misc.Mode4
alt1.meMA.Ode1C12.melody = he.h.m4_gr_GR_cog.heHE.AnoixoToStomaMou.name
alt1.meMA.Ode1C12.incipit = "Ὁ κῆρυξ ὁ ἔνθεος, Εὐαγγελίου…"
alt1.meMA.Ode1C12.text = "Ὁ κῆρυξ ὁ ἔνθεος, Εὐαγγελίου τῆς χάριτος, ἡ σάλπιγξ ἡ εὔσημος, τῶν ἱερῶν διδαχῶν, ἀνυμνείσθω μοι, Κοσμᾶς ὁ θεηγόρος, τὸ στόμα τὸ πάνσοφον, βίου σεμνότητος."

alt1.meMA.Ode1C13.ode = miscellanea_gr_GR_cog.misc.Ode1
alt1.meMA.Ode1C13.mode = miscellanea_gr_GR_cog.misc.Mode4
alt1.meMA.Ode1C13.melody = he.h.m4_gr_GR_cog.heHE.AnoixoToStomaMou.name
alt1.meMA.Ode1C13.incipit = "Σοφίᾳ τοῦ Πνεύματος, καταυγασθεὶς τὴν…"
alt1.meMA.Ode1C13.text = "Σοφίᾳ τοῦ Πνεύματος, καταυγασθεὶς τὴν διάνοιαν, φωστὴρ ὥσπερ μέγιστος, ἐξ Ἄθω ἔλαμψας, καὶ ἐπύρσευσας, τῶν διδαχῶν ἀκτῖσι, Κοσμᾶ Ἰσαπόστολε, Ἑλλάδα ἅπασαν."

alt1.meMA.Ode1C1T.ode = miscellanea_gr_GR_cog.misc.Ode1
alt1.meMA.Ode1C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
alt1.meMA.Ode1C1T.melody = he.h.m4_gr_GR_cog.heHE.AnoixoToStomaMou.name
alt1.meMA.Ode1C1T.incipit = "Μὴ φλέξας τὴν μήτραν σου…"
alt1.meMA.Ode1C1T.text = "Μὴ φλέξας τὴν μήτραν σου, πυρὶ Ἁγνὴ τῆς Θεότητος, ὁ Λόγος ὁ ἄναρχος, ἐν σοὶ ἐσκήνωσε, καὶ σεσάρκωται, ἐκ σοῦ ἀνερμηνεύτως· διὸ Θεοτόκον σε πάντες κηρύττομεν."

alt1.meMA.Ode3C11.ode = miscellanea_gr_GR_cog.misc.Ode3
alt1.meMA.Ode3C11.mode = miscellanea_gr_GR_cog.misc.Mode4
alt1.meMA.Ode3C11.greekmelody = he.h.m4_gr_GR_cog.heHE.TousSousYmnologous.greekname
alt1.meMA.Ode3C11.melody = he.h.m4_gr_GR_cog.heHE.TousSousYmnologous.name
alt1.meMA.Ode3C11.incipit = "Ἀγάπῃ τρωθεὶς Πάτερ τῇ θείᾳ…"
alt1.meMA.Ode3C11.text = "Ἀγάπῃ τρωθεὶς Πάτερ τῇ θείᾳ, ἠγάπησας ὥσπερ σεαυτόν, καὶ τὸν πλησίον Ἅγιε, ὑπὲρ οὗ πλείστους ἤνεγκας, κινδύνους ὡς ὁμότροπος, τῶν Ἀποστόλων ἐν ἅπασι."

alt1.meMA.Ode3C12.ode = miscellanea_gr_GR_cog.misc.Ode3
alt1.meMA.Ode3C12.mode = miscellanea_gr_GR_cog.misc.Mode4
alt1.meMA.Ode3C12.melody = he.h.m4_gr_GR_cog.heHE.TousSousYmnologous.name
alt1.meMA.Ode3C12.incipit = "Συνέσει τῇ θείᾳ διαλάμπων, κηρύττεις…"
alt1.meMA.Ode3C12.text = "Συνέσει τῇ θείᾳ διαλάμπων, κηρύττεις τὸν λόγον πανταχοῦ, τῆς εὐσεβείας Ὅσιε, ὡς θεῖος Ἰσαπόστολος, καὶ τῶν πιστῶν ἐστήριξας, τῷ θείῳ φόβῳ τὸ φρόνημα."

alt1.meMA.Ode3C13.ode = miscellanea_gr_GR_cog.misc.Ode3
alt1.meMA.Ode3C13.mode = miscellanea_gr_GR_cog.misc.Mode4
alt1.meMA.Ode3C13.melody = he.h.m4_gr_GR_cog.heHE.TousSousYmnologous.name
alt1.meMA.Ode3C13.incipit = "Ὁ νοῦς σου δεξάμενος ἐν…"
alt1.meMA.Ode3C13.text = "Ὁ νοῦς σου δεξάμενος ἐν Ἄθῳ, τὰς θείας ἐλλάμψεις μυστικῶς, Κοσμᾶ θεομακάριστε, διδάγματα σωτήρια, οἷα κρουνὸς ἀνέβλυσεν, εἰς εὐσεβῶν περιποίησιν."

alt1.meMA.Ode3C1T.ode = miscellanea_gr_GR_cog.misc.Ode3
alt1.meMA.Ode3C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
alt1.meMA.Ode3C1T.melody = he.h.m4_gr_GR_cog.heHE.TousSousYmnologous.name
alt1.meMA.Ode3C1T.incipit = "Ἰσχύν μοι παράσχου Θεοτόκε, πατεῖν…"
alt1.meMA.Ode3C1T.text = "Ἰσχύν μοι παράσχου Θεοτόκε, πατεῖν τοῦ ἐχθροῦ τὰς μηχανάς, τοῦ καθʼ ἑκάστην Πάναγνε, ὑπούλως ἐφορμῶντός μοι, καὶ τὴν ψυχήν μου στήριξον, τοῖς τοῦ Υἱοῦ σου προστάγμασι."

alt1.meMA.Ode3Kathisma1.mode = miscellanea_gr_GR_cog.misc.Mode4
alt1.meMA.Ode3Kathisma1.greekmelody = he.a.m4_gr_GR_cog.heAU.TachyProkatalave.greekname
alt1.meMA.Ode3Kathisma1.melody = he.a.m4_gr_GR_cog.heAU.TachyProkatalave.name
alt1.meMA.Ode3Kathisma1.incipit = "Σοφίᾳ τῶν λόγων σου, καὶ…"
alt1.meMA.Ode3Kathisma1.text = "Σοφίᾳ τῶν λόγων σου, καὶ τῶν θαυμάτων φωτί, πολλοὺς κατεφώτισας, τοὺς ἐν νυκτὶ τῶν παθῶν, καὶ πλάνης ὑπάρχοντας· ἴχνεσι γὰρ τῶν θείων, Ἀποστόλων βαδίσας, τούτων τὸ ἔργον Πάτερ, ἀκριβῶς ἐμιμήσω· διό σε ἡ θεία χάρις, Κοσμᾶ ἐθαυμάστωσε."

alt1.meMA.Ode3KathismaT.mode = miscellanea_gr_GR_cog.misc.Mode4
alt1.meMA.Ode3KathismaT.melody = he.a.m4_gr_GR_cog.heAU.TachyProkatalave.name
alt1.meMA.Ode3KathismaT.incipit = "Μυρίοις βαλλόμενοι, τῇ ἐπηρείᾳ ἐχθροῦ…"
alt1.meMA.Ode3KathismaT.text = "Μυρίοις βαλλόμενοι, τῇ ἐπηρείᾳ ἐχθροῦ, κινδύνοις καὶ θλίψεσι, καὶ λογισμοῖς χαλεποῖς, Παρθένε πανύμνητε, σπεύδομεν τῇ σῇ σκέπῃ, σωτηρίαν αἰτοῦντες· λύτρωσαι Θεοτόκε, τῆς αὐτῶν ἡμᾶς ζάλης, καὶ δίδου τῇ ζωῇ ἡμῶν, εἰρήνην καὶ μετάνοιαν."

alt1.meMA.Ode4C11.ode = miscellanea_gr_GR_cog.misc.Ode4
alt1.meMA.Ode4C11.mode = miscellanea_gr_GR_cog.misc.Mode4
alt1.meMA.Ode4C11.greekmelody = he.h.m4_gr_GR_cog.heHE.OKathimenosEnDoxi.greekname
alt1.meMA.Ode4C11.melody = he.h.m4_gr_GR_cog.heHE.OKathimenosEnDoxi.name
alt1.meMA.Ode4C11.incipit = "Ἐν καιρῷ τῷ τῆς δουλείας…"
alt1.meMA.Ode4C11.text = "Ἐν καιρῷ τῷ τῆς δουλείας, καὶ πικρᾶς χαλεπότητος, ἀπεστάλης Πάτερ, παρὰ τοῦ Θεοῦ Παντοκράτορος, τῶν κλονουμένων στηρίξαι τὴν διάνοιαν, καὶ ἐκλείποντας, λόγῳ σοφῷ ἀνασώσασθαι."

alt1.meMA.Ode4C12.ode = miscellanea_gr_GR_cog.misc.Ode4
alt1.meMA.Ode4C12.mode = miscellanea_gr_GR_cog.misc.Mode4
alt1.meMA.Ode4C12.melody = he.h.m4_gr_GR_cog.heHE.OKathimenosEnDoxi.name
alt1.meMA.Ode4C12.incipit = "Νόμον θεῖον καταγγέλλων, ὡς Ἀπόστολος…"
alt1.meMA.Ode4C12.text = "Νόμον θεῖον καταγγέλλων, ὡς Ἀπόστολος ἔνθεος, πόλεις καὶ χωρία, Πάτερ καὶ τὰς νήσους διέδραμες, καὶ τοὺς ἐν σκότει παθῶν κειμένους ἤγειρας, Κοσμᾶ ἔνδοξε, πρὸς τὴν αὐγὴν τὴν σωτήριον."

alt1.meMA.Ode4C13.ode = miscellanea_gr_GR_cog.misc.Ode4
alt1.meMA.Ode4C13.mode = miscellanea_gr_GR_cog.misc.Mode4
alt1.meMA.Ode4C13.melody = he.h.m4_gr_GR_cog.heHE.OKathimenosEnDoxi.name
alt1.meMA.Ode4C13.incipit = "Θεοῤῥήμονί σου γλώττῃ, τὸ τῆς…"
alt1.meMA.Ode4C13.text = "Θεοῤῥήμονί σου γλώττῃ, τὸ τῆς πίστεως κήρυγμα, ἐν τῷ Βυζαντίῳ, Πάτερ θεοσόφως ἐκήρυξας, Θεσσαλονίκην καὶ Βέῤῥοιαν δὲ ηὔγασας, Κοσμᾶ Ἅγιε, τῶν διδαχῶν σου ταῖς χάρισι."

alt1.meMA.Ode4C1T.ode = miscellanea_gr_GR_cog.misc.Ode4
alt1.meMA.Ode4C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
alt1.meMA.Ode4C1T.melody = he.h.m4_gr_GR_cog.heHE.OKathimenosEnDoxi.name
alt1.meMA.Ode4C1T.incipit = "Ἐκ γαστρός σου σαρκοφόρος, ὑπὲρ…"
alt1.meMA.Ode4C1T.text = "Ἐκ γαστρός σου σαρκοφόρος, ὑπὲρ λόγον καὶ ἔννοιαν, Δέσποινα προῆλθεν, ὁ διδοὺς τοῖς πᾶσι τὴν ὕπαρξιν, ἀναλλοιώτως ἀτρέπτως καὶ διέσωσε, τοὺς δοξάζοντας, τὰ μεγαλεῖά σου Ἄχραντε."

alt1.meMA.Ode5C11.ode = miscellanea_gr_GR_cog.misc.Ode5
alt1.meMA.Ode5C11.mode = miscellanea_gr_GR_cog.misc.Mode4
alt1.meMA.Ode5C11.greekmelody = he.h.m4_gr_GR_cog.heHE.ExestiTaSympanta.greekname
alt1.meMA.Ode5C11.melody = he.h.m4_gr_GR_cog.heHE.ExestiTaSympanta.name
alt1.meMA.Ode5C11.incipit = "Ὁ μέγας διδάσκαλος, καὶ ὑποφήτης…"
alt1.meMA.Ode5C11.text = "Ὁ μέγας διδάσκαλος, καὶ ὑποφήτης ἔνθεος, τοῦ Εὐαγγελίου τῆς εἰρήνης, ζήλῳ τοῦ Παύλου καταφλεχθεὶς τὴν ψυχήν, πᾶσαν τὴν Ἑλλάδα διελθών, μέχρις Ἰλλυρίας δέ, Κοσμᾶ χαίρων ἐκήρυξας."

alt1.meMA.Ode5C12.ode = miscellanea_gr_GR_cog.misc.Ode5
alt1.meMA.Ode5C12.mode = miscellanea_gr_GR_cog.misc.Mode4
alt1.meMA.Ode5C12.melody = he.h.m4_gr_GR_cog.heHE.ExestiTaSympanta.name
alt1.meMA.Ode5C12.incipit = "Νοσούσας ἰάτρευσας, ψυχὰς λόγῳ τῆς…"
alt1.meMA.Ode5C12.text = "Νοσούσας ἰάτρευσας, ψυχὰς λόγῳ τῆς χάριτος, καὶ τὰς τῶν σωμάτων ἀσθενείας, ἰᾶσαι Πάτερ τῇ ἐπικλήσει Χριστοῦ, Ἀποστολικὴν χάριν λαβών, Κοσμᾶ Ἰσαπόστολε, ἐν ὑστέροις τοῖς ἔτεσι."

alt1.meMA.Ode5C13.ode = miscellanea_gr_GR_cog.misc.Ode5
alt1.meMA.Ode5C13.mode = miscellanea_gr_GR_cog.misc.Mode4
alt1.meMA.Ode5C13.melody = he.h.m4_gr_GR_cog.heHE.ExestiTaSympanta.name
alt1.meMA.Ode5C13.incipit = "Ἀκτῖσι τῶν λόγων σου, Κοσμᾶ…"
alt1.meMA.Ode5C13.text = "Ἀκτῖσι τῶν λόγων σου, Κοσμᾶ θεόφρον Ὅσιε, ἅπασαν τὴν Ἤπειρον λαμπρύνεις, Κεφαλληνίαν δὲ καὶ τὴν Ζάκυνθον, καὶ τὴν Ναυπακτίαν δαψιλῶς, ἄρδεις θείοις νάμασι, τῶν σοφῶν διδαγμάτων σου."

alt1.meMA.Ode5C1T.ode = miscellanea_gr_GR_cog.misc.Ode5
alt1.meMA.Ode5C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
alt1.meMA.Ode5C1T.melody = he.h.m4_gr_GR_cog.heHE.ExestiTaSympanta.name
alt1.meMA.Ode5C1T.incipit = "Δυνάμωσον Δέσποινα, τὴν ἀσθενοῦσαν πάθεσι…"
alt1.meMA.Ode5C1T.text = "Δυνάμωσον Δέσποινα, τὴν ἀσθενοῦσαν πάθεσι, καὶ ταῖς προσβολαῖς τοῦ ἀλλοτρίου, ψυχήν μου Κόρη τῇ συμπαθεῖ σου ῥοπῇ, καὶ κατεύθυνόν με πρὸς ὁδόν, μετανοίας δέομαι, ὁ προστρέχων τῇ σκέπῃ σου."

alt1.meMA.Ode6C11.ode = miscellanea_gr_GR_cog.misc.Ode6
alt1.meMA.Ode6C11.mode = miscellanea_gr_GR_cog.misc.Mode4
alt1.meMA.Ode6C11.greekmelody = he.h.m4_gr_GR_cog.heHE.TinTheianTaftin.greekname
alt1.meMA.Ode6C11.melody = he.h.m4_gr_GR_cog.heHE.TinTheianTaftin.name
alt1.meMA.Ode6C11.incipit = "Ὡς Ἀποστόλων ὁμόζηλος, τοὺς δρόμους…"
alt1.meMA.Ode6C11.text = "Ὡς Ἀποστόλων ὁμόζηλος, τοὺς δρόμους τοὺς αὐτῶν Πάτερ ἤνυσας, στεῤῥῷ φρονήματι, καὶ φυτοκόμος θεόσοφος, τῆς εὐσεβείας ὤφθης, Κοσμᾶ μακάριε."

alt1.meMA.Ode6C12.ode = miscellanea_gr_GR_cog.misc.Ode6
alt1.meMA.Ode6C12.mode = miscellanea_gr_GR_cog.misc.Mode4
alt1.meMA.Ode6C12.melody = he.h.m4_gr_GR_cog.heHE.TinTheianTaftin.name
alt1.meMA.Ode6C12.incipit = "Μέγαν διδάσκαλον Ἅγιε, γνωρίζει σε…"
alt1.meMA.Ode6C12.text = "Μέγαν διδάσκαλον Ἅγιε, γνωρίζει σε ἡ Ἤπειρος ἅπασα· ταύτην γὰρ ἔσωσας, λόγοις σοφοῖς καὶ διδάγμασι, βυθοῦ τῆς ἀσεβείας, Κοσμᾶ μακάριε."

alt1.meMA.Ode6C13.ode = miscellanea_gr_GR_cog.misc.Ode6
alt1.meMA.Ode6C13.mode = miscellanea_gr_GR_cog.misc.Mode4
alt1.meMA.Ode6C13.melody = he.h.m4_gr_GR_cog.heHE.TinTheianTaftin.name
alt1.meMA.Ode6C13.incipit = "Ἐξ Αἰτωλίας ἀνέτειλας, καὶ πάσῃ…"
alt1.meMA.Ode6C13.text = "Ἐξ Αἰτωλίας ἀνέτειλας, καὶ πάσῃ τῇ Ἑλλάδι ἐξέλαμψας, ζήλῳ τῆς πίστεως· διὸ λαμπρῶς μεγαλύνει σε, ἡ Ἐκκλησία Πάτερ, ὡς Ἰσαπόστολον."

alt1.meMA.Ode6C1T.ode = miscellanea_gr_GR_cog.misc.Ode6
alt1.meMA.Ode6C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
alt1.meMA.Ode6C1T.melody = he.h.m4_gr_GR_cog.heHE.TinTheianTaftin.name
alt1.meMA.Ode6C1T.incipit = "Λυχνία ὤφθης ἑπτάφωτος, τεκοῦσα τοῦ…"
alt1.meMA.Ode6C1T.text = "Λυχνία ὤφθης ἑπτάφωτος, τεκοῦσα τοῦ Πατρὸς τὸ ἀπαύγασμα, σαρκὸς παχύτητι, Θεοχαρίτωτε Δέσποινα, εἰς σωτηρίαν κόσμου, διʼ ἀγαθότητα."

alt1.meMA.Kontakion1.mode = miscellanea_gr_GR_cog.misc.Mode4
alt1.meMA.Kontakion1.greekmelody = he.a.m4_gr_GR_cog.heAU.EpefanisSimeron.greekname
alt1.meMA.Kontakion1.melody = he.a.m4_gr_GR_cog.heAU.EpefanisSimeron.name
alt1.meMA.Kontakion1.incipit = "Ὡς φωστὴρ νεόφωτος τὴν Ἐκκλησίαν…"
alt1.meMA.Kontakion1.text = "Ὡς φωστὴρ νεόφωτος τὴν Ἐκκλησίαν, καταυγάζεις ἅπασαν, Εὐαγγελίου διδαχαῖς, Κοσμᾶ Χριστοῦ Ἰσαπόστολε· διὸ ἀξίως, γεραίρει τὴν μνήμην σου."

alt1.meMA.Oikos1.text = "Τῶν Ἀποστόλων μιμητής, καὶ Ἐκκλησίας στῦλος, τῶν ἱερέων καλλονή, καὶ τῶν Ὁσίων τύπος, καὶ τῶν Μαρτύρων σύναθλος, ὡς κοινωνήσας τῶν ἀγώνων αὐτοῖς, Ἱερομάρτυς ἀναδέδειξαι· ἐξ Αἰτωλίας γὰρ ἀνατείλας, καὶ ἐν Ἄθῳ ἁγιασθείς, ἐν δὲ τῇ ἱερᾷ Φιλοθέου Μονῇ, τὴν θείαν ἀποστολὴν μυστικῶς παρὰ Θεοῦ δεξάμενος, ἐν πληροφορίᾳ θείᾳ, καὶ ἐνεργείᾳ θείου φωτός, ὡς Ἡσαΐας ὁ θεοειδέστατος, ἐξῆλθες κηρύσσων καὶ διδάσκων τοὺς λαούς, καὶ πολλοὺς ἐπιστρέφων πρὸς Κύριον, ἐξ ὁδοῦ ἀπωλείας· καὶ ἐκ Βυζαντίου τὴν σωτήριον ἀρξάμενος πορείαν, καὶ πλείστας πόλεις καὶ χώρας καὶ νήσους περιελθών, κατὰ Παῦλον τὸν μέγαν, εὐαγγελιζόμενος τὴν εἰρήνην καὶ τὰ ἀγαθὰ τῆς βασιλείας τῶν οὐρανῶν, πᾶσαν δὲ τὴν Ἤπειρον παριλαβῶν τῷ φθόγγῳ, μαρτυρικῶς Θεῷ ἐξεδήμησας, καὶ πλουσίαις δωρεαῖς κεκόσμησαι, Κοσμᾶ Ἰσαπόστολε· διὸ ἡ Ἐκκλησία Χριστοῦ ἀξίως, γεραίρει τὴν μνήμην σου."

alt1.meMA.Ode7C11.ode = miscellanea_gr_GR_cog.misc.Ode7
alt1.meMA.Ode7C11.mode = miscellanea_gr_GR_cog.misc.Mode4
alt1.meMA.Ode7C11.greekmelody = he.h.m4_gr_GR_cog.heHE.OukElatrefsan.greekname
alt1.meMA.Ode7C11.melody = he.h.m4_gr_GR_cog.heHE.OukElatrefsan.name
alt1.meMA.Ode7C11.incipit = "Οὐκ ἐνάρκησας, τοῖς δρόμοις τοῦ…"
alt1.meMA.Ode7C11.text = "Οὐκ ἐνάρκησας, τοῖς δρόμοις τοῦ κηρύγματος, Πάτερ θλιβόμενος, ἀλλὰ προθύμῳ ψυχῇ, τοῖς πᾶσιν ἐκήρυξας, λόγον τῆς πίστεως, καὶ διήγειρας, πρὸς ἐργασίαν Ἅγιε, ἐναρέτου πολιτείας."

alt1.meMA.Ode7C12.ode = miscellanea_gr_GR_cog.misc.Ode7
alt1.meMA.Ode7C12.mode = miscellanea_gr_GR_cog.misc.Mode4
alt1.meMA.Ode7C12.melody = he.h.m4_gr_GR_cog.heHE.OukElatrefsan.name
alt1.meMA.Ode7C12.incipit = "Σταυρὸν σύμβολον, τῆς θείας ἀγαθότητος…"
alt1.meMA.Ode7C12.text = "Σταυρὸν σύμβολον, τῆς θείας ἀγαθότητος, πηγνύεις Ἅγιε, ἀνακηρύττων σοφῶς, τὰ θεῖα διδάγματα, ὡς ἐνθεώτατος, τῷ δὲ λόγῳ σου, πλήθη πιστῶν παρείποντο, ὥσπερ ἄρνες τῷ ποιμένι."

alt1.meMA.Ode7C13.ode = miscellanea_gr_GR_cog.misc.Ode7
alt1.meMA.Ode7C13.mode = miscellanea_gr_GR_cog.misc.Mode4
alt1.meMA.Ode7C13.melody = he.h.m4_gr_GR_cog.heHE.OukElatrefsan.name
alt1.meMA.Ode7C13.incipit = "Γνῶσιν ἔνθεον, καὶ φόβον τὸν…"
alt1.meMA.Ode7C13.text = "Γνῶσιν ἔνθεον, καὶ φόβον τὸν σωτήριον, καὶ βίον κρείττονα, καὶ πολιτείαν σεμνήν, Κοσμᾶ ἐγεώργησας, ἀρότρῳ χάριτος, τοῖς τὸ πρότερον, ἐν ἀγνωσίᾳ Ἅγιε, καὶ ἀπάτῃ προσκειμένοις."

alt1.meMA.Ode7C1T.ode = miscellanea_gr_GR_cog.misc.Ode7
alt1.meMA.Ode7C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
alt1.meMA.Ode7C1T.melody = he.h.m4_gr_GR_cog.heHE.OukElatrefsan.name
alt1.meMA.Ode7C1T.incipit = "Ἔχεις Δέσποινα, πολλὴν πρὸς ὃν…"
alt1.meMA.Ode7C1T.text = "Ἔχεις Δέσποινα, πολλὴν πρὸς ὃν ἐκύησας, Θεὸν καὶ Κύριον, οἷάπερ Μήτηρ αὐτοῦ, παῤῥησίαν Ἄχραντε· διὸ μεσίτευσον, οἰκτιρῆσαί με, τὸν ἐν ἀμέτροις πάθεσι, κυλινδούμενον ἀφρόνως."

alt1.meMA.Ode8C11.ode = miscellanea_gr_GR_cog.misc.Ode8
alt1.meMA.Ode8C11.mode = miscellanea_gr_GR_cog.misc.Mode4
alt1.meMA.Ode8C11.greekmelody = he.h.m4_gr_GR_cog.heHE.PaidasEvageis.greekname
alt1.meMA.Ode8C11.melody = he.h.m4_gr_GR_cog.heHE.PaidasEvageis.name
alt1.meMA.Ode8C11.incipit = "Ῥείθροις τῶν ἐνθέων διδαχῶν σου…"
alt1.meMA.Ode8C11.text = "Ῥείθροις τῶν ἐνθέων διδαχῶν σου, Κοσμᾶ Ἰσαπόστολε κατήρδευσας, Ἐκκλησίας Ἅγιε, πάντα τὰ πληρώματα, καὶ πᾶσαν τάξιν ἴθυνας πρὸς βίον ἔνθεον· μεγίστη ὡς πηγὴ γὰρ προῆλθες, κατὰ τὸν Προφήτην, ἐξ Ἄθω θείᾳ νεύσει."

alt1.meMA.Ode8C12.ode = miscellanea_gr_GR_cog.misc.Ode8
alt1.meMA.Ode8C12.mode = miscellanea_gr_GR_cog.misc.Mode4
alt1.meMA.Ode8C12.melody = he.h.m4_gr_GR_cog.heHE.PaidasEvageis.name
alt1.meMA.Ode8C12.incipit = "Ἄρτῳ τῶν ἁγίων διδαχῶν σου…"
alt1.meMA.Ode8C12.text = "Ἄρτῳ τῶν ἁγίων διδαχῶν σου, καιρῷ νοητοῦ λιμοῦ διέθρεψας, πλήθη πιστῶν Ἅγιε, πεινώντων τὰ κρείττονα, καὶ σεμνοτάτοις ἤθεσι Κοσμᾶ ἐκόσμησας, τοὺς πρὶν ἐν ἀκοσμίᾳ βιοῦντας, βίου ἐσφαλμένου, ψυχῶν ὡς οἰκονόμος."

alt1.meMA.Ode8C13.ode = miscellanea_gr_GR_cog.misc.Ode8
alt1.meMA.Ode8C13.mode = miscellanea_gr_GR_cog.misc.Mode4
alt1.meMA.Ode8C13.melody = he.h.m4_gr_GR_cog.heHE.PaidasEvageis.name
alt1.meMA.Ode8C13.incipit = "Στόμα τοῦ Θεοῦ ἐδείχθης Πάτερ…"
alt1.meMA.Ode8C13.text = "Στόμα τοῦ Θεοῦ ἐδείχθης Πάτερ, καὶ στόματος ἐχθροῦ λόγῳ τῆς χάριτος, οὓς πρὶν βρῶμα ἔσχηκεν, ἥρπασας καὶ ἴθυνας, πρὸς ἐντολῶν ἐκπλήρωσιν τοῦ Παντοκράτορος, ὡς μέγας εὐσεβείας ἐκφάντωρ, καὶ τῆς μετανοίας, μυσταγωγὸς θεόφρον."

alt1.meMA.Ode8C1T.ode = miscellanea_gr_GR_cog.misc.Ode8
alt1.meMA.Ode8C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
alt1.meMA.Ode8C1T.melody = he.h.m4_gr_GR_cog.heHE.PaidasEvageis.name
alt1.meMA.Ode8C1T.incipit = "Ἵνα μεγαλύνω σου τὴν δόξαν…"
alt1.meMA.Ode8C1T.text = "Ἵνα μεγαλύνω σου τὴν δόξαν, Παρθένε Εὐλογημένη Ὑπερένδοξε, ἀδοξίας ῥῦσαί με, καὶ δεινῆς φαυλότητος, παθῶν ἀλλοτριούντων με τοῦ ἀγαθοῦ σου Υἱοῦ, καὶ λάμπρυνον φωτὶ μετανοίας, τὴν ἐσκοτισμένην, ψυχήν μου τῇ ἀπάτῃ."

alt1.meMA.Ode9C11.ode = miscellanea_gr_GR_cog.misc.Ode9
alt1.meMA.Ode9C11.mode = miscellanea_gr_GR_cog.misc.Mode4
alt1.meMA.Ode9C11.greekmelody = he.h.m4_gr_GR_cog.heHE.ApasGigenis.greekname
alt1.meMA.Ode9C11.melody = he.h.m4_gr_GR_cog.heHE.ApasGigenis.name
alt1.meMA.Ode9C11.incipit = "Μάρτυς εὐκλεής, καὶ θεῖος Ἀπόστολος…"
alt1.meMA.Ode9C11.text = "Μάρτυς εὐκλεής, καὶ θεῖος Ἀπόστολος, καὶ μύστης ἔνθεος, ὤφθης θείῳ ἔρωτι, τοῦ Βασιλέως τῆς δόξης Ἅγιε, μαρτυρικῶς σφραγίσας σου τὸ θεῖον κήρυγμα, καὶ τῆς ἄνω, δόξης καὶ λαμπρότητος, ἠξιώθης Κοσμᾶ Ἰσαπόστολε."

alt1.meMA.Ode9C12.ode = miscellanea_gr_GR_cog.misc.Ode9
alt1.meMA.Ode9C12.mode = miscellanea_gr_GR_cog.misc.Mode4
alt1.meMA.Ode9C12.melody = he.h.m4_gr_GR_cog.heHE.ApasGigenis.name
alt1.meMA.Ode9C12.incipit = "Ὅρμον γαληνόν, ζωῆς τῆς ἀμείνονος…"
alt1.meMA.Ode9C12.text = "Ὅρμον γαληνόν, ζωῆς τῆς ἀμείνονος, Πάτερ κατέλαβες, διʼ ἀγχόνης Ἅγιε, λαβὼν τὸ τέλος σοι τὸ ποθούμενον, καὶ τὸ σεπτόν σου λείψανον Μᾶρκος ὁ ἔνθεος, ποταμίων, ῥείθρων ἀνεκόμισεν, ἀναβλύζον ἰάσεων νάματα."

alt1.meMA.Ode9C13.ode = miscellanea_gr_GR_cog.misc.Ode9
alt1.meMA.Ode9C13.mode = miscellanea_gr_GR_cog.misc.Mode4
alt1.meMA.Ode9C13.melody = he.h.m4_gr_GR_cog.heHE.ApasGigenis.name
alt1.meMA.Ode9C13.incipit = "Ὕψωσον Θεῷ, Κοσμᾶ Ἰσαπόστολε, χεῖρας…"
alt1.meMA.Ode9C13.text = "Ὕψωσον Θεῷ, Κοσμᾶ Ἰσαπόστολε, χεῖρας ἱκέτιδας, πάσης πλάνης ῥύσασθαι, τὴν Ἐκκλησίαν νῦν τὴν Ὀρθόδοξον, εἰρήνη τῇ Ἑλλάδι δὲ πάσῃ δωρήσασθαι, καὶ πταισμάτων, κἀμοὶ Πάτερ ἄφεσιν, τῷ τοὺς ὕμνους σοι τούτους ὑφάναντι."

alt1.meMA.Ode9C1T.ode = miscellanea_gr_GR_cog.misc.Ode9
alt1.meMA.Ode9C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
alt1.meMA.Ode9C1T.melody = he.h.m4_gr_GR_cog.heHE.ApasGigenis.name
alt1.meMA.Ode9C1T.incipit = "Ὕψωσον ἡμῶν, καρδίας τὴν ἔφεσιν…"
alt1.meMA.Ode9C1T.text = "Ὕψωσον ἡμῶν, καρδίας τὴν ἔφεσιν, πρὸς θεῖον ἔρωτα, πάσης ἀπαλλάττουσα, περιπεζίου ἡμᾶς φαυλότητος, εὐλογημένη Πάναγνε, ὡς ἂν τῆς κρείττονος, τοῦ Υἱοῦ σου, μυστικῆς ἐλλάμψεως, οἱ ὑμνοῦντές σε Κόρη μετάσχωμεν."

alt1.meMA.Exaposteilarion1.mode = miscellanea_gr_GR_cog.misc.Mode2
alt1.meMA.Exaposteilarion1.greekmelody = he.a.m2_gr_GR_cog.heAU.GynaikesAkoutisthite.greekname
alt1.meMA.Exaposteilarion1.melody = he.a.m2_gr_GR_cog.heAU.GynaikesAkoutisthite.name
alt1.meMA.Exaposteilarion1.incipit = "Τὸν μέγαν Ἰσαπόστολον, Κοσμᾶν τὸν…"
alt1.meMA.Exaposteilarion1.text = "Τὸν μέγαν Ἰσαπόστολον, Κοσμᾶν τὸν ἱερώτατον, τῆς Ἐκκλησίας τὴν βάσιν, καὶ ἀληθείας τὸ στόμα, τὸν κήρυκα τῆς πίστεως, Μαρτύρων τὸν ὁμότροπον, πιστῶς ἀνευφημήσωμεν, ἵνα ταῖς τούτου πρεσβείαις, ῥυώμεθα πάσης βλάβης."

alt1.meMA.ExapTheotokion.mode = miscellanea_gr_GR_cog.misc.Mode2
alt1.meMA.ExapTheotokion.melody = he.a.m2_gr_GR_cog.heAU.GynaikesAkoutisthite.name
alt1.meMA.ExapTheotokion.incipit = "Θεὸν τὸν ὑπερούσιον, ὑπερφυῶς κυήσασα…"
alt1.meMA.ExapTheotokion.text = "Θεὸν τὸν ὑπερούσιον, ὑπερφυῶς κυήσασα, ἐν ἀνθρωπίνῃ οὐσίᾳ, Θεχαρίτωτε Κόρη, διʼ ἄκραν ἀγαθότητα, τὸν κόσμον ἀναπλάττοντα, ἀνάπλασιν σωτήριον, παράσχου νῦν τῇ ψυχῇ μου, ἐκ παραπτώσεων πλείστων."

alt1.meMA.Lauds1.mode = miscellanea_gr_GR_cog.misc.Mode1
alt1.meMA.Lauds1.greekmelody = he.a.m1_gr_GR_cog.heAU.TonOuranionTagmaton.greekname
alt1.meMA.Lauds1.melody = he.a.m1_gr_GR_cog.heAU.TonOuranionTagmaton.name
alt1.meMA.Lauds1.incipit = "Τῶν Ἀποστόλων τὸν ζῆλον ἐκμιμησάμενος…"
alt1.meMA.Lauds1.text = "Τῶν Ἀποστόλων τὸν ζῆλον ἐκμιμησάμενος, τρανῶς ἀνακηρύττεις, εὐσεβείας τὸν λόγον, Κοσμᾶ πνευματοφόρε καὶ ὁδηγεῖς, πρὸς τὸ φῶς τὸ ἀνέσπερον, τῶν ἐντολῶν τοῦ Σωτῆρος πλείστους λαούς, ὡς διδάσκαλος τῆς πίστεως."

alt1.meMA.Lauds2.mode = miscellanea_gr_GR_cog.misc.Mode1
alt1.meMA.Lauds2.melody = he.a.m1_gr_GR_cog.heAU.TonOuranionTagmaton.name
alt1.meMA.Lauds2.incipit = "Ἐξ Ἄθω Πάτερ ἐκλάμψας ὡς…"
alt1.meMA.Lauds2.text = "Ἐξ Ἄθω Πάτερ ἐκλάμψας ὡς μέγας ἥλιος, ταῖς σαῖς διδασκαλίαις, καὶ θαυμάτων ἀκτῖσι, λαοὺς τοὺς ἐν ἀγνοίᾳ φωταγωγεῖς, καὶ στηρίζεις μὴ σφάλλεσθαι, καὶ ἀποκλίνειν τῆς πίστεως τοῦ Χριστοῦ, τῆς δουλείας ἐν ταῖς θλίψεσι."

alt1.meMA.Lauds3.mode = miscellanea_gr_GR_cog.misc.Mode1
alt1.meMA.Lauds3.melody = he.a.m1_gr_GR_cog.heAU.TonOuranionTagmaton.name
alt1.meMA.Lauds3.incipit = "Χαίρει ἐν σοὶ Αἰτωλία ἡ…"
alt1.meMA.Lauds3.text = "Χαίρει ἐν σοὶ Αἰτωλία ἡ σὲ βλαστήσασα, καὶ Ἄθως χαρμοσύνως, τὴν σὴν χάριν κηρύττει, καὶ πᾶσα Ἐκκλησία ἡ τοῦ Χριστοῦ, μακαρίζει σε Ἅγιε, στηριζομένη ταῖς θείαις σου διδαχαῖς, Κοσμᾶ θεῖε Ἰσαπόστολε."

alt1.meMA.Lauds4.mode = miscellanea_gr_GR_cog.misc.Mode1
alt1.meMA.Lauds4.melody = he.a.m1_gr_GR_cog.heAU.TonOuranionTagmaton.name
alt1.meMA.Lauds4.incipit = "Τοῦ Παρακλήτου τὴν χάριν φέρων…"
alt1.meMA.Lauds4.text = "Τοῦ Παρακλήτου τὴν χάριν φέρων τῷ λόγῳ σου, πόλεις πολλὰς διῆλθες, Ἰσαπόστολε θεῖε, καὶ φθάσας Ἰλλυρίαν Πάτερ Κοσμᾶ, ἐν αὐτῇ χαίρων ἤνυσας, τοῦ μαρτυρίου τὸν δρόμον· ὅθεν Χριστός, θαυμαστῶς σε ἐμεγάλυνε."

alt1.meMA.LaudsGlory.mode = miscellanea_gr_GR_cog.misc.Mode5
alt1.meMA.LaudsGlory.melody = ""
alt1.meMA.LaudsGlory.incipit = "Εὐαγγελικῶς πολιτευσάμενος, τοῦ Εὐαγγελίου τῆς…"
alt1.meMA.LaudsGlory.text = "Εὐαγγελικῶς πολιτευσάμενος, τοῦ Εὐαγγελίου τῆς χάριτος, μυσταγωγὸς καὶ κῆρυξ, ἀναδέδειξαι Πάτερ· τῷ γὰρ θείῳ θελήματι, διακονῶν ὁλοτρόπως, διδαχαῖς ἱεραῖς παιδεύεις, τῶν εὐσεβῶν τὰς ψυχάς, τὰ ἄνω ζητεῖν καὶ ποθεῖν, ἀναστροφαῖς ἁγίαις· καὶ τύπον σεαυτὸν παραστήσας, ἐν λόγῳ καὶ ἔργῳ, τῆς Ἐκκλησίας στῦλος ὤφθης, ἐν ὑστέροις καιροῖς, Ἀποστολικοῖς προτερήμασι κοσμούμενος· διὸ Χριστός τε ἐδόξασεν, ὡς θεῖον Ἰσαπόστολον, καὶ ἀθλητὴν περιφανῆ, Κοσμᾶ μακάριε· ᾧ πρέσβευε δεόμεθα, ὑπὲρ τῶν τιμώντων σε."

