heHE.ApasGigenis.name = "Ἅπας γηγενής."
heHE.ApasGigenis.incipit = "Ἅπας γηγενής…"
heHE.ApasGigenis.melody = miscellanea_gr_GR_cog.misc.Heirmos
heHE.ApasGigenis.mode = miscellanea_gr_GR_cog.misc.Mode4
heHE.ApasGigenis.ode = miscellanea_gr_GR_cog.misc.Ode9
heHE.ApasGigenis.text = "Ἅπας γηγενής, σκιρτάτω τῷ πνεύματι λαμπαδουχούμενος, πανηγυριζέτω δέ, ἀΰλων Νόων φύσις γεραίρουσα, τὰ ἱερὰ θαυμάσια τῆς Θεομήτορος, καὶ βοάτω· Χαίροις παμμακάριστε, Θεοτόκε Ἁγνὴ ἀειπάρθενε."

heHE.ApasGigenis_Akathist.name = "Ἅπας γηγενής."
heHE.ApasGigenis_Akathist.incipit = "Ἅπας γηγενής…"
heHE.ApasGigenis_Akathist.melody = miscellanea_gr_GR_cog.misc.Heirmos
heHE.ApasGigenis_Akathist.mode = miscellanea_gr_GR_cog.misc.Mode4
heHE.ApasGigenis_Akathist.ode = miscellanea_gr_GR_cog.misc.Ode9
heHE.ApasGigenis_Akathist.text = "Ἅπας γηγενής, σκιρτάτω τῷ πνεύματι λαμπαδουχούμενος, πανηγυριζέτω δέ, ἀΰλων Νόων φύσις γεραίρουσα, τὴν ἱερὰν πανήγυριν τῆς Θεομήτορος, καὶ βοάτω· Χαίροις παμμακάριστε, Θεοτόκε Ἁγνὴ ἀειπάρθενε."

