alt1.meVE.Apolytikion2.text = ""

alt1.meMA.Kathisma11.mode = misc_gr_GR_cog.Mode4
alt1.meMA.Kathisma11.melody = he.a.m4_gr_GR_cog.OYpsotheisEnToStavro.name
alt1.meMA.Kathisma11.incipit = "Οι αγώνες σου Σεμνή και…"
alt1.meMA.Kathisma11.text = "Οι αγώνες σου Σεμνή και ο Στέφανος, οι ιδρώτές σου αγνή και τα θαύματα του διαβόλου ήσχυναν τα τρόπαια. Χριστόν γαρ τον Σωτήρά σου εκ καρδίας ποθούσα, ου μάστιγας, ου θάνατον δι’ αυτόν επτοήθης άλλ’ ανετέθης όλη τω Θεώ, του Μαρτυρίου λαβούσα τον στέφανον."

