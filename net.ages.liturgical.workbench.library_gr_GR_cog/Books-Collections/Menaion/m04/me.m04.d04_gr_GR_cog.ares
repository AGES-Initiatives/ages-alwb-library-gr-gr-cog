meMA.Ode4C11.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C11.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode4C11.greekmelody = he.h.m2_gr_GR_cog.heHE.EisakikoaKyrieTinAkoin.greekname
meMA.Ode4C11.melody = he.h.m2_gr_GR_cog.heHE.EisakikoaKyrieTinAkoin.name
meMA.Ode4C11.incipit = "Πεποικιλμένος κάλλεσι, τῆς ἀρετῆς, καὶ…"
meMA.Ode4C11.text = "Πεποικιλμένος κάλλεσι, τῆς ἀρετῆς, καὶ ἤθεσι φωσφόροις, πρὸς Θεὸν ἀνέπτης μάκαρ Γεώργιε."

meMA.Ode4C12.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C12.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode4C12.melody = he.h.m2_gr_GR_cog.heHE.EisakikoaKyrieTinAkoin.name
meMA.Ode4C12.incipit = "Ῥευστῶν γῆς Πάτερ Ὅσιε, τὴν…"
meMA.Ode4C12.text = "Ῥευστῶν γῆς Πάτερ Ὅσιε, τὴν ταραχήν, διέφυγες, γαλήνης, φιλοθέου βίου ἀντιποιούμενος."

meMA.Ode4C13.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C13.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode4C13.melody = he.h.m2_gr_GR_cog.heHE.EisakikoaKyrieTinAkoin.name
meMA.Ode4C13.incipit = "Οἱ ἀνομίας χείμαῤῥοι, σοῦ τὴν…"
meMA.Ode4C13.text = "Οἱ ἀνομίας χείμαῤῥοι, σοῦ τὴν ψυχήν, οὐκ ἴσχυσαν ταράξαι· ὁ τρυφῆς χειμάῤῥους σε γὰρ κατήρδευσεν."

meMA.Ode4C14.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C14.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode4C14.melody = he.h.m2_gr_GR_cog.heHE.EisakikoaKyrieTinAkoin.name
meMA.Ode4C14.incipit = "Θείᾳ μελέτῃ πάνσοφε, διηνεκῶς, προσέχων…"
meMA.Ode4C14.text = "Θείᾳ μελέτῃ πάνσοφε, διηνεκῶς, προσέχων τοὺς ἀστάτους, λογισμοὺς ἀπώσω Πάτερ Γεώργιε."

meMA.Ode4C1T.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C1T.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode4C1T.melody = he.h.m2_gr_GR_cog.heHE.EisakikoaKyrieTinAkoin.name
meMA.Ode4C1T.incipit = "Ὑμνολογοῦμεν Ἄχραντε, τὸν ἐν γαστρί…"
meMA.Ode4C1T.text = "Ὑμνολογοῦμεν Ἄχραντε, τὸν ἐν γαστρί, τῇ σῇ σωματωθέντα, καὶ σὲ εὐλογοῦμεν ὡς Θεομήτορα."

meMA.Ode5C1H.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C1H.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode5C1H.greekmelody = he.h.m2_gr_GR_cog.heHE.OTouFotosChorigos.greekname
