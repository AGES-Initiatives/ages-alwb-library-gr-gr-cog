meMA.Ode3C1H.text = he.h.m5_gr_GR_cog.heHE.DynameiTouStavrouSou.text

meMA.Ode3C11.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C11.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode3C11.greekmelody = he.h.m5_gr_GR_cog.heHE.DynameiTouStavrouSou.greekname
meMA.Ode3C11.melody = he.h.m5_gr_GR_cog.heHE.DynameiTouStavrouSou.name
meMA.Ode3C11.incipit = "Λατρεύειν ᾐρετίσω Θεῷ, τὸ φῶς…"
meMA.Ode3C11.text = "Λατρεύειν ᾐρετίσω Θεῷ, τὸ φῶς ποθήσας, ἑβδελύξω δέ, τὴν ζοφερὰν Κόνων ἔνδοξε, τῶν εἰδώλων ἀθεότητα."

meMA.Ode3C12.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C12.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode3C12.melody = he.h.m5_gr_GR_cog.heHE.DynameiTouStavrouSou.name
meMA.Ode3C12.incipit = "Ἱέρευσας προθύμως Χριστῷ, ψυχὴν καὶ…"
meMA.Ode3C12.text = "Ἱέρευσας προθύμως Χριστῷ, ψυχὴν καὶ σῶμα, καὶ εὐπρόσδεκτος, ὡς μόνος μόνῳ ὡμίλησας, τῷ Δεσπότῃ παμμακάριστε."

meMA.Ode3C13.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C13.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode3C13.melody = he.h.m5_gr_GR_cog.heHE.DynameiTouStavrouSou.name
meMA.Ode3C13.incipit = "Νομίμων τῶν πατρῴων σοφῶς, λαοὺς…"
meMA.Ode3C13.text = "Νομίμων τῶν πατρῴων σοφῶς, λαοὺς ἀπείρους ἠλευθέρωσας, τὸν ἀπωλείας φερώνυμον, θριαμβεύων Κόνων ἔνδοξε."

meMA.Ode3C14.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C14.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode3C14.melody = he.h.m5_gr_GR_cog.heHE.DynameiTouStavrouSou.name
meMA.Ode3C14.incipit = "Ἱκέτευε παθῶν ψυχικῶν, σωματικῶν τε…"
meMA.Ode3C14.text = "Ἱκέτευε παθῶν ψυχικῶν, σωματικῶν τε λύσιν Ἔνδοξε, παρασχεθῆναι τοῖς ἐν πίστει, ἑορτάζουσι τὴν μνήμην σου."

meMA.Ode3C1T.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C1T.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode3C1T.melody = he.h.m5_gr_GR_cog.heHE.DynameiTouStavrouSou.name
meMA.Ode3C1T.incipit = "Ἱκέτευε ἀπαύστως Ἁγνή, τὸν προελθόντα…"
meMA.Ode3C1T.text = "Ἱκέτευε ἀπαύστως Ἁγνή, τὸν προελθόντα ἐκ λαγόνων σου, ῥυσθῆναι πλάνης διαβόλου, τοὺς ὑμνοῦντάς σε Μητέρα Θεοῦ."

meMA.Ode3Kathisma1.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode3Kathisma1.greekmelody = he.a.m1_gr_GR_cog.heAU.TonTafonSouSotir.greekname
meMA.Ode3Kathisma1.melody = he.a.m1_gr_GR_cog.heAU.TonTafonSouSotir.name
meMA.Ode3Kathisma1.incipit = "Ἐκ βρέφους τῷ Θεῷ, κολληθεὶς…"
meMA.Ode3Kathisma1.text = "Ἐκ βρέφους τῷ Θεῷ, κολληθεὶς Θεοφόρε, τοῦ Πνεύματος σεπτόν, ἀνεδείχθης δοχεῖον, καὶ τὰ πονηρότατα, καθυπέταξας πνεύματα, ἐναθλήσας δέ, ἐμεγαλύνθης ἀξίως· ὅθεν ἅπαντες, τὴν παναγίαν σου μνήμην, πιστῶς ἑορτάζομεν."

meMA.Ode3KathismaT.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode3KathismaT.melody = he.a.m1_gr_GR_cog.heAU.TonTafonSouSotir.name
meMA.Ode3KathismaT.incipit = "Κυβέρνησον Ἁγνή, τὴν ἀθλίαν ψυχήν…"
meMA.Ode3KathismaT.text = "Κυβέρνησον Ἁγνή, τὴν ἀθλίαν ψυχήν μου, καὶ οἴκτειρον αὐτήν, ὑπὸ πλήθους πταισμάτων, βυθῷ ὀλισθαίνουσαν, ἀπωλείας Πανάμωμε, καὶ ἐν ὥρᾳ με, τῇ φοβερᾷ τοῦ θανάτου, ἐλευθέρωσον, κατηγορούντων δαιμόνων, καὶ σῶσόν με δέομαι."

meMA.Ode3KathismaSt.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode3KathismaSt.melody = he.a.m1_gr_GR_cog.heAU.TonTafonSouSotir.name
meMA.Ode3KathismaSt.incipit = "Εἰς σὴν τῆς καθαρᾶς, καὶ…"
meMA.Ode3KathismaSt.text = "Εἰς σὴν τῆς καθαρᾶς, καὶ ἀμώμου Παρθένου, διῆλθεν ἀληθῶς, τὴν καρδίαν ῥομφαία, Σταυρῷ ὡς ἑώρακας, τὸν Υἱόν σου ὑψούμενον, Παναμώμητε, εὐλογημένη Μαρία, τὸ προσφύγιον, ἁμαρτωλῶν Θεοτόκε, πιστῶν τὸ κραταίωμα."

meMA.Ode4C1H.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C1H.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode4C1H.greekmelody = he.h.m5_gr_GR_cog.heHE.AkikoaKyrieTinEkTouTafou.greekname
