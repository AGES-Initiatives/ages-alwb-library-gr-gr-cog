A_Resource_Whose_Name = sy.m07.d27_gr_GR_cog

S01.Com = "Τῇ ΚΖʹ τοῦ αὐτοῦ μηνός, μνήμη τοῦ Ἁγίου ἐνδόξου Μεγαλομάρτυρος καὶ ἰαματικοῦ Παντελεήμονος."
S01.Ver1 = "Γαλακτόμικτον, Μάρτυς, αἷμα σῆς κάρας, "
S01.Ver2 = "Δι' ἣν ὑδατόμικτον ὁ Χριστὸς χέει. "
S01.Ver3 = "Φάσγανον ἑβδομάτῃ λάχεν εἰκάδι Παντελεήμων."
S01.Nar = ""

S02.Com = "Τῇ αὐτῇ ἡμέρᾳ, ὁ ὑπὸ τοῦ Ἁγίου Παντελεήμονος ἰαθεὶς τυφλὸς ξίφει τελειοῦται."
S02.Ver1 = "Αὐγὴν ἐφευρὼν σαρκικῶν τυφλὸς λύχνων, "
S02.Ver2 = "Καὶ ψυχικὴν ἤθρησεν αὐγὴν ἐκ ξίφους."
S02.Nar = ""

S03.Com = "Οἱ Ἅγιοι ἑκατὸν πεντήκοντα τρεῖς Μάρτυρες, οἱ ἐκ τῆς Θράκης, ἐν τῇ θαλάσσῃ τελειοῦνται."
S03.Ver = ""
S03.Nar = ""

S04.Com = "Μνήμη τῆς Ὁσίας Μητρὸς ἡμῶν Ἀνθούσης τῆς ὁμολογητρίας, τῆς ἐν τῇ ἁγιωτάτῃ Μονῇ τοῦ Μαντινέου."
S04.Ver = ""
S04.Nar = ""

S05.Com = "Ὁ Ἅγιος Νεομάρτυς Χριστόδουλος, ὁ ἐκ Κασσάνδρας, ἐν Θεσσαλονίκῃ μαρτυρήσας ἐν ἔτει 1777, ἀγχόνῃ τελειοῦται."
S05.Ver = ""
S05.Nar = ""

S99.LastLine = "Ταῖς αὐτῶν ἁγίαις πρεσβείαις, ὁ Θεός, ἐλέησον ἡμᾶς. Ἀμήν."