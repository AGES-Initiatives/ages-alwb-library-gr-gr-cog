meVE.Apolytikion1ger.poet = poets_gr_GR_cog.po.Gerasimou
meVE.Apolytikion1ger.mode = miscellanea_gr_GR_cog.misc.Mode3
meVE.Apolytikion1ger.greekmelody = he.a.m3_gr_GR_cog.heAU.TheiasPisteos.greekname
meVE.Apolytikion1ger.melody = he.a.m3_gr_GR_cog.heAU.TheiasPisteos.name
meVE.Apolytikion1ger.incipit = "Θείαν ὄνησιν, ἰχνηλατοῦντες, πορφυρίζοντα, ὤφθητε…"
meVE.Apolytikion1ger.text = "Θείαν ὄνησιν, ἰχνηλατοῦντες, πορφυρίζοντα, ὤφθητε κρίνα, Ὀνησιφόρε κλεινὲ καὶ Πορφύριε· ὅθεν ὡς ἅρμα τοῦ Λόγου κρυσήλατον, πρὸς τὴν οὐράνιον νύσσαν ἠλάσατε. Θεῖοι Μάρτυρες, Χριστῷ τῷ Θεῷ πρεσβεύσατε, δωρήσασθαι ἡμῖν τὸ μέγα ἔλεος."

meVE.Apolytikion2ger.poet = poets_gr_GR_cog.po.Gerasimou
meVE.Apolytikion2ger.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.Apolytikion2ger.greekmelody = he.a.m4_gr_GR_cog.heAU.TachyProkatalave.greekname
meVE.Apolytikion2ger.melody = he.a.m4_gr_GR_cog.heAU.TachyProkatalave.name
meVE.Apolytikion2ger.incipit = "Χριστὸν ἀγαπήσασα, τῶν ἀγαθῶν τὴν…"
meVE.Apolytikion2ger.text = "Χριστὸν ἀγαπήσασα, τῶν ἀγαθῶν τὴν πηγήν, Ἀγγέλων ἐζήλωσας, μετὰ σαρκὸς τὴν ζωήν, Μαρτῶνα θεόπνευστε· σὺ γὰρ συνεύνου φίλτρον, παριδοῦσα ἐμφρόνως, ᾔσχυνας τὸν Βελίαρ, τῷ πανσόφῳ σου τρόπῳ· διὸ τῆς ἀκαταλύτου δόξης ἠξίωσαι."

meVE.Apolytikion3.poet = poets_gr_GR_cog.po.Gerasimou
meVE.Apolytikion3.mode = miscellanea_gr_GR_cog.misc.Mode1
meVE.Apolytikion3.greekmelody = he.a.m1_gr_GR_cog.heAU.TisErimouPolitis.greekname
meVE.Apolytikion3.melody = he.a.m1_gr_GR_cog.heAU.TisErimouPolitis.name
meVE.Apolytikion3.incipit = alt1.meVE.Apolytikion1.incipit
meVE.Apolytikion3.text = alt1.meVE.Apolytikion1.text

meMA.C1.acrostic.title = "Ὁ Κανὼν τῶν Μαρτύρων,"
meMA.C1.acrostic.notice1 = "οὗ ἡ Ἀκροστιχίς·"
meMA.C1.acrostic.text = "Φέρεις ὄνησιν σοῖς ἐπαινέταις Μάκαρ. Ἰωσήφ."
meMA.C1.title = titles_gr_GR_cog.ti.CanonMartyrs
meMA.C1.verse = prayers_gr_GR_cog.pr.verse_SaintsM.text

meMA.C2.acrostic.title = "Ὁ Κανὼν τῆς Ὁσίας,"
meMA.C2.acrostic.notice1 = "οὗ ἡ Ἀκροστιχίς·"
meMA.C2.acrostic.text = "Τὸ τῆς Ματρώνης προφρόνως ᾄδω κλέος. Ἰωσήφ."
meMA.C2.title = titles_gr_GR_cog.ti.CanonDevoutW
meMA.C2.verse = prayers_gr_GR_cog.pr.verse_SaintW.text

meMA.Ode1C11.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode1C11.greekmelody = he.h.m4_gr_GR_cog.heHE.AsomaiSoiKyrie.greekname
meMA.Ode1C11.melody = he.h.m4_gr_GR_cog.heHE.AsomaiSoiKyrie.name
meMA.Ode1C11.incipit = "Φέρουσα τοῖς πίστει σε εὐφημοῦσιν…"
meMA.Ode1C11.text = "Φέρουσα τοῖς πίστει σε εὐφημοῦσιν, ὄνησιν θείαν ἡ σεπτή, ἐφέστηκε μνήμη σου, ἐν ᾗ σὲ ἱκετεύομεν, Παμμάκαρ ἡμῶν μνήσθητι."

meMA.Ode1C12.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode1C12.melody = he.h.m4_gr_GR_cog.heHE.AsomaiSoiKyrie.name
meMA.Ode1C12.incipit = "Ἔχων παῤῥησίας μεμεστωμένον, τὸν σὸν…"
meMA.Ode1C12.text = "Ἔχων παῤῥησίας μεμεστωμένον, τὸν σὸν γενναῖον λογισμόν, ἀλόγου προστάγματος, ἠλόγησας Θεόπνευστε, καὶ χαίρων Μάκαρ ἤθλησας."

meMA.Ode1C13.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode1C13.melody = he.h.m4_gr_GR_cog.heHE.AsomaiSoiKyrie.name
meMA.Ode1C13.incipit = "Ῥώμῃ δυναμούμενος οὐρανίῳ, Ὀνησιφόρε πρὸς…"
meMA.Ode1C13.text = "Ῥώμῃ δυναμούμενος οὐρανίῳ, Ὀνησιφόρε πρὸς δεινά, ἐχώρησας βάσανα, καὶ πᾶσαν κατεπάλαισας, ἰσχὺν τοῦ πολεμήτορος."

meMA.Ode1C1T.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode1C1T.melody = he.h.m4_gr_GR_cog.heHE.AsomaiSoiKyrie.name
meMA.Ode1C1T.incipit = "Ἔστησας θανάτου τὴν τυραννίδα, ἀθανασίαν…"
meMA.Ode1C1T.text = "Ἔστησας θανάτου τὴν τυραννίδα, ἀθανασίαν καὶ ζωήν, τῷ κόσμῳ κυήσασα, Χριστὸν τὸν λυτρωτὴν ἡμῶν, Παρθένε ἀπειρόγαμε."

meMA.Ode1C21.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C21.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C21.greekmelody = he.h.m8_gr_GR_cog.heHE.AsomenToKyrioPantesLaoi.greekname
meMA.Ode1C21.melody = he.h.m8_gr_GR_cog.heHE.AsomenToKyrioPantesLaoi.name
meMA.Ode1C21.incipit = "Τάξεσιν οὐρανίαις, πάντοτε συνοῦσα καὶ…"
meMA.Ode1C21.text = "Τάξεσιν οὐρανίαις, πάντοτε συνοῦσα καὶ μεθέξεσι, θεϊκαῖς θεουμένη, τοὺς τιμῶντάς σε, σῷζε πρεσβείαις σου."

meMA.Ode1C22.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C22.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C22.melody = he.h.m8_gr_GR_cog.heHE.AsomenToKyrioPantesLaoi.name
meMA.Ode1C22.incipit = "Ὅλη τῷ θείῳ πόθῳ, περικαιομένη…"
meMA.Ode1C22.text = "Ὅλη τῷ θείῳ πόθῳ, περικαιομένη ὑπεκκαύματα, ἡδονῶν ψυχοφθόρων, ἐγκρατείας τοῖς ὄμβροις κατέσβεσας."

meMA.Ode1C23.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C23.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C23.melody = he.h.m8_gr_GR_cog.heHE.AsomenToKyrioPantesLaoi.name
meMA.Ode1C23.incipit = "Τίμιον τοῦ Δεσπότου, σκεῦος ἀνεδείχθης…"
meMA.Ode1C23.text = "Τίμιον τοῦ Δεσπότου, σκεῦος ἀνεδείχθης ἐκκαθάρασα, τὴν καρδίαν Ματρῶνα, τῶν παθῶν τῆς σαρκὸς διʼ ἀσκήσεως."

meMA.Ode1C2T.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C2T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C2T.melody = he.h.m8_gr_GR_cog.heHE.AsomenToKyrioPantesLaoi.name
meMA.Ode1C2T.incipit = "Ἡ κυήσασα κόσμῳ, θείαν σωτηρίαν…"
meMA.Ode1C2T.text = "Ἡ κυήσασα κόσμῳ, θείαν σωτηρίαν καὶ ἀνάκλησιν, ταῖς πρεσβείαις σου σῷζε, τοὺς πιστῶς σοι προστρέχοντας Ἄχραντε."

meMA.Ode3C11.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C11.greekmelody = he.h.m4_gr_GR_cog.heHE.ToxonDynatonIsthenise.greekname
meMA.Ode3C11.melody = he.h.m4_gr_GR_cog.heHE.ToxonDynatonIsthenise.name
meMA.Ode3C11.incipit = "Ἴσον τῷ Πατρὶ καὶ Πνεύματι…"
meMA.Ode3C11.text = "Ἴσον τῷ Πατρὶ καὶ Πνεύματι, Λόγον ἐν σταδίῳ, ὁμολογοῦντες οἱ Μάρτυρες, τρικυμίαν τὴν τῶν βασάνων, ἀνδρικῶς ἐναπεκρούσαντο."

meMA.Ode3C12.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C12.melody = he.h.m4_gr_GR_cog.heHE.ToxonDynatonIsthenise.name
meMA.Ode3C12.incipit = "Στήσας ἀσφαλῶς τοὺς πόδας σου…"
meMA.Ode3C12.text = "Στήσας ἀσφαλῶς τοὺς πόδας σου, Μάρτυς ἐπὶ πέτραν, ὁμολογίας καὶ πίστεως, ταῖς μάστιξιν, Ὀνησιφόρε, τῶν ἀνόμων οὐ σεσάλευσαι."

meMA.Ode3C13.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C13.melody = he.h.m4_gr_GR_cog.heHE.ToxonDynatonIsthenise.name
meMA.Ode3C13.incipit = "Ὅλω τῷ νοῒ μακάριε, Μάρτυς…"
meMA.Ode3C13.text = "Ὅλω τῷ νοῒ μακάριε, Μάρτυς ἐκδημήσας, πρὸς Χριστὸν τὸν ποθούμενον, οὐκ ἠσθάνου τῶν ἐκ μαστίγων, ἀλγηδόνων Παμμακάριστε."

meMA.Ode3C1T.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C1T.melody = he.h.m4_gr_GR_cog.heHE.ToxonDynatonIsthenise.name
meMA.Ode3C1T.incipit = "Νεῦσον Πάναγνε σωθῆναί με· τὸν…"
meMA.Ode3C1T.text = "Νεῦσον Πάναγνε σωθῆναί με· τὸν γὰρ θείῳ νεύματι, τὸ πᾶν περιάγοντα, ἐσωμάτωσας Θεὸν Λόγον, ὑπὲρ λόγον καὶ διάνοιαν."

meMA.Ode3C21.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C21.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C21.greekmelody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.greekname
meMA.Ode3C21.melody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.name
meMA.Ode3C21.incipit = "Ῥάβδῳ θείας πίστεως, παθῶν διέτεμες…"
meMA.Ode3C21.text = "Ῥάβδῳ θείας πίστεως, παθῶν διέτεμες θάλασσαν, θείαις ψυχαῖς προοδοποιοῦσα, τὴν εὐθεῖαν διάβασιν."

meMA.Ode3C22.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C22.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C22.melody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.name
meMA.Ode3C22.incipit = "Ὤφθης ἐγκαλλώπισμα, Μοναζουσῶν σαφῶς Πάνσεμνε…"
meMA.Ode3C22.text = "Ὤφθης ἐγκαλλώπισμα, Μοναζουσῶν σαφῶς Πάνσεμνε, καὶ Μοναστῶν βίον κτησαμένη, καθαρὸν καὶ ἀμώμητον."

meMA.Ode3C23.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C23.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C23.melody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.name
meMA.Ode3C23.incipit = "Νέκρωσιν ἐνδέδυσαι, φθοροποιῶν παθῶν Ἔνδοξε…"
meMA.Ode3C23.text = "Νέκρωσιν ἐνδέδυσαι, φθοροποιῶν παθῶν Ἔνδοξε, καὶ τὸν ἐχθρὸν διὰ πολιτείας, εὐσεβοῦς ἀπεγύμνωσας."

meMA.Ode3C2T.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C2T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C2T.melody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.name
meMA.Ode3C2T.incipit = "Ἥνωται ἐν μήτρᾳ σου, ὁ…"
meMA.Ode3C2T.text = "Ἥνωται ἐν μήτρᾳ σου, ὁ πλαστουργὸς σαρκὶ Πάναγνε, μείνας ὃ ἦν, ὅπως τὸ εὖ εἶναι, τοῖς ἀνθρώποις δωρήσηται."

meMA.Ode3H.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3H.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3H.greekmelody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.greekname
meMA.Ode3H.melody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.melody
meMA.Ode3H.incipit = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.incipit
meMA.Ode3H.text = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.text

meMA.Ode3Kathisma1.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode3Kathisma1.greekmelody = he.a.m1_gr_GR_cog.heAU.TonTafonSouSotir.greekname
meMA.Ode3Kathisma1.melody = he.a.m1_gr_GR_cog.heAU.TonTafonSouSotir.name
meMA.Ode3Kathisma1.incipit = "Πυρὶ τοῦ θεϊκοῦ, ἀναπτόμενοι πόθου…"
meMA.Ode3Kathisma1.text = "Πυρὶ τοῦ θεϊκοῦ, ἀναπτόμενοι πόθου, πυρὸς τοῦ αἰσθητοῦ, τῇ προσψαύσει οὐδόλως, Μακάριοι ἐφλέχθητε, τὴν δὲ πλάνην ἐφλέξατε, καὶ συρόμενοι ἀνηλεῶς Ἀθλοφόροι, τὸ ἀοίδιμον, καθυπεδέξασθε τέλος, καὶ δόξης ἐτύχετε."

meMA.Ode3Kathisma2.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3Kathisma2.greekmelody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.greekname
meMA.Ode3Kathisma2.melody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.name
meMA.Ode3Kathisma2.incipit = "Ἀνδρειοφρόνως λαθοῦσα τὰς τοῦ ἐχθροῦ…"
meMA.Ode3Kathisma2.text = "Ἀνδρειοφρόνως λαθοῦσα τὰς τοῦ ἐχθροῦ, μεθοδείας Ματρῶνα πρὸς τὴν ζωήν, χαίρουσα προσέδραμες, ἀληθῶς τὴν ἐν πνεύματι, καὶ ἀκλινῶς τὸν δρόμον, Θεόφρον τελέσασα, παρὰ Χριστοῦ ἐδέξω, τὴν χάριν τοῦ Πνεύματος· ὅθεν καὶ τὰ ῥεῖθρα, τῶν ἰαμάτων προχέεις, τοῖς πόθῳ γεραίρουσι, τὴν σὴν μνήμην ἀοίδιμε, Μοναζουσῶν τὸ καύχημα· Πρέσβευε Χριστῷ τῷ Θεῷ, τῶν πταισμάτων ἄφεσιν δωρήσασθαι, τοῖς ἑορτάζουσι πόθῳ, τὴν ἁγίαν μνήμην σου."

meMA.Ode3KathismaT.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3KathismaT.melody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.name
meMA.Ode3KathismaT.incipit = me.m06.d29_gr_GR_cog.meMA.Kathisma2T.incipit
meMA.Ode3KathismaT.text = me.m06.d29_gr_GR_cog.meMA.Kathisma2T.text

meMA.Ode3KathismaSt.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3KathismaSt.melody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.name
meMA.Ode3KathismaSt.incipit = oc.m8.d4_gr_GR_cog.ocMA.Kathisma13.incipit
meMA.Ode3KathismaSt.text = oc.m8.d4_gr_GR_cog.ocMA.Kathisma13.text

meMA.Ode4C11.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C11.greekmelody = he.h.m4_gr_GR_cog.heHE.DiAgapisinOiktirmon.greekname
meMA.Ode4C11.melody = he.h.m4_gr_GR_cog.heHE.DiAgapisinOiktirmon.name
meMA.Ode4C11.incipit = "Ἡ ἀστράπτουσα τῶν πόνων σου…"
meMA.Ode4C11.text = "Ἡ ἀστράπτουσα τῶν πόνων σου φωταυγία, Ὀνησιφόρε Μάρτυς, ἐφειλκύσατο πόθῳ, πίστει συναθλῆσαί σοι, τὸν θεῖον Πορφύριον."

meMA.Ode4C12.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C12.melody = he.h.m4_gr_GR_cog.heHE.DiAgapisinOiktirmon.name
meMA.Ode4C12.incipit = "Συνωθήσαντες ἐσχάρᾳ πυρακτωθείσῃ, προσεφαπλοῦσιν ἅμα…"
meMA.Ode4C12.text = "Συνωθήσαντες ἐσχάρᾳ πυρακτωθείσῃ, προσεφαπλοῦσιν ἅμα, τοὺς Χριστοῦ στρατιώτας, χαίροντας οἱ τύραννοι, καὶ ζέοντας πνεύματι."

meMA.Ode4C13.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C13.melody = he.h.m4_gr_GR_cog.heHE.DiAgapisinOiktirmon.name
meMA.Ode4C13.incipit = "Ἱερεῖα ὡς ὁλόκληρα Ἀθλοφόροι, καὶ…"
meMA.Ode4C13.text = "Ἱερεῖα ὡς ὁλόκληρα Ἀθλοφόροι, καὶ καθαραὶ θυσίαι τῷ Δεσπότῃ τῶν ὅλων, χαίροντες προσήχθητε· διὸ μακαρίζεσθε."

meMA.Ode4C14.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C14.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C14.melody = he.h.m4_gr_GR_cog.heHE.DiAgapisinOiktirmon.name
meMA.Ode4C14.incipit = "Νουνεχῶς σε ἀνακείμενον τῷ Δεσπότῃ…"
meMA.Ode4C14.text = "Νουνεχῶς σε ἀνακείμενον τῷ Δεσπότῃ, προσλαλιαὶ αἱ δόλιαι, ἀπατῆσαι οὐδόλως, ἴσχυσαν Πορφύριε, Θεόφρον πανόλβιε."

meMA.Ode4C1T.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C1T.melody = he.h.m4_gr_GR_cog.heHE.DiAgapisinOiktirmon.name
meMA.Ode4C1T.incipit = "Σωματούμενον ἐκύησας Θεὸν Λόγον, ὁμοιωθῆναι…"
meMA.Ode4C1T.text = "Σωματούμενον ἐκύησας Θεὸν Λόγον, ὁμοιωθῆναι Κόρη, διὰ σπλάγχνα ἐλέους, βροτοῖς εὐδοκήσαντα, πανάμωμε Δέσποινα."

meMA.Ode4C21.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C21.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C21.greekmelody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.greekname
meMA.Ode4C21.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
meMA.Ode4C21.incipit = "Σταλαγμοῖς τῶν δακρύων σου, ἤρδευσας…"
meMA.Ode4C21.text = "Σταλαγμοῖς τῶν δακρύων σου, ἤρδευσας καρδίαν καὶ ἐγεώργησας, ἀρετῶν ἑκατοστεύοντα, Μακαρία στάχυν θείᾳ χάριτι."

meMA.Ode4C22.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C22.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C22.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
meMA.Ode4C22.incipit = "Πρὸς Χριστὸν ἐπερείσασα, τὰς τῆς…"
meMA.Ode4C22.text = "Πρὸς Χριστὸν ἐπερείσασα, τὰς τῆς διανοίας βάσεις διέμεινας, ἐπηρείαις ἀπερίτρεπτος, πονηρῶν δαιμόνων Ἀξιάγαστε."

meMA.Ode4C23.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C23.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C23.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
meMA.Ode4C23.incipit = "Ῥωμαλέῳ φρονήματι, σύζυγον λιποῦσα καὶ…"
meMA.Ode4C23.text = "Ῥωμαλέῳ φρονήματι, σύζυγον λιποῦσα καὶ βίου τάραχον, τὸν ζυγὸν Χριστοῦ ἐπόθησας, καὶ αὐτοῦ ὀπίσω ἠκολούθησας."

meMA.Ode4C2T.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C2T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C2T.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
meMA.Ode4C2T.incipit = "Οὐ λιπὼν τὰ οὐράνια, Ἄχραντε…"
meMA.Ode4C2T.text = "Οὐ λιπὼν τὰ οὐράνια, Ἄχραντε κενοῦται Θεὸς ἐν μήτρᾳ σου, καὶ μετρεῖται ὁ ἀμέτρητος, τὰ ἀμέτρητά μου λύων πταίσματα."

meMA.Ode5C11.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C11.greekmelody = he.h.m4_gr_GR_cog.heHE.SyKyrieMouFos.greekname
meMA.Ode5C11.melody = he.h.m4_gr_GR_cog.heHE.SyKyrieMouFos.name
meMA.Ode5C11.incipit = "Οἱ πύργοι τῆς Χριστοῦ, Ἐκκλησίας…"
meMA.Ode5C11.text = "Οἱ πύργοι τῆς Χριστοῦ, Ἐκκλησίας οἱ ἄσειστοι, μοχλεύσεσι τῶν βασάνων, ἀπερίτρεπτοι σθένει, τοῦ Πνεύματος ἐδείχθησαν."

meMA.Ode5C12.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C12.melody = he.h.m4_gr_GR_cog.heHE.SyKyrieMouFos.name
meMA.Ode5C12.incipit = "Ἰνδάλματα Θεῶν, μιαρῶν ἐλεπτύνατε, καὶ…"
meMA.Ode5C12.text = "Ἰνδάλματα Θεῶν, μιαρῶν ἐλεπτύνατε, καὶ ὤφθητε εὐσεβείας, καὶ ἀνδρείας εἰκόνες, σεπτοὶ Μεγαλομάρτυρες."

meMA.Ode5C13.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C13.melody = he.h.m4_gr_GR_cog.heHE.SyKyrieMouFos.name
meMA.Ode5C13.incipit = "Στρατείαν ἀληθῆ, ἐκ Θεοῦ διεζώσασθε…"
meMA.Ode5C13.text = "Στρατείαν ἀληθῆ, ἐκ Θεοῦ διεζώσασθε, τὴν πρόσκαιρον δυναστείαν, παρωσάμενοι πίστει, καὶ χαίροντες ἠθλήσατε."

meMA.Ode5C1T.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C1T.melody = he.h.m4_gr_GR_cog.heHE.SyKyrieMouFos.name
meMA.Ode5C1T.incipit = "Ἐνῴκησεν ἐν σοί, χωρηθεὶς ὁ…"
meMA.Ode5C1T.text = "Ἐνῴκησεν ἐν σοί, χωρηθεὶς ὁ ἀχώρητος, καὶ ἔδειξεν οὐρανῶν σε, πλατυτέραν Παρθένε· διό σε μακαρίζομεν."

meMA.Ode5C21.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C21.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C21.greekmelody = he.h.m8_gr_GR_cog.heHE.OrthrizontesVoomenSoi.greekname
meMA.Ode5C21.melody = he.h.m8_gr_GR_cog.heHE.OrthrizontesVoomenSoi.name
meMA.Ode5C21.incipit = "Φορέσασα τὸ σχῆμα τὸ ἅγιον…"
meMA.Ode5C21.text = "Φορέσασα τὸ σχῆμα τὸ ἅγιον, ὦ Ματρῶνα, ἐχθρὸν τὸν ἀλάστορα, ἀσχημοσύνην ἐνέδυσας."

meMA.Ode5C22.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C22.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C22.melody = he.h.m8_gr_GR_cog.heHE.OrthrizontesVoomenSoi.name
meMA.Ode5C22.incipit = "Ῥεόντων ἀντηλλάξω τὰ μένοντα, καὶ…"
meMA.Ode5C22.text = "Ῥεόντων ἀντηλλάξω τὰ μένοντα, καὶ συζύγου, μνήστορα ἀθάνατον, φωταγωγὸν τῶν ψυχῶν ἡμῶν."

meMA.Ode5C23.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C23.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C23.melody = he.h.m8_gr_GR_cog.heHE.OrthrizontesVoomenSoi.name
meMA.Ode5C23.incipit = "Ὁλόνυκτον προσάγουσα δέησιν, τῷ Δεσπότῃ…"
meMA.Ode5C23.text = "Ὁλόνυκτον προσάγουσα δέησιν, τῷ Δεσπότῃ, Ματρῶνα ἐκοίμισας, τὰς ἡδονὰς τὰς τοῦ σώματος."

meMA.Ode5C2T.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C2T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C2T.melody = he.h.m8_gr_GR_cog.heHE.OrthrizontesVoomenSoi.name
meMA.Ode5C2T.incipit = "Νεκρῶν τε καὶ τῶν ζώντων…"
meMA.Ode5C2T.text = "Νεκρῶν τε καὶ τῶν ζώντων Δεσπότην, ἀποτεκοῦσα, τὰ πάθη Πανάμωμε, τὰ τῆς σαρκός μου θανάτωσον."

meMA.Ode6C11.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C11.greekmelody = he.h.m4_gr_GR_cog.heHE.IlthonEisTaVathi.greekname
meMA.Ode6C11.melody = he.h.m4_gr_GR_cog.heHE.IlthonEisTaVathi.name
meMA.Ode6C11.incipit = "Πόνοις αἰκισμῶν ἐγκαρτερήσας, Ὀνησιφόρε Μάρτυς…"
meMA.Ode6C11.text = "Πόνοις αἰκισμῶν ἐγκαρτερήσας, Ὀνησιφόρε Μάρτυς, δρόμον ἤνυσας τοῦ μαρτυρίου, καὶ νικητής, γεγονὼς ἀπείληφας, τὰ βραβεῖα τὰ οὐράνια."

meMA.Ode6C12.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C12.melody = he.h.m4_gr_GR_cog.heHE.IlthonEisTaVathi.name
meMA.Ode6C12.incipit = "Ἀλλήλοις συνδούμενοι τῇ πίστει, προσδεδεμένοι…"
meMA.Ode6C12.text = "Ἀλλήλοις συνδούμενοι τῇ πίστει, προσδεδεμένοι ἄμφω, καὶ συρόμενοι λύσιν τοῦ σκήνους, οἷα στεῤῥοί, Ἀθληταὶ ὑπέστητε, τῷ Κυρίῳ συναπτόμενοι."

meMA.Ode6C13.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C13.melody = he.h.m4_gr_GR_cog.heHE.IlthonEisTaVathi.name
meMA.Ode6C13.incipit = "Ἵπποις προσδεθέντες καὶ βιαίως, διασυρθέντες…"
meMA.Ode6C13.text = "Ἵπποις προσδεθέντες καὶ βιαίως, διασυρθέντες γνώμῃ, τοῦ δικάζοντος βιαιοτάτῃ, οἱ εὐκλεεῖς, τοῦ Κυρίου Μάρτυρες, θείου τέλους ἠξιώθησαν."

meMA.Ode6C1T.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C1T.melody = he.h.m4_gr_GR_cog.heHE.IlthonEisTaVathi.name
meMA.Ode6C1T.incipit = "Νόμον δίχα φύσεως Παρθένε, τὸν…"
meMA.Ode6C1T.text = "Νόμον δίχα φύσεως Παρθένε, τὸν νομοδότην τίκτεις, ἀναπλάττοντα τὴν συντριβεῖσαν, φύσιν ἡμῶν, ὃν δυσώπει σῶσαί με, ἁμαρτίαις συντριβόμενον."

meMA.Ode6C21.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C21.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C21.greekmelody = he.h.m8_gr_GR_cog.heHE.TinDeisinEkcheo.greekname
meMA.Ode6C21.melody = he.h.m8_gr_GR_cog.heHE.TinDeisinEkcheo.name
meMA.Ode6C21.incipit = "Ὡς ὄρνις εὐκέλαδος ἐν τῷ…"
meMA.Ode6C21.text = "Ὡς ὄρνις εὐκέλαδος ἐν τῷ ἄλσει τῆς ἀσκήσεως ἡδὺ προσφωνοῦσα, ὡς νοητά, ἐφειλκύσω στρουθία, ἱερωτάτων γυναίων συνάθροισιν, τὰ θήρατρα τοῦ δυσμενοῦς, διαδρᾶσα Ματρῶνα ἐν Πνεύματι."

meMA.Ode6C22.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C22.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C22.melody = he.h.m8_gr_GR_cog.heHE.TinDeisinEkcheo.name
meMA.Ode6C22.incipit = "Σαρκὸς μὲν φιλοσοφίᾳ κρατοῦσα, καὶ…"
meMA.Ode6C22.text = "Σαρκὸς μὲν φιλοσοφίᾳ κρατοῦσα, καὶ δαμάζουσα τὰ πάθη Ὁσία, τῆς δὲ ψυχῆς, τὸ τερπνότατον κάλλος, περιφανῶς τῷ Χριστῷ εὐτρεπίζουσα, ὡραία τε καὶ εὐπρεπής, ἐγνωρίσθης Ματρῶνα θεόσοφε."

meMA.Ode6C23.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C23.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C23.melody = he.h.m8_gr_GR_cog.heHE.TinDeisinEkcheo.name
meMA.Ode6C23.incipit = "Ἀγγέλων ἐπὶ τῆς γῆς πολιτείαν…"
meMA.Ode6C23.text = "Ἀγγέλων ἐπὶ τῆς γῆς πολιτείαν, ἐπεπόθησας ὑμνοῦσα ἀπαύστως, τὸν διʼ ἡμᾶς, ἐξ ἁγίας Παρθένου, ὑπερβολῇ σαρκωθέντα χρηστότητος, τὴν φύσιν τε τῶν γυναικῶν, κατʼ ἐχθροῦ τοῦ δολίου νευρώσαντα."

meMA.Ode6C2T.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C2T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C2T.melody = he.h.m8_gr_GR_cog.heHE.TinDeisinEkcheo.name
meMA.Ode6C2T.incipit = "Δεδούλωτο ἁμαρτίᾳ ἡ φύσις, τῶν…"
meMA.Ode6C2T.text = "Δεδούλωτο ἁμαρτίᾳ ἡ φύσις, τῶν ἀνθρώπων, ἀλλὰ ταύτην ἐῤῥύσω τῆς πονηρᾶς, Θεοτόκε δουλείας, τὸν τῶν ἁπάντων Δεσπότην κυήσασα, τὸν δείξαντα πᾶσιν, τῆς ζωῆς τὰς εἰσόδους Πανάμωμε."

meMA.Ode6H.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6H.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6H.greekmelody = he.h.m8_gr_GR_cog.heHE.TinDeisinEkcheo.greekname
meMA.Ode6H.melody = he.h.m8_gr_GR_cog.heHE.TinDeisinEkcheo.melody
meMA.Ode6H.incipit = he.h.m8_gr_GR_cog.heHE.TinDeisinEkcheo.incipit
meMA.Ode6H.text = he.h.m8_gr_GR_cog.heHE.TinDeisinEkcheo.text

meMA.Kontakion1.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Kontakion1.greekmelody = he.a.m2_gr_GR_cog.heAU.TaAnoZiton.greekname
meMA.Kontakion1.melody = he.a.m2_gr_GR_cog.heAU.TaAnoZiton.name
meMA.Kontakion1.incipit = "Μαρτύρων δυάς, ἀθλήσαντες στεῤῥότατα, ἐχθροῦ…"
meMA.Kontakion1.text = "Μαρτύρων δυάς, ἀθλήσαντες στεῤῥότατα, ἐχθροῦ τὴν ὀφρύν, εἰς γῆν κατηδάφισαν, ἐλλαμφθέντες χάριτι, τῆς ἀκτίστου Τριάδος οἱ ἔνδοξοι· καὶ νῦν μέτʼ Ἀγγέλων αὐτῇ, πρεσβεύουσιν ἀπαύστως ὑπὲρ πάντων ἡμῶν."

meMA.Kontakion2.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Kontakion2.greekmelody = he.a.m2_gr_GR_cog.heAU.TaAnoZiton.greekname
meMA.Kontakion2.melody = he.a.m2_gr_GR_cog.heAU.TaAnoZiton.name
meMA.Kontakion2.incipit = "Τὸ σῶμα τὸ σόν, νηστείαις…"
meMA.Kontakion2.text = "Τὸ σῶμα τὸ σόν, νηστείαις κατατήξασα, ἐν μέσῳ ἀνδρῶν, Ματρῶνα κατοικήσασα, προσευχαῖς σχολάζουσα, τὸν Δεσπότην ἐνθέως ἐθεράπευσας, διʼ ὃν πάντα κατέλιπες, ὁσίως τὸν βίον διανύσασα."

meMA.Oikos2.text = "Ἄνοιξόν μου τὸ στόμα Χριστέ μου, ἀνυμνῆσαι καὶ λέγειν τοὺς ἀγῶνας τῆς σῆς Ὁσίας φιλάνθρωπε· ὅπως τὰ πάντα καταλιποῦσα, καὶ ποθήσασα μόνον σὲ τὸν Νυμφίον, τὰ ἐπὶ γῆς ὡς φθαρτὰ ἐλογίσατο ἅπαντα· καὶ τὸν τύπον τοῦ ζωηφόρου Σταυροῦ ἐν ἑαυτῇ τυπώσασα, δαιμόνων θράση κατήργησε, καὶ εἰς τέλος αὐτοὺς ἐξηφάνισεν, ὁσίως τὸν βίον διανύσασα."

meMA.Ode7C11.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C11.greekmelody = he.h.m4_gr_GR_cog.heHE.AvramiaioiPote.greekname
meMA.Ode7C11.melody = he.h.m4_gr_GR_cog.heHE.AvramiaioiPote.name
meMA.Ode7C11.incipit = "Εὐγενεστάτης ψυχῆς, τοὺς εὐγενεῖς σου…"
meMA.Ode7C11.text = "Εὐγενεστάτης ψυχῆς, τοὺς εὐγενεῖς σου τρόπους, ὑποδεικνύων ὡμολόγησας, τυράννων ἐνώπιον, Ὁνησιφόρε Μάρτυς, Θεὸν σεσαρκωμένον."

meMA.Ode7C12.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C12.melody = he.h.m4_gr_GR_cog.heHE.AvramiaioiPote.name
meMA.Ode7C12.incipit = "Τὴν διʼ αἰῶνος θείαν, ἐλευθερίαν…"
meMA.Ode7C12.text = "Τὴν διʼ αἰῶνος θείαν, ἐλευθερίαν στέργων, τῷ σῷ δεσπότῃ ὡς εὐήκοος, οἰκέτης συνήθλησας, καὶ σὺν αὐτῷ χορεύεις, Πορφύριε παμμάκαρ."

meMA.Ode7C13.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C13.melody = he.h.m4_gr_GR_cog.heHE.AvramiaioiPote.name
meMA.Ode7C13.incipit = "Αἵμασι θείοις ὑμῶν, τὴν ἐπηρμένην…"
meMA.Ode7C13.text = "Αἵμασι θείοις ὑμῶν, τὴν ἐπηρμένην φλόγα, τῆς ἀθεΐας κατασβέσαντες, βοᾶτε Πανεύφημοι· Ὁ τῶν Πατέρων ἡμῶν, Θεὸς εὐλογητὸς εἶ."

meMA.Ode7C14.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C14.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C14.melody = he.h.m4_gr_GR_cog.heHE.AvramiaioiPote.name
meMA.Ode7C14.incipit = "Ἴνα τῆς μακαρίας, καὶ ἀθανάτου…"
meMA.Ode7C14.text = "Ἴνα τῆς μακαρίας, καὶ ἀθανάτου δόξης, ἀξιωθῆτε τὸν μακάριον, ὑπέστησε θάνατον, θεοφεγγεῖς ἀστέρες, τοῦ νοητοῦ Ἠλίου."

meMA.Ode7C1T.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C1T.melody = he.h.m4_gr_GR_cog.heHE.AvramiaioiPote.name
meMA.Ode7C1T.incipit = "Σεσαρκωμένον Λόγον, σῶν ἐξ αἱμάτων…"
meMA.Ode7C1T.text = "Σεσαρκωμένον Λόγον, σῶν ἐξ αἱμάτων τίκτεις, βροτοὺς θεῶσαι εὐδοκήσαντα, Παρθένε τοὺς μέλποντας· Ὁ τῶν Πατέρων ἡμῶν, Θεὸς εὐλογητὸς εἶ."

meMA.Ode7C21.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C21.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode7C21.greekmelody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.greekname
meMA.Ode7C21.melody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.name
meMA.Ode7C21.incipit = "Ὥσπερ τρυγὼν ὡραιοτάτη, διετήρησας Θεῷ…"
meMA.Ode7C21.text = "Ὥσπερ τρυγὼν ὡραιοτάτη, διετήρησας Θεῷ τὴν σωφροσύνην, εὐτεκνίαν, παθῶν στειρώσει Μακαρία, ὡς ἀληθῶς πλουτήσασα, διὰ σοῦ τῶν σεσωσμένων."

meMA.Ode7C22.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C22.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode7C22.melody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.name
meMA.Ode7C22.incipit = "Κόσμον σεμνὴ καὶ τὰ ἐν…"
meMA.Ode7C22.text = "Κόσμον σεμνὴ καὶ τὰ ἐν κόσμῳ, καταλείψασα ἐνθέῳ πεποιθήσει, ὑπερκόσμιον νῦν, κληροῦσαι κληρουχίαν, Μοναζουσῶν ὑπάρχουσα, ἀληθῶς Ματρῶνα κόσμος."

meMA.Ode7C2T.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C2T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode7C2T.melody = he.h.m8_gr_GR_cog.heHE.PaidesEvraion.name
meMA.Ode7C2T.incipit = "Λύουσα Εὔας τὴν κατάραν, παναμώμητε…"
meMA.Ode7C2T.text = "Λύουσα Εὔας τὴν κατάραν, παναμώμητε ἐγέννησας Παρθένε, τὸν τῶν ὅλων Θεόν, ἀφράστως συλλαβοῦσα, καὶ ὑπὲρ νοῦν κυήσασα, σωτηρίαν τοῖς ἀνθρώποις."

meMA.Ode8C11.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C11.greekmelody = he.h.m4_gr_GR_cog.heHE.TonEnToStavroSarki.greekname
meMA.Ode8C11.melody = he.h.m4_gr_GR_cog.heHE.TonEnToStavroSarki.name
meMA.Ode8C11.incipit = "Μέλη σπαραγμοῖς ἐκδεδωκότες, κατεσπαράξατε ἐχθροῦ…"
meMA.Ode8C11.text = "Μέλη σπαραγμοῖς ἐκδεδωκότες, κατεσπαράξατε ἐχθροῦ, ἀσύνετον καρδίαν, μείναντες ἀσάλευτοι τὸν νοῦν, μεγαλώνυμοι στεφανηφόροι."

meMA.Ode8C12.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C12.melody = he.h.m4_gr_GR_cog.heHE.TonEnToStavroSarki.name
meMA.Ode8C12.incipit = "Αἵμασιν ὑμῶν τὸ πῦρ ἐσβέσθη…"
meMA.Ode8C12.text = "Αἵμασιν ὑμῶν τὸ πῦρ ἐσβέσθη, τῆς ἀγνωσίας Ἀθληταί, πιστῶν δὲ κατηρδεύθη, πάντων καὶ καρδία καὶ ψυχή, ἀναθάλλουσα θεογνωσίαν."

meMA.Ode8C13.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C13.melody = he.h.m4_gr_GR_cog.heHE.TonEnToStavroSarki.name
meMA.Ode8C13.incipit = "Κύματα δεινῶν κολαστηρίων, τῇ κυβερνήσει…"
meMA.Ode8C13.text = "Κύματα δεινῶν κολαστηρίων, τῇ κυβερνήσει τοῦ Χριστοῦ, διήλθετε καὶ ὅρμῳ, θείας βασιλείας οὐρανῶν, προσωρμίσθητε ἐν ἀφθαρσίᾳ."

meMA.Ode8C14.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C14.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C14.melody = he.h.m4_gr_GR_cog.heHE.TonEnToStavroSarki.name
meMA.Ode8C14.incipit = "Ἄνθρακες πυρὶ τοῦ Παρακλήτου, προσαναπτόμενοι…"
meMA.Ode8C14.text = "Ἄνθρακες πυρὶ τοῦ Παρακλήτου, προσαναπτόμενοι σαφῶς, οἱ Μάρτυρες δειχθέντες, ἔφλεξαν τὴν πλάνην τοῦ ἐχθροῦ, καὶ ἐφώτισαν τὴν οἰκουμένην."

meMA.Ode8C1T.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C1T.melody = he.h.m4_gr_GR_cog.heHE.TonEnToStavroSarki.name
meMA.Ode8C1T.incipit = "Ῥόδον ἐκλεκτὸν ἐν ταῖς κοιλάσι…"
meMA.Ode8C1T.text = "Ῥόδον ἐκλεκτὸν ἐν ταῖς κοιλάσι, Κόρη τοῦ βίου εὑρηκώς, ἠράσθη σου τοῦ κάλλους, Λόγος τοῦ Θεοῦ καὶ σαρκωθείς, εὐωδίασε τὴν οἰκουμένην."

meMA.Ode8C21.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C21.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C21.greekmelody = he.h.m8_gr_GR_cog.heHE.NikitaiTyrannou.greekname
meMA.Ode8C21.melody = he.h.m8_gr_GR_cog.heHE.NikitaiTyrannou.name
meMA.Ode8C21.incipit = "Εὐσεβείας τρόποις, τὴν ψυχὴν κοσμήσασα…"
meMA.Ode8C21.text = "Εὐσεβείας τρόποις, τὴν ψυχὴν κοσμήσασα Ματρῶνα, θεῖον τῷ Χριστῷ προσῆξας, μελῳδοῦντα πάνσεμνε χορὸν παρθένων· Εὐλογεῖτε πάντα τὰ ἔργα τὸν Κύριον, καὶ ὑπερυψοῦτε, εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8C22.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C22.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C22.melody = he.h.m8_gr_GR_cog.heHE.NikitaiTyrannou.name
meMA.Ode8C22.incipit = "Οἱ τῶν σῶν ἱδρώτων, ὄμβροι…"
meMA.Ode8C22.text = "Οἱ τῶν σῶν ἱδρώτων, ὄμβροι θείᾳ χάριτι τῶν παθημάτων, τοὺς ὄμβρους Ματρῶνα, θαλεροὺς ξηραίνουσι καὶ τὰς καρδίας, τῶν πιστῶς φοιτώντων πάντοτε ἀρδεύουσιν· ὅθεν σε τιμῶμεν, εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8C23.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C23.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C23.melody = he.h.m8_gr_GR_cog.heHE.NikitaiTyrannou.name
meMA.Ode8C23.incipit = "Σεαυτὴν τοῖς ὅπλοις, πίστει καθοπλίσασα…"
meMA.Ode8C23.text = "Σεαυτὴν τοῖς ὅπλοις, πίστει καθοπλίσασα τῆς ἐγκρατείας, οὐκ ἐτρώθης βέλει, ἡδονῆς διάγουσα ἀνδρῶν ἐν μέσῳ· τὸν τὴν Εὔαν πάλαι τρώσαντα δὲ ἔτρωσας, λόγχῃ σωφροσύνης, Ματρῶνα μακαρία."

meMA.Ode8C2T.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C2T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C2T.melody = he.h.m8_gr_GR_cog.heHE.NikitaiTyrannou.name
meMA.Ode8C2T.incipit = "Ἰδοὺ ἡ Παρθένος, ὥς φησιν…"
meMA.Ode8C2T.text = "Ἰδοὺ ἡ Παρθένος, ὥς φησιν ἐν Πνεύματι ὁ Ἠσαΐας, ἔσχεν ἐν κοιλίᾳ, τὸν ἡμᾶς ῥυσάμενον ᾌδου κοιλίας, εὐσεβῶς βοῶντας· Ὑμνεῖτε τὸν Κύριον, καὶ ὑπερυψοῦτε, εἰς πάντας τους αἰῶνας."

meMA.Ode8H.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8H.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8H.greekmelody = he.h.m8_gr_GR_cog.heHE.NikitaiTyrannou.greekname
meMA.Ode8H.melody = he.h.m8_gr_GR_cog.heHE.NikitaiTyrannou.melody
meMA.Ode8H.incipit = he.h.m8_gr_GR_cog.heHE.NikitaiTyrannou.incipit
meMA.Ode8H.text = he.h.m8_gr_GR_cog.heHE.NikitaiTyrannou.text

meMA.Ode9C11.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode9C11.greekmelody = he.h.m4_gr_GR_cog.heHE.EvaMenToTisParakois.greekname
meMA.Ode9C11.melody = he.h.m4_gr_GR_cog.heHE.EvaMenToTisParakois.name
meMA.Ode9C11.incipit = "Ἴσχυσαν δυνάμει τοῦ Χριστοῦ νευρούμενοι…"
meMA.Ode9C11.text = "Ἴσχυσαν δυνάμει τοῦ Χριστοῦ νευρούμενοι, τοὺς ἀσάρκους μετὰ σώματος, καταπαλαῖσαι οἱ γενναῖοι ἀδάμαντες, οἱ στῦλοι τῆς πίστεως, καὶ νῦν εἰς οὐρανοὺς συνευφραίνονται, τοῖς ἀπʼ αἰῶνος ἐναθλήσασιν."

meMA.Ode9C12.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode9C12.melody = he.h.m4_gr_GR_cog.heHE.EvaMenToTisParakois.name
meMA.Ode9C12.incipit = "Ὡς ἅρμα τοῦ Λόγου ἱερὸν…"
meMA.Ode9C12.text = "Ὡς ἅρμα τοῦ Λόγου ἱερὸν γενόμενοι, τοῖς ἀλόγοις προσεδέθητε, Μάρτυρες θεῖοι καὶ πρὸς νύσσαν, οὐράνιον ἐφθάσατε χαίροντες, καὶ νίκης τὰ βραβεῖα ἐδέξασθε· ὅθεν ἀξίως μοκαρίζεσθε."

meMA.Ode9C13.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode9C13.melody = he.h.m4_gr_GR_cog.heHE.EvaMenToTisParakois.name
meMA.Ode9C13.incipit = "Σώματα ὑμῶν Παγκεανῶν περίδοξος, νῦν…"
meMA.Ode9C13.text = "Σώματα ὑμῶν Παγκεανῶν περίδοξος, νῦν κατέχει κώμη Μάρτυρες, θείους πλουτήσασα προστάτας, ὑμᾶς καὶ ἰατροὺς θεραπεύοντας, σωμάτων καὶ ψυχῶν ἀῤῥωστήματα, τῶν προσιόντων πίστει πάντοτε."

meMA.Ode9C14.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C14.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode9C14.melody = he.h.m4_gr_GR_cog.heHE.EvaMenToTisParakois.name
meMA.Ode9C14.incipit = "Ἡ θεία δυὰς καὶ φωταυγὴς…"
meMA.Ode9C14.text = "Ἡ θεία δυὰς καὶ φωταυγὴς Πορφύριε, Ἀθλοφόρε παναοίδιμε, Ὀνησιφόρε γενναιόφρον, Τριάδι τῇ Ἁγίᾳ πρεσβεύσατε, εἰρήνην καὶ δεινῶν ἀπολύτρωσιν, παρασχεθῆναι τοῖς ὑμνοῦσιν ὑμᾶς."

meMA.Ode9C1T.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode9C1T.melody = he.h.m4_gr_GR_cog.heHE.EvaMenToTisParakois.name
meMA.Ode9C1T.incipit = "Φορέσας με ὅλον ἐκ τῶν…"
meMA.Ode9C1T.text = "Φορέσας με ὅλον ἐκ τῶν σῶν προέρχεται, ὡς νυμφίος Παναμώμητε, θείων αἱμάτων ὁ Δεσπότης, καὶ γάλακτι ἐκτρέφεται ἅπασαν, τὴν κτίσιν διατρέφων βουλήματι, εὐλογημένη ἀειπάρθενε."

meMA.Ode9C21.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C21.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode9C21.greekmelody = he.h.m8_gr_GR_cog.heHE.EfrixePasaAkoi.greekname
meMA.Ode9C21.melody = he.h.m8_gr_GR_cog.heHE.EfrixePasaAkoi.name
meMA.Ode9C21.incipit = "Ὡς θεία σὺ περιστερά, ἀρετῶν…"
meMA.Ode9C21.text = "Ὡς θεία σὺ περιστερά, ἀρετῶν κεχρυσωμέναις ταῖς πτέρυξι, Ματρῶνα ἔνδοξε, ἀνεκουφίσθης, καὶ νῦν κατέπαυσας, ἔνθα Ὁσίων οἱ χοροί, Δικαίων τὰ τάγματα, καὶ ἡ αἰώνιος, εὐφροσύνη καὶ χαρὰ ἀνεκλάλητος."

meMA.Ode9C22.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C22.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode9C22.melody = he.h.m8_gr_GR_cog.heHE.EfrixePasaAkoi.name
meMA.Ode9C22.incipit = "Στέργουσα τὸν ἐκ γυναικός, διὰ…"
meMA.Ode9C22.text = "Στέργουσα τὸν ἐκ γυναικός, διὰ σπλάγχνα οἰκτιρμῶν ἀνατείλαντα, ἄδυτον Ἥλιον, τὴν καυστικήν τε καὶ δυσδιόδευτον, ῥᾳδίως ὥδευσας Σεμνή, τρίβον τῆς ἀσκήσεως, δαίμονας φλέγουσα, καὶ τὰ πάθη τῆς σαρκὸς ἐκλεαίνουσα."

meMA.Ode9C23.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C23.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode9C23.melody = he.h.m8_gr_GR_cog.heHE.EfrixePasaAkoi.name
meMA.Ode9C23.incipit = "Ἡ θεία μνήμη σου ἡμῖν…"
meMA.Ode9C23.text = "Ἡ θεία μνήμη σου ἡμῖν, τηλαυγέστερον ἡλίου ἀνέτειλε, φωταγωγοῦσα ἡμῶν τὰς διανοίας καὶ τὰ νοήματα, ἐν ᾗ μνημόνευε ἡμῶν, τῶν μνημονευόντων σου, Ματρῶνα πάνσεμνε, παῤῥησίαν πρὸς Χριστὸν ὄντως ἔχουσα."

meMA.Ode9C2T.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C2T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode9C2T.melody = he.h.m8_gr_GR_cog.heHE.EfrixePasaAkoi.name
meMA.Ode9C2T.incipit = "Φέρουσα Κόρη ταῖς χερσί, τὸν…"
meMA.Ode9C2T.text = "Φέρουσα Κόρη ταῖς χερσί, τὸν τὰ σύμπαντα τῷ νεύματι φέροντα, χειρός με λύτρωσαι, τοῦ ἀλλοτρίου τῇ μεσιτείᾳ σου, καὶ τῷ φωτί σου τὴν ἐμήν, διάνοιαν φώτισον, καὶ ἀλλοτρίωσον, τῶν παθῶν τῶν ἀναιδῶς πολεμούντων με."

meMA.Ode9H.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9H.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode9H.greekmelody = he.h.m8_gr_GR_cog.heHE.EfrixePasaAkoi.greekname
meMA.Ode9H.melody = he.h.m8_gr_GR_cog.heHE.EfrixePasaAkoi.melody
meMA.Ode9H.incipit = he.h.m8_gr_GR_cog.heHE.EfrixePasaAkoi.incipit
meMA.Ode9H.text = he.h.m8_gr_GR_cog.heHE.EfrixePasaAkoi.text

meMA.Exaposteilarion1.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Exaposteilarion1.greekmelody = he.a.m2_gr_GR_cog.heAU.GynaikesAkoutisthite.greekname
meMA.Exaposteilarion1.melody = he.a.m2_gr_GR_cog.heAU.GynaikesAkoutisthite.name
meMA.Exaposteilarion1.incipit = "Τοὺς Μάρτυρας τιμήσωμεν…"
meMA.Exaposteilarion1.text = "Τοὺς Μάρτυρας τιμήσωμεν, τοὺς ἐν ἀθλήσει λάμψαντας, καὶ καταυγάσαντας κόσμον, μαρτυρικαῖς ἀγλαΐαις, Ὀνησιφόρον τὸν μέγαν, καὶ τὸν σεπτὸν Πορφύριον· καὶ γὰρ αὐτοὶ πρεσβεύουσιν, ὑπὲρ ἡμῶν τῶν ἐν πίστει, αὐτῶν τελούντων τὴν μνήμην."

meMA.Exaposteilarion2.mode = miscellanea_gr_GR_cog.misc.Mode3
meMA.Exaposteilarion2.greekmelody = he.a.m3_gr_GR_cog.heAU.OOuranonToisAstrois.greekname
meMA.Exaposteilarion2.melody = he.a.m3_gr_GR_cog.heAU.OOuranonToisAstrois.name
meMA.Exaposteilarion2.incipit = "Ὅλον ὑψώσασα τὸν νοῦν, τῆς…"
meMA.Exaposteilarion2.text = "Ὅλον ὑψώσασα τὸν νοῦν, τῆς σχέσεως τῶν γηΐνων, ἀνεπιστρόφως Ματρῶνα, ὥδευσας τρίβον θεόφρον, τῶν ἐντολῶν τοῦ Σωτῆρος· διὸ ἡμῶν ὑπερεύχου."

meMA.ExapTheotokion.mode = miscellanea_gr_GR_cog.misc.Mode3
meMA.ExapTheotokion.melody = he.a.m3_gr_GR_cog.heAU.OOuranonToisAstrois.name
meMA.ExapTheotokion.incipit = "Ἱερωτάταις φωναῖς σε, σεπτοὶ Προφῆται…"
meMA.ExapTheotokion.text = "Ἱερωτάταις φωναῖς σε, σεπτοὶ Προφῆται Παρθένε, κατήγγειλαν ἐσομένην, Θεοῦ Μητέρα· διό σε, εἰδότες πίστει καὶ πόθῳ, γεραίρομεν Θεοτόκον."

meLI.Kontakion1.mode = me.m11.d09_gr_GR_cog.meMA.Kontakion1.mode
