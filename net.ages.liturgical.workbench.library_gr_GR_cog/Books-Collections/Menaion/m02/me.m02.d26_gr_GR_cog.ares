meMA.Ode4C11.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C11.greekmelody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.greekname
meMA.Ode4C11.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
meMA.Ode4C11.incipit = "Ἰσουργὸν τῷ Γεννήτορι…"
meMA.Ode4C11.text = "Ἰσουργὸν τῷ Γεννήτορι, Λόγον διαγγέλλων τοῖς ματαιόφροσι, πρὸς εὐσέβειαν ἐζώγρησας, καὶ Θεῷ τῶν ὅλων καθιέρωσας."

meMA.Ode4C12.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C12.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
meMA.Ode4C12.incipit = "Ζωτικαῖς ἐπιπνεύσεσι…"
meMA.Ode4C12.text = "Ζωτικαῖς ἐπιπνεύσεσι, τὴν πρὸς τελευταίαν ὥραν ἐγγίζουσαν, ἱερώτατε ἐζώωσας, εἰς πολλῶν καὶ ταύτης ἀναγέννησιν."

meMA.Ode4C13.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C13.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
meMA.Ode4C13.incipit = "Ὁλονύκτοις δεήσεσι…"
meMA.Ode4C13.text = "Ὁλονύκτοις δεήσεσι, καὶ ταῖς πανημέροις Ὅσιε στάσεσι, τὴν καρδίαν ἀπετέλεσας, τοῦ Θεοῦ τῶν ὅλων καταγώγιον."

meMA.Ode4C1T.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C1T.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
meMA.Ode4C1T.incipit = "Νεκρωθέντα με Πάναγνε…"
meMA.Ode4C1T.text = "Νεκρωθέντα με Πάναγνε, πάλιν ἀνεζώωσας ὡς κυήσασα, τὴν ζωὴν τὴν ἐνυπόστατον, τὸν Υἱὸν καὶ Λόγον τοῦ Γεννήτορος."

meMA.Ode5C1H.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C1H.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C1H.greekmelody = he.h.m8_gr_GR_cog.heHE.FotisonImas.greekname
meMA.Ode5C1H.melody = he.h.m8_gr_GR_cog.heHE.FotisonImas.melody
meMA.Ode5C1H.incipit = he.h.m8_gr_GR_cog.heHE.FotisonImas.incipit
meMA.Ode5C1H.text = he.h.m8_gr_GR_cog.heHE.FotisonImas.text

meMA.Ode5C11.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C11.greekmelody = he.h.m8_gr_GR_cog.heHE.FotisonImas.greekname
meMA.Ode5C11.melody = he.h.m8_gr_GR_cog.heHE.FotisonImas.name
meMA.Ode5C11.incipit = "Πρόῤῥιζον τεμών…"
meMA.Ode5C11.text = "Πρόῤῥιζον τεμών, τὴν ἄκανθαν τὴν φιλήδονον, ἐκαρποφόρησας τὸν ἄσταχυν, τῆς εὐσεβείας, Ἱεράρχα πανσεβάσμιε."

meMA.Ode5C12.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C12.melody = he.h.m8_gr_GR_cog.heHE.FotisonImas.name
meMA.Ode5C12.incipit = "Ὄμβροις σῶν εὐχῶν…"
meMA.Ode5C12.text = "Ὄμβροις σῶν εὐχῶν, οὐρανοὺς σοφὲ διήνοιξας, καὶ ὑετὸν τῇ γῇ διένειμας, αἱρετιζόντων, ἀποπνίξας τὰ ζιζάνια."

meMA.Ode5C13.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C13.melody = he.h.m8_gr_GR_cog.heHE.FotisonImas.name
meMA.Ode5C13.incipit = "Ἴθυνας ῥοπῇ…"
meMA.Ode5C13.text = "Ἴθυνας ῥοπῇ, θείου Πνεύματος ὁλόκληρον, λαὸν καὶ πόλιν εἰς λιμένα ζωῆς, διεκφυγόντα, ἀθεΐας τὸ κλυδώνιον."

meMA.Ode5C1T.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C1T.melody = he.h.m8_gr_GR_cog.heHE.FotisonImas.name
meMA.Ode5C1T.incipit = "Μόνην γενεῶν…"
meMA.Ode5C1T.text = "Μόνην γενεῶν, ἐκ πασῶν σε ἐξελέξατο, ὁ Πλαστουργὸς καὶ ἀναπλάττει ἡμᾶς, ἐν σοὶ οἰκήσας, Θεοτόκε ἀειπάρθενε."

meMA.Ode6C1H.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C1H.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C1H.greekmelody = he.h.m8_gr_GR_cog.heHE.TinDeisinEkcheo.greekname
meMA.Ode6C1H.melody = he.h.m8_gr_GR_cog.heHE.TinDeisinEkcheo.melody
