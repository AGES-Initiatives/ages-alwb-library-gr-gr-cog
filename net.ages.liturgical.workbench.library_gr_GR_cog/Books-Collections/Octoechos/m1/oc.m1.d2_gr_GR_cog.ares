octrMA.Photagogic.mode = miscellanea_gr_GR_cog.misc.Mode1
octrMA.Photagogic.incipit = "Ὁ τὸ φῶς ἀνατέλλων Χριστέ…"
octrMA.Photagogic.text = "Ὁ τὸ φῶς ἀνατέλλων Χριστέ, τὴν ψυχήν μου καθάρισον ἀπὸ πάσης ἁμαρτίας,"
