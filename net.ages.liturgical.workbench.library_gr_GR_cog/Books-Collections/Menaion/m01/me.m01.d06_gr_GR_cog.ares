meMA.Ode4C21.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C21.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode4C21.greekmelody = he.h.m2_gr_GR_cog.heHE.PyrsoKathartheis.greekname
meMA.Ode4C21.melody = he.h.m2_gr_GR_cog.heHE.PyrsoKathartheis.name
meMA.Ode4C21.incipit = "Πεμφθεὶς ὁ Πατρὸς παμφαέστατος Λόγος…"
meMA.Ode4C21.text = "Πεμφθεὶς ὁ Πατρὸς παμφαέστατος Λόγος, * Νυκτὸς διῶσαι τὴν καχέσπερον σχέσιν, * Ἔκριζον ἥκεις, καὶ βροτῶν ἁμαρτίας, * Υἷας συνελκύσαι τε τῇ σῇ Βαπτίσει, * Μάκαρ φαεινούς, ἐκ ῥοῶν Ἰορδάνου."
meMA.Ode4C211.text = "Πεμφθεὶς ὁ Πατρὸς παμφαέστατος Λόγος,"
meMA.Ode4C212.text = "Νυκτὸς διῶσαι τὴν καχέσπερον σχέσιν,"
meMA.Ode4C213.text = "Ἔκριζον ἥκεις, καὶ βροτῶν ἁμαρτίας,"
meMA.Ode4C214.text = "Υἷας συνελκύσαι τε τῇ σῇ Βαπτίσει,"
meMA.Ode4C215.text = "Μάκαρ φαεινούς, ἐκ ῥοῶν Ἰορδάνου."

meMA.Ode4C22.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C22.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode4C22.melody = he.h.m2_gr_GR_cog.heHE.PyrsoKathartheis.name
meMA.Ode4C22.incipit = "Αὐτὸν προσιδὼν τὸν περίκλυτον Λόγον…"
meMA.Ode4C22.text = "Αὐτὸν προσιδὼν τὸν περίκλυτον Λόγον, * Τρανῶς ὁ κήρυξ ἐκβοᾶται τῇ κτίσει, * Οὗτος προών μου, δεύτερος τῷ σαρκίῳ. * Σύμμορφος ἐξέλαμψεν ἐνθέῳ σθένει, * Ἔχθιστον ἡμῶν ἐξελεῖν ἁμαρτίαν."
meMA.Ode4C221.text = "Αὐτὸν προσιδὼν τὸν περίκλυτον Λόγον,"
meMA.Ode4C222.text = "Τρανῶς ὁ κήρυξ ἐκβοᾶται τῇ κτίσει,"
meMA.Ode4C223.text = "Οὗτος προών μου, δεύτερος τῷ σαρκίῳ."
meMA.Ode4C224.text = "Σύμμορφος ἐξέλαμψεν ἐνθέῳ σθένει,"
meMA.Ode4C225.text = "Ἔχθιστον ἡμῶν ἐξελεῖν ἁμαρτίαν."

meMA.Ode4C23.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C23.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode4C23.melody = he.h.m2_gr_GR_cog.heHE.PyrsoKathartheis.name
meMA.Ode4C23.incipit = "Νομὴν πρὸς αὐτὴν τὴν φερέσβιον…"
meMA.Ode4C23.text = "Νομὴν πρὸς αὐτὴν τὴν φερέσβιον φέρων, * Θηρᾷ δρακόντων φωλεοῖς ἐπιτρέχων. * Ἄπλητα κύκλα καββαλὼν Θεὸς Λόγος, * Πτέρνῃ τε τὸν πλήττοντα παμπήδην γένος, * Τοῦτον καθειργνύς, ἐκσαῴζει τὴν κτίσιν."
meMA.Ode4C231.text = "Νομὴν πρὸς αὐτὴν τὴν φερέσβιον φέρων,"
meMA.Ode4C232.text = "Θηρᾷ δρακόντων φωλεοῖς ἐπιτρέχων."
meMA.Ode4C233.text = "Ἄπλητα κύκλα καββαλὼν Θεὸς Λόγος,"
meMA.Ode4C234.text = "Πτέρνῃ τε τὸν πλήττοντα παμπήδην γένος,"
meMA.Ode4C235.text = "Τοῦτον καθειργνύς, ἐκσαῴζει τὴν κτίσιν."

