hoCA.title6 = "Κοιμηθέντα ἐν εἰρήνῃ τῇ 12ῃ Ἰουνίου 1995."
hoCA.poet = "Ποίημα Δρος Χαραλάμπους Μ. Μπούσια"
hoCA.translator = ""
hoCA.Insert1 = "τοῦ ὁσίου καὶ θεοφόρου πατρὸς ἡμῶν Γερασίμου τοῦ ἐν Ἀμερικῇ,"

