meVE.SticStavrotheotokion.text = me.m01.d23_gr_GR_cog.meVE.AposStavrotheotokion.text

meVE.Apolytikion1ger.poet = poets_gr_GR_cog.po.Gerasimou
meVE.Apolytikion1ger.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.Apolytikion1ger.greekmelody = he.a.m4_gr_GR_cog.heAU.TachyProkatalave.greekname
meVE.Apolytikion1ger.melody = he.a.m4_gr_GR_cog.heAU.TachyProkatalave.name
meVE.Apolytikion1ger.incipit = "Χριστῷ στρατευσάμενος, τῷ Βασιλεῖ τοῦ…"
meVE.Apolytikion1ger.text = "Χριστῷ στρατευσάμενος, τῷ Βασιλεῖ τοῦ παντός, γενναίως διιεκοψας, τὰς παρατάξεις ἐχθρῶν, Ἑρμεία πανένδοξε· σὺ γὰρ ἐγκαρτερήσας, πολυτρόποις αἰκίαις, ἤθλησας ἐν τῷ γήρᾳ, ὡς τοῦ Λόγου ὁπλίτης· ᾧ πρέσβευε Ἀθλοφόρε, σῴζεσθαι ἅπαντας."

meMA.C1.acrostic.title = "Ὁ Κανὼν τοῦ Ἁγίου,"
meMA.C1.acrostic.notice1 = "οὗ ἡ Ἀκροστιχίς·"
meMA.C1.acrostic.text = "ᾌδω Μάκαρ σου τοὺς πόνους θερμῷ πόθῳ. Ἰωσήφ."
meMA.C1.title = titles_gr_GR_cog.ti.CanonSaintM
meMA.C1.verse = prayers_gr_GR_cog.pr.verse_SaintM.text

meMA.Ode1C11.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C11.greekmelody = he.h.m8_gr_GR_cog.heHE.YgranDiodefsas.greekname
meMA.Ode1C11.melody = he.h.m8_gr_GR_cog.heHE.YgranDiodefsas.name
meMA.Ode1C11.incipit = "Ἀστὴρ διαλάμπων θείῳ φωτί, τῷ…"
meMA.Ode1C11.text = "Ἀστὴρ διαλάμπων θείῳ φωτί, τῷ τῆς ἀληθείας ἀνεδείχθης μαρτυρικῶς, πάντας καταυγάζων τοὺς ἐν πίστει, εἰλικρινεῖ σε Ἑρμεία γεραίροντας."

meMA.Ode1C12.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C12.melody = he.h.m8_gr_GR_cog.heHE.YgranDiodefsas.name
meMA.Ode1C12.incipit = "Διόλου τῇ νεύσει τῇ πρὸς…"
meMA.Ode1C12.text = "Διόλου τῇ νεύσει τῇ πρὸς Θεόν, πάντων ἀλογήσας, τῶν ἐν βίῳ Μάρτυς σοφέ, μαρτύρων ὑπέδυς τοὺς ἀγῶνας, καὶ τὸν ἐχθρὸν καταῤῥάσσων ἐνίκησας."

meMA.Ode1C13.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C13.melody = he.h.m8_gr_GR_cog.heHE.YgranDiodefsas.name
meMA.Ode1C13.incipit = "Ὡραῖος τὸ εἶδος καὶ τὴν…"
meMA.Ode1C13.text = "Ὡραῖος τὸ εἶδος καὶ τὴν ψυχήν, ὑπάρχων Ἑρμεία, ἐστρατεύθης τῷ Βασιλεῖ, τῷ ἐπουρανίῳ καὶ τοὺς τούτου, Μάρτυς εἰς τὸ τέλος ἐχθροὺς ἐταπείνωσας."

meMA.Ode1C14.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C14.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C14.melody = he.h.m8_gr_GR_cog.heHE.YgranDiodefsas.name
meMA.Ode1C14.incipit = "Μὴ θύσας εἰδώλοις Μάκαρ τοὺς…"
meMA.Ode1C14.text = "Μὴ θύσας εἰδώλοις Μάκαρ τοὺς σούς, ἐθλάσθης ὀδόντας, σιαγόνας τῶν νοητῶν, συνθλάσας λεόντων, καὶ τῆς νίκης, ἀναδησάμενος στέφος ἀμάραντον."

meMA.Ode1C1T.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode1C1T.melody = he.h.m8_gr_GR_cog.heHE.YgranDiodefsas.name
meMA.Ode1C1T.incipit = "Ἁγία Παρθένε Μήτηρ Χριστοῦ, Μαρτύρων…"
meMA.Ode1C1T.text = "Ἁγία Παρθένε Μήτηρ Χριστοῦ, Μαρτύρων ἡ δόξα, καὶ Ἀγγέλων ἡ χαρμονή, σὺν τούτοις δυσώπει τὸν Υἱόν σου, τοῦ οἰκτιρῆσαι καὶ σῶσαι τοὺς δούλους σου."

meMA.Ode3C11.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C11.greekmelody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.greekname
meMA.Ode3C11.melody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.name
meMA.Ode3C11.incipit = "Κόσμος ἐχρημάτισας, τῶν ἀθλητῶν Σοφὲ…"
meMA.Ode3C11.text = "Κόσμος ἐχρημάτισας, τῶν ἀθλητῶν Σοφὲ χάριτι, θεολαμπής· ὅθεν οἱ ἐν κόσμῳ, εὐσεβῶς σε γεραίρομεν."

meMA.Ode3C12.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C12.melody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.name
meMA.Ode3C12.incipit = "Αἶνον τῷ Θεῷ ἡμῶν, τὰς…"
meMA.Ode3C12.text = "Αἶνον τῷ Θεῷ ἡμῶν, τὰς σιαγόνας σου θλώμενος, Μάρτυς σοφέ, τῷ σὲ δυναμοῦντι, εὐχαρίστως ἀνέμελπες."

meMA.Ode3C13.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C13.melody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.name
meMA.Ode3C13.incipit = "Ῥώμῃ θείου Πνεύματος, σὺ κρατυνόμενος…"
meMA.Ode3C13.text = "Ῥώμῃ θείου Πνεύματος, σὺ κρατυνόμενος Πάνσοφε, τὸν δυνατόν, χαίρων ἐν κακίᾳ, ἐταπείνωσας δράκοντα."

meMA.Ode3C1T.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C1T.melody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.name
meMA.Ode3C1T.incipit = "Στόματι καὶ γλώττῃ σε, Θεοῦ…"
meMA.Ode3C1T.text = "Στόματι καὶ γλώττῃ σε, Θεοῦ πανάγιον τέμενος, ὁμολογῶ, Πάναγνε· διό με, πάσης ῥῦσαι κολάσεως."

meMA.Ode3H.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3H.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3H.greekmelody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.greekname
meMA.Ode3H.melody = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.melody
meMA.Ode3H.incipit = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.incipit
meMA.Ode3H.text = he.h.m8_gr_GR_cog.heHE.SyEiToStereoma.text

meMA.Ode3Kathisma1.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3Kathisma1.greekmelody = he.a.m4_gr_GR_cog.heAU.EpefanisSimeron.greekname
meMA.Ode3Kathisma1.melody = he.a.m4_gr_GR_cog.heAU.EpefanisSimeron.name
meMA.Ode3Kathisma1.incipit = "Ὡς ἀστέρα μέγιστον, τῇ οἰκουμένῃ…"
meMA.Ode3Kathisma1.text = "Ὡς ἀστέρα μέγιστον, τῇ οἰκουμένῃ, ὁ Χριστός σε ἔδειξε, φωταγωγοῦντα τοὺς πιστούς, ταῖς τῶν ἀγώνων σου λάμψεσι, Μάρτυς Ἑρμεία, Ἀγγέλων συνόμιλε."

meMA.Ode3KathismaT.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3KathismaT.melody = he.a.m4_gr_GR_cog.heAU.EpefanisSimeron.name
meMA.Ode3KathismaT.incipit = me.m05.d30_gr_GR_cog.meMA.Ode3KathismaT.incipit
meMA.Ode3KathismaT.text = me.m05.d30_gr_GR_cog.meMA.Ode3KathismaT.text

meMA.Ode3KathismaSt.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3KathismaSt.melody = he.a.m4_gr_GR_cog.heAU.EpefanisSimeron.name
meMA.Ode3KathismaSt.incipit = me.m05.d30_gr_GR_cog.meMA.Ode3KathismaSt.incipit
meMA.Ode3KathismaSt.text = me.m05.d30_gr_GR_cog.meMA.Ode3KathismaSt.text

meMA.Ode4C11.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C11.greekmelody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.greekname
meMA.Ode4C11.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
meMA.Ode4C11.incipit = "Οἱ κρουνοὶ τῶν αἱμάτων σου…"
meMA.Ode4C11.text = "Οἱ κρουνοὶ τῶν αἱμάτων σου, τῆς πολυθεΐας τὸ πῦρ κατέσβεσαν, τοῖς πιστοῖς δὲ δρόσος ὤφθησαν, ἀθλοφόρε Μάρτυς ἀξιάγαστε."

meMA.Ode4C12.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C12.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
meMA.Ode4C12.incipit = "Ὑπεισῆλθες ἀπτόητος, τὴν πυρακτωθεῖσαν Ἔνδοξε…"
meMA.Ode4C12.text = "Ὑπεισῆλθες ἀπτόητος, τὴν πυρακτωθεῖσαν Ἔνδοξε κάμινον, ὡς οἱ Παῖδες δὲ διέμεινας θεϊκῇ δυνάμει ἀκατάφλεκτος."

meMA.Ode4C13.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C13.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
meMA.Ode4C13.incipit = "Τῆς Τριάδος τῇ χάριτι, Μάρτυς…"
meMA.Ode4C13.text = "Τῆς Τριάδος τῇ χάριτι, Μάρτυς δροσιζόμενος μένεις ἄφλεκτος, τῆς φλογὸς μέσον ἱστάμενος, ἐν Τρισὶν ἡμέραις παναοίδιμε."

meMA.Ode4C14.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C14.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C14.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
meMA.Ode4C14.incipit = "Ὁ χορὸς τῶν Δυνάμεων, ἄνωθεν…"
meMA.Ode4C14.text = "Ὁ χορὸς τῶν Δυνάμεων, ἄνωθεν ἐσκόπει τὴν γενναιότητα, καὶ τῶν πόνων σου τὸ εὔτονον, ἀθλητὰ Ἑρμεία ἐκπληττόμενος."

meMA.Ode4C1T.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C1T.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
meMA.Ode4C1T.incipit = "Ὑπερύμνητε Δέσποινα, τὸν ἐν τοῖς…"
meMA.Ode4C1T.text = "Ὑπερύμνητε Δέσποινα, τὸν ἐν τοῖς Ἁγίοις ἀναπαυόμενον, ὑπὲρ λόγον ἀπεκύησας· διὰ τοῦτο ὕμνοις σε γεραίρομεν."

meMA.Ode5C11.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C11.greekmelody = he.h.m8_gr_GR_cog.heHE.InaTiMeAposo.greekname
meMA.Ode5C11.melody = he.h.m8_gr_GR_cog.heHE.InaTiMeAposo.name
meMA.Ode5C11.incipit = "Συντριβόμενος σῶμα, Μάρτυς παναοίδιμε πολλαῖς…"
meMA.Ode5C11.text = "Συντριβόμενος σῶμα, Μάρτυς παναοίδιμε πολλαῖς κολάσεσι, συντριβὴν εἰργάσω, ἀπωλείας ξοάνων ἐν χάριτι· διὰ τοῦτο πάντων, τῶν εὐσεβῶν χάριτι θείᾳ, θεραπεύεις ψυχῶν τὰ συντρίμματα."

meMA.Ode5C12.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C12.melody = he.h.m8_gr_GR_cog.heHE.InaTiMeAposo.name
meMA.Ode5C12.incipit = "Πικροτάτων φαρμάκων, Μάκαρ καταπόσεσι μένεις…"
meMA.Ode5C12.text = "Πικροτάτων φαρμάκων, Μάκαρ καταπόσεσι μένεις ἀπήμαντος, εἰς βεβαίαν πίστιν, ἐκκαλούμενος τούτων τὸν πάροχον, μετακυλισθέντα, τῇ σῇ λαμπρᾷ θαυματουργίᾳ, μαρτυρίου πρὸς κλέος ὑπέρλαμπρον."

meMA.Ode5C13.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C13.melody = he.h.m8_gr_GR_cog.heHE.InaTiMeAposo.name
meMA.Ode5C13.incipit = "Οὐδαμῶς παρετράπης, στάσεως τῆς κρείττονος…"
meMA.Ode5C13.text = "Οὐδαμῶς παρετράπης, στάσεως τῆς κρείττονος Μακαριώτατε, τὸν κρατύνοντά σε, καὶ στηρίζοντα καὶ κατευθύνοντα, τὰς κινήσεις πάσας, τὰς πρὸς αὐτόν σου γινομένας, προορώμενος γνώμης εὐθύτητι."

meMA.Ode5C1T.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C1T.melody = he.h.m8_gr_GR_cog.heHE.InaTiMeAposo.name
meMA.Ode5C1T.incipit = "Νοεράν σε λυχνίαν, πάλαι ὁ…"
meMA.Ode5C1T.text = "Νοεράν σε λυχνίαν, πάλαι ὁ Προφήτης κατεῖδεν ἐν Πνεύματι, κεκτημένην Κόρη, τὸ λαμπάδιον Χριστὸν τὸν Κύριον, τὸν φωτὶ ἀῤῥήτῳ, πάντας ἡμᾶς φωταγωγοῦντα, τοὺς πιστῶς σε τιμῶντας πανάμωμε."

meMA.Ode6C11.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C11.greekmelody = he.h.m8_gr_GR_cog.heHE.IlasthitiMoiSotir.greekname
meMA.Ode6C11.melody = he.h.m8_gr_GR_cog.heHE.IlasthitiMoiSotir.name
meMA.Ode6C11.incipit = "Ὁλόκληρον πρὸς Θεόν, Παμμάκαρ ἔχων…"
meMA.Ode6C11.text = "Ὁλόκληρον πρὸς Θεόν, Παμμάκαρ ἔχων τὴν ἔφεσιν, τοὺς πόνους καὶ σπαραγμούς, τοῦ θείου σου σώματος, ὥσπερ ἄλλου πασχοντος, ἐκαρτέρεις· Χαίρων, τοὺς στεφάνους προορώμενος."

meMA.Ode6C12.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C12.melody = he.h.m8_gr_GR_cog.heHE.IlasthitiMoiSotir.name
meMA.Ode6C12.incipit = "Ὑπέμεινας καρτερῶς, τῶν νεύρων σου…"
meMA.Ode6C12.text = "Ὑπέμεινας καρτερῶς, τῶν νεύρων σου τὴν ἀφαίρεσιν, Ἑρμεία Μάρτυς Χριστοῦ, ἐχθρὸν πολυμήχανον, ἐκνευρίζων χάριτι, καὶ ἀνδρείας πάσης, ὑποτύπωσις δεικνύμενος."

meMA.Ode6C13.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C13.melody = he.h.m8_gr_GR_cog.heHE.IlasthitiMoiSotir.name
meMA.Ode6C13.incipit = "Στομούμενος τῷ πυρί, τῆς τοῦ…"
meMA.Ode6C13.text = "Στομούμενος τῷ πυρί, τῆς τοῦ Χριστοῦ ἀγαπήσεως, ὡς δίστομος ἀληθῶς, γεγένησαι μάχαιρα, στρατεύματα ἄθεα, ὡς στεῤῥὸς ὁπλίτης, διακόπτων ἐμφανέστατα."

meMA.Ode6C1T.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C1T.melody = he.h.m8_gr_GR_cog.heHE.IlasthitiMoiSotir.name
meMA.Ode6C1T.incipit = "Θηλάζεις μητροπρεπῶς, τὸν νεύματι πάντας…"
meMA.Ode6C1T.text = "Θηλάζεις μητροπρεπῶς, τὸν νεύματι πάντας τρέφοντα, ἀγκάλαις περικρατεῖς, πανάμωμε Δέσποινα, τὸν πάντας κατέχοντα· διὰ τοῦτο πίστει, κατὰ χρέος σε δοξάζομεν."

meMA.Ode6H.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6H.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6H.greekmelody = he.h.m8_gr_GR_cog.heHE.IlasthitiMoiSotir.greekname
meMA.Ode6H.melody = he.h.m8_gr_GR_cog.heHE.IlasthitiMoiSotir.melody
