meVE.Apolytikion1.mode = misc_gr_GR_cog.Mode4
meVE.Apolytikion1.melody = he.a.m4_gr_GR_cog.OYpsotheisEnToStavro.name
meVE.Apolytikion1.incipit = "Στρατολογίᾳ ἀληθεῖ Ἀθλοφόρε, τοῦ οὐρανίου…"
meVE.Apolytikion1.text = "Στρατολογίᾳ ἀληθεῖ Ἀθλοφόρε, τοῦ οὐρανίου στρατηγὸς Βασιλέως, περικαλὴς γεγένησαι Θεόδωρε· ὅπλοις γὰρ τῆς πίστεως, παρετάξω ἐμφρόνως, καὶ κατεξωλόθρευσας, τῶν δαιμόνων τὰ στίφη, καὶ νικηφόρος ὤφθης ἀθλητής· ὅθεν σε πίστει ἀεὶ μακαρίζομεν."

meVE.Apolytikion2.poet = poets_gr_GR_cog.Gerasimou
meVE.Apolytikion2.mode = misc_gr_GR_cog.Mode4
meVE.Apolytikion2.melody = he.a.m4_gr_GR_cog.TachyProkatalave.name
meVE.Apolytikion2.incipit = "Τὴν κλῆσιν κατάλληλον, δείξας τοῖς…"
meVE.Apolytikion2.text = "Τὴν κλῆσιν κατάλληλον, δείξας τοῖς ἔργοις σοφέ, ταμεῖον ἐπάξιον, τῆς ἐπιπνοίας Θεοῦ, Ζαχαρία γεγένησαι· ἔχων γὰρ ἐν τῷ βίῳ, συλλαλοῦντας Ἀγγέλους, ὤφθης τῶν ἐσομένων, θεηγόρος Προφήτης. Καί νῦν ἡμῶν τὰς αἰτήσεις, ἄνωθεν πλήρωσον."

meMA.Kathisma11.mode = misc_gr_GR_cog.Mode4
meMA.Kathisma11.melody = he.a.m4_gr_GR_cog.OYpsotheisEnToStavro.name
meMA.Kathisma11.incipit = "Ὁ μεγαλώνυμος Χριστοῦ ἀθλοφόρος, ὁ…"
meMA.Kathisma11.text = "Ὁ μεγαλώνυμος Χριστοῦ ἀθλοφόρος, ὁ ἐπιπόθητος Θεῷ καὶ Ἀγγέλοις, ὁ θαυμαστὸς καὶ ἔνδοξος Θεόδωρος, πάσας τὰς ὑφήλιον, ἱερὰς Ἐκκλησίας, σήμερον τῷ Πνεύματι, τῷ ἁγίῳ εὐφραίνει, καὶ ἑορτάζειν πάντας ἐν χαρᾷ, παρασκευάζει αὐτοῦ τὸ μνημόσυνον."

meMA.Kathisma12.mode = misc_gr_GR_cog.Mode4
meMA.Kathisma12.melody = he.a.m4_gr_GR_cog.KateplagiIosif.name
meMA.Kathisma12.incipit = me.m02.d02_gr_GR_cog.meMA.Ode3Kathisma1.incipit
meMA.Kathisma12.text = me.m02.d02_gr_GR_cog.meMA.Ode3Kathisma1.text

meMA.Kathisma21.mode = misc_gr_GR_cog.Mode1
meMA.Kathisma21.melody = he.a.m1_gr_GR_cog.TouLithouSfragisthentos.name
meMA.Kathisma21.incipit = "Θησαυρὲ τῶν αἰώνων, ἡ ζωὴ…"
meMA.Kathisma21.text = "Θησαυρὲ τῶν αἰώνων, ἡ ζωὴ τῶν ἁπάντων, διʼ ἐμὲ νηπιάσας ὑπὸ νόμον ἐγένου, ὁ πάλαι χαράξας ἐν πλαξί, τὸν νόμον ἐν τῷ ὄρει τῷ Σινᾷ, ἵνα πάντας ἀπαλλάξῃς, ἐκ τῆς τοῦ νόμου πάλαι δουλείας. Δόξα τῇ εὐσπλαγχνίᾳ σου Σωτήρ, δόξα τῇ βασιλείᾳ σου, δόξα τῇ οἰκονομίᾳ σου, μόνε φιλάνθρωπε."

meMA.C1.acrostic.title = "Ὁ Κανὼν τοῦ Μάρτυρος,"
meMA.C1.acrostic.notice1 = "οὗ ἡ Ἀκροστιχίς·"
meMA.C1.acrostic.text = "Φερώνυμον δῶρόν σε τοῦ Θεοῦ σέβω."
meMA.C1.acrostic.notice2 = "Ἐν δὲ τοῖς Θεοτοκίοις· Νικολάου."
meMA.C1.title = titles_gr_GR_cog.ti.CanonMartyrM
meMA.C1.verse = prayers_gr_GR_cog.verse_SaintM.text

meMA.C2.acrostic.title = "Ὁ Κανὼν τοῦ Προφήτου,"
meMA.C2.acrostic.notice1 = "οὗ ἡ Ἀκροστιχίς·"
meMA.C2.acrostic.text = "Ἐκπλήττομαί σου τοὺς λόγους Ζαχαρία."
meMA.C2.poet = poets_gr_GR_cog.Theofanous
meMA.C2.title = titles_gr_GR_cog.ti.CanonProphet
meMA.C2.verse = prayers_gr_GR_cog.verse_SaintM.text

meMA.Ode1C1H.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C1H.mode = misc_gr_GR_cog.Mode8
meMA.Ode1C1H.melody = he.h.m8_gr_GR_cog.ArmatilatinFarao.melody
meMA.Ode1C1H.incipit = he.h.m8_gr_GR_cog.ArmatilatinFarao.incipit
meMA.Ode1C1H.text = he.h.m8_gr_GR_cog.ArmatilatinFarao.text

meMA.Ode1C11.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C11.mode = misc_gr_GR_cog.Mode8
meMA.Ode1C11.melody = he.h.m8_gr_GR_cog.ArmatilatinFarao.name
meMA.Ode1C11.incipit = "Φερωνυμίᾳ θεολήπτου κλήσεως καλλωπιζόμενος, καὶ…"
meMA.Ode1C11.text = "Φερωνυμίᾳ θεολήπτου κλήσεως καλλωπιζόμενος, καὶ δωρεῶν θείων, θησαυρὸς δεικνύμενος, καὶ μαρτυρίου χάριτι, Μάκαρ κεκοσμημένος, τοὺς ὑμνητάς σου χαρίτωσον, ὕμνοις σε τιμῶντας Θεόδωρε."

meMA.Ode1C12.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C12.mode = misc_gr_GR_cog.Mode8
meMA.Ode1C12.melody = he.h.m8_gr_GR_cog.ArmatilatinFarao.name
meMA.Ode1C12.incipit = "Ἔργῳ καὶ λόγῳ σοῦ τὴν…"
meMA.Ode1C12.text = "Ἔργῳ καὶ λόγῳ σοῦ τὴν θεοδώρητον, κλῆσιν πιστούμενος, τὰς δωρεὰς νέμεις, δωρεὰν τοῖς χρῄζουσι· δώρημα γὰρ πᾶν τέλειον, ἄνωθεν καταβαῖνον, ἐκ τοῦ Πατρός προσδεχόμενος, ἅπαντας πλουτίζεις τιμῶντάς σε,"

meMA.Ode1C13.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C13.mode = misc_gr_GR_cog.Mode8
meMA.Ode1C13.melody = he.h.m8_gr_GR_cog.ArmatilatinFarao.name
meMA.Ode1C13.incipit = "Ῥώμην θεόθεν νηπιόθεν πάνσοφε, λαβὼν…"
meMA.Ode1C13.text = "Ῥώμην θεόθεν νηπιόθεν πάνσοφε, λαβὼν καὶ σύνεσιν, πάθη σαρκός, πάντα, πρότερον καὶ δαίμονας, ἀνδρείως καταβέβληκας, ὕστερον δὲ καθεῖλες, ἐχθρῶν τυράννων τὸ φρύαγμα, Μάκαρ συμμαχίᾳ τοῦ Πνεύματος."

meMA.Ode1C14.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C14.mode = misc_gr_GR_cog.Mode8
meMA.Ode1C14.melody = he.h.m8_gr_GR_cog.ArmatilatinFarao.name
meMA.Ode1C14.incipit = "Ὡς ἐντρεχὴς καὶ νουνεχὴς καὶ…"
meMA.Ode1C14.text = "Ὡς ἐντρεχὴς καὶ νουνεχὴς καὶ φρόνιμος, κατεσοφίσω σοφῶς, τὸν σοφιστὴν Μάκαρ, τῆς κακίας ῥήμασι, καὶ πράγμασι Λικίνιον, ἐδελέασας δείξας, νεκροῖς ξοάνοις λατρεύοντα, καὶ κεναῖς ἐλπίσι τρεφόμενον."

meMA.Ode1C1T.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C1T.mode = misc_gr_GR_cog.Mode8
meMA.Ode1C1T.melody = he.h.m8_gr_GR_cog.ArmatilatinFarao.name
meMA.Ode1C1T.incipit = "Νῖκός μοι δίδου Παναγία ἄχραντε…"
meMA.Ode1C1T.text = "Νῖκός μοι δίδου Παναγία ἄχραντε, τῷ καταφεύγοντι, ὑπὸ τὴν σὴν σκέπην, τῶν δεινῶν ἐξαίρουσα, ἀεὶ με ταῖς πρεσβείαις σου· καὶ γὰρ τέτοκας Λόγον, τὸν τῷ Πατρὶ συναΐδιον, ὁμοῦ τε τῷ Πνεύματι σύνθρονον."

meMA.Ode1C2H.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C2H.mode = misc_gr_GR_cog.Mode8
meMA.Ode1C2H.melody = he.h.m8_gr_GR_cog.AsomenToKyrio.melody
meMA.Ode1C2H.incipit = he.h.m8_gr_GR_cog.AsomenToKyrio.incipit
meMA.Ode1C2H.text = he.h.m8_gr_GR_cog.AsomenToKyrio.text

meMA.Ode1C21.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C21.mode = misc_gr_GR_cog.Mode8
meMA.Ode1C21.melody = he.h.m8_gr_GR_cog.AsomenToKyrio.name
meMA.Ode1C21.incipit = "Ἔχων τὸν φωτοδότην, ταῖς σαῖς…"
meMA.Ode1C21.text = "Ἔχων τὸν φωτοδότην, ταῖς σαῖς ὑπακούοντα δεήσεσι, φωτισμὸν μοι παράσχου, Ζαχαρία Προφῆτα θεσπέσιε."

meMA.Ode1C22.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C22.mode = misc_gr_GR_cog.Mode8
meMA.Ode1C22.melody = he.h.m8_gr_GR_cog.AsomenToKyrio.name
meMA.Ode1C22.incipit = "Κατακεκοσμημένος, κάλλεσι τῆς θείας ὡραιότητος…"
meMA.Ode1C22.text = "Κατακεκοσμημένος, κάλλεσι τῆς θείας ὡραιότητος, τῶν Ἀγγέλων τὸ κάλλος, ἐθεώρεις ψυχῆς ὡραιότητι."

meMA.Ode1C23.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C23.mode = misc_gr_GR_cog.Mode8
meMA.Ode1C23.melody = he.h.m8_gr_GR_cog.AsomenToKyrio.name
meMA.Ode1C23.incipit = "Πλήρης ἁγιωσύνης, ἀγγελομιμήτου καὶ φαιδρότητος…"
meMA.Ode1C23.text = "Πλήρης ἁγιωσύνης, ἀγγελομιμήτου καὶ φαιδρότητος, συλλαλοῦντας Ἀγγέλους, καὶ φωτίζοντας ἔσχες θεόληπτε."

meMA.Ode1C2T.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C2T.mode = misc_gr_GR_cog.Mode8
meMA.Ode1C2T.melody = he.h.m8_gr_GR_cog.AsomenToKyrio.name
meMA.Ode1C2T.incipit = "Λέλυται ἡ κατάρα, Εὔας τῆς…"
meMA.Ode1C2T.text = "Λέλυται ἡ κατάρα, Εὔας τῆς Προμήτορος Πανάμωμε· τὸν Χριστὸν γὰρ τεκοῦσα, εὐλογίαις τὸν κόσμον ἐπλούτισας."

meMA.Ode3C1H.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C1H.mode = misc_gr_GR_cog.Mode8
meMA.Ode3C1H.melody = he.h.m8_gr_GR_cog.OuraniasApsidos.melody
meMA.Ode3C1H.incipit = he.h.m8_gr_GR_cog.OuraniasApsidos.incipit
meMA.Ode3C1H.text = he.h.m8_gr_GR_cog.OuraniasApsidos.text

meMA.Ode3C11.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C11.mode = misc_gr_GR_cog.Mode8
meMA.Ode3C11.melody = he.h.m8_gr_GR_cog.OuraniasApsidos.name
meMA.Ode3C11.incipit = "Νεανίας ὡραῖος, πανευπρεπὴς δέδειξαι, κάλλει…"
meMA.Ode3C11.text = "Νεανίας ὡραῖος, πανευπρεπὴς δέδειξαι, κάλλει καταλλήλως ἐμπρέπων, ψυχῆς καὶ σώματος, ὡραϊζόμενος, τῶν ἀρετῶν εὐμορφίᾳ καὶ μαρτύρων στίγμασι, καλλωπιζόμενος."

meMA.Ode3C12.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C12.mode = misc_gr_GR_cog.Mode8
meMA.Ode3C12.melody = he.h.m8_gr_GR_cog.OuraniasApsidos.name
meMA.Ode3C12.incipit = "Ὑπὸ θείας προνοίας, ὁδηγηθεὶς ὥδευσας…"
meMA.Ode3C12.text = "Ὑπὸ θείας προνοίας, ὁδηγηθεὶς ὥδευσας, τρίβον μαρτυρίου Δεσπότου, τοῦ τὴν καρδίαν σου, Μάκαρ πλατύναντος, ταῖς δωρεαῖς τῶν χαρίτων, καὶ πρὸς τὴν οὐράνιον κλίσιν ἰθύναντος."

meMA.Ode3C13.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C13.mode = misc_gr_GR_cog.Mode8
meMA.Ode3C13.melody = he.h.m8_gr_GR_cog.OuraniasApsidos.name
meMA.Ode3C13.incipit = "Μεγαλόφρονι γνώμῃ, περιφρονῶν Πάνσοφε, τῶν…"
meMA.Ode3C13.text = "Μεγαλόφρονι γνώμῃ, περιφρονῶν Πάνσοφε, τῶν ἡδέων τοῦ κόσμου, καὶ τῶν τοῦ βίου τερπνῶν, οὐδὲν προέκρινας, τῆς τοῦ Σωτῆρος ἀγάπης, ἀλλʼ αὐτὸν ἐπόθησας, ψυχῆς εὐθύτητι."

meMA.Ode3C1T.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C1T.mode = misc_gr_GR_cog.Mode8
meMA.Ode3C1T.melody = he.h.m8_gr_GR_cog.OuraniasApsidos.name
meMA.Ode3C1T.incipit = "Ἱκετεύω σε μόνην, τὴν τὸν…"
meMA.Ode3C1T.text = "Ἱκετεύω σε μόνην, τὴν τὸν Θεὸν τέξασαν, τὸν εἰς εὐλογίαν κατάραν, τὴν πρὶν ἀμείψαντα, καὶ χρηματίσαντα, ὑπὲρ ἀνθρώπων κατάρα, Κόρη παντευλόγητε, καὶ κόσμον σώσαντα."

meMA.Ode3C2H.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C2H.mode = misc_gr_GR_cog.Mode8
meMA.Ode3C2H.melody = he.h.m8_gr_GR_cog.OukEstinAgios.melody
meMA.Ode3C2H.incipit = he.h.m8_gr_GR_cog.OukEstinAgios.incipit
meMA.Ode3C2H.text = he.h.m8_gr_GR_cog.OukEstinAgios.text

meMA.Ode3C21.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C21.mode = misc_gr_GR_cog.Mode8
meMA.Ode3C21.melody = he.h.m8_gr_GR_cog.OukEstinAgios.name
meMA.Ode3C21.incipit = "Ἡ ἄνωθεν χάρις σοι ἐπεσκίασε…"
meMA.Ode3C21.text = "Ἡ ἄνωθεν χάρις σοι ἐπεσκίασε τῆς προνοίας τὴν ἄφραστον, θείαν κυβέρνησιν, διʼ Ἁγίων Ἀγγέλων διδάσκουσα Πάνσοφε, Μάκαρ καὶ φωτίζουσα."

meMA.Ode3C22.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C22.mode = misc_gr_GR_cog.Mode8
meMA.Ode3C22.melody = he.h.m8_gr_GR_cog.OukEstinAgios.name
meMA.Ode3C22.incipit = "Τὴν θείαν χρηστότητα παιδευόμενος, ἐμυήθης…"
meMA.Ode3C22.text = "Τὴν θείαν χρηστότητα παιδευόμενος, ἐμυήθης Πανόλβιε, ὡς καθαρώτατος κατακάρπως οἰκεῖσθαι, τὸ θεῖον ἁγίασμα, τὴν πόλιν τὴν πανίερον."

meMA.Ode3C23.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C23.mode = misc_gr_GR_cog.Mode8
meMA.Ode3C23.melody = he.h.m8_gr_GR_cog.OukEstinAgios.name
meMA.Ode3C23.incipit = "Τὸ τεῖχος τὸ ἄῤῥηκτον, ἡ…"
meMA.Ode3C23.text = "Τὸ τεῖχος τὸ ἄῤῥηκτον, ἡ ἀσφάλεια τοῦ λαοῦ τοῦ θεόφρονος, γέγονας Κύριε, καὶ ψυχῶν σωτηρία, ὡς πάλαι προήγγειλε, Προφήτης ὁ θεσπέσιος."

meMA.Ode3C2T.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C2T.mode = misc_gr_GR_cog.Mode8
meMA.Ode3C2T.melody = he.h.m8_gr_GR_cog.OukEstinAgios.name
meMA.Ode3C2T.incipit = "Ὁ τόκος ὁ ἄφθορος, καὶ…"
meMA.Ode3C2T.text = "Ὁ τόκος ὁ ἄφθορος, καὶ ἡ ἄχραντος Θεοτόκε σου κύησις, τὸν κόσμον ἐῤῥύσατο, ἐκ φθορᾶς καὶ θανάτου· Σωτῆρα γὰρ ἔτεκες, Λόγον τὸν ἀΐδιον."

meMA.Ode3Kathisma1.mode = misc_gr_GR_cog.Mode8
meMA.Ode3Kathisma1.melody = he.a.m8_gr_GR_cog.TinSofianKaiLogon.name
meMA.Ode3Kathisma1.incipit = "Θεϊκὴν παντευχίαν ἀναλαβών, καὶ εἰδώλων…"
meMA.Ode3Kathisma1.text = "Θεϊκὴν παντευχίαν ἀναλαβών, καὶ εἰδώλων τὴν πλάνην καταλαβών, Ἀγγέλους διήγειρας, εὐφημεῖν τοὺς ἀγῶνάς σου· τῷ γὰρ θείῳ πόθῳ, τὸν νοῦν πυρπολούμενος, τοῦ ἐν πυρὶ θανάτου, γενναίως κατετόλμησας· ὅθεν φερωνύμως, τοῖς αἰτοῦσί σε νέμεις, τὰ θεῖα δωρήματα, ἀθλοφόρε Θεόδωρε· διὰ τοῦτο βοῶμέν σοι· Πρέσβευε Χριστῷ τῷ Θεῷ, τῶν πταισμάτων ἄφεσιν δωρήσασθαι, τοῖς ἑορτάζουσι πόθῳ, τὴν ἁγίαν μνήμην σου."

meMA.Ode3Kathisma2.mode = misc_gr_GR_cog.Mode3
meMA.Ode3Kathisma2.melody = he.a.m3_gr_GR_cog.TinOraiotita.name
meMA.Ode3Kathisma2.incipit = "Διϊππευόμενα ἅρματα ἔβλεψας, ἡνιοχούμενα χειρὶ…"
meMA.Ode3Kathisma2.text = "Διϊππευόμενα ἅρματα ἔβλεψας, ἡνιοχούμενα χειρὶ τοῦ πάντων Θεοῦ, τοὺς ἀσωμάτους λειτουργούς, Ζαχαρία θεσπέσιε, οἷς συναγαλλόμενος, ὡς προφήτης ἀοίδιμε, αἴτησαι ἰθύνεσθαι, πρὸς ὁδὸν θείας γνώσεως, τοὺς πίστει ἀδιστάκτῳ τελοῦντάς σου, μνήμην Προφῆτα τὴν πανίερον."

meMA.Ode3Kathisma3.mode = misc_gr_GR_cog.Mode3
meMA.Ode3Kathisma3.melody = he.a.m3_gr_GR_cog.TinOraiotita.name
meMA.Ode3Kathisma3.incipit = "Ἐκ τῆς Παρθένου σε σωματωθέντα…"
meMA.Ode3Kathisma3.text = "Ἐκ τῆς Παρθένου σε σωματωθέντα Χριστέ, χαίρων ἐδέξατο ὁ Ἱερὸς Συμεών· Νῦν ἀπολύεις ἐκβοῶν, τὸν δοῦλόν σου ὦ Δέσποτα, Ἄννα δὲ ἡ ἄμεμπτος, Προφῆτις καὶ ἔνδοξος, τὴν ἀνθομολόγησιν, καὶ τὸν ὕμνον προσῆγέ σοι. Ἡμεῖς δὲ Ζωοδότα βοῶμέν σοι· Δόξα τῷ οὕτως εὐδοκήσαντι."

meMA.Ode4C1H.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C1H.mode = misc_gr_GR_cog.Mode8
meMA.Ode4C1H.melody = he.h.m8_gr_GR_cog.SyMouIschys.melody
meMA.Ode4C1H.incipit = he.h.m8_gr_GR_cog.SyMouIschys.incipit
meMA.Ode4C1H.text = he.h.m8_gr_GR_cog.SyMouIschys.text

meMA.Ode4C11.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C11.mode = misc_gr_GR_cog.Mode8
meMA.Ode4C11.melody = he.h.m8_gr_GR_cog.SyMouIschys.name
meMA.Ode4C11.incipit = "Ὁ πρὸς Θεόν, πόθος σου…"
meMA.Ode4C11.text = "Ὁ πρὸς Θεόν, πόθος σου πᾶσαν ἠμαύρωσε, προσπαθείας, ϋλην Παμμακάριστε, δόξαν τερπνήν, πλοῦτον καὶ τρυφήν, καὶ περιφανείας τὸ περιβόητον ὕψωμα· ἐντεῦθεν ἀνυψώθης, πρὸς περίβλεπον ὕψος, μαρτυρίου πρὸς δόξαν ἀμάραντον."

meMA.Ode4C12.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C12.mode = misc_gr_GR_cog.Mode8
meMA.Ode4C12.melody = he.h.m8_gr_GR_cog.SyMouIschys.name
meMA.Ode4C12.incipit = "Νόμῳ καινῷ, ζωοποιοῦντι τῷ Πνεύματι…"
meMA.Ode4C12.text = "Νόμῳ καινῷ, ζωοποιοῦντι τῷ Πνεύματι, πεφραγμένος, νόμον ἀνομώτατον, παρανομούντων νεανικῶς, ἀνέτρεψας μάκαρ· διὸ νομίμως ἐνήθλησας, καὶ στέφος ἀνεδήσω, τὸ τῆς δικαιοσύνης, ἐκ χειρὸς τοῦ Δεσπότου Θεόδωρε."

meMA.Ode4C13.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C13.mode = misc_gr_GR_cog.Mode8
meMA.Ode4C13.melody = he.h.m8_gr_GR_cog.SyMouIschys.name
meMA.Ode4C13.incipit = "Δῶρον Θεοῦ, δέδοσαι κόσμῳ φερώνυμον…"
meMA.Ode4C13.text = "Δῶρον Θεοῦ, δέδοσαι κόσμῳ φερώνυμον, δῶρον θεῖον, δῶρον ἀξιέραστον, δῶρον πλουτίζον πάντα πιστόν, δῶρον δεχομένοις, γλυκὺ καὶ πρᾶγμα καὶ ὄνομα, τὸ πᾶσι καταπέμπον, καταλλήλως τὰς δόσεις, καὶ πληροῦν τὰς αἰτήσεις τῶν δούλων σου."

meMA.Ode4C14.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C14.mode = misc_gr_GR_cog.Mode8
meMA.Ode4C14.melody = he.h.m8_gr_GR_cog.SyMouIschys.name
meMA.Ode4C14.incipit = "Ὡς μιμητής, τοῦ προσπαγέντος ἐν…"
meMA.Ode4C14.text = "Ὡς μιμητής, τοῦ προσπαγέντος ἐν ξύλῳ Χριστοῦ, μετὰ πᾶσαν, αἰκίαν τοῦ σώματος, μετὰ πολλὰς στρέβλας πονηράς, τὰς τῶν μιαιφόνων, χριστομιμήτως ἐτάθης σταυρῷ, καὶ βέλεσιν ἐτρώθης, καὶ δεινῶς συνεκόπης, ἀλλʼ ἐῤῥύσθης ἐκ τούτων δυνάμει Χριστοῦ."

meMA.Ode4C1T.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C1T.mode = misc_gr_GR_cog.Mode8
meMA.Ode4C1T.melody = he.h.m8_gr_GR_cog.SyMouIschys.name
meMA.Ode4C1T.incipit = "Καλλοποιόν, καὶ γῆς Δεσπότην κυήσασαν…"
meMA.Ode4C1T.text = "Καλλοποιόν, καὶ γῆς Δεσπότην κυήσασαν, ἀποῤῥήτῳ τόκῳ κατὰ χρέος σε, πάντες πιστοί, γλώσσῃ καὶ ψυχῇ, κόσμου Βασιλίδα, καὶ Δέσποιναν ὀνομάζομεν· διὸ τῆς τυραννούσης, πονηρᾶς συνηθείας, ἐλευθέρωσον τοὺς σὲ γεραίροντας."

meMA.Ode4C2H.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C2H.mode = misc_gr_GR_cog.Mode8
meMA.Ode4C2H.melody = he.h.m8_gr_GR_cog.ExOrousKataskiou.melody
meMA.Ode4C2H.incipit = he.h.m8_gr_GR_cog.ExOrousKataskiou.incipit
meMA.Ode4C2H.text = he.h.m8_gr_GR_cog.ExOrousKataskiou.text

meMA.Ode4C21.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C21.mode = misc_gr_GR_cog.Mode8
meMA.Ode4C21.melody = he.h.m8_gr_GR_cog.ExOrousKataskiou.name
meMA.Ode4C21.incipit = "Μίαν μὲν φωτιστικήν, οὐσίαν σέβειν…"
meMA.Ode4C21.text = "Μίαν μὲν φωτιστικήν, οὐσίαν σέβειν, ἐν Ἡλίοις δέ, τρισὶν ὁριζομένην, ταῖς εἰς τὰ ὄντα προόδοις πληθυνομένην, σαφῶς διδάσκεις Παμμακάριστε."

meMA.Ode4C22.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C22.mode = misc_gr_GR_cog.Mode8
meMA.Ode4C22.melody = he.h.m8_gr_GR_cog.ExOrousKataskiou.name
meMA.Ode4C22.incipit = "Ἅρμα τοῦ Θεοῦ, γενόμενος Προφῆτα…"
meMA.Ode4C22.text = "Ἅρμα τοῦ Θεοῦ, γενόμενος Προφῆτα, ἅρματα ὁρᾶν, ποικίλα τῶν Ἀγγέλων, περιοδεῦσαι τὰ πάντα κατηξιώθης, μετὰ εὐταξίας πορευόμενα."

meMA.Ode4C23.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C23.mode = misc_gr_GR_cog.Mode8
meMA.Ode4C23.melody = he.h.m8_gr_GR_cog.ExOrousKataskiou.name
meMA.Ode4C23.incipit = "Ἱεροπρεπῆ πολύφωτον λυχνίαν, σύμβολον ὁρᾷς…"
meMA.Ode4C23.text = "Ἱεροπρεπῆ πολύφωτον λυχνίαν, σύμβολον ὁρᾷς, τῆς θείας φωταυγείας, ἐπισκοπῆς τε Παμμάκαρ τῆς παντεπόπτου, διʼ ἧς τὰ πάντα περισώζεται."

meMA.Ode4C2T.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C2T.mode = misc_gr_GR_cog.Mode8
meMA.Ode4C2T.melody = he.h.m8_gr_GR_cog.ExOrousKataskiou.name
meMA.Ode4C2T.incipit = "Σύμβολα τῆς σῆς, Ἁγνή, κυοφορίας…"
meMA.Ode4C2T.text = "Σύμβολα τῆς σῆς, Ἁγνή, κυοφορίας, οἱ θεοειδεῖς, Προφῆται προτυποῦσι, πολυειδῶς προχαράττοντες, καὶ ποικίλως, διʼ αἰνιγμάτων οἱ θεόφρονες."

meMA.Ode5C1H.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C1H.mode = misc_gr_GR_cog.Mode8
meMA.Ode5C1H.melody = he.h.m8_gr_GR_cog.InaTiMeAposo.melody
meMA.Ode5C1H.incipit = he.h.m8_gr_GR_cog.InaTiMeAposo.incipit
meMA.Ode5C1H.text = he.h.m8_gr_GR_cog.InaTiMeAposo.text

meMA.Ode5C11.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C11.mode = misc_gr_GR_cog.Mode8
meMA.Ode5C11.melody = he.h.m8_gr_GR_cog.InaTiMeAposo.name
meMA.Ode5C11.incipit = "Ῥητορεύεις τὴν δόξαν, ἔνδοξε Θεόδωρε…"
meMA.Ode5C11.text = "Ῥητορεύεις τὴν δόξαν, ἔνδοξε Θεόδωρε σὺν παῤῥησίᾳ πολλῇ, τοῦ σαρκὶ παθόντος, καὶ θανάτῳ τὸν θάνατον λύσαντος, καὶ μαστίγων πεῖραν, πολλῶν λαβὼν λύσσῃ τυράννου, σταυρικὴν καταδίκην ὑπήνεγκας."

meMA.Ode5C12.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C12.mode = misc_gr_GR_cog.Mode8
meMA.Ode5C12.melody = he.h.m8_gr_GR_cog.InaTiMeAposo.name
meMA.Ode5C12.incipit = "Ὁλοτρόπῳ προθέσει, σταύρωσιν ἑκούσιον Χριστοῦ…"
meMA.Ode5C12.text = "Ὁλοτρόπῳ προθέσει, σταύρωσιν ἑκούσιον Χριστοῦ μιμούμενος, τὴν αὐτὴν ὑπέστης, τιμωρίαν σταυρῷ προσηλούμενος, ἀντὶ δὲ τῆς λόγχης, βελῶν πολλῶν πληγὰς ἐδέξω, ὀφθαλμῶν κόρας τε ἐξορυττόμενος."

meMA.Ode5C13.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C13.mode = misc_gr_GR_cog.Mode8
meMA.Ode5C13.melody = he.h.m8_gr_GR_cog.InaTiMeAposo.name
meMA.Ode5C13.incipit = "Νικηφόρος ὡράθης, ἄρτιος ὁλόκληρος μετὰ…"
meMA.Ode5C13.text = "Νικηφόρος ὡράθης, ἄρτιος ὁλόκληρος μετὰ τὴν σταύρωσιν, καὶ τὴν ἄλλην πᾶσαν, τῶν μελῶν συγκοπήν τε καὶ νέκρωσιν, ὁ νικήσας κόσμον· σὲ γὰρ Χριστὸς χειρὶ Ἀγγέλου, ὡς ζωῆς ἀρχηγὸς ἀνεζώωσεν."

meMA.Ode5C14.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C14.mode = misc_gr_GR_cog.Mode8
meMA.Ode5C14.melody = he.h.m8_gr_GR_cog.InaTiMeAposo.name
meMA.Ode5C14.incipit = "Συνιδόντα τὰ πλήθη, τὸ ἐν…"
meMA.Ode5C14.text = "Συνιδόντα τὰ πλήθη, τὸ ἐν σοὶ γενόμενον θαῦμα Θεόδωρε, τὴν φωνὴν ἐπῆραν, μεγαλύνοντα Χριστοῦ τὴν δύναμιν, τοῦ δὲ λυμεῶνος, καὶ ἀλαζόνος Λικινίου, στηλιτεύοντα λύσσαν τὴν ἄλογον."

meMA.Ode5C1T.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C1T.mode = misc_gr_GR_cog.Mode8
meMA.Ode5C1T.melody = he.h.m8_gr_GR_cog.InaTiMeAposo.name
meMA.Ode5C1T.incipit = "Ὅλον τέτοκας Κόρη, τὸ φῶς…"
meMA.Ode5C1T.text = "Ὅλον τέτοκας Κόρη, τὸ φῶς τὸ αἰώνιον τοῖς ἀνυμνοῦσί σε, τὸν Χριστὸν ἀσπόρως, σωματώσασα, ὃν ἐξιλέωσαι, τοῦ σωθῆναι πάντας, ἐκ πειρασμῶν τοῦ ἀλλοτρίου, Παναγία Παρθένε Θεόνυμφε."

meMA.Ode5C2H.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C2H.mode = misc_gr_GR_cog.Mode8
meMA.Ode5C2H.melody = he.h.m8_gr_GR_cog.OEkNyktosAgnoias.melody
meMA.Ode5C2H.incipit = he.h.m8_gr_GR_cog.OEkNyktosAgnoias.incipit
meMA.Ode5C2H.text = he.h.m8_gr_GR_cog.OEkNyktosAgnoias.text

meMA.Ode5C21.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C21.mode = misc_gr_GR_cog.Mode8
meMA.Ode5C21.melody = he.h.m8_gr_GR_cog.OEkNyktosAgnoias.name
meMA.Ode5C21.incipit = "Ὁ καθαρὸς καρδίᾳ, καὶ διανοίας…"
meMA.Ode5C21.text = "Ὁ καθαρὸς καρδίᾳ, καὶ διανοίας προβλέπων ὀξύτητι, πάθος τὸ τοῦ Σωτῆρος, ἀξιοπρεπῶς μακαρίζεται."

meMA.Ode5C22.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C22.mode = misc_gr_GR_cog.Mode8
meMA.Ode5C22.melody = he.h.m8_gr_GR_cog.OEkNyktosAgnoias.name
meMA.Ode5C22.incipit = "Ὑπερφυῶς ἁγία, χαῖρε Σιών, ὁ…"
meMA.Ode5C22.text = "Ὑπερφυῶς ἁγία, χαῖρε Σιών, ὁ Προφήτης προτρέπεται, ἥξει ὁ Βασιλεύς σου δίκαιος καὶ σῴζων πρᾳότητι."

meMA.Ode5C23.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C23.mode = misc_gr_GR_cog.Mode8
meMA.Ode5C23.melody = he.h.m8_gr_GR_cog.OEkNyktosAgnoias.name
meMA.Ode5C23.incipit = "Τῶν Προφητῶν ἡ δόξα, τῶν…"
meMA.Ode5C23.text = "Τῶν Προφητῶν ἡ δόξα, τῶν θεηγόρων ἀνδρῶν ἡ εὐπρέπεια, κόσμον περιαυγάζει, τῇ τῆς προφητείας λαμπρότητι."

meMA.Ode5C2T.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C2T.mode = misc_gr_GR_cog.Mode8
meMA.Ode5C2T.melody = he.h.m8_gr_GR_cog.OEkNyktosAgnoias.name
meMA.Ode5C2T.incipit = "Ὁ τῆς Παρθένου τόκος, ἀναγεννήσας…"
meMA.Ode5C2T.text = "Ὁ τῆς Παρθένου τόκος, ἀναγεννήσας ἡμᾶς ἠλευθέρωσε, τῶν πρὶν ἁμαρτημάτων, διʼ ἡμᾶς ἁμαρτίᾳ γενόμενος."

meMA.Ode6C1H.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C1H.mode = misc_gr_GR_cog.Mode8
meMA.Ode6C1H.melody = he.h.m8_gr_GR_cog.IlasthitiMoiSotir.melody
meMA.Ode6C1H.incipit = he.h.m8_gr_GR_cog.IlasthitiMoiSotir.incipit
meMA.Ode6C1H.text = he.h.m8_gr_GR_cog.IlasthitiMoiSotir.text

meMA.Ode6C11.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C11.mode = misc_gr_GR_cog.Mode8
meMA.Ode6C11.melody = he.h.m8_gr_GR_cog.IlasthitiMoiSotir.name
meMA.Ode6C11.incipit = "Ἐξέλαμψας ἀρεταῖς, καὶ διδαχαῖς διὰ…"
meMA.Ode6C11.text = "Ἐξέλαμψας ἀρεταῖς, καὶ διδαχαῖς διὰ βίου σου, ἐξήστραψας δὲ λαμπροῖς, ἀγῶσι Θεόδωρε, καὶ νῦν περιβόητος, ἐν θαυματουργίαις, καὶ θερμὸς προστάτης πέφηνας."

meMA.Ode6C12.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C12.mode = misc_gr_GR_cog.Mode8
meMA.Ode6C12.melody = he.h.m8_gr_GR_cog.IlasthitiMoiSotir.name
meMA.Ode6C12.incipit = "Τοῖς κάμνουσιν ἰατρός, τοῖς αἰχμαλώτοις…"
meMA.Ode6C12.text = "Τοῖς κάμνουσιν ἰατρός, τοῖς αἰχμαλώτοις ἀνάῤῥυσις, μεσίτης ἁμαρτωλοῖς, ὑπευθύνων ἄφεσις, πενθοῦσι παράκλησις, πρὸς Θεοῦ ἐδόθης συμπαθέστατε Θεόδωρε."

meMA.Ode6C13.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C13.mode = misc_gr_GR_cog.Mode8
meMA.Ode6C13.melody = he.h.m8_gr_GR_cog.IlasthitiMoiSotir.name
meMA.Ode6C13.incipit = "Οὐκ ἔστι κόρος τῶν σῶν…"
meMA.Ode6C13.text = "Οὐκ ἔστι κόρος τῶν σῶν, καλῶν ἀπείρων Θεόδωρε, οὐδὲ τῆς σῆς καλλονῆς, σταθμὸς ἰσοστάσιος· διὸ τὸν διάπυρον, πόθον μου προσδέχου, χορηγῶν μοι χάριν ἄφθονον."

meMA.Ode6C1T.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C1T.mode = misc_gr_GR_cog.Mode8
meMA.Ode6C1T.melody = he.h.m8_gr_GR_cog.IlasthitiMoiSotir.name
meMA.Ode6C1T.incipit = "Λαμψάτω νῦν ἐπ ἐμέ, ἡ…"
meMA.Ode6C1T.text = "Λαμψάτω νῦν ἐπ ἐμέ, ἡ ἄφατος εὐσπλαγχνία σου, βυθοῦ με ἁμαρτιῶν, καὶ τῆς ἀπογνώσεως, Κόρη ἀπαλλάττουσα, καὶ πρὸς μετανοίας, τοὺς λιμένας ἐμβιβάζουσα."

meMA.Ode6C2H.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C2H.mode = misc_gr_GR_cog.Mode8
meMA.Ode6C2H.melody = he.h.m8_gr_GR_cog.TinDeisinEkcheo.melody
meMA.Ode6C2H.incipit = he.h.m8_gr_GR_cog.TinDeisinEkcheo.incipit
meMA.Ode6C2H.text = he.h.m8_gr_GR_cog.TinDeisinEkcheo.text

meMA.Ode6C21.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C21.mode = misc_gr_GR_cog.Mode8
meMA.Ode6C21.melody = he.h.m8_gr_GR_cog.TinDeisinEkcheo.name
meMA.Ode6C21.incipit = "Ὑπάρχων προφητικῆς ἀνάπλεως, ἐπιπνοίας ἐσομένων…"
meMA.Ode6C21.text = "Ὑπάρχων προφητικῆς ἀνάπλεως, ἐπιπνοίας ἐσομένων τὴν γνῶσιν, ὑπερφυῶς, μυηθεὶς ἀπεστάλης, ἀνακηρύττειν τὴν χάριν τὴν μέλλουσαν, ἡμέραν τε τὴν τοῦ σταυροῦ, τὴν γνωστὴν τῷ Κυρίῳ θεόληπτε."

meMA.Ode6C22.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C22.mode = misc_gr_GR_cog.Mode8
meMA.Ode6C22.melody = he.h.m8_gr_GR_cog.TinDeisinEkcheo.name
meMA.Ode6C22.incipit = "Συνέσει πνευματικῇ κοσμούμενος, δωρεῶν τῶν…"
meMA.Ode6C22.text = "Συνέσει πνευματικῇ κοσμούμενος, δωρεῶν τῶν ὑπὲρ νοῦν ἠξιώθης, ἀγγελικάς, ἐμφανείας καὶ τάξεις, καὶ ὁμιλίας καὶ δόξαν θεώμενος, Πανόλβιε τὰ θαυμαστά, διδασκούσας καὶ θεῖα Μυστήρια."

meMA.Ode6C23.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C23.mode = misc_gr_GR_cog.Mode8
meMA.Ode6C23.melody = he.h.m8_gr_GR_cog.TinDeisinEkcheo.name
meMA.Ode6C23.incipit = "Λαμπάδιον ἀρετῶν κτησάμενος, φωτισμὸν θεογνωσίας…"
meMA.Ode6C23.text = "Λαμπάδιον ἀρετῶν κτησάμενος, φωτισμὸν θεογνωσίας πυρσεύεις, τὴν μυστικήν, προμηνύων λαμπάδα, τῆς ὑπὲρ λόγον τοῦ Λόγου σαρκώσεως, φανήσεσθαι θεοπρεπῶς, ἐκ Σιὼν Ζαχαρία θεόληπτε."

meMA.Ode6C2T.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C2T.mode = misc_gr_GR_cog.Mode8
meMA.Ode6C2T.melody = he.h.m8_gr_GR_cog.TinDeisinEkcheo.name
meMA.Ode6C2T.incipit = "Ὁ στήσας τὸν οὐρανὸν ἀνέδειξεν…"
meMA.Ode6C2T.text = "Ὁ στήσας τὸν οὐρανὸν ἀνέδειξεν, οὐρανὸν σε λογικὸν Θεοτόκε, τὴν καλλονήν, Ἰακὼβ ἀγαπήσας, καὶ ἐκ γαστρός σου Θεότητος Ἥλιος, ἀνέτειλε σωματικῶς, καὶ τὸν κόσμον ἐφαίδρυνε χάριτι."

meMA.Kontakion1.mode = misc_gr_GR_cog.Mode2
meMA.Kontakion1.melody = he.a.m2_gr_GR_cog.TaAnoZiton.name
meMA.Kontakion1.incipit = "Ἀνδρείᾳ ψυχῆς, τὴν πίστιν ὁπλισάμενος…"
meMA.Kontakion1.text = "Ἀνδρείᾳ ψυχῆς, τὴν πίστιν ὁπλισάμενος, καὶ ῥῆμα Θεοῦ, ὡς λόγχην χειρισάμενος, τὸν ἐχθρὸν κατέτρωσας τῶν Μαρτύρων κλέος Θεόδωρε, σὺν αὐτοῖς Χριστῷ τῷ Θεῷ, πρεσβεύων μὴ παύσῃ ὑπὲρ πάντων ἡμῶν."

meMA.Oikos1.text = "Τῶν ἀθλητῶν τὸ ἔκλαμπρον κάλλος δεῦτε πάντες, τοῖς ὕμνοις τῶν στεφάνων, πιστοὶ Θεόδωρον καταστέψωμεν· δῶρον Θεοῦ γὰρ μέγα τῷ κόσμῳ ἀναδέδεικται, λάμψεσι ταῖς τῶν θαυμάτων· τὸν γὰρ ἐχθρὸν ἐκνικήσας Βελίαρ ἄθλοις σεπτοῖς, ὀμβρίζει ἀντὶ αἱμάτων, κρουνηδὸν ἰαμάτων τὰ ῥεύματα. Ἐν τούτοις οὖν πᾶσι χαίρει Χριστῷ, καὶ δίδωσιν εἰρήνην ἀνέκλειπτον· διὸ βοῶμεν αὐτῷ· Πρεσβεύων μὴ παύσῃ ὑπὲρ πάντων ἡμῶν."

meMA.Ode7C1H.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C1H.mode = misc_gr_GR_cog.Mode8
meMA.Ode7C1H.melody = he.h.m8_gr_GR_cog.TheouSynkatavasin.melody
meMA.Ode7C1H.incipit = he.h.m8_gr_GR_cog.TheouSynkatavasin.incipit
meMA.Ode7C1H.text = he.h.m8_gr_GR_cog.TheouSynkatavasin.text

meMA.Ode7C11.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C11.mode = misc_gr_GR_cog.Mode8
meMA.Ode7C11.melody = he.h.m8_gr_GR_cog.TheouSynkatavasin.name
meMA.Ode7C11.incipit = "Ὑψούμενον Κύριον, ἐπὶ τοῦ ξύλου…"
meMA.Ode7C11.text = "Ὑψούμενον Κύριον, ἐπὶ τοῦ ξύλου καὶ καταράξαντα, τὸν μεγάλαυχον ὄφιν, κατανοήσας συνανυψώθης αὐτῷ, χριστομιμήτως ἐν σταυρῷ καὶ θάνατον, ὑπενεγκὼν Ἀθλητά, συνεξανέστης αὐτῷ."

meMA.Ode7C12.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C12.mode = misc_gr_GR_cog.Mode8
meMA.Ode7C12.melody = he.h.m8_gr_GR_cog.TheouSynkatavasin.name
meMA.Ode7C12.incipit = "Θεοῦ σε Θεόδωρε, τῆς ἀκηράτου…"
meMA.Ode7C12.text = "Θεοῦ σε Θεόδωρε, τῆς ἀκηράτου δόξης θερμὸν ἐραστήν, ἐγνωκὼς τῷ σῷ πόθῳ, ὁλοκαρδίως ψυχὴν προσέδησα, τὴν προσδοκίαν πᾶσαν τῆς ἐλπίδος μου, ἀνατεθεὶς ἐπὶ σοί, μὴ ὑπερίδῃς με."

meMA.Ode7C13.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C13.mode = misc_gr_GR_cog.Mode8
meMA.Ode7C13.melody = he.h.m8_gr_GR_cog.TheouSynkatavasin.name
meMA.Ode7C13.incipit = "Ἐδόξασας Κύριον, τοῖς μέλεσί σου…"
meMA.Ode7C13.text = "Ἐδόξασας Κύριον, τοῖς μέλεσί σου ἐνδοξαζόμενον, ἐν βουλαῖς τῶν Ἁγίων, τὸν σὲ μεγάλως ἀντιδοξάσαντα, τερατουργίαις καὶ ποικίλοις θαύμασι, καὶ περιόντα τῇ γῇ, καὶ μετὰ τέλος Σοφέ."

meMA.Ode7C1T.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C1T.mode = misc_gr_GR_cog.Mode8
meMA.Ode7C1T.melody = he.h.m8_gr_GR_cog.TheouSynkatavasin.name
meMA.Ode7C1T.incipit = "Ἁγίων τὸν ἅγιον, τέτοκας μόνη…"
meMA.Ode7C1T.text = "Ἁγίων τὸν ἅγιον, τέτοκας μόνη θεοχαρίτωτε, ἀποῤῥήτῳ κυήσει· καὶ γὰρ Παρθένος ἔμεινας ἄχραντε, ὡς πρὸ τοῦ τόκου· διὸ σοι κραυγάζομεν τὴν τοῦ Ἀγγέλου φωνήν· Εὐλογημένη Ἁγνή."

meMA.Ode7C2H.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C2H.mode = misc_gr_GR_cog.Mode8
meMA.Ode7C2H.melody = he.h.m8_gr_GR_cog.PaidesEvraion.melody
meMA.Ode7C2H.incipit = he.h.m8_gr_GR_cog.PaidesEvraion.incipit
meMA.Ode7C2H.text = he.h.m8_gr_GR_cog.PaidesEvraion.text

meMA.Ode7C21.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C21.mode = misc_gr_GR_cog.Mode8
meMA.Ode7C21.melody = he.h.m8_gr_GR_cog.PaidesEvraion.name
meMA.Ode7C21.incipit = "Γῆν ἐπὶ πᾶσαν σοῦ Προφῆτα…"
meMA.Ode7C21.text = "Γῆν ἐπὶ πᾶσαν σοῦ Προφῆτα, διελήλυθεν ὁ φθόγγος ὁ θεόπνους, καὶ ῥημάτων τῶν σῶν, ἡ δύναμις βοῶσα· Εὐλογητὸς εἶ Κύριε, ὁ Θεὸς εἰς τοὺς αἰῶνας."

meMA.Ode7C22.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C22.mode = misc_gr_GR_cog.Mode8
meMA.Ode7C22.melody = he.h.m8_gr_GR_cog.PaidesEvraion.name
meMA.Ode7C22.incipit = "Ὅλος ταῖς ἄνω λαμπηδόσι, φωτιζόμενος…"
meMA.Ode7C22.text = "Ὅλος ταῖς ἄνω λαμπηδόσι, φωτιζόμενος τὰ μέλλοντα ἑώρας, ὡς παρόντα· διό, Πανόλβιε ἐβόας· Εὐλογητὸς εἶ Κύριε, ὁ Θεὸς εἰς τοὺς αἰῶνας."

meMA.Ode7C2T.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C2T.mode = misc_gr_GR_cog.Mode8
meMA.Ode7C2T.melody = he.h.m8_gr_GR_cog.PaidesEvraion.name
meMA.Ode7C2T.incipit = "Ὕμνοις γεραίρω σε Παρθένε, τὴν…"
meMA.Ode7C2T.text = "Ὕμνοις γεραίρω σε Παρθένε, τὴν τιμήσασαν τὴν φύσιν τῶν ἀνθρώπων, ἀτιμίᾳ δεινῇ, τὸ πρὶν κατακριθεῖσαν· Εὐλογημένος πάναγνε, ὁ καρπὸς τῆς σῆς κοιλίας."

meMA.Ode8C1H.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C1H.mode = misc_gr_GR_cog.Mode8
meMA.Ode8C1H.melody = he.h.m8_gr_GR_cog.EptaplasiosKaminon.melody
meMA.Ode8C1H.incipit = he.h.m8_gr_GR_cog.EptaplasiosKaminon.incipit
meMA.Ode8C1H.text = he.h.m8_gr_GR_cog.EptaplasiosKaminon.text

meMA.Ode8C11.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C11.mode = misc_gr_GR_cog.Mode8
meMA.Ode8C11.melody = he.h.m8_gr_GR_cog.EptaplasiosKaminon.name
meMA.Ode8C11.incipit = "Ὁ λυμεὼν Λικίνιος, ὁ λυσσώδης…"
meMA.Ode8C11.text = "Ὁ λυμεὼν Λικίνιος, ὁ λυσσώδης καὶ βέβηλος, ὅλον ἑαυτὸν ὁλοτελῶς τοῖς δαίμοσιν, ἐκδοὺς ὑποβάλλει σε, τιμωριῶν ποικίλων ποιναῖς, νῶτον καὶ τὸ στῆθος, τοῖς βουνεύροις συγκόπτων, ἐν σφαίραις μολυβδίναις, τὸν αὐχένα συντρίβων, πλευρὰς ὄνυξι ξαίνων, καὶ φλέγων ταῖς λαμπάσιν."

meMA.Ode8C12.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C12.mode = misc_gr_GR_cog.Mode8
meMA.Ode8C12.melody = he.h.m8_gr_GR_cog.EptaplasiosKaminon.name
meMA.Ode8C12.incipit = "Ὑπομονὴν ἀνένδοτον, καρτερίαν ἀνάλωτον, καὶ…"
meMA.Ode8C12.text = "Ὑπομονὴν ἀνένδοτον, καρτερίαν ἀνάλωτον, καὶ ὑπερφυῆ ἀνδρείαν ἐνδειξάμενος, ποικίλαις κολάσεσι, καὶ χαλεπαῖς στρεβλώσεσι, πάντων τῶν Μαρτύρων, ὑπερέβης τοὺς ἄθλους, παθῶν Δεσποτικῶν δέ, τῷ σταυρῷ κοινωνήσας, καὶ πλέον ὑπομείνας, μειζόνως ἐδοξάσθης."

meMA.Ode8C13.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C13.mode = misc_gr_GR_cog.Mode8
meMA.Ode8C13.melody = he.h.m8_gr_GR_cog.EptaplasiosKaminon.name
meMA.Ode8C13.incipit = "Σταυρῷ ταθεὶς Θεόδωρε, τὰ Χριστοῦ…"
meMA.Ode8C13.text = "Σταυρῷ ταθεὶς Θεόδωρε, τὰ Χριστοῦ ὑστερήματα, ἐν σαρκὶ οἰκείᾳ ἐκπληροῖς τῶν θλίψεων· οὐ χεῖρας καὶ πόδας γάρ, μόνον προσηλωθεὶς καρτερεῖς, ἀλλὰ καὶ περόνῃ, σιδηρᾶ διεπάρης, τὸ κρύφιόν σου μέλος, ἐκτμηθεὶς τοὺς διδύμους, καὶ βέλεσιν ὀμμάτων, ἐξορυχθεὶς τὰς κόρας."

meMA.Ode8C1T.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C1T.mode = misc_gr_GR_cog.Mode8
meMA.Ode8C1T.melody = he.h.m8_gr_GR_cog.EptaplasiosKaminon.name
meMA.Ode8C1T.incipit = "Οὐρανομήκη κλίμακα, νοητὴν σε γινώσκομεν…"
meMA.Ode8C1T.text = "Οὐρανομήκη κλίμακα, νοητὴν σε γινώσκομεν, διʼ ἧς καταβάς, μετὰ σαρκὸς ὁ Ὕψιστος, ἀνθρώποις ὡμίλησε, καὶ ταπεινοὺς ἀνύψωσε, πρὸς τὴν ὑψηλήν, τῶν οὐρανῶν πολιτείαν· ἐντεῦθεν ἀνωτέραν, οὐρανῶν καὶ Ἀγγέλων, καὶ πάντων ποιημάτων, τιμῶμέν σε Παρθένε."

meMA.Ode8C2H.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C2H.mode = misc_gr_GR_cog.Mode8
meMA.Ode8C2H.melody = he.h.m8_gr_GR_cog.TonEnOreiAgio.melody
meMA.Ode8C2H.incipit = he.h.m8_gr_GR_cog.TonEnOreiAgio.incipit
meMA.Ode8C2H.text = he.h.m8_gr_GR_cog.TonEnOreiAgio.text

meMA.Ode8C21.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C21.mode = misc_gr_GR_cog.Mode8
meMA.Ode8C21.melody = he.h.m8_gr_GR_cog.TonEnOreiAgio.name
meMA.Ode8C21.incipit = "Σιὼν ὄρος ἡ πόλις ἡ…"
meMA.Ode8C21.text = "Σιὼν ὄρος ἡ πόλις ἡ ἁγία, χαῖρε σφόδρα ὁ θεῖος Ζαχαρίας, Ἀγγέλων ὁ συνόμιλος προτρέπεται, καὶ σὺ δὲ ἀγάλλου, Ἱερουσαλήμ, εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8C22.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C22.mode = misc_gr_GR_cog.Mode8
meMA.Ode8C22.melody = he.h.m8_gr_GR_cog.TonEnOreiAgio.name
meMA.Ode8C22.incipit = "Ζαχαρία Προφῆτα κατατέρπου, πληρωθέντας θεώμενος…"
meMA.Ode8C22.text = "Ζαχαρία Προφῆτα κατατέρπου, πληρωθέντας θεώμενος τοὺς λόγους, οὓς προηγόρευσας τῷ Πνεύματι λαμπόμενος· Κύριον ὑμνεῖτε, καὶ ὑπερυψοῦτε, βοῶν εἰς τοὺς αἰῶνας."

meMA.Ode8C23.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C23.mode = misc_gr_GR_cog.Mode8
meMA.Ode8C23.melody = he.h.m8_gr_GR_cog.TonEnOreiAgio.name
meMA.Ode8C23.incipit = "Ἀναμέσον ὀρέων κατασκίων, ἐθεώρεις ἑστῶτας…"
meMA.Ode8C23.text = "Ἀναμέσον ὀρέων κατασκίων, ἐθεώρεις ἑστῶτας τοὺς Ἀγγέλους, τὴν ἐπὶ πάντων πρόνοιαν μυοῦντάς σε, ἔνδοξε Προφῆτα, καὶ ὑμνολογοῦντας, Χριστὸν εἰς τοὺς αἰῶνας."

meMA.Ode8C2T.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C2T.mode = misc_gr_GR_cog.Mode8
meMA.Ode8C2T.melody = he.h.m8_gr_GR_cog.TonEnOreiAgio.name
meMA.Ode8C2T.incipit = "Χαῖρε θρόνε πυρίμορφε Κυρίου, χαῖρε…"
meMA.Ode8C2T.text = "Χαῖρε θρόνε πυρίμορφε Κυρίου, χαῖρε πύλη ἀΰλου φρυκτωρίας, χαῖρε νεφέλη κούφη ἡ τὸν Ἥλιον, τῆς δικαιοσύνης, λάμψασα τῷ κόσμῳ ἁγνὴ Θεοκυῆτορ."

meMA.Ode9C1H.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C1H.mode = misc_gr_GR_cog.Mode8
meMA.Ode9C1H.melody = he.h.m8_gr_GR_cog.ExestiEpiTouto.melody
meMA.Ode9C1H.incipit = he.h.m8_gr_GR_cog.ExestiEpiTouto.incipit
meMA.Ode9C1H.text = he.h.m8_gr_GR_cog.ExestiEpiTouto.text

meMA.Ode9C11.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C11.mode = misc_gr_GR_cog.Mode8
meMA.Ode9C11.melody = he.h.m8_gr_GR_cog.ExestiEpiTouto.name
meMA.Ode9C11.incipit = "Ἐξέστη τῶν Ἀγγέλων δῆμος τὴν…"
meMA.Ode9C11.text = "Ἐξέστη τῶν Ἀγγέλων δῆμος τὴν σήν, Στρατηλάτα τηρῶν γενναιότητα, πᾶς ὁ παρών, ὄχλος κατεπλάγη τότε τὰς σάς, ἀνδραγαθίας ἔνδοξε, ἔφριξε δαιμόνων ἡ στρατιά, τὰ σὰ τρόπαια, νῦν δέ, κροτεῖ κόσμος τὰ γέρα, καὶ πᾶς πιστὸς ὑμνεῖ τὰ θαύματα."

meMA.Ode9C12.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C12.mode = misc_gr_GR_cog.Mode8
meMA.Ode9C12.melody = he.h.m8_gr_GR_cog.ExestiEpiTouto.name
meMA.Ode9C12.incipit = "Βοᾶται παρὰ πάσης σχεδὸν πνοῆς…"
meMA.Ode9C12.text = "Βοᾶται παρὰ πάσης σχεδὸν πνοῆς, τὰ πολλά σου καὶ μέγιστα τρόπαια, τὰ ἐπὶ σοί, τέρατα δειχθέντα καὶ διὰ σοῦ, ἡ ἐκ νεκρῶν ἀνέγερσις, ἡ ὁλοκληρία ἐκ συντριβῆς, ἡ ῥῶσις τῶν νοσούντων, ἡ λύσις τῶν δεσμίων, καὶ τῶν ἐν θλίψει ἡ παράκλησις."

meMA.Ode9C13.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C13.mode = misc_gr_GR_cog.Mode8
meMA.Ode9C13.melody = he.h.m8_gr_GR_cog.ExestiEpiTouto.name
meMA.Ode9C13.incipit = "Ὡς ἔχων παῤῥησίαν πρὸς Χριστόν…"
meMA.Ode9C13.text = "Ὡς ἔχων παῤῥησίαν πρὸς Χριστόν, ὑπὲρ οὗ πάσας ὑπήνεγκας, μέχρι τομῆς, καύσεως θανάτου τε καὶ σταυροῦ, ἐγκαρτερῶν Θεόδωρε, αἴτησαι συγγνώμην ἡμῖν κακῶν, τῷ κόσμῳ τὴν εἰρήνην, νίκας τοῖς Βασιλεῦσι, καὶ σωτηρίαν τοῖς τιμῶσί σε."

meMA.Ode9C1T.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C1T.mode = misc_gr_GR_cog.Mode8
meMA.Ode9C1T.melody = he.h.m8_gr_GR_cog.ExestiEpiTouto.name
meMA.Ode9C1T.incipit = "Ὑψώθη ἐδοξάσθη γένος βροτῶν, καὶ…"
meMA.Ode9C1T.text = "Ὑψώθη ἐδοξάσθη γένος βροτῶν, καὶ τιμῆς ἠξιώθη τῆς κρείττονος, διὰ τῆς σῆς, Κόρη συγγενείας καὶ θεϊκῆς, υἱοθεσίας ἔτυχε, τῇ σῇ μεσιτείᾳ· καὶ γὰρ Θεός, ὡράθη σαρκοφόρος, ἐκ σοῦ ἀῤῥήτῳ τρόπῳ, ἄτρεπτος μείνας τῇ Θεότητι."

meMA.Ode9C2H.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C2H.mode = misc_gr_GR_cog.Mode8
meMA.Ode9C2H.melody = he.h.m8_gr_GR_cog.SeTinApeirogamon.melody
meMA.Ode9C2H.incipit = he.h.m8_gr_GR_cog.SeTinApeirogamon.incipit
meMA.Ode9C2H.text = he.h.m8_gr_GR_cog.SeTinApeirogamon.text

meMA.Ode9C21.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C21.mode = misc_gr_GR_cog.Mode8
meMA.Ode9C21.melody = he.h.m8_gr_GR_cog.SeTinApeirogamon.name
meMA.Ode9C21.incipit = "Ἄπασαν τὴν αἴσθησιν, καταλιπὼν νοῒ…"
meMA.Ode9C21.text = "Ἄπασαν τὴν αἴσθησιν, καταλιπὼν νοῒ τῷ θείῳ, καὶ καθαρωτάτῳ πάνσοφε, φωτοφόρος παρίστασαι, τὰς ὑπὲρ τοῦ κόσμου ἱκεσίας ποιούμενος· ὅθεν πάντες σε Ζαχαρία μεγαλύνομεν"

meMA.Ode9C22.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C22.mode = misc_gr_GR_cog.Mode8
meMA.Ode9C22.melody = he.h.m8_gr_GR_cog.SeTinApeirogamon.name
meMA.Ode9C22.incipit = "Ῥήσεσιν ἑπόμενοι, τῶν σῶν χειλέων…"
meMA.Ode9C22.text = "Ῥήσεσιν ἑπόμενοι, τῶν σῶν χειλέων θεοφάντορ, Κύριον ἐπικαλούμεθα, διʼ αὐτοῦ τε σω ζόμενοι, ὡς τῆς ἀληθείας σε Προφήτην καὶ κήρυκα, Ζαχαρία τῆς εὐσεβείας μακαρίζομεν."

meMA.Ode9C23.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C23.mode = misc_gr_GR_cog.Mode8
meMA.Ode9C23.melody = he.h.m8_gr_GR_cog.SeTinApeirogamon.name
meMA.Ode9C23.incipit = "Ἵλεων ἀπέργασαι, ταῖς σαῖς πρεσβείαις…"
meMA.Ode9C23.text = "Ἵλεων ἀπέργασαι, ταῖς σαῖς πρεσβείαις τὸν Δεσπότην, τοῖς τὴν Ἱερὰν καὶ εὔσημον, ἑορτήν σου θεόπνευστε, πίστει ἐκτελοῦσι, καὶ σὲ πόθω γεραίρουσιν, ὡς Προφήτην καὶ θεηγόρον ἀληθέστατον."

meMA.Ode9C2T.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C2T.mode = misc_gr_GR_cog.Mode8
meMA.Ode9C2T.melody = he.h.m8_gr_GR_cog.SeTinApeirogamon.name
meMA.Ode9C2T.incipit = "Ἄβυσσον θαυμάτων σε, καὶ χαρισμάτων…"
meMA.Ode9C2T.text = "Ἄβυσσον θαυμάτων σε, καὶ χαρισμάτων Θεοτόκε, πέλαγος Ἁγνὴ γινώσκοντες, καὶ σαφῶς ἐπιστάμενοι, τῇ σῇ προστασίᾳ πεπειθότες προστρέχομεν, καὶ συντόνως πρός τὴν σὴν σκέπην καταφεύγομεν."

meMA.Exaposteilarion1.mode = misc_gr_GR_cog.Mode2
meMA.Exaposteilarion1.melody = he.a.m2_gr_GR_cog.ToisMathitaisSynelthomen.name
meMA.Exaposteilarion1.incipit = "Μεγαλομάρτυς ἔνδοξε, τοῦ Χριστοῦ Στρατηλάτα…"
meMA.Exaposteilarion1.text = "Μεγαλομάρτυς ἔνδοξε, τοῦ Χριστοῦ Στρατηλάτα, καὶ θεῖον ἐγκαλλώπισμα, τῶν σεπτῶν ἀθλοφόρων, Θεόδωρε γενναιόφρον, μὴ ἐλλίπῃς αἰτεῖσθαι, τῷ Βασιλεῖ τὰ τρόπαια, καὶ εἰρήνην τῷ κόσμω, καὶ ἱλασμόν, τῶν ἁμαρτημάτων καὶ σωτηρίαν, τοῖς ἐκτελοῦσι πάνσοφε, τὴν φωσφόρον σου μνήμην."

meMA.Exaposteilarion2.mode = misc_gr_GR_cog.Mode3
meMA.Exaposteilarion2.melody = misc_gr_GR_cog.Automelon
meMA.Exaposteilarion2.incipit = me.m02.d02_gr_GR_cog.meMA.Exaposteilarion1.incipit
meMA.Exaposteilarion2.text = me.m02.d02_gr_GR_cog.meMA.Exaposteilarion1.text

meMA.Lauds1.mode = misc_gr_GR_cog.Mode4
meMA.Lauds1.melody = he.a.m4_gr_GR_cog.OsGennaionEnMartysi.name
meMA.Lauds1.incipit = "Ἀθλητὴς γενναιότατος, στρατιώτης ἀήττητος, ἐν…"
meMA.Lauds1.text = "Ἀθλητὴς γενναιότατος, στρατιώτης ἀήττητος, ἐν Ἁγίῳ Πνεύματι ἀναδέδειξαι, καταβαλὼν τὸν πολέμιον, σοφίᾳ τῶν λόγων σου, καὶ τῶν ἔργων σου σοφέ, καρτεραῖς ἐπιδείξεσιν· ὅθεν εἴληφας, τοὺς στεφάνους τῆς νίκης, καὶ ταῖς ἄνω, ὁμηγύρεσι συνήφθης, κλέος Μαρτύρων Θεόδωρε."

meMA.Lauds2.mode = misc_gr_GR_cog.Mode4
meMA.Lauds2.melody = he.a.m4_gr_GR_cog.OsGennaionEnMartysi.name
meMA.Lauds2.incipit = "Καὶ σταυρῷ ἀναρτώμενος, καὶ τὰς…"
meMA.Lauds2.text = "Καὶ σταυρῷ ἀναρτώμενος, καὶ τὰς σάρκας τυπτόμενος, ὀξυτάτοις βέλεσι τιτρωσκόμενος, καὶ ἐπὶ ξύλου τεινόμενος, καὶ πλευρὰς ξεόμενος, καὶ παντοίαις χαλεπαῖς, ἀλγηδόσι κυκλούμενος, ἀπαράτρωτος, καὶ ἀήττητος ὤφθης τῇ δυνάμει, τοῦ Σταυρῷ προσηλωθέντος, κλέος Μαρτύρων Θεόδωρε."

meMA.Lauds3.mode = misc_gr_GR_cog.Mode4
meMA.Lauds3.melody = he.a.m4_gr_GR_cog.OsGennaionEnMartysi.name
meMA.Lauds3.incipit = "Θεῖον ἄγαλμα γέγονας, εὐσεβείας πανόλβιε…"
meMA.Lauds3.text = "Θεῖον ἄγαλμα γέγονας, εὐσεβείας πανόλβιε, δυσσεβῶν ἀγάλματα βδελυξάμενος, καὶ Ἱερεῖον ὁλόκληρον, καὶ θῦμα εὐπρόσδεκτον τῷ τυθέντι διὰ σέ, καθαρῶς, προσαγόμενος, τῷ δοξάσαντι, τὴν ἁγίαν σου μνήμην καὶ θαυμάτων, θησαυρὸν σε τοῖς ἐν κόσμῳ, δωρησαμένῳ Θεόδωρε."

meMA.LaudsGlory.mode = misc_gr_GR_cog.Mode8
meMA.LaudsGlory.melody = ""
meMA.LaudsGlory.incipit = "Ἀθλητικὴν ἀνδρείαν, καὶ λογικὴν λατρείαν…"
meMA.LaudsGlory.text = "Ἀθλητικὴν ἀνδρείαν, καὶ λογικὴν λατρείαν εὐσεβῶς καθοπλισθείς, Ἀθλοφόρε Χριστοῦ, καὶ στεῤῥῶς παραταχθεὶς τῇ δυνάμει αὐτοῦ, τῶν εἰδώλων τὸ ἀσεβές, καὶ τῶν Τυράννων τὸ ἀπηνές, ἀσθενὲς ἀπέδειξας, καταφρονῶν τῶν βασάνων, καὶ τοῦ προσκαίρου πυρός. Ἀλλʼ ὦ θείων δωρεῶν καὶ πρᾶγμα καὶ ὄνομα, ἀπὸ πάσης περιστάσεως, τῇ πρεσβείᾳ σου σῷζε, τοὺς τελοῦντας τὴν μνήμην σου."

meMA.LaudsBoth.poet = poets_gr_GR_cog.Germanou
meMA.LaudsBoth.mode = misc_gr_GR_cog.Mode2
meMA.LaudsBoth.melody = ""
meMA.LaudsBoth.incipit = me.m02.d02_gr_GR_cog.meVE.Lity6.incipit
meMA.LaudsBoth.text = me.m02.d02_gr_GR_cog.meVE.Lity6.text

meMA.Aposticha1.mode = misc_gr_GR_cog.Mode2
meMA.Aposticha1.melody = he.a.m2_gr_GR_cog.OikosTouEfratha.name
meMA.Aposticha1.incipit = "Ὁ πάντα τῇ χειρί, περιέχων…"
meMA.Aposticha1.text = "Ὁ πάντα τῇ χειρί, περιέχων ὡς Κτίστης, καὶ δεσπόζων ἁπάντων, ὑπὸ χειρῶν Πρεσβύτου, ἐν τῷ ναῷ προσάγεται."

meMA.Aposticha2.mode = misc_gr_GR_cog.Mode2
meMA.Aposticha2.melody = he.a.m2_gr_GR_cog.OikosTouEfratha.name
meMA.Aposticha2.incipit = "Ἀγάλλου Συμεών, καὶ ἡ Ἄννα…"
meMA.Aposticha2.text = "Ἀγάλλου Συμεών, καὶ ἡ Ἄννα εὐφραίνου· ἰδοὺ γὰρ ἐπεφάνη, ὁ Λυτρωτὴς τοῦ κόσμου, ὡς βρέφος προσαγόμενος."

meMA.Aposticha3.mode = misc_gr_GR_cog.Mode2
meMA.Aposticha3.melody = he.a.m2_gr_GR_cog.OikosTouEfratha.name
meMA.Aposticha3.incipit = "Λέλυσαι τῆς φθορᾶς, Συμεὼν θεοφόρε…"
meMA.Aposticha3.text = "Λέλυσαι τῆς φθορᾶς, Συμεὼν θεοφόρε, ἐν χερσὶ τὸν πάντα, κατέχοντα βαστάζων· εἶδες γὰρ τὸ σωτήριον."

meMA.AposGlory.mode = misc_gr_GR_cog.Mode8
meMA.AposGlory.melody = ""
meMA.AposGlory.incipit = "Ἀθλητικὸν συστησάμενος στάδιον κατὰ τῆς…"
meMA.AposGlory.text = "Ἀθλητικὸν συστησάμενος στάδιον κατὰ τῆς πλάνης, ἠνδραγάθησας Θεόδωρε, τῇ πυριφθόγγῳ γλώττῃ σου, καταισχύνας Λικινίου τὸ ἀπάνθρωπον· διὸ καὶ σὺν Ἀγγέλοις χορεύων μακάριε, αἴτησαι Χριστῷ τῷ Θεῷ, ὅπως ἵλεως γενήσεται ἡμῖν, ἐν τῇ ἡμέρᾳ τῆς κρίσεως."

meMA.AposBoth.mode = misc_gr_GR_cog.Mode7
meMA.AposBoth.melody = ""
meMA.AposBoth.incipit = "Φῶς, εἰς ἀποκάλυψιν ἐθνῶν ἦλθες…"
meMA.AposBoth.text = "Φῶς, εἰς ἀποκάλυψιν ἐθνῶν ἦλθες ἐξ οὐρανοῦ Σωτὴρ ἡμῶν, καὶ προελθὼν ἐκ τῆς Παρθένου, ἐπανεπαύσω ἐν ἀγκάλαις τοῦ δικαίου Συμεών· ἔδει γὰρ σε Ζωοδότα τῶν ἁπάντων, ὑπὸ τοῦ Πρεσβύτου γνωρισθῆναι, ὅτι παρεγένου ἀπολῦσαι αὐτόν, κατὰ τὸ ῥῆμά σου, ὁ ἔχων τὸ μέγα ἔλεος."


meLI.Kontakion1.mode = meMA.Kontakion1.mode
meLI.Kontakion1.melody = meMA.Kontakion1.melody
meLI.Kontakion1.incipit = meMA.Kontakion1.incipit
meLI.Kontakion1.text = meMA.Kontakion1.text

meMegalynarion.text = "Χαίροις τῶν Μαρτύρων ἡ καλλονή, καὶ τῆς Ἐκκλησίας, ἀπροσμάχητος βοηθός, χαίροις δωρημάτων, θησαύρισμα τῶν θείων, Θεόδωρε τρισμάκαρ, ἡμῶν ἀντίληψις."

//These extra hymns are found in the back of the Feb. Menaion

meVE.SticTheotokion.mode = misc_gr_GR_cog.Mode5
meVE.SticTheotokion.melody = ""
meVE.SticTheotokion.incipit = oc.m5.d3_gr_GR_cog.ocVE.AposTheotokion.incipit
meVE.SticTheotokion.text = oc.m5.d3_gr_GR_cog.ocVE.AposTheotokion.text

meVE.SticStavrotheotokion.mode = misc_gr_GR_cog.Mode5
meVE.SticStavrotheotokion.melody = ""
meVE.SticStavrotheotokion.incipit = "Ὡς ἀμνὸν σε Λόγε, ἡ…"
meVE.SticStavrotheotokion.text = "Ὡς ἀμνὸν σε Λόγε, ἡ ἀμνὰς καὶ Παρθένος, πρὸς σφαγὴν χωροῦντα, καθορῶσα ἐβόα· Ὢ ξένου τολμήματος! πῶς ἄνομοι σφάττουσι, τὸν τοὺς ἀνθρώπους ζωοῦντα; Μέγα σου Υἱέ μου τὸ ἔλεος!"

meVE.SticTheotokion2.mode = misc_gr_GR_cog.Mode8
meVE.SticTheotokion2.melody = he.a.m8_gr_GR_cog.TiYmasKalesomen.name
meVE.SticTheotokion2.incipit = "Νύμφην σε Παρθένον ἐκήρυξαν, τῆς…"
meVE.SticTheotokion2.text = "Νύμφην σε Παρθένον ἐκήρυξαν, τῆς ἀξίας τοῦ Πατρός, καὶ τῆς Θεότητος αὐτοῦ, οἱ Θεὸν ἀξιωθέντες, κατιδεῖν σωματικῶς, καὶ Λόγου καὶ Θεοῦ Κόρη Γεννήτριαν, καὶ Πνεύματος Ἁγίου οἰκητήριον· ἐν σοὶ γὰρ πᾶν τῆς Θεότητος, σωματικῶς κατεσκήνωσε, τὸ πλήρωμα, πληρεστάτης οὔσης χάριτος."

meVE.SticStavrotheotokion2.mode = misc_gr_GR_cog.Mode8
meVE.SticStavrotheotokion2.melody = he.a.m8_gr_GR_cog.TiYmasKalesomen.name
meVE.SticStavrotheotokion2.incipit = "Ξύλῳ προσπηγνύμενον Κύριε, ὡς ἑώρακεν…"
meVE.SticStavrotheotokion2.text = "Ξύλῳ προσπηγνύμενον Κύριε, ὡς ἑώρακεν ἀσπόρως, ἡ τεκοῦσά σε Ἀμνάς, κατεξαίνετο τὰς ὄψεις, καὶ ἐβόα ἐν κλαυθμῷ· Υἱέ μου πῶς σφαγὴν φέρεις τὴν ἄδικον, πῶς θνῄσκεις, ὁ ἀθάνατος ὡς ἄνθρωπος; Λόγον μοι δὸς φῶς γλυκύτατον, βλέψον Μητέρα θρηνοῦσάν σε, καὶ δόξασον, ταύτην Λόγε τῇ ἐγέρσει σου."

meMA.Ode3KathismaT.mode = misc_gr_GR_cog.Mode3
meMA.Ode3KathismaT.melody = he.a.m3_gr_GR_cog.TinOraiotita.name
meMA.Ode3KathismaT.incipit = "Ἀκατανόητον, καὶ ἀκατάληπτον, ὑπάρχει Δέσποινα…"
meMA.Ode3KathismaT.text = "Ἀκατανόητον, καὶ ἀκατάληπτον, ὑπάρχει Δέσποινα θεοχαρίτωτε, τὸ πεπραγμένον ἐπὶ σοί, φρικτὸν ὄντως μυστήριον! τὸν γὰρ ἀπερίληπτον, συλλαβοῦσα ἐκύησας, σάρκα περιθέμενον, ἐξ ἀχράντων αἱμάτων σου, ὃν πάντοτε Ἁγνὴ ὡς Υἱόν σου, δυσώπει τοῦ σῶσαι τὰς ψυχὰς ἡμῶν."

