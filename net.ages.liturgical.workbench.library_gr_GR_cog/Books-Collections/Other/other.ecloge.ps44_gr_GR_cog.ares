other.title = ""
other.poet = ""
other.verse01.text = "Χαῖρε Παντάνασσα Πανύμνητε, Μῆτερ Χριστοῦ τοῦ Θεοῦ."
other.verse02.text = "Χαῖρε Βασίλισσα τῶν Ἀγγέλων, Δέσποινα τοῦ Κόσμου."
other.verse03.text = "Χαῖρε τῶν Προφητῶν τὸ κήρυγμα, Πατριαρχῶν ἡ δόξα."
other.verse04.text = "Χαῖρε ἀγνὴ Παρθένε, Θεόνυμφε Δέσποινα."
other.verse05.text = "Χαῖρε ἄσπιλε, ἀμόλυντε, ἄφθορε Παναγία."
other.verse06.text = "Χαῖρε ἡ ἀπηλπισμένων ἐλπὶς καὶ τῶν πολεμουμένων βοήθεια."
other.verse07.text = "Χαῖρε Κεχαριτωμένη, ὁ Κύριος μετὰ σοῦ καὶ διὰ σοῦ μεθ’ ἡμῶν."
other.verse08.text = "Χαῖρε εὐλογημένη σὺ ἐν γυναιξί, καὶ εὐλογημένος ὁ καρπὸς τῆς κοιλίας σου."
other.verse09.text = "Χαῖρε Παρθένε Θεοτόκε ἐν γυναιξὶν εὐλογημένη."
other.verse10.text = "Χαῖρε Μαρία Κυρία πάντων ἡμῶν, ὁ Κύριος μετὰ σοῦ τῶν Δυνάμεων."
other.verse11.text = "Χαῖρε Θρόνε πυρίμορφε, τῶν τετραμόρφων ὑπερενδοξοτέρα."
other.verse12.text = "Χαῖρε Μαρία Κυρία πάντων ἡμῶν· χαῖρε Μῆτερ τῆς ζωῆς."
other.verse13.text = "Χαῖρε ὑπερευλογημένη, ὑπερδεδοξασμένη."
other.verse14.text = "Χαῖρε Ἄχραντε Θεοτόκε, ὅτι Σωτῆρα ἔτεκες τῶν ψυχῶν ἡμῶν."
other.verse15.text = "Χαῖρε Παράδεισε ἁγιώτατε· χαῖρε Νύμφη ἀνύμφευτε."
other.verse16.text = "Χαῖρε ἁγία, ἁγίων μείζων· χαῖρε Νύμφη ἀνύμφευτε."
other.verse17.text = "Παναγία Παρθένε, ἐπάκουσον τῆς φωνῆς τοῦ ἀχρείου ἱκέτου σου, στεναγμοὺς τῆς καρδίας προσφέρειν σοι, ἁεννάως εὐόδωσον Δέσποινα."
other.verse18.text = "Παναγία Παρθἐνε Θεόνυμφε, τὴν οἰκτρὰν λειτουργίαν μου πρόσδεξαι, καὶ Θεῷ τῷ εὐσπλάχνῳ προσάγαγε, ὄπως χαίρων δοξάζω σε Πανάμωμε."
