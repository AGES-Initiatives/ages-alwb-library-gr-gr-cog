meVE.Stichera01.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.Stichera01.greekmelody = he.a.m4_gr_GR_cog.heAU.EdokasSimeiosin.greekname
meVE.Stichera01.melody = he.a.m4_gr_GR_cog.heAU.EdokasSimeiosin.name
meVE.Stichera01.incipit = "Ἡλίου λαμπρότερον, λελαμπρυσμένη ἡ μνήμη…"
meVE.Stichera01.text = "Ἡλίου λαμπρότερον, λελαμπρυσμένη ἡ μνήμη σου, τοῖς πιστοῖς ἐξανέτειλε, τὴν κτίσιν φωτίζουσα, θείαις φρυκτωρίαις, Εὐλάμπιε μάκαρ, καὶ παθημάτων τὴν ἀχλύν, καὶ τῶν δαιμόνων νύκτα διώκουσα· διὸ σε μακαρίζομεν, καὶ ἐτησίως γεραίρομεν, ὡς φωστῆρα παγκόσμιον, πρεσβευτὴν ὡς θερμότατον."

meVE.Stichera02.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.Stichera02.melody = he.a.m4_gr_GR_cog.heAU.EdokasSimeiosin.name
meVE.Stichera02.incipit = "Σιδήρῳ ξεόμενος, καὶ ταῖς λαμπάσι…"
meVE.Stichera02.text = "Σιδήρῳ ξεόμενος, καὶ ταῖς λαμπάσι φλεγόμενος, καὶ εἱρκτῇ συγκλειόμενος, ξύλῳ ἀναρτώμενος, καὶ θηρίοις βρῶμα, διδόμενος μάκαρ, ἀπαρασάλευτος τὸν νοῦν, διεφυλάχθης τῇ θείᾳ χάριτι, καὶ νίκης διαδήματι, κατεκοσμήθης Εὐλάμπιε, διὰ τοῦτο τὴν μνήμην σου, χαρμοσύνως γεραίρομεν."

meVE.Stichera03.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.Stichera03.melody = he.a.m4_gr_GR_cog.heAU.EdokasSimeiosin.name
meVE.Stichera03.incipit = "Τὴν Εὔαν τὸν τρώσαντα, καὶ…"
meVE.Stichera03.text = "Τὴν Εὔαν τὸν τρώσαντα, καὶ Παραδείσου ἐξώσαντα, πολυμήχανον ἔτρωσας, τρωθεῖσα τῷ ἔρωτι, τοῦ Παμβασιλέως, Χριστοῦ Εὐλαμπία, καὶ ὑπομείνασα στεῤῥῶς, σώματος τρῶσιν καὶ πᾶσαν βάσανον· διὸ μεθέξει κρείττονι, θείας θεώσεως ἔτυχες, ὑπὲρ πάντων πρεσβεύουσα, τῶν πιστῶς εὐφημούντων σε."

meVE.SticGlory.poet = poets_gr_GR_cog.po.IoannouMonachou
meVE.SticGlory.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.SticGlory.melody = ""
meVE.SticGlory.incipit = "Τῇ φιλαδελφίᾳ ἡ ὁμωνυμία συγκραθεῖσα…"
meVE.SticGlory.text = "Τῇ φιλαδελφίᾳ ἡ ὁμωνυμία συγκραθεῖσα, καὶ ἡ ἁγνεία τῇ ἀπαθείᾳ συμμιχθεῖσα, ἀβλαβῶς διεφύλαξε τῆς γνώμης τὸ εὔτονον· ὅπου γὰρ Θεὸς ὁ ποθούμενος, κόσμος ὅλος καταπεφρόνηται. Ὢ τοῦ θαύματος! ὁ ὄφις νενέκρωται, καὶ ὁ εἰς ὕψος ἀδικίαν λαλήσας, ὑποχθόνιος αὐταδέλφοις Μάρτυσι καταπέπτωκεν, Εὐλαμπίῳ τῷ σοφῷ καὶ Εὐλαμπία. Τούτοις εὐστοχωτάτοις ᾄσμασι βοήσωμεν, οἱ καλῶς ἐν Χριστῷ τὸν δρόμον τελέσαντες, αἰτήσασθε τῷ κόσμῳ τὴν εἰρήνην, καὶ ταῖς ψυχαῖς ἡμῶν τὸ μέγα ἔλεος."

meVE.SticTheotokion.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.SticTheotokion.greekmelody = he.a.m4_gr_GR_cog.heAU.EdokasSimeiosin.greekname
meVE.SticTheotokion.melody = he.a.m4_gr_GR_cog.heAU.EdokasSimeiosin.name
meVE.SticTheotokion.incipit = "Χαῖρε ἡλιόμορφε…"
meVE.SticTheotokion.text = "Χαῖρε ἡλιόμορφε, ἠλίου ἄδυτον ὄχημα, ἡ τὸν Ἥλιον λάμψασα, τὸν ἀπερινόητον· χαῖρε νοῦς ἀστράπτων, θείαις φρυκτωρίαις, ἡ λαμπηδὼν τῆς ἀστραπῆς, ἡ διαυγάζουσα γῆς τὰ πέρατα· ἡ ὄντως χρυσαυγίζουσα, ἡ παγκαλὴς καὶ πανάμωμος, ἡ τὸ φῶς τὸ ἀνέσπερον, τοῖς πιστοῖς ἐξαστράψασα."

meVE.SticStavrotheotokion.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.SticStavrotheotokion.greekmelody = he.a.m4_gr_GR_cog.heAU.EdokasSimeiosin.greekname
meVE.SticStavrotheotokion.melody = he.a.m4_gr_GR_cog.heAU.EdokasSimeiosin.name
meVE.SticStavrotheotokion.incipit = "Ῥήγνυται χειρόγραφον, τὸ ἀπʼ αἰῶνος…"
meVE.SticStavrotheotokion.text = "Ῥήγνυται χειρόγραφον, τὸ ἀπʼ αἰῶνος νυγείσης σου, τῆς πλευρᾶς Πολυέλεε, Ἀδὰμ τοῦ προπάτορος, καὶ ἡ ἀπωσμένη, φύσις τῶν ἀνθρώπων, ῥανίσιν αἵματος τοῦ σοῦ, καθαγιάζεται ἀνακράζουσα· Δόξα τῇ εὐσπλαγχνία σου· δόξα τῇ θείᾳ σταυρώσει σου, Ἰησοῦ παντοδύναμε, ὁ Σωτὴρ τῶν ψυχῶν ἡμῶν."

meVE.AposGlory.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.AposGlory.melody = ""
meVE.AposGlory.incipit = "Τῷ τῆς Τριάδος φωτί, ἡ…"
meVE.AposGlory.text = "Τῷ τῆς Τριάδος φωτί, ἡ αὐτάδελφος ξυνωρὶς λαμπρυνθεῖσα, τῶν τυράννων τὴν ὠμότητα κατέβαλεν· ὅθεν καὶ τὴν φλόγα καταπατοῦντες, χορεύοντες ἔψαλλον· Ἰδοὺ δὴ τί καλόν, ἢ τί τερπνόν, ἀλλʼ ἢ τὸ κατοικεῖν ἀδελφοὺς ἅμα; Καὶ πρὸς ἔνθεον δόξαν ἀποβλέποντες, οὐρανίου δόξης κατηξιώθησαν, Χριστῷ τῷ Θεῷ, ὑπὲρ ἡμῶν ἀεὶ πρεσβεύοντες, τοῦ σωθῆναι τὰς ψυχὰς ἡμῶν."

meVE.AposTheotokion.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.AposTheotokion.greekmelody = he.a.m4_gr_GR_cog.heAU.EdokasSimeiosin.greekname
meVE.AposTheotokion.melody = he.a.m4_gr_GR_cog.heAU.EdokasSimeiosin.name
meVE.AposTheotokion.incipit = oc.m4.d6_gr_GR_cog.ocVE.Stichera4.incipit
meVE.AposTheotokion.text = oc.m4.d6_gr_GR_cog.ocVE.Stichera4.text

meVE.AposStavrotheotokion.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.AposStavrotheotokion.greekmelody = he.a.m4_gr_GR_cog.heAU.EdokasSimeiosin.greekname
meVE.AposStavrotheotokion.melody = he.a.m4_gr_GR_cog.heAU.EdokasSimeiosin.name
meVE.AposStavrotheotokion.incipit = "Μήτηρ ἀπειρόγαμος, Θεοῦ ἐδείχθης Πανάμωμε…"
meVE.AposStavrotheotokion.text = "Μήτηρ ἀπειρόγαμος, Θεοῦ ἐδείχθης Πανάμωμε, τοῦ Σταυρῷ ὁμιλήσαντος, καὶ πάθη μειώσαντος, τοῖς Τιμίοις τούτου, πάθεσι καὶ πόνοις, οὓς περ ὑπέμεινεν ἑκών, διὰ τὸ σῶσαι βροτοὺς ἐν χάριτι· διὸ κἀμὲ δυσώπησον, σωθῆναι σαῖς παρακλήσεσι, Μητροπάρθενε Δέσποινα, τῶν Ἀγγέλων ὑπέρτιμε."

meVE.Apolytikion1ger.poet = poets_gr_GR_cog.po.Gerasimou
meVE.Apolytikion1ger.mode = miscellanea_gr_GR_cog.misc.Mode1
meVE.Apolytikion1ger.melody = he.a.m1_gr_GR_cog.heAU.TonTafonSouSotir.name
meVE.Apolytikion1ger.incipit = "Τῆς φύσεως θεσμῷ συνημμένοι ἐνθέως…"
meVE.Apolytikion1ger.text = "Τῆς φύσεως θεσμῷ συνημμένοι ἐνθέως, ὁμόψυχοι στεῤῥῶς, ὡς ὁμαίμονες θεῖοι, αὐτάδελφοι Μάρτυρες, ἐν ἀθλήσει ὡράθητε, ὦ Εὐλάμπιε, σὴν τῇ σεμνῇ Εὐλαμπίᾳ· ὅθεν στέφανον, νικητικὸν δεδεγμένοι, ἡμᾶς διασῴζετε."

meMA.C1.acrostic.title = "Κανὼν τῶν Ἁγίων,"
meMA.C1.acrostic.notice1 = "οὗ ἡ Ἀκροστιχίς·"
meMA.C1.acrostic.text = "Τὴν λαμπρὰν ὑμνῶ Μαρτύρων ξυνωρίδα. Ἰωσήφ."
meMA.C1.title = titles_gr_GR_cog.ti.CanonMartyrs
meMA.C1.verse = prayers_gr_GR_cog.pr.verse_SaintsM.text

meMA.Ode1C11.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C11.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode1C11.greekmelody = he.h.m6_gr_GR_cog.heHE.OsEnIpeiroPezefsas.greekname
meMA.Ode1C11.melody = he.h.m6_gr_GR_cog.heHE.OsEnIpeiroPezefsas.name
meMA.Ode1C11.incipit = "Ταῖς φανοτάταις ἀκτῖσι τῆς τριλαμποῦς…"
meMA.Ode1C11.text = "Ταῖς φανοτάταις ἀκτῖσι τῆς τριλαμποῦς, ἀθλοφόροι Μάρτυρες, ἐλλαμπόμενοι αὐγῆς, φωτισμὸν αἰτήσασθε ἡμῖν, τὴν πανέορτον ὑμῶν μνήμην γεραίρουσιν."

meMA.Ode1C12.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C12.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode1C12.melody = he.h.m6_gr_GR_cog.heHE.OsEnIpeiroPezefsas.name
meMA.Ode1C12.incipit = "Ἡλιακὰς ἀπαστράπτει μαρμαρυγάς, ἡ σεπτὴ…"
meMA.Ode1C12.text = "Ἡλιακὰς ἀπαστράπτει μαρμαρυγάς, ἡ σεπτὴ καὶ εὔσημος, τῶν Μαρτύρων Ἑορτή, καὶ τῆς γῆς τὰ πέρατα ἀεί, καταυγάζει μυστικῶς, σθένει τοῦ Πνευματος."

meMA.Ode1C13.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C13.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode1C13.melody = he.h.m6_gr_GR_cog.heHE.OsEnIpeiroPezefsas.name
meMA.Ode1C13.incipit = "Νέῳ ἐν σώματι Μάρτυς τὸν…"
meMA.Ode1C13.text = "Νέῳ ἐν σώματι Μάρτυς τὸν παλαιόν, τῆς κακίας ἄρχοντα, κατεπάλαισας στεῤῥῶς, ὑπομείνας βάσανα καὶ πῦρ, καὶ δεινῶν ἐπιφοράς, μάκαρ Εὐλάμπιε."

meMA.Ode1C1T.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C1T.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode1C1T.melody = he.h.m6_gr_GR_cog.heHE.OsEnIpeiroPezefsas.name
meMA.Ode1C1T.incipit = "Λελυτρωμένοι Παρθένε προγονικῆς, καταδίκης ἄχραντε…"
meMA.Ode1C1T.text = "Λελυτρωμένοι Παρθένε προγονικῆς, καταδίκης ἄχραντε, τῇ γεννήσει σου σαφῶς, ἐν φωναῖς ᾀσμάτων σε ἀεί, μακαρίζομεν πιστοί, ὡς Θεομήτορα."

meMA.Ode3C11.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C11.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode3C11.greekmelody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.greekname
meMA.Ode3C11.melody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.name
meMA.Ode3C11.incipit = "Ἀγῶνας ἤνυσας στεῤῥῶς, ὑπομείνας βασάνους…"
meMA.Ode3C11.text = "Ἀγῶνας ἤνυσας στεῤῥῶς, ὑπομείνας βασάνους, καὶ πικρὰς τιμωρίας, τῶν μελλόντων ἀγαθῶν, νευρούμενος προφανῶς, ταῖς ἐλπίσιν, ἔνδοξε Εὐλάμπιε."

meMA.Ode3C12.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C12.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode3C12.melody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.name
meMA.Ode3C12.incipit = "Μαρτύρων εὔκλειαν φαιδράν, περικείμενον Μάρτυς…"
meMA.Ode3C12.text = "Μαρτύρων εὔκλειαν φαιδράν, περικείμενον Μάρτυς, καθορῶσά σε ὅλην, ταῖς βασάνοις ἑαυτήν, ἐκδέδωκεν ἡ σεμνή, Εὐλαμπία, ἀδελφὰ φρονοῦσά σοι."

meMA.Ode3C13.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C13.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode3C13.melody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.name
meMA.Ode3C13.incipit = "Ποθήσας Μάρτυς τὸν Χριστόν, τὰ…"
meMA.Ode3C13.text = "Ποθήσας Μάρτυς τὸν Χριστόν, τὰ ὁρώμενα πάντα, εἰς οὐδέν ἑλογίσω, τῶν τυραννούντων ὁρμάς, Εὐλάμπιε ἀθλητά, ὀλεθρίους, πίστει τροπωσάμενος."

meMA.Ode3C1T.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C1T.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode3C1T.melody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.name
meMA.Ode3C1T.incipit = "Ῥυσθῆναι πάσης πονηρᾶς, ἐναντίων ἐφόδου…"
meMA.Ode3C1T.text = "Ῥυσθῆναι πάσης πονηρᾶς, ἐναντίων ἐφόδου, καὶ παθῶν ψυχοφθόρων, καὶ κινδύνων χαλεπῶν, τοὺς σὲ τιμῶντας ἀεί, Θεοτόκε, πάναγνε ἱκέτευε."

meMA.Ode3H.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3H.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode3H.greekmelody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.greekname
meMA.Ode3H.melody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.melody
meMA.Ode3H.incipit = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.incipit
meMA.Ode3H.text = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.text

meMA.Ode3Kathisma1.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3Kathisma1.melody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.name
meMA.Ode3Kathisma1.incipit = "Τῶν βασάνων τὰ νέφη τὰ…"
meMA.Ode3Kathisma1.text = "Τῶν βασάνων τὰ νέφη τὰ χαλεπά, Ἀθληταὶ παριδόντες καρτερικῶς, ὡς ἥλιος χάριτι, τοῦ Σωτῆρος ἐλάμψατε, ἀδελφικῇ στοργῇ δέ, ἐνθέως συνδούμενοι, ἐφʼ ὁμοίοις πόνοις, λαμπρῶς ἐδοξάσθητε· ὅθεν μετὰ τέλος, ἀτελεύτητον χάριν, ἐξ ὕψους ἐδέξασθε, θεραπεύειν νοσήματα, Ἀθλοφόροι αὐτάδελφοι, πρεσβεύσατε Χριστῷ τῷ Θεῷ, τῶν πταισμάτων ἄφεσιν δωρήσασθαι, τοῖς ἑορτάζουσι πόθῳ, τὴν ἁγίαν μνήμην ὑμῶν."

meMA.Ode3KathismaT.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3KathismaT.melody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.name
meMA.Ode3KathismaT.incipit = "Τὴν ψυχήν μου Παρθένε, τὴν…"
meMA.Ode3KathismaT.text = "Τὴν ψυχήν μου Παρθένε, τὴν ταπεινήν, τὴν ἐν ζάλῃ τοῦ βίου τῶν πειρασμῶν, νῦν ὡς ἀκυβέρνητον, ποντουμένην πανάμωμε, ἁμαρτιῶν τε φόρτῳ, ὀφθεῖσαν ὑπέραντλον, καὶ ἐν πυθμένι ᾍδου, ἐλθεῖν κινδυνεύουσαν, φθάσον Θεοτόκε, τῇ θερμῇ σου πρεσβείᾳ, καὶ σῶσον παρέχουσα, τὸν λιμένα τὸν εὔδιον, ἵνα πίστει κραυγάζω σοι· Πρέσβευε τῷ σῷ Υἱῷ καὶ Θεῷ, τῶν πταισμάτων δοῦναί μοι τὴν ἄφεσιν· σὲ γὰρ ἔχω ἐλπίδα, ὁ δοῦλός σου Δέσποινα."

meMA.Ode3KathismaSt.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3KathismaSt.melody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.name
meMA.Ode3KathismaSt.incipit = oc.m8.d4_gr_GR_cog.ocMA.Kathisma13.incipit
meMA.Ode3KathismaSt.text = oc.m8.d4_gr_GR_cog.ocMA.Kathisma13.text

meMA.Ode4C11.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C11.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode4C11.greekmelody = he.h.m6_gr_GR_cog.heHE.ChristosMouDynamis.greekname
meMA.Ode4C11.melody = he.h.m6_gr_GR_cog.heHE.ChristosMouDynamis.name
meMA.Ode4C11.incipit = "Ἀγρίων χάσματα, θηρῶν ἐφίμωσας, ἐπικλήσεσι…"
meMA.Ode4C11.text = "Ἀγρίων χάσματα, θηρῶν ἐφίμωσας, ἐπικλήσεσι θείαις Ἀγγελικῇ, δόξῃ καλλυνόμενος, καὶ μαρτυρίου ἱεραῖς, φωταυγίαις λαμπρυνόμενος."

meMA.Ode4C12.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C12.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode4C12.melody = he.h.m6_gr_GR_cog.heHE.ChristosMouDynamis.name
meMA.Ode4C12.incipit = "Ναὸν σε ἔμψυχον, ναὸν πανάγιον…"
meMA.Ode4C12.text = "Ναὸν σε ἔμψυχον, ναὸν πανάγιον, ἡ τοῦ Πνεύματος χάρις Μάρτυς σοφέ, εὕρατο Εὐλάμπιε, ᾧ κραταιούμενος Ναούς, τῶν εἰδώλων κατηδάφισας."

meMA.Ode4C13.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C13.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode4C13.melody = he.h.m6_gr_GR_cog.heHE.ChristosMouDynamis.name
meMA.Ode4C13.incipit = "Ὑπῆρξας ἄσειστος, ἀκαταπτόητος, ἀπερίτρεπτος πάσαις…"
meMA.Ode4C13.text = "Ὑπῆρξας ἄσειστος, ἀκαταπτόητος, ἀπερίτρεπτος πάσαις ἐπιβολαῖς, χαλεπῶν κολάσεων, κραταιουμένη ἐν Χριστῷ, Εὐλαμπία παναοίδιμε."

meMA.Ode4C14.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C14.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode4C14.melody = he.h.m6_gr_GR_cog.heHE.ChristosMouDynamis.name
meMA.Ode4C14.incipit = "Μαρτύρων αἵμασι, καλλωπιζόμενοι, καὶ ὁμόνοιαν…"
meMA.Ode4C14.text = "Μαρτύρων αἵμασι, καλλωπιζόμενοι, καὶ ὁμόνοιαν πίστει ἀδελφικῇ, σῴζοντες Μακάριοι, τῶν διωκτῶν τὰς ζοφεράς, ἐπινοίας διεκρούσασθε."

meMA.Ode4C1T.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C1T.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode4C1T.melody = he.h.m6_gr_GR_cog.heHE.ChristosMouDynamis.name
meMA.Ode4C1T.incipit = "Νοήσας πόῤῥωθεν, ἐμφαντικώτατα, Ἀββακοὺμ σε…"
meMA.Ode4C1T.text = "Νοήσας πόῤῥωθεν, ἐμφαντικώτατα, Ἀββακοὺμ σε ἐκάλει ὄρος Ἁγνή, ἀρεταῖς κατάσκιον, ἐξ οὗ ἐπέφανεν ἡμῖν, ὁ φωτίζων τὰς ψυχὰς ἡμῶν."

meMA.Ode5C11.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C11.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode5C11.greekmelody = he.h.m6_gr_GR_cog.heHE.ToTheioFengeiSou.greekname
meMA.Ode5C11.melody = he.h.m6_gr_GR_cog.heHE.ToTheioFengeiSou.name
meMA.Ode5C11.incipit = "Ὡς ὄρθρος ἔλαμψας εὐπρεπής, ὡς…"
meMA.Ode5C11.text = "Ὡς ὄρθρος ἔλαμψας εὐπρεπής, ὡς ἡμερινὸς ὄντως ἀστήρ, ὡς φαεινότατος ἥλιος, ἄθλων καὶ σημείων μαρμαρυγαῖς τοὺς πιστούς, ἐνθέως καταυγάζων, Μάρτυς Εὐλάμπιε."

meMA.Ode5C12.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C12.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode5C12.melody = he.h.m6_gr_GR_cog.heHE.ToTheioFengeiSou.name
meMA.Ode5C12.incipit = "Μεγίστοις πόνοις ἐγκαρτερῶν, καὶ τὰς…"
meMA.Ode5C12.text = "Μεγίστοις πόνοις ἐγκαρτερῶν, καὶ τὰς ἀνενδότους τῶν δεινῶν, ἐπιφορὰς λογιζόμενος, Μάρτυς ὡς ἡδίστας τρυφὰς Εὔλάμπιε, τὰς θείας ἀντιδόσεις, χαίρων κεκλήρωσαι."

meMA.Ode5C13.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C13.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode5C13.melody = he.h.m6_gr_GR_cog.heHE.ToTheioFengeiSou.name
meMA.Ode5C13.incipit = "Αἱμάτων ῥεῖθρα Μάρτυς σεμνή, χέουσα…"
meMA.Ode5C13.text = "Αἱμάτων ῥεῖθρα Μάρτυς σεμνή, χέουσα ἐκτήσω τῆς τρυφῆς, σὺ τὸν χειμάῤῥουν πανεύφημε, καὶ τὴν διʼ αἰῶνος δόξαν ἀμάραντον, καὶ τὴν ἐν Παραδείσῳ, τερπνὴν ἀπόλαυσιν."

meMA.Ode5C1T.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C1T.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode5C1T.melody = he.h.m6_gr_GR_cog.heHE.ToTheioFengeiSou.name
meMA.Ode5C1T.incipit = "Ῥομφαῖαι πᾶσαι τοῦ δυσμενοῦς, ἄχραντε…"
meMA.Ode5C1T.text = "Ῥομφαῖαι πᾶσαι τοῦ δυσμενοῦς, ἄχραντε πανάμωμε Ἁγνή, ὄντως εἰς τέλος ἐξέλιπον· σὺ γὰρ τὸν ἁπάντων Θεὸν ἐκύησας, τὸν Σταυρῷ καθελόντα, τούτου τὸ φρύαγμα."

meMA.Ode6C11.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C11.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode6C11.greekmelody = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.greekname
meMA.Ode6C11.melody = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.name
meMA.Ode6C11.incipit = "Τεινόμενοι Μάρτυρες, καὶ στρεβλούμενοι δεινῶς…"
meMA.Ode6C11.text = "Τεινόμενοι Μάρτυρες, καὶ στρεβλούμενοι δεινῶς, καὶ τοῖς θηρσὶ ῥιπτόμενοι, καὶ μεληδὸν κοπτόμενοι καὶ πυρί, ἐνύλῳ φλεγόμενοι, τὴν ἀμώμητον πίστιν οὐκ ἠρνήσασθε."

meMA.Ode6C12.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C12.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode6C12.melody = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.name
meMA.Ode6C12.incipit = "Ὑψούμενα κύματα, τῶν κολάσεων σφοδρῶς…"
meMA.Ode6C12.text = "Ὑψούμενα κύματα, τῶν κολάσεων σφοδρῶς, δικαστικαῖς προστάξεσι, τὸ σταθηρὸν τῆς γνώμης τῶν Ἀθλητῶν βυθίσαι οὐκ ἴσχυσε· τῇ γὰρ θείᾳ παλάμῃ ἐκρατύνοντο."

meMA.Ode6C13.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C13.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode6C13.melody = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.name
meMA.Ode6C13.incipit = "Ῥοαῖς ἀπεπνίξατε, Φαραὼ τὸν δυσμενῆ…"
meMA.Ode6C13.text = "Ῥοαῖς ἀπεπνίξατε, Φαραὼ τὸν δυσμενῆ, τοῦ ἐκχυθέντος αἵματος, τὴν δὲ Χριστοῦ ἠρδεύσατε εὐσεβῶς, ἀήττητοι Μάρτυρες, Ἐκκλησίαν, τῇ πίστει ἀναθάλλουσαν."

meMA.Ode6C1T.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C1T.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode6C1T.melody = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.name
meMA.Ode6C1T.incipit = "Ὡς ὄμβρος κεκένωται, ἐν τῇ…"
meMA.Ode6C1T.text = "Ὡς ὄμβρος κεκένωται, ἐν τῇ μήτρᾳ σου Θεός, διʼ εὐσπλαγχνίαν ἄφατον, καὶ τὸν χειμάῤῥουν ἅπαντας τῆς τρυφῆς, ἐπότισε Δέσποινα, καὶ τὴν κτίσιν φθαρεῖσαν ἐκαινούργησε."

meMA.Ode6H.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6H.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode6H.greekmelody = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.greekname
meMA.Ode6H.melody = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.melody
meMA.Ode6H.incipit = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.incipit
meMA.Ode6H.text = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.text

meMA.Kontakion1.mode = miscellanea_gr_GR_cog.misc.Mode3
meMA.Kontakion1.melody = he.a.m3_gr_GR_cog.heAU.IParthenosSimeron.name
meMA.Kontakion1.incipit = "Τοὺς γενναίους Μάρτυρας, καὶ ἀδελφοὺς…"
meMA.Kontakion1.text = "Τοὺς γενναίους Μάρτυρας, καὶ ἀδελφοὺς κατὰ σάρκα, τὸν σοφὸν Εὐλάμπιον, καὶ Εὐλαμπίαν τιμῶμεν· οὗτοι γὰρ τῶν τυραννούντων μηχανουργίας, ᾔσχυναν τῇ δυναστείᾳ τοῦ σταυρωθέντος. ἀνεδείχθησαν διόπερ, Μαρτύρων δόξα, ὁμοῦ καὶ καύχημα."

meMA.Oikos1.text = "Τὴν δυάδα πιστοὶ τῶν Ἀθλοφόρων σήμερον, ἐν ᾠδαῖς ἱεραῖς καὶ ὕμνοις εὐφημήσωμεν· ὅτι τῶν εἰδώλων καθεῖλον τὴν πλάνην, πολυθεΐας τὸ πῦρ κατασβέσαντες, καὶ δαίμονας ᾔσχυναν· τῶν δὲ τυράννων τὸν θυμὸν οὐκ ἔπτηξαν ξίφη τε καὶ πῦρ μὴ δειλιάσαντες, οὔτε θηρίων ὁρμὰς ἀγρίων, ἀγωνισάμενοι καλῶς, Εὐλάμπιος ὁ εὐκλεής, σὺν τῇ σεπτῇ Εὐλαμπίᾳ, ἀδελφοὶ σύναθλοι δειχθέντες, Μαρτύρων δόξα, ὁμοῦ καὶ καύχημα."

meMA.Ode7C11.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C11.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode7C11.greekmelody = he.h.m6_gr_GR_cog.heHE.DrosovolonMenTinKaminon.greekname
meMA.Ode7C11.melody = he.h.m6_gr_GR_cog.heHE.DrosovolonMenTinKaminon.name
meMA.Ode7C11.incipit = "Νεανίας τρεῖς ἐν χάριτι μιμούμενοι…"
meMA.Ode7C11.text = "Νεανίας τρεῖς ἐν χάριτι μιμούμενοι, τὸ πῦρ κατεπατήσατε, δροσιζόμενοι τῷ ἀΰλῳ Πνεύματος πυρί, καὶ ψάλλοντες Μάρτυρες Χριστῷ· Εὐλογητὸς εἶ ὁ Θεός, ὁ τῶν Πατέρων ἡμῶν."

meMA.Ode7C12.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C12.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode7C12.melody = he.h.m6_gr_GR_cog.heHE.DrosovolonMenTinKaminon.name
meMA.Ode7C12.incipit = "Ξεομένης τῆς σαρκὸς ἡ πρὸς…"
meMA.Ode7C12.text = "Ξεομένης τῆς σαρκὸς ἡ πρὸς τὸν Κτίσαντα, ἀγάπη ἐκρατύνετο· οἱ γὰρ Ἅγιοι ὁλοτρόπῳ νεύσει πρὸς Θεόν, θεούμενοι ἔμελπον πιστῶς· Εὔλογητός εἶ ὁ Θεός, ὁ τῶν Πατέρων ἡμῶν."

meMA.Ode7C13.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C13.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode7C13.melody = he.h.m6_gr_GR_cog.heHE.DrosovolonMenTinKaminon.name
meMA.Ode7C13.incipit = "Ὑπερείδων σου καρδίας τὰ κινήματα…"
meMA.Ode7C13.text = "Ὑπερείδων σου καρδίας τὰ κινήματα, ταῖς θείαις ἀναβάσεσιν, ἐν ἀσαλεύτῳ, Ἀθλοφόρε πέτρᾳ τῆς ζωῆς, ἀκλόνητος ἵστασο βοῶν· Εὐλογητὸς εἶ ὁ Θεός, ὁ τῶν Πατέρων ἡμῶν."

meMA.Ode7C1T.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C1T.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode7C1T.melody = he.h.m6_gr_GR_cog.heHE.DrosovolonMenTinKaminon.name
meMA.Ode7C1T.incipit = "Νεανίας τρεῖς ἡ κάμινος οὐκ…"
meMA.Ode7C1T.text = "Νεανίας τρεῖς ἡ κάμινος οὐκ ἔφλεξε, γέννησιν προτυποῦσα τὴν σήν· τὸ γὰρ θεῖον πῦρ, σὲ μὴ φλέξαν ᾤκησεν ἐν σοί, καὶ πάντας ἐφώτισε βοᾶν· Εὐλογητὸς εἶ ὁ Θεός, ὁ τῶν Πατέρων ἡμῶν."

meMA.Ode8C11.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C11.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode8C11.greekmelody = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.greekname
meMA.Ode8C11.melody = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.name
meMA.Ode8C11.incipit = "Ὡραιότατος ὤφθης ψυχῇ καὶ σώματι…"
meMA.Ode8C11.text = "Ὡραιότατος ὤφθης ψυχῇ καὶ σώματι, τῶν βασάνων νιφάσι καταχωννύμενος, Μάρτυς ἀθλητά, οὐδαμῶς δὲ ἡττώμενος, μέλπεις τῷ Δεσπότῃ, Χριστῷ εἰς τοὺς αἰῶνας."

meMA.Ode8C12.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C12.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode8C12.melody = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.name
meMA.Ode8C12.incipit = "Ῥητορεύοντες ἅμα λόγον τὸν ἔνθεον…"
meMA.Ode8C12.text = "Ῥητορεύοντες ἅμα λόγον τὸν ἔνθεον, ἐξεφαύλισαν πόθῳ δόγμα τὸ ἄθεον, οἱ ἀδελφικῇ, ἀγχιστείᾳ συνδούμενοι, Μάρτυρες τοῦ πάντων Θεοῦ καὶ Βασιλέως."

meMA.Ode8C13.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C13.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode8C13.melody = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.name
meMA.Ode8C13.incipit = "Ἱερεῖα καὶ θεῖα ὁλοκαυτώματα, ἐθελόθυτοι…"
meMA.Ode8C13.text = "Ἱερεῖα καὶ θεῖα ὁλοκαυτώματα, ἐθελόθυτοι ἄρνες ἄμωμα σφάγια, κάρπωμα δεκτόν, τῷ Θεῷ προσηνέχθητε, ἐν ἐπουρανίῳ, τραπέζῃ Ἀθλοφόροι."

meMA.Ode8C14.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C14.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode8C14.melody = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.name
meMA.Ode8C14.incipit = "Διαυγῆ σε ἀστέρα φωτοειδέστατον, ἰαμάτων…"
meMA.Ode8C14.text = "Διαυγῆ σε ἀστέρα φωτοειδέστατον, ἰαμάτων ἀκτῖσι καταλαμπρύνοντα, πάντων τὰς ψυχάς, ἀθλοφόρε Εὐλάμπιε, πίστει ἐγνωκότες, ὑμνοῦμεν εἰς αἰῶνας."

meMA.Ode8C1T.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C1T.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode8C1T.melody = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.name
meMA.Ode8C1T.incipit = "Ἀπειρόγαμε Κορη χαῖρε πανάμωμε, ὁ…"
meMA.Ode8C1T.text = "Ἀπειρόγαμε Κορη χαῖρε πανάμωμε, ὁ λιμὴν τῶν ἐν ζάλῃ χαῖρε Θεόνυμφε, τῶν ἁμαρτωλῶν, χαῖρε τὸ ἱλαστήριον, χαῖρε ἡ τεκοῦσα, Θεὸν σεσαρκωμένον."

meMA.Ode8H.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8H.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode8H.greekmelody = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.greekname
meMA.Ode8H.melody = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.melody
meMA.Ode8H.incipit = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.incipit
meMA.Ode8H.text = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.text

meMA.Ode9C11.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C11.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode9C11.greekmelody = he.h.m6_gr_GR_cog.heHE.TheonAnthropois.greekname
meMA.Ode9C11.melody = he.h.m6_gr_GR_cog.heHE.TheonAnthropois.name
meMA.Ode9C11.incipit = "Ἰδεῖν τὴν δόξαν τοῦ Παντοκράτορος…"
meMA.Ode9C11.text = "Ἰδεῖν τὴν δόξαν τοῦ Παντοκράτορος, ἐπιποθῶν καὶ κάλλος δὲ αὐτοῦ τὸ ἀμήχανον, ἀδοξίαν τοῦ βίου παρέδραμες, ἄτιμον ὑπομείνας, θάνατον ἔνδοξε, δόξαν προξενοῦντά σοι ἀεί, Μάρτυς Εὐλάμπιε."

meMA.Ode9C12.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C12.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode9C12.melody = he.h.m6_gr_GR_cog.heHE.TheonAnthropois.name
meMA.Ode9C12.incipit = "Ὡς φῶς ὡς λύχνος ἀειλαμπέστατος…"
meMA.Ode9C12.text = "Ὡς φῶς ὡς λύχνος ἀειλαμπέστατος, τοῖς ἐν νυκτὶ τοῦ βίου καθωράθης Εὐλάμπιε, ἀγνωσίας τὸ σκότος ἐδίωξας, ἔλυσας παθημάτων τὴν ἀμαυρότητα, φέγγει ἰαμάτων, Ἀθλητὰ Θεομακάριστε."

meMA.Ode9C13.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C13.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode9C13.melody = he.h.m6_gr_GR_cog.heHE.TheonAnthropois.name
meMA.Ode9C13.incipit = "Στολαῖς βαφείσαις ὑμῶν ἐξ αἵματος…"
meMA.Ode9C13.text = "Στολαῖς βαφείσαις ὑμῶν ἐξ αἵματος, μαρτυρικοῦ ὡραίους ἑαυτοὺς περιστείλαντες, στεφηφόροι Κυρίῳ παρίστασθε, μέλποντες σὺν Ἀγγέλοις, Ἅγιος, Ἅγιος, Ἅγιος Τριάς, ἡ παντουργὸς καὶ παντοδύναμος."

meMA.Ode9C14.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C14.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode9C14.melody = he.h.m6_gr_GR_cog.heHE.TheonAnthropois.name
meMA.Ode9C14.incipit = "Ἡμῶν τῶν πίστει τὴν ἀεισέβαστον…"
meMA.Ode9C14.text = "Ἡμῶν τῶν πίστει τὴν ἀεισέβαστον, καὶ ἱερὰν καὶ πλήρη φωτισμοῦ καὶ λαμπρότητος, ἀθλοφόροι Μάρτυρες, τελούντων ὑμῶν, Μνήμην καὶ προσκυνούντων, πίστει τὰ λείψανα, μέμνησθε παντοίων πειρασμῶν, πάντας λυτρούμενοι."

meMA.Ode9C1T.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C1T.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode9C1T.melody = he.h.m6_gr_GR_cog.heHE.TheonAnthropois.name
