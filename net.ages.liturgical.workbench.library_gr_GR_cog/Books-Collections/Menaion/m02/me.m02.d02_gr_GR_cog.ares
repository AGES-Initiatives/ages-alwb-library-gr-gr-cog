meMA.C1.acrostic.title = "Ὁ Κανών,"
meMA.C1.acrostic.notice1 = "οὗ ἡ Ἀκροστιχίς·"
meMA.C1.acrostic.text = "Χριστὸν γεγηθὼς Πρέσβυς ἀγκαλίζεται."
