meMA.Ode3C1H.text = he.h.m2_gr_GR_cog.heHE.ExinthisenIErimos.text

meMA.Ode3C11.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C11.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode3C11.greekmelody = he.h.m2_gr_GR_cog.heHE.ExinthisenIErimos.greekname
meMA.Ode3C11.melody = he.h.m2_gr_GR_cog.heHE.ExinthisenIErimos.name
meMA.Ode3C11.incipit = "Ἀστέρα ἐν τῷ ὕψει σε…"
meMA.Ode3C11.text = "Ἀστέρα ἐν τῷ ὕψει σε, Ἐκκλησίας λάμποντα, ὁ φωτισμὸς ἀνέδειξε, τῶν ἐν σκότει Χριστὸς πανεύφημε, ἀσκητῶν ὡραιότης Πάτερ πάνσοφε"

meMA.Ode3C12.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C12.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode3C12.melody = he.h.m2_gr_GR_cog.heHE.ExinthisenIErimos.name
meMA.Ode3C12.incipit = "Σταυρῷ τὸν ὁμιλήσαντα, καὶ τὸν…"
meMA.Ode3C12.text = "Σταυρῷ τὸν ὁμιλήσαντα, καὶ τὸν κόσμον σώσαντα, Κασσιανὲ μιμούμενος, ἐσταυρώθης κόσμῳ καὶ πάθεσι, πανουργίας δαιμόνων τροπωσάμενος."

meMA.Ode3C13.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C13.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode3C13.melody = he.h.m2_gr_GR_cog.heHE.ExinthisenIErimos.name
meMA.Ode3C13.incipit = "Σοφίας πληρωθεῖσά σου, ἡ καρδία…"
meMA.Ode3C13.text = "Σοφίας πληρωθεῖσά σου, ἡ καρδία Πάνσοφε, διδασκαλίας ἄβυσσον, ἐν Ἁγίῳ ἔβλυσε Πνεύματι, Μοναστῶν τὰς ἀγέλας καταρδεύουσα."

meMA.Ode3C1T.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C1T.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode3C1T.melody = he.h.m2_gr_GR_cog.heHE.ExinthisenIErimos.name
meMA.Ode3C1T.incipit = "Ἰάτρευσον Πανάμωμε, τὴν ψυχήν μου…"
meMA.Ode3C1T.text = "Ἰάτρευσον Πανάμωμε, τὴν ψυχήν μου δέομαι, ἐξασθενοῦσαν πάθεσι, καὶ παντοίαις ἐπαναστάσεσι, τῶν κακίστων δαιμόνων μεσιτείαις σου."

meMA.Ode3Kathisma1.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3Kathisma1.greekmelody = he.a.m4_gr_GR_cog.heAU.OYpsotheisEnToStavro.greekname
meMA.Ode3Kathisma1.melody = he.a.m4_gr_GR_cog.heAU.OYpsotheisEnToStavro.name
meMA.Ode3Kathisma1.incipit = "Ὁσιωθεὶς σὺ τῷ Θεῷ ἀνετέθης…"
meMA.Ode3Kathisma1.text = "Ὁσιωθεὶς σὺ τῷ Θεῷ ἀνετέθης, καὶ λαμπρυνθεὶς σὺ τῶν καλῶν ταῖς ἰδέαις, Κασσιανὲ ὡς ἥλιος ἀπήστραψας, φέγγει τῶν ἐνθέων σου διδαχῶν τὰς καρδίας, πάντων τῶν τιμώντων σε, ἀενάως φωτίζων, ἀλλʼ ἐκτενῶς ἱκέτευε Χριστόν, ὑπὲρ τῶν πόθῳ, θερμῶς εὐφημούντων σε."

meMA.Ode3KathismaT.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3KathismaT.melody = he.a.m4_gr_GR_cog.heAU.OYpsotheisEnToStavro.name
meMA.Ode3KathismaT.incipit = oc.m4.d3_gr_GR_cog.ocVE.ApolTheotokionVM.incipit
meMA.Ode3KathismaT.text = oc.m4.d3_gr_GR_cog.ocVE.ApolTheotokionVM.text

meMA.Ode3KathismaSt.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3KathismaSt.melody = he.a.m4_gr_GR_cog.heAU.OYpsotheisEnToStavro.name
meMA.Ode3KathismaSt.incipit = "Τὸν ἐξ ἀνάρχου τοῦ Πατρὸς…"
meMA.Ode3KathismaSt.text = "Τὸν ἐξ ἀνάρχου τοῦ Πατρὸς γεννηθέντα, ἡ ἀνωδίνως σε σαρκὶ τετοκυῖα, ἐπὶ Σταυροῦ κρεμάμενον ὁρῶσά σε Χριστέ· Οἴμοι! ποθεινότατε, Ἰησοῦ ἀνεβόα· πῶς ὁ δοξαζόμενος, ὡς Θεὸς ὑπʼ Ἀγγέλων, ὑπὸ ἀνόμων νῦν βροτῶν Υἱέ, θέλων σταυροῦσαι; Ὑμνῶ σε μακρόθυμε."

meMA.Ode4C1H.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C1H.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode4C1H.greekmelody = he.h.m2_gr_GR_cog.heHE.ElilythasEkParthenou.greekname
