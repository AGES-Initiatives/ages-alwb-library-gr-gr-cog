meMA.Ode5C1H.incipit = he.h.m4_gr_GR_cog.heHE.OAnateilasToFos.incipit
meMA.Ode5C1H.text = he.h.m4_gr_GR_cog.heHE.OAnateilasToFos.text

meMA.Ode5C11.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C11.greekmelody = he.h.m4_gr_GR_cog.heHE.OAnateilasToFos.greekname
meMA.Ode5C11.melody = he.h.m4_gr_GR_cog.heHE.OAnateilasToFos.name
meMA.Ode5C11.incipit = "Ὁ ἀναδείξας ἡμῖν, τοῦ γενναίου…"
meMA.Ode5C11.text = "Ὁ ἀναδείξας ἡμῖν, τοῦ γενναίου τὴν μνήμην, ἡλίου λαμπροτέραν, δόξα σοι δόξα σοι, Ἰησοῦ Υἱὲ τοῦ Θεοῦ."

meMA.Ode5C12.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C12.melody = he.h.m4_gr_GR_cog.heHE.OAnateilasToFos.name
meMA.Ode5C12.incipit = "Ἱερωτάταις φρεσί, καὶ πισταῖς διανοίαις…"
meMA.Ode5C12.text = "Ἱερωτάταις φρεσί, καὶ πισταῖς διανοίαις, τὴν μνήμην ἀνυμνοῦμεν, σοῦ Μάρτυς ἔνδοξε, τὸν Χριστὸν μεγαλύνοντες."

meMA.Ode5C13.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C13.melody = he.h.m4_gr_GR_cog.heHE.OAnateilasToFos.name
meMA.Ode5C13.incipit = "Ξίφη καὶ θῆρας καὶ πῦρ…"
meMA.Ode5C13.text = "Ξίφη καὶ θῆρας καὶ πῦρ, καὶ τὸν θάνατον Μάρτυς, οὐδόλως ἑπτοήθης, κραυγάζων· Δόξα σοι Ἰησοῦ Υἱὲ τοῦ Θεοῦ."

meMA.Ode5C1T.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C1T.melody = he.h.m4_gr_GR_cog.heHE.OAnateilasToFos.name
meMA.Ode5C1T.incipit = "Τὴν τοῦ Ἀγγέλου φωνήν, ἀσιγήτως…"
meMA.Ode5C1T.text = "Τὴν τοῦ Ἀγγέλου φωνήν, ἀσιγήτως ἡ κτίσις, προσφέρει σοι Παρθένε· Χαίροις ἡ τέξασα, Ἰησοῦν τὸν Λόγον Θεοῦ."

meMA.Ode6C1H.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C1H.mode = miscellanea_gr_GR_cog.misc.Mode4
