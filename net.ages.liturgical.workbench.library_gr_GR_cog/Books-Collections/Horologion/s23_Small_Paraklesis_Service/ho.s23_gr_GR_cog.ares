hoCA.Apolytikion2.mode = miscellanea_gr_GR_cog.misc.Mode4
hoCA.Apolytikion2.melody = he.a.m4_gr_GR_cog.OYpsotheisEnToStavro.name
hoCA.Apolytikion2.incipit = "Οὐ σιωπήσωμέν ποτε…"
hoCA.Apolytikion2.text = "Οὐ σιωπήσωμέν ποτε, Θεοτόκε, τὰς δυναστείας σου λαλεῖν οἱ ἀνάξιοι· εἰμὴ γὰρ σὺ προΐστασο πρεσβεύουσα, τίς ἡμᾶς ἐῤῥύσατο ἐκ τοσούτων κινδύνων; Τίς δὲ διεφύλαξεν ἕως νῦν ἐλευθέρους; Οὐκ ἀποστῶμεν, Δέσποινα, ἐκ σοῦ· σοὺς γὰρ δούλους σῴζεις ἀεί, ἐκ παντοίων δεινῶν."

R05 = ""

hoCA.Ode1H.ode = miscellanea_gr_GR_cog.misc.Ode1
hoCA.Ode1H.mode = miscellanea_gr_GR_cog.misc.Mode8
hoCA.Ode1H.melody = miscellanea_gr_GR_cog.misc.Heirmos
