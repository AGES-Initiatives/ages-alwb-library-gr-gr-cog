meMA.Ode5C1H.incipit = he.h.m4_gr_GR_cog.heHE.AnateilonMoiKyrie.incipit
meMA.Ode5C1H.text = he.h.m4_gr_GR_cog.heHE.AnateilonMoiKyrie.text

meMA.Ode5C11.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C11.greekmelody = he.h.m4_gr_GR_cog.heHE.AnateilonMoiKyrie.greekname
meMA.Ode5C11.melody = he.h.m4_gr_GR_cog.heHE.AnateilonMoiKyrie.name
meMA.Ode5C11.incipit = "Ἀκοίμητον λαμπάδα σε, καὶ λύχνον…"
meMA.Ode5C11.text = "Ἀκοίμητον λαμπάδα σε, καὶ λύχνον μὴ σβεννύμενον, τὸ φῶς τὸ ἄχρονον ἀνέδειξε, τοῖς ζόφῳ κρατουμένοις, τῶν παθῶν Θεόδωρε, καὶ τῇ ἀμαυρότητι, τῶν πολλῶν ἁμαρτιῶν."

meMA.Ode5C12.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C12.melody = he.h.m4_gr_GR_cog.heHE.AnateilonMoiKyrie.name
meMA.Ode5C12.incipit = "Καλῶς παιδοτριβοῦντά σε, τὸν Μάρτυρα…"
meMA.Ode5C12.text = "Καλῶς παιδοτριβοῦντά σε, τὸν Μάρτυρα Γεώργιον, ἐξ ἁπαλῶν ὀνύχων ἔσχηκας, καὶ τούτου ἐμιμήσω, τὸ ἐν πᾶσι πρόθυμον, καὶ τὴν πρὸς τὸν Κύριον, Πάτερ Ὅσιε στοργήν."

meMA.Ode5C13.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C13.melody = he.h.m4_gr_GR_cog.heHE.AnateilonMoiKyrie.name
meMA.Ode5C13.incipit = "Ἀνόθευτον ἐγκράτειαν, ὑπομονὴν ἀσύγκριτον, πίστιν…"
meMA.Ode5C13.text = "Ἀνόθευτον ἐγκράτειαν, ὑπομονὴν ἀσύγκριτον, πίστιν ἐλπίδα καὶ ταπείνωσιν, ἔσχες ὑπὲρ ἄνθρωπον· ὅθεν ὑπερβάλλουσαν, χάριν εὗρες ἐκ Θεοῦ."

meMA.Ode5C1T.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C1T.melody = he.h.m4_gr_GR_cog.heHE.AnateilonMoiKyrie.name
meMA.Ode5C1T.incipit = "Ῥανίσι τοῦ ἐλέους σου, κατάρδευσον…"
meMA.Ode5C1T.text = "Ῥανίσι τοῦ ἐλέους σου, κατάρδευσον Πανάχραντε, τὴν ἐκτακεῖσάν μου διάνοιαν, φλογμῷ τῆς ἁμαρτίας, καὶ τὸν ἐσβεσμένον μου, τῆς καρδίας ἄναψον, λύχνον πύλη τοῦ φωτός."

meMA.Ode6C1H.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C1H.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C1H.greekmelody = he.h.m4_gr_GR_cog.heHE.ZaliMeLogismon.greekname
meMA.Ode6C1H.melody = he.h.m4_gr_GR_cog.heHE.ZaliMeLogismon.melody
meMA.Ode6C1H.incipit = he.h.m4_gr_GR_cog.heHE.ZaliMeLogismon.incipit
meMA.Ode6C1H.text = he.h.m4_gr_GR_cog.heHE.ZaliMeLogismon.text

meMA.Ode6C11.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C11.greekmelody = he.h.m4_gr_GR_cog.heHE.ZaliMeLogismon.greekname
meMA.Ode6C11.melody = he.h.m4_gr_GR_cog.heHE.ZaliMeLogismon.name
meMA.Ode6C11.incipit = "Μέριμναν τῶν γηΐνων ἀπεκρούσω, ὅλον…"
meMA.Ode6C11.text = "Μέριμναν τῶν γηΐνων ἀπεκρούσω, ὅλον δὲ ἐκένωσας, τὸν πόθον πρὸς τὸν Δεσπότην, ἐνηδόμενος τούτου καλλοναῖς, καὶ ταῖς ἀναβλυζούσαις, Πάτερ ἐκεῖθεν ἐλλάμψεσιν."

meMA.Ode6C12.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C12.melody = he.h.m4_gr_GR_cog.heHE.ZaliMeLogismon.name
meMA.Ode6C12.incipit = "Ἔλυσας παγετὸν τῆς ἁμαρτίας, κρύει…"
meMA.Ode6C12.text = "Ἔλυσας παγετὸν τῆς ἁμαρτίας, κρύει πιεζόμενος, κλωβοῦ ἱστάμενος μέσον, καὶ ὡς ἄσαρκος Πάτερ, προσβολάς, ὑπήνεγκας ἀέρων, θείᾳ θαλπόμενος χάριτι."

meMA.Ode6C13.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C13.melody = he.h.m4_gr_GR_cog.heHE.ZaliMeLogismon.name
meMA.Ode6C13.incipit = "Λάρνακι ἱερᾷ σου προσιόντες, ὀσμῆς…"
meMA.Ode6C13.text = "Λάρνακι ἱερᾷ σου προσιόντες, ὀσμῆς θείας γνώσεως, πληρούμεθα Ἱεράρχα, δυσωδίας λυτρούμενοι παθῶν, οἱ πόθῳ σε τιμῶντες, θαυματουργὲ Πάτερ Ὅσιε."

meMA.Ode6C1T.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C1T.melody = he.h.m4_gr_GR_cog.heHE.ZaliMeLogismon.name
meMA.Ode6C1T.incipit = "Παθῶν με τρικυμία συνταράσσει, βυθὸς…"
meMA.Ode6C1T.text = "Παθῶν με τρικυμία συνταράσσει, βυθὸς ἀπογνώσεως, χειμάζει μου τὴν καρδίαν, κυβερνήτην κυήσασα Χριστόν, Παρθένε καὶ Σωτῆρα, ῥῦσαι καὶ σῶσόν με δέομαι."

meMA.Kontakion1.mode = miscellanea_gr_GR_cog.misc.Mode3
meMA.Kontakion1.greekmelody = he.a.m3_gr_GR_cog.heAU.IParthenosSimeron.greekname
meMA.Kontakion1.melody = he.a.m3_gr_GR_cog.heAU.IParthenosSimeron.name
meMA.Kontakion1.incipit = "Ὡς πυρίνῳ ἅρματι, ταῖς ἀρεταῖς…"
meMA.Kontakion1.text = "Ὡς πυρίνῳ ἅρματι, ταῖς ἀρεταῖς Θεοφόρε, ἐπιβὰς ἀνέδραμες, εἰς οὐρανίους οἰκήσεις, Ἄγγελος μετὰ ἀνθρώπων συμβιοτεύων, ἄνθρωπος σὺν τοῖς Ἀγγέλοις περιχορεύων· διὰ τοῦτο ἀνεδείχθης, θαυμάτων θεῖον, δοχεῖον Θεόδωρε."

meMA.Ode7C1H.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C1H.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C1H.greekmelody = he.h.m4_gr_GR_cog.heHE.TonEnKaminoTouPyros.greekname
meMA.Ode7C1H.melody = he.h.m4_gr_GR_cog.heHE.TonEnKaminoTouPyros.melody
meMA.Ode7C1H.incipit = he.h.m4_gr_GR_cog.heHE.TonEnKaminoTouPyros.incipit
meMA.Ode7C1H.text = he.h.m4_gr_GR_cog.heHE.TonEnKaminoTouPyros.text

meMA.Ode7C11.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C11.greekmelody = he.h.m4_gr_GR_cog.heHE.TonEnKaminoTouPyros.greekname
meMA.Ode7C11.melody = he.h.m4_gr_GR_cog.heHE.TonEnKaminoTouPyros.name
meMA.Ode7C11.incipit = "Ὡς ἀψευδὴς Ἱεράρχης, θυσίαν ἀναίμακτον…"
meMA.Ode7C11.text = "Ὡς ἀψευδὴς Ἱεράρχης, θυσίαν ἀναίμακτον, προσήγαγες ἐν Πνεύματι, καὶ παθῶν νεκρότητι, θῦμα εὐῶδες Παμμάκαρ, σεαυτὸν τῷ Δεσπότῃ, προσήνεγκας ἐν πίστει."

meMA.Ode7C12.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C12.melody = he.h.m4_gr_GR_cog.heHE.TonEnKaminoTouPyros.name
meMA.Ode7C12.incipit = "Πῦρ ἡδονῶν ἐγκρατείας, ἱδρῶσι κατέσβεσας…"
meMA.Ode7C12.text = "Πῦρ ἡδονῶν ἐγκρατείας, ἱδρῶσι κατέσβεσας, αὐχμόν τε γῆς ἀνέστειλας, ὄμβρων ἐπικλύσεσι θαυματουργέ, ὡς Ἠλίας οὐρανοὺς κεκλεισμένους, εὐχαῖς σου ὑπανοίγων."

meMA.Ode7C13.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C13.melody = he.h.m4_gr_GR_cog.heHE.TonEnKaminoTouPyros.name
meMA.Ode7C13.incipit = "Ἁγιασθεὶς ἀπὸ βρέφους, εὐχῆς γενναιότητι…"
meMA.Ode7C13.text = "Ἁγιασθεὶς ἀπὸ βρέφους, εὐχῆς γενναιότητι στειρευούσας ἐποίησας, γαστέρας πολύπαιδας, τυφλοῖς παρέσχες τὸ βλέπειν, καὶ χωλοῖς εὐδρομίαν, Θεόδωρε τρισμάκαρ."

meMA.Ode7C1T.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C1T.melody = he.h.m4_gr_GR_cog.heHE.TonEnKaminoTouPyros.name
meMA.Ode7C1T.incipit = "Τὴν καλλονὴν Ἰακὼβ σε, Θεὸς…"
meMA.Ode7C1T.text = "Τὴν καλλονὴν Ἰακὼβ σε, Θεὸς ἐξελέξατο, καὶ ἐν σοὶ κατεσκήνωσε, καὶ σὲ διετήρησε, καὶ μετὰ τόκον Παρθένον, ὥσπερ ἦς πρὸ τοῦ τόκου, Παρθένε Θεοτόκε."

meMA.Ode8C1H.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C1H.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C1H.greekmelody = he.h.m4_gr_GR_cog.heHE.GiKaiPantaTaEnAfti.greekname
meMA.Ode8C1H.melody = he.h.m4_gr_GR_cog.heHE.GiKaiPantaTaEnAfti.melody
meMA.Ode8C1H.incipit = he.h.m4_gr_GR_cog.heHE.GiKaiPantaTaEnAfti.incipit
meMA.Ode8C1H.text = he.h.m4_gr_GR_cog.heHE.GiKaiPantaTaEnAfti.text

meMA.Ode8C11.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C11.greekmelody = he.h.m4_gr_GR_cog.heHE.GiKaiPantaTaEnAfti.greekname
meMA.Ode8C11.melody = he.h.m4_gr_GR_cog.heHE.GiKaiPantaTaEnAfti.name
meMA.Ode8C11.incipit = "Ἐπὶ πέτραν ὑπομονῆς, ἐρείσας σου…"
meMA.Ode8C11.text = "Ἐπὶ πέτραν ὑπομονῆς, ἐρείσας σου τὰς βάσεις τῆς ψυχῆς, οὐ παρετράπης προσβολαῖς ἐναντίαις, οὐκ ἐσαλεύθης, ἐχθρῶν ἐπηρείαις, ἀλλὰ ὕψος πρὸς θεῖον ἀνέδραμες, ὑπὲρ τῶν πίστει τιμώντων σε πρεσβεύων."

meMA.Ode8C12.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C12.melody = he.h.m4_gr_GR_cog.heHE.GiKaiPantaTaEnAfti.name
meMA.Ode8C12.incipit = "Ῥεῖθρον πρόεισιν ἐκ σοροῦ, ἰάσεων…"
meMA.Ode8C12.text = "Ῥεῖθρον πρόεισιν ἐκ σοροῦ, ἰάσεων τῆς σῆς θαυματουργέ, καὶ ἀποπλύνει μολυσμοὺς παθημάτων, καὶ σηπεδόνας δεινῶν ἐκκαθαίρει, καὶ πιστῶν καταρδεύει Θεόδωρε, πάντων τὰς καρδίας, τῶν πόθῳ σε τιμώντων."

meMA.Ode8C13.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C13.melody = he.h.m4_gr_GR_cog.heHE.GiKaiPantaTaEnAfti.name
meMA.Ode8C13.incipit = "Ὁ τῶν δώρων τῶν θεϊκῶν…"
meMA.Ode8C13.text = "Ὁ τῶν δώρων τῶν θεϊκῶν, ἐπώνυμος, καὶ μύστης τοῦ Χριστοῦ, τῶν Ἀσκητῶν ἡ καλλονή, Ἱερέων ὡραιότης, θαυμάτων ἡ βρύσις, ἡ σεπτὴ μυροθήκη τοῦ Πνεύματος, νῦν ἀνευφημείσθω, Θεόδωρος ὁ μέγας."

meMA.Ode8C1T.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C1T.melody = he.h.m4_gr_GR_cog.heHE.GiKaiPantaTaEnAfti.name
meMA.Ode8C1T.incipit = "Ἱεραὶ σεπτῶν Προφητῶν, κηρύττουσι φωναὶ…"
meMA.Ode8C1T.text = "Ἱεραὶ σεπτῶν Προφητῶν, κηρύττουσι φωναὶ συμβολικῶς, πύλην καὶ ὄρος καὶ σκηνήν, καὶ ἁγίαν φωτὸς νεφέλην, ἐξ ἧς τοῖς ἐν σκότει καὶ σκιᾷ καθημένοις ἀνέτειλεν, Ἥλιος Παρθένε, Χριστὸς ὁ φωτοδότης."

meMA.Ode9C1H.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C1H.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode9C1H.greekmelody = he.h.m4_gr_GR_cog.heHE.OtiEpoiiseMoiMegaleia.greekname
meMA.Ode9C1H.melody = he.h.m4_gr_GR_cog.heHE.OtiEpoiiseMoiMegaleia.melody
meMA.Ode9C1H.incipit = he.h.m4_gr_GR_cog.heHE.OtiEpoiiseMoiMegaleia.incipit
meMA.Ode9C1H.text = he.h.m4_gr_GR_cog.heHE.OtiEpoiiseMoiMegaleia.text

meMA.Ode9C11.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode9C11.greekmelody = he.h.m4_gr_GR_cog.heHE.OtiEpoiiseMoiMegaleia.greekname
meMA.Ode9C11.melody = he.h.m4_gr_GR_cog.heHE.OtiEpoiiseMoiMegaleia.name
meMA.Ode9C11.incipit = "Ὡς ὄρθρος, ὡς ἡμέρα διαυγής…"
meMA.Ode9C11.text = "Ὡς ὄρθρος, ὡς ἡμέρα διαυγής, ἡ σὴ ἑορτή, ἐπέφανε φωτίζουσα ἡμῶν, τὰς καρδίας τῶν πιστῶς ἀνευφημούντων, τοὺς σεπτοὺς πόνους σου Πάνσοφε."

meMA.Ode9C12.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode9C12.melody = he.h.m4_gr_GR_cog.heHE.OtiEpoiiseMoiMegaleia.name
meMA.Ode9C12.incipit = "Συνόμιλος, Ἀγγέλων, Ἀποστόλων καὶ Ἀθλητῶν…"
meMA.Ode9C12.text = "Συνόμιλος, Ἀγγέλων, Ἀποστόλων καὶ Ἀθλητῶν, Ὁσίων καὶ Δικαίων γεγονώς, Πάτερ καὶ Ἱεραρχῶν, ἐν οὐρανοῖς ὑπὲρ ἡμῶν, Χριστῷ πρεσβεύεις ἀεί."

meMA.Ode9C13.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode9C13.melody = he.h.m4_gr_GR_cog.heHE.OtiEpoiiseMoiMegaleia.name
meMA.Ode9C13.incipit = "Ἡ θήκη τῶν ἁγίων σου…"
meMA.Ode9C13.text = "Ἡ θήκη τῶν ἁγίων σου, λειψάνων Πάτερ σοφέ, ὡς ἄλλη κολυμβήθρα Σιλωάμ, ἐγνωρίσθη τοῖς πιστοῖς· ᾗ προσιόντες, τὰς ψυχὰς ἁγιαζόμεθα."

meMA.Ode9C1T.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode9C1T.melody = he.h.m4_gr_GR_cog.heHE.OtiEpoiiseMoiMegaleia.name
meMA.Ode9C1T.incipit = "Φρικτή σου ἡ λοχεία, Θεοτόκε…"
meMA.Ode9C1T.text = "Φρικτή σου ἡ λοχεία, Θεοτόκε Μήτηρ Θεοῦ· διὸ σὲ μακαρίζομεν ἀεί, καὶ δοξάζομεν πιστῶς, αἱ γενεαὶ τῶν γενεῶν, εἰς τοὺς αἰῶνας. Ἀμήν."

