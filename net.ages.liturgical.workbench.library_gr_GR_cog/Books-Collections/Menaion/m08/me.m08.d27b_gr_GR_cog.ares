meLI.CommunionHymn.text = "" // for media

meMegalynarion.text = "Χαίροις ὦ Φανούριε ἀθλητά, ὁ πᾶσι παρέχων, τὰ αἰτήματα συμπαθῶς· χαίροις εὐσεβούντων, ὁ μέγας ἀντιλήπτωρ, καὶ πάσης Ἐκκλησίας, θεῖον ἀγλάισμα."

