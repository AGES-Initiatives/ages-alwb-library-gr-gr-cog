ocMA.Kathisma13.mode = miscellanea_gr_GR_cog.misc.Mode3
ocMA.Kathisma13.greekmelody = he.a.m3_gr_GR_cog.heAU.TinOraiotita.greekname
ocMA.Kathisma13.melody = he.a.m3_gr_GR_cog.heAU.TinOraiotita.name
ocMA.Kathisma13.incipit = "Χαρὰν ἡ σύμπασα κτίσις…"
ocMA.Kathisma13.text = "Χαρὰν ἡ σύμπασα, κτίσις ὁρῶσά σε, χαίρει Μακρόθυμε, Σταυρῷ ὑψούμενον, ἡ πανακήρατος Ἁγνή, ἐβόα καὶ πανάμωμος· λύτρωσιν γὰρ εὕρατο, διὰ σοῦ τέκνον φίλτατον, ξύλῳ γὰρ ἰάτρευσας, τὴν τοῦ ξύλου κατάκρισιν· ἐγὼ δὲ νῦν τὰ σπλάγχνα δονοῦμαι, μὴ φέρουσα ὁρᾷν σε θανατούμενον."

ocMA.Kathisma21.mode = miscellanea_gr_GR_cog.misc.Mode3
ocMA.Kathisma21.greekmelody = he.a.m3_gr_GR_cog.heAU.TinOraiotita.greekname
ocMA.Kathisma21.melody = he.a.m3_gr_GR_cog.heAU.TinOraiotita.name
ocMA.Kathisma21.incipit = oc.m3.d4_gr_GR_cog.ocMA.Kathisma22.incipit
ocMA.Kathisma21.text = oc.m3.d4_gr_GR_cog.ocMA.Kathisma22.text

ocMA.Kathisma22.mode = miscellanea_gr_GR_cog.misc.Mode3
ocMA.Kathisma22.greekmelody = he.a.m3_gr_GR_cog.heAU.TinOraiotita.greekname
ocMA.Kathisma22.melody = he.a.m3_gr_GR_cog.heAU.TinOraiotita.name
ocMA.Kathisma22.incipit = "Σταυρὸν καὶ θάνατον…"
ocMA.Kathisma22.text = "Σταυρὸν καὶ θάνατον, παθεῖν ἑλόμενος, μέσον τῆς κτίσεως, ταῦτα ὑπέμεινας, ὅτε ηὐδόκησας Σωτήρ, τὸ σῶμά σου προσηλῶσαι, τότε καὶ ὁ ἥλιος, τὰς ἀκτῖνας ἀπέκρυψε, ταῦτα καὶ Λῃστὴς ὁρῶν, Θεόν σε ὡμολόγησε, βοῶν σοι εὐλαβῶς· Μνήσθητί μου, καὶ ἔλαβε πιστεύσας τὸν Παράδεισον."

ocMA.Kathisma23.mode = miscellanea_gr_GR_cog.misc.Mode3
ocMA.Kathisma23.greekmelody = he.a.m3_gr_GR_cog.heAU.TinOraiotita.greekname
ocMA.Kathisma23.melody = he.a.m3_gr_GR_cog.heAU.TinOraiotita.name
ocMA.Kathisma23.incipit = "Ἡ ἀπειρόζυγος…"
ocMA.Kathisma23.text = "Ἡ ἀπειρόζυγος, καὶ θεία δάμαλις, κατανοήσασα, μόσχον τὸν ἴδιον, ἐθελουσίως ἐν Σταυρῷ, τὸ πάθος ὑπομένοντα, Οἴμοι τέκνον! ἔλεγε, τί τὸ ξένον μυστήριον; πῶς ὁ ὢν ἀθάνατος, κατεδέξω τὸν θάνατον; ὑμνῶ σου τὸ μακρόθυμον Λόγε, δοξάζω τὸ ἀμέτρητον ἔλεος."

ocMA.Kathisma31.mode = miscellanea_gr_GR_cog.misc.Mode3
ocMA.Kathisma31.melody = ""
ocMA.Kathisma31.incipit = oc.m3.d4_gr_GR_cog.ocMA.Kathisma21.incipit
ocMA.Kathisma31.text = oc.m3.d4_gr_GR_cog.ocMA.Kathisma21.text

ocMA.Kathisma32.mode = miscellanea_gr_GR_cog.misc.Mode3
ocMA.Kathisma32.melody = ""
ocMA.Kathisma32.incipit = "Τὸ εὔψυχον τῆς καρτερίας ὑμῶν…"
ocMA.Kathisma32.text = "Τὸ εὔψυχον τῆς καρτερίας ὑμῶν, ἐνίκησε τὰ μηχανήματα τοῦ ἀρχεκάκου ἐχθροῦ, Ἀθλοφόροι πανεύφημοι· διὰ τοῦτο τῆς αἰωνίου κατηξιώθητε μακαριότητος. Ἀλλὰ πρεσβεύσατε τῷ Κυρίῳ τοῦ φιλοχρίστου λαοῦ σῶσαι τὸ ποίμνιον, Μάρτυρες ὑπάρχοντες τῆς ἀληθείας."

ocMA.Kathisma33.mode = miscellanea_gr_GR_cog.misc.Mode3
ocMA.Kathisma33.greekmelody = he.a.m3_gr_GR_cog.heAU.TinOraiotita.greekname
ocMA.Kathisma33.melody = he.a.m3_gr_GR_cog.heAU.TinOraiotita.name
ocMA.Kathisma33.incipit = "Τὰ ἐπουράνια…"
ocMA.Kathisma33.text = "Τὰ ἐπουράνια, ἰδόντα ἔφριξαν, καὶ τὰ ἐπίγεια, φόβῳ ἐτρόμαξαν, ὅτε ὑψώθης ἐν Σταυρῷ, ἑκὼν ὁ ἀναμάρτητος· ἥλιος ἐσκότασε, καὶ σελήνη ἠμαύρωται· τότε καὶ ἡ Μήτηρ σου, θρηνῳδοῦσα ἐφθέγγετο· Υἱέ μου, τί τὸ ὅραμα τοῦτο; πῶς ὑπὲρ πάντων μόνος πέπονθας;"

ocMA.C1.acrostic.title = "Κανὼν Σταυρώσιμος,"
ocMA.C1.acrostic.notice1 = "οὗ ἡ ἀκροστιχίς·"
ocMA.C1.acrostic.text = "Καύχημα πιστῶν Σταυρός ἐστι καὶ κλέος. Ἰωσήφ."

ocMA.CA.notice = ""

