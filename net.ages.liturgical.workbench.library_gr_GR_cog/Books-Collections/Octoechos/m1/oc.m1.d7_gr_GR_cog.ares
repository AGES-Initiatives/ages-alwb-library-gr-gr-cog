ocMA.Lauds3.mode = miscellanea_gr_GR_cog.misc.Mode1
ocMA.Lauds3.melody = ""
ocMA.Lauds3.incipit = oc.m1.d4_gr_GR_cog.ocMA.Aposticha3.incipit
ocMA.Lauds3.text = oc.m1.d4_gr_GR_cog.ocMA.Aposticha3.text

ocMA.Lauds4.mode = miscellanea_gr_GR_cog.misc.Mode1
ocMA.Lauds4.melody = ""
ocMA.Lauds4.incipit = oc.m1.d5_gr_GR_cog.ocMA.Aposticha3.incipit
ocMA.Lauds4.text = oc.m1.d5_gr_GR_cog.ocMA.Aposticha3.text

ocMA.Lauds5.mode = miscellanea_gr_GR_cog.misc.Mode1
ocMA.Lauds5.melody = ""
ocMA.Lauds5.incipit = "Ἔργῳ Σωτήρ μου δεικνύς…"
ocMA.Lauds5.text = "Ἔργῳ Σωτήρ μου δεικνύς, ὅτι σὺ εἶ ἡ πάντων ἀνάστασις, λόγῳ Λόγε Λάζαρον ἐκ νεκρῶν ἐξανέστησας. Τότε μοχλοὶ συνετρίβησαν, πύλαι δὲ ᾅδου συνεταράχθησαν, τότε ὕπνος ὁ τῶν ἀνθρώπων θάνατος ἀπεδείκνυτο, ἀλλʼ ὁ εἰς τὸ σῶσαι τὸ πλάσμα σου, καὶ οὐκ εἰς τὸ κρῖναι παραγενόμενος, οὓς ἐξελέξω ἀνάπαυσον, ὡς φιλάνθρωπος."

ocMA.LaudsTheotokion.mode = miscellanea_gr_GR_cog.misc.Mode1
ocMA.LaudsTheotokion.melody = ""
ocMA.LaudsTheotokion.incipit = "Χαῖρε Μαρία Θεοτόκε…"
ocMA.LaudsTheotokion.text = "Χαῖρε Μαρία Θεοτόκε, ὅτι ἔτεκες τὸν Βασιλέα, τὸν Σωτῆρα καὶ φωστῆρα πάντων τῶν αἰώνων."

ocMA.LaudsRubric1.text = "Εἰ δέ ἐστιν, Ἀλληλούϊα, λέγομεν τὰ Μαρτυρικὰ εἰς τοὺς Αἴνους, Ἀπόστιχα δὲ τὰ ἑπόμενα Προσόμοια τοῦ κυρίου Θεοφάνους."
ocMA.LaudsRubric2.text = "ὧν, καὶ τῶν ἐν τοῖς ἄλλοις ἤχοις τοιούτων Προσομοίων, ἡ ἀκροστιχίς ἐστι· Σούς, Χριστέ, δούλους γράψον ἐν ζώντων βίβλῳ."

ocMA.Aposticha1.poet = poets_gr_GR_cog.po.Theofanous
ocMA.Aposticha1.mode = miscellanea_gr_GR_cog.misc.Mode1
ocMA.Aposticha1.greekmelody = he.a.m1_gr_GR_cog.heAU.PaneffimoiMartyres.greekname
ocMA.Aposticha1.melody = he.a.m1_gr_GR_cog.heAU.PaneffimoiMartyres.name
ocMA.Aposticha1.incipit = "Σοῦ Σῶτερ δεόμεθα…"
ocMA.Aposticha1.text = "Σοῦ Σῶτερ δεόμεθα τῆς σῆς, γλυκείας μεθέξεως, τοὺς μεταστάντας ἀξίωσον, καὶ κατασκήνωσον, ἐν σκηναῖς Δικαίων, ἐν μοναῖς Ἁγίων σου, καὶ ἐν ἐπουρανίοις σκηνώμασι, τῇ εὐσπλαγχνίᾳ σου, παρορῶν τὰ πλημμελήματα, καὶ παρέχων αὐτοῖς τὴν συγχώρησιν."

ocMA.Aposticha2.mode = miscellanea_gr_GR_cog.misc.Mode1
ocMA.Aposticha2.greekmelody = he.a.m1_gr_GR_cog.heAU.PaneffimoiMartyres.greekname
ocMA.Aposticha2.melody = he.a.m1_gr_GR_cog.heAU.PaneffimoiMartyres.name
ocMA.Aposticha2.incipit = "Οὐδεὶς ἀναμάρτητος οὐδείς…"
ocMA.Aposticha2.text = "Οὐδεὶς ἀναμάρτητος οὐδείς, τῶν ἀνθρώπων γέγονεν, εἰμὴ σὺ μόνε Ἀθάνατε· διὸ τοὺς δούλους σου, ὡς Θεὸς οἰκτίρμων, ἐν φωτὶ κατάταξον, σὺν ταῖς χοροστασίαις Ἀγγέλων σου, τῇ εὐσπλαγχνίᾳ σου, παρορῶν τὰ ἀνομήματα, καὶ παρέχων αὐτοῖς τὴν συγχώρησιν."

ocMA.Aposticha3.mode = miscellanea_gr_GR_cog.misc.Mode1
ocMA.Aposticha3.greekmelody = he.a.m1_gr_GR_cog.heAU.PaneffimoiMartyres.greekname
ocMA.Aposticha3.melody = he.a.m1_gr_GR_cog.heAU.PaneffimoiMartyres.name
ocMA.Aposticha3.incipit = "Ὑπὲρ τὰ ὁρώμενα τὰ σά…"
ocMA.Aposticha3.text = "Ὑπὲρ τὰ ὁρώμενα τὰ σά, Σῶτερ ἐπαγγέλματα! ἃ ὀφθαλμὸς οὐ τεθέαται, καὶ οὓς οὐκ ἤκουσε, καὶ ἐπὶ καρδίαν, οὐκ ἀνέβη πώποτε, ἀνθρώπου, δυσωποῦμέν σε Δέσποτα, τυχεῖν εὐδόκησον, τοὺς πρὸς σὲ μεταχωρήσαντας, καὶ παράσχου ζωὴν τὴν αἰώνιον."

ocMA.Aposticha4.mode = miscellanea_gr_GR_cog.misc.Mode1
ocMA.Aposticha4.greekmelody = he.a.m1_gr_GR_cog.heAU.PaneffimoiMartyres.greekname
ocMA.Aposticha4.melody = he.a.m1_gr_GR_cog.heAU.PaneffimoiMartyres.name
ocMA.Aposticha4.incipit = "Σταυρῷ σου γηθόμενοι Σωτήρ…"
ocMA.Aposticha4.text = "Σταυρῷ σου γηθόμενοι Σωτήρ, θαῤῥοῦντες οἱ δοῦλοί σου, πρὸς σὲ μετέστησαν Κύριε, ὃν νῦν ἀντίλυτρον, τῶν αὐτῶν πταισμάτων, ὡς δεσπότης δώρησαι, ἐφ οὗ σου τὸ ζωήῤῥυτον ἔχεας, αἷμα καὶ τίμιον, καὶ τῆς δόξης ἀνεσπέρου σου, ἀπολαύειν τούτους καταξίωσον."

ocMA.AposTheotokion.mode = miscellanea_gr_GR_cog.misc.Mode1
ocMA.AposTheotokion.greekmelody = he.a.m1_gr_GR_cog.heAU.PaneffimoiMartyres.greekname
ocMA.AposTheotokion.melody = he.a.m1_gr_GR_cog.heAU.PaneffimoiMartyres.name
ocMA.AposTheotokion.incipit = "Χριστὸν ἐκδυσώπησον…"
ocMA.AposTheotokion.text = "Χριστὸν ἐκδυσώπησον τὸν σόν, τόκον Μητροπάρθενε, τὴν τῶν πταισμάτων συγχώρησιν, δοῦναι τοῖς δούλοις σου, τοῖς σὲ Θεοτόκον, εὐσεβῶς κηρύξασι, καὶ λόγῳ ἀληθεῖ δογματίσασι, καὶ τῆς λαμπρότητος, τῶν ἁγίων καὶ φαιδρότητος, ἀξιῶσαι ἐν τῇ βασιλείᾳ αὐτοῦ."

ocMA.ApolTheotokion.mode = miscellanea_gr_GR_cog.misc.Mode1
ocMA.ApolTheotokion.melody = ""
ocMA.ApolTheotokion.incipit = oc.m1.d3_gr_GR_cog.ocMA.ApolTheotokion.incipit
ocMA.ApolTheotokion.text = oc.m1.d3_gr_GR_cog.ocMA.ApolTheotokion.text

ocLI.Beatitudes1.mode = miscellanea_gr_GR_cog.misc.Mode1
ocLI.Beatitudes1.melody = he.h.m1_gr_GR_cog.heHE.StereothitoIKardiaMou.name
ocLI.Beatitudes1.incipit = oc.m1.d1_gr_GR_cog.ocLI.Beatitudes1.incipit
ocLI.Beatitudes1.text = oc.m1.d1_gr_GR_cog.ocLI.Beatitudes1.text

ocLI.Beatitudes2.mode = miscellanea_gr_GR_cog.misc.Mode1
ocLI.Beatitudes2.melody = he.h.m1_gr_GR_cog.heHE.StereothitoIKardiaMou.name
ocLI.Beatitudes2.incipit = "Ἀθλητῶν πληθὺς ἀμέτρητος, Ἀρχιερέων καὶ…"
ocLI.Beatitudes2.text = "Ἀθλητῶν πληθὺς ἀμέτρητος, Ἀρχιερέων καὶ σοφῶν γυναικῶν, καὶ Προφητῶν πανευκλεῶν, δυσωπεῖ σε Ἰησοῦ ὁ Θεός, δίδου πᾶσιν ἄφεσιν, τῶν πταισμάτων, ὡς μόνος φιλάνθρωπος."

ocLI.Beatitudes3.mode = miscellanea_gr_GR_cog.misc.Mode1
ocLI.Beatitudes3.melody = he.h.m1_gr_GR_cog.heHE.StereothitoIKardiaMou.name
ocLI.Beatitudes3.incipit = "Οἱ τὸν δρόμον διανύσαντες, τὸν…"
ocLI.Beatitudes3.text = "Οἱ τὸν δρόμον διανύσαντες, τὸν ἱερώτατον Χριστοῦ Ἀσκηταί, σὺν Ἱεράρχαις ἱεροῖς, καὶ Προφήταις ἠξιώθητε, πόλιν τὴν οὐράνιον, σὺν Ἀγγέλοις, οἰκεῖν εὐφραινόμενοι."

ocLI.Beatitudes4.mode = miscellanea_gr_GR_cog.misc.Mode1
ocLI.Beatitudes4.melody = he.h.m1_gr_GR_cog.heHE.StereothitoIKardiaMou.name
ocLI.Beatitudes4.incipit = "Ἐν φωτὶ Χριστὲ κατάταξον, τῷ…"
ocLI.Beatitudes4.text = "Ἐν φωτὶ Χριστὲ κατάταξον, τῷ ἀνεσπέρῳ, οὓς μετέστησας, τὰ παραπτώματα αὐτῶν παραβλέψας, ὡς οἰκτίρμων Θεός, ὅπως σοῦ δοξάζωμεν, Εὐεργέτα, τὸ πλούσιον ἔλεος."

ocLI.BeatitudesTr.mode = miscellanea_gr_GR_cog.misc.Mode1
ocLI.BeatitudesTr.melody = he.h.m1_gr_GR_cog.heHE.StereothitoIKardiaMou.name
ocLI.BeatitudesTr.incipit = "Σὺν Πατρὶ τῷ προανάρχῳ σου…"
ocLI.BeatitudesTr.text = "Σὺν Πατρὶ τῷ προανάρχῳ σου, καὶ τῷ ἁγίῳ Πνεύματι Χριστὲ ὁ Θεός, πάντες δοξάζομεν τὴν σήν, φιλανθρωπίαν ἀνακράζοντες· Μνήσθητι τῶν δούλων σου, ἐν τῇ ὥρᾳ τῆς κρίσεως Κύριε."

ocLI.BeatitudesT.mode = miscellanea_gr_GR_cog.misc.Mode1
ocLI.BeatitudesT.melody = he.h.m1_gr_GR_cog.heHE.StereothitoIKardiaMou.name
ocLI.BeatitudesT.incipit = "Ὡς παλάτιον εὐρύχωρον, καὶ θρόνον…"
ocLI.BeatitudesT.text = "Ὡς παλάτιον εὐρύχωρον, καὶ θρόνον δόξης καὶ νεφέλην φωτός, ὑμνολογοῦμέν σε Ἁγνή, καὶ δεόμεθα, διάλυσον νέφη τὰ δεινότατα, καὶ τὰ πάθη, Κόρη, τῶν ψυχῶν ἡμῶν."

ocLI.MM.Beatitudes1234TrTas78910GB.text = properties_gr_GR_cog.media.key

ocBK.end.title = "ΤΕΛΟΣ ΤΟΥ ΠΡΩΤΟΥ ΗΧΟΥ"
ocBK.end.verse1.text = "Τέχνη μελουργός, σοὺς ἀγασθεῖσα κρότους"
ocBK.end.verse2.text = "Πρώτην νέμει σοι τάξιν· ὢ τῆς ἀξίας!"
ocBK.end.verse3.text = "Ἦχος ὁ πρῶτος μουσικῇ κληθεὶς τέχνῃ,"
ocBK.end.verse4.text = "Πρῶτος παρʼ ἡμῶν εὐλογείσθω τοῖς λόγοις."
ocBK.end.verse5.text = "Τὰ πρῶτα πρῶτε τῶν καλῶν λαχὼν φέρεις."
ocBK.end.verse6.text = "Πρωτεῖα νίκης πανταχοῦ πάντων ἔχεις."

