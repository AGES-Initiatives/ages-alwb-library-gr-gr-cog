ocMA.OdeW1C11.mode = miscellanea_gr_GR_cog.misc.Mode6
ocMA.OdeW1C11.ode = miscellanea_gr_GR_cog.misc.Ode1
ocMA.OdeW1C11.melody = he.h.m6_gr_GR_cog.heHE.KymatiThalassis.name
ocMA.OdeW1C11.incipit = "Πάθει σου τιμίῳ, τὴν ἀτιμασθεῖσαν…"
ocMA.OdeW1C11.text = "Πάθει σου τιμίῳ, τὴν ἀτιμασθεῖσαν φύσιν ἐτίμησας, τῆς ἀνθρωπότητος· διὸ τιμῶντές σε φόβῳ, μεγαλύνομεν τὸ κράτος σου, καὶ πιστῶς δοξάζομεν· Ἐνδόξως γὰρ δεδόξασται."

ocMA.OdeW1C12.mode = miscellanea_gr_GR_cog.misc.Mode6
ocMA.OdeW1C12.ode = miscellanea_gr_GR_cog.misc.Ode1
ocMA.OdeW1C12.melody = he.h.m6_gr_GR_cog.heHE.KymatiThalassis.name
ocMA.OdeW1C12.incipit = "Αἵματί σου Λόγε, αἱμάτων ἀδίκων…"
ocMA.OdeW1C12.text = "Αἵματί σου Λόγε, αἱμάτων ἀδίκων χύσιν ἀνέστειλας, καὶ ἀπεκάθηρας, ῥύπου κακίας τὴν φύσιν, τῶν ἀνθρώπων Παντοδύναμε· διό περ δοξάζομεν σῳζόμενοι τὸ κράτος σου."

ocMA.OdeW1C13.mode = miscellanea_gr_GR_cog.misc.Mode6
ocMA.OdeW1C13.ode = miscellanea_gr_GR_cog.misc.Ode1
ocMA.OdeW1C13.melody = he.h.m6_gr_GR_cog.heHE.KymatiThalassis.name
ocMA.OdeW1C13.incipit = "Θῆρας αἱμοβόρους, ἐνίκησεν ὄντως ἡ…"
ocMA.OdeW1C13.text = "Θῆρας αἱμοβόρους, ἐνίκησεν ὄντως ἡ ἀξιέπαινος, Μαρτύρων ἔνστασις, πυρὸς ὑπέταξε φύσιν, δροσισμῷ ἐνθέου χάριτος, καὶ σφοδρὸν κλυδώνιον, πολυθεΐας ἔπαυσε."

ocMA.OdeW1C14.mode = miscellanea_gr_GR_cog.misc.Mode6
ocMA.OdeW1C14.ode = miscellanea_gr_GR_cog.misc.Ode1
ocMA.OdeW1C14.melody = he.h.m6_gr_GR_cog.heHE.KymatiThalassis.name
ocMA.OdeW1C14.incipit = "Ὄμβροις τῶν αἱμάτων, ὀμβρήματα πλάνης…"
ocMA.OdeW1C14.text = "Ὄμβροις τῶν αἱμάτων, ὀμβρήματα πλάνης ἀποξηράναντες, ὄμβρους ἰάσεων, ἀποχετεύετε κόσμῳ, καὶ φλογμὸν παθῶν διώκετε, ἀληθείας Μάρτυρες, ἐντεῦθεν μακαρίζεσθε."

ocMA.OdeW1C1T.mode = miscellanea_gr_GR_cog.misc.Mode6
ocMA.OdeW1C1T.ode = miscellanea_gr_GR_cog.misc.Ode1
ocMA.OdeW1C1T.melody = he.h.m6_gr_GR_cog.heHE.KymatiThalassis.name
ocMA.OdeW1C1T.incipit = "Κάλλος ἐν τῷ πάσχειν ὡραῖον…"
ocMA.OdeW1C1T.text = "Κάλλος ἐν τῷ πάσχειν ὡραῖον ἐν κάλλει, οὐδʼ εἶδος ἔχοντα, ἡ ἀπειρόγαμος, κατανοοῦσά σε Κόρη τῶν πιστῶν τὸ ἐγκαλλώπισμα, μητρικῶς ἠλάλαζε, καὶ πόθῳ σε ἐδόξαζε."

ocMA.OdeW1C21.mode = miscellanea_gr_GR_cog.misc.Mode6
ocMA.OdeW1C21.ode = miscellanea_gr_GR_cog.misc.Ode1
ocMA.OdeW1C21.greekmelody = he.h.m6_gr_GR_cog.heHE.KymatiThalassis.greekname
ocMA.OdeW1C21.melody = he.h.m6_gr_GR_cog.heHE.KymatiThalassis.name
ocMA.OdeW1C21.incipit = "Ἄχραντε Παρθένε, κεχαριτωμένη Μαρία ἔνδοξε…"
ocMA.OdeW1C21.text = "Ἄχραντε Παρθένε, κεχαριτωμένη Μαρία ἔνδοξε, τῆς χαρᾶς πρόξενε, τοὺς ὑμνητάς σου ἐνθέως, θείαις χάρισι χαρίτωσον, ὅπως χαριστήριον ᾠδήν σοι ἀναπέμψωμεν."

ocMA.OdeW1C22.mode = miscellanea_gr_GR_cog.misc.Mode6
ocMA.OdeW1C22.ode = miscellanea_gr_GR_cog.misc.Ode1
ocMA.OdeW1C22.melody = he.h.m6_gr_GR_cog.heHE.KymatiThalassis.name
ocMA.OdeW1C22.incipit = "Βαβαὶ Θεομῆτορ, τῶν σῶν μυστηρίων!…"
ocMA.OdeW1C22.text = "Βαβαὶ Θεομῆτορ, τῶν σῶν μυστηρίων! τῶν ποιημάτων γάρ, ἐδείχθης Δέσποινα, ὃν οὐ τολμᾷ ἀτενίσαι τῶν Ἀγγέλων τὰ στρατεύματα, ἐν ἀγκάλαις φέρουσα· διό σε μακαρίζομεν."

ocMA.OdeW1C23.mode = miscellanea_gr_GR_cog.misc.Mode6
ocMA.OdeW1C23.ode = miscellanea_gr_GR_cog.misc.Ode1
ocMA.OdeW1C23.melody = he.h.m6_gr_GR_cog.heHE.KymatiThalassis.name
ocMA.OdeW1C23.incipit = "Γένος βροτῶν ἄπαν, φθορᾷ τοῦ…"
ocMA.OdeW1C23.text = "Γένος βροτῶν ἄπαν, φθορᾷ τοῦ θανάτου καταδεδίκασται, τῇ τῆς προμήτορος, ἀπογεύσει τοῦ ξύλου, διὰ σοῦ δὲ ἀνακέκληται, ὡς ζωὴν τεκούσης σου, Ἁγνὴ τὴν ἀδιάφθορον."

ocMA.OdeW3C1H.mode = miscellanea_gr_GR_cog.misc.Mode6
ocMA.OdeW3C1H.ode = miscellanea_gr_GR_cog.misc.Ode3
ocMA.OdeW3C1H.greekmelody = he.h.m6_gr_GR_cog.heHE.SeTonEpiYdaton.greekname
