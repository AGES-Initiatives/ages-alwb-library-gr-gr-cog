syMA.commemoration06.couplet1.verse2 = ""

syMA.commemoration07.text = "Ὁ Ὅσιος Πατὴρ ἡμῶν Στέφανος ὁ θαυματουργὸς ἐν εἱρήνῃ τελειοῦται."
