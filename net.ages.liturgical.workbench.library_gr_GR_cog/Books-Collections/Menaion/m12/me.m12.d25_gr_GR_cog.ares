meMA.Ode6C214a.text = "Κ"
meMA.Ode6C214b.text = "αθεὶς ἑαυτόν, δευτέρᾳ κοινωνίᾳ"
meMA.Ode6C215.text = "Αὖθις προφαίνων, τῶν παθῶν ἐλευθέραν."
meMA.Ode6C215a.text = "Α"
meMA.Ode6C215b.text = "ὖθις προφαίνων, τῶν παθῶν ἐλευθέραν."

