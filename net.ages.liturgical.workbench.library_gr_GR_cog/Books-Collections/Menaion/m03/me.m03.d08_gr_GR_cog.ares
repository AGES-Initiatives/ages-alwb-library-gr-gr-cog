meVE.Apolytikion1ger.melody = he.a.m3_gr_GR_cog.heAU.TheiasPisteos.name
meVE.Apolytikion1ger.incipit = "Φύλαξ ἄγρυπνος, τῆς Ἐκκλησίας, καὶ…"
meVE.Apolytikion1ger.text = "Φύλαξ ἄγρυπνος, τῆς Ἐκκλησίας, καὶ καθαίρεσις, τῆς δυσσεβείας, Ἱεράρχα Θεοφύλακτε πέφηνας· τοῦ γὰρ Χριστοῦ τὴν Εἰκόνα σεβόμενος, ὑπερορίας καὶ θλίψεις ὑπέμεινας· Πάτερ Ὅσιε, Χριστὸν τὸν Θεὸν ἱκέτευε, δωρήσασθαι ἡμῖν τὸ μέγα ἔλεος."

meMA.C1.acrostic.title = "Ὁ Κανὼν τοῦ Ἁγίου,"
meMA.C1.acrostic.notice1 = "οὗ ἡ Ἀκροστιχίς·"
