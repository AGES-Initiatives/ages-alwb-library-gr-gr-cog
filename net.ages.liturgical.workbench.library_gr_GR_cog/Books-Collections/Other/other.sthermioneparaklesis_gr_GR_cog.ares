hoCA.title2 = "ΕΙΣ ΤΗΝ ΕΝΔΟΞΟΝ ΠΑΡΘΕΝΟΜΑΡΤΥΡΑ"
hoCA.title3 = "ΕΡΜΙΟΝΗ"
hoCA.poet = "Ποίημα Αρχιμ. Νικοδήμου Γ. Ἀεράκη Ἱεροκήρυκος"
hoCA.translator = ""
hoCA.Insert1 = "τῆς ἁγίας ἐνδόξου παρθενομάρτυρος Ἑρμιόνης,"

hoCA.media.text = "" // for complete music

hoCA.Apolytikion1.mode = miscellanea_gr_GR_cog.misc.Mode4
hoCA.Apolytikion1.melody = he.a.m4_gr_GR_cog.heAU.OYpsotheisEnToStavro.name
hoCA.Apolytikion1.incipit = "…"
hoCA.Apolytikion1.text = "Τὴν Ἑρμιόνην ὡς παρθένον τιμῶμεν, καὶ ὡς φιλάνθρωπον ᾠδαῖς ἀνυμνοῦμεν, καταβαλοῦσαν πάντα πειρασμὸν τοῦ ἐχθροῦ· Κύριος γὰρ ὡς Πετρώνιος, ἐπεφάνη τῇ μάρτυρι, νίκας χορηγῶν αὐτῇ κατὰ τοῦ βασιλέως, καὶ εἰς παστάδα θείαν οὐρανοῦ, ταύτην εἰσάγων, ἐν δόξῃ ὁ Πρώταθλος."

