meMA.AposGlory.melody = ""
meMA.AposGlory.incipit = me.m08.d15_gr_GR_cog.meVE.Lity1.incipit
meMA.AposGlory.text = me.m08.d15_gr_GR_cog.meVE.Lity1.text

meLI.Kontakion1.mode = me.m08.d16_gr_GR_cog.meMA.Kontakion1ger.mode
meLI.Kontakion1.melody = me.m08.d16_gr_GR_cog.meMA.Kontakion1ger.melody
meLI.Kontakion1.incipit = me.m08.d16_gr_GR_cog.meMA.Kontakion1ger.incipit
meLI.Kontakion1.text = me.m08.d16_gr_GR_cog.meMA.Kontakion1ger.text

meLI.Megalynarion.text = "Ἧκεν ἐξ Ἐδέσσης ἡ σὴ Εἰκών, ἐν τῇ Βασιλίδι κομισθεῖσα περιφανῶς, καὶ τῆς σῆς προνοίας, ἐκλάμπει τὰς ἀκτῖνας, ἡλιακὸς ὡς δίσκος ἡμῖν φιλάνθρωπε."

alt1.meDA.date = "Τῌ ΙϚʹ ΑΥΓΟΥΣΤΟΥ" // date, e.g. Τῌ ΙϚʹ τοῦ ΑΥΤΟΥ ΜΗΝΟΣ or July 16
alt1.meDA.commemoration.full = "Μνήμη τοῦ Ὁσίου καὶ Θεοφόρου Πατρὸς ἡμῶν Γερασίμου τοῦ Νέου Ἀσκητοῦ" //full title of day
alt1.meDA.commemoration.first_short = "Τοῦ Οσίου καὶ Θεοφόρου Πατρὸς ἡμῶν Γερασίμου τοῦ Νέου Ἀσκητοῦ"
alt1.meDA.commemoration.first_title = "Τοῦ Ἁγ. Γερασίμου."
alt1.meDA.commemoration.first_name = ""
alt1.meDA.commemoration.second_title = ""
