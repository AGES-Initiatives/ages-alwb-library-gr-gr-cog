ocMA.OdeW1C11.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW1C11.ode = miscellanea_gr_GR_cog.misc.Ode1
ocMA.OdeW1C11.melody = he.h.m2_gr_GR_cog.heHE.AtriptonAsynithi.name
ocMA.OdeW1C11.incipit = "Σταύρωσιν κατεδέξω, καὶ ἥλοις προσεπάγης…"
ocMA.OdeW1C11.text = "Σταύρωσιν κατεδέξω, καὶ ἥλοις προσεπάγης ἀτίμως Λόγε, πάντας τιμῆσαι θέλων τοὺς βροτούς, τοὺς τὰ σὰ ἑκούσια, πάθη δοξάζοντας."

ocMA.OdeW1C12.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW1C12.ode = miscellanea_gr_GR_cog.misc.Ode1
ocMA.OdeW1C12.melody = he.h.m2_gr_GR_cog.heHE.AtriptonAsynithi.name
ocMA.OdeW1C12.incipit = "Τείνας Σταυρῷ παλάμας, ὁ τείνας…"
ocMA.OdeW1C12.text = "Τείνας Σταυρῷ παλάμας, ὁ τείνας πόλον Σῶτερ, καθάπερ δέῤῥιν, ἐνηγκαλίσω ἔθνη καὶ λαούς, τοὺς τὰ σὰ ἑκούσια, πάθη δοξάζοντας."

ocMA.OdeW1C13.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW1C13.ode = miscellanea_gr_GR_cog.misc.Ode1
ocMA.OdeW1C13.melody = he.h.m2_gr_GR_cog.heHE.AtriptonAsynithi.name
ocMA.OdeW1C13.incipit = "Ἄραντες ἐπὶ ὤμων, Σταυρὸν οἱ…"
ocMA.OdeW1C13.text = "Ἄραντες ἐπὶ ὤμων, Σταυρὸν οἱ ἀθλοφόροι, τῷ σταυρωθέντι, ἀκολουθῆσαι εἵλοντο Χριστῷ, τούτου συμμορφούμενοι, τοῖς θείοις πάθεσιν."

ocMA.OdeW1C14.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW1C14.ode = miscellanea_gr_GR_cog.misc.Ode1
ocMA.OdeW1C14.melody = he.h.m2_gr_GR_cog.heHE.AtriptonAsynithi.name
ocMA.OdeW1C14.incipit = "Ὕμνησαν τῶν Ἀγγέλων, δυνάμεις κατιδοῦσαι…"
ocMA.OdeW1C14.text = "Ὕμνησαν τῶν Ἀγγέλων, δυνάμεις κατιδοῦσαι ὑμῶν τοὺς ἄθλους, ἐθρήνησε δαιμόνων δὲ πληθύς, νικηφόροι Μάρτυρες, θοειδέστατοι."

ocMA.OdeW1C1T.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW1C1T.ode = miscellanea_gr_GR_cog.misc.Ode1
ocMA.OdeW1C1T.melody = he.h.m2_gr_GR_cog.heHE.AtriptonAsynithi.name
ocMA.OdeW1C1T.incipit = "Ῥῆσις σεπτοῦ Προφήτου, πεπλήρωται· ῥομφαία…"
ocMA.OdeW1C1T.text = "Ῥῆσις σεπτοῦ Προφήτου, πεπλήρωται· ῥομφαία γὰρ τὴν καρδίαν τὴν σὴν διῆλθεν, ὅτε ἐν Σταυρῷ λογχευθέντα Δέσποινα, τὸν σὸν Υἱὸν ἔβλεψας."

ocMA.OdeW1C21.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW1C21.ode = miscellanea_gr_GR_cog.misc.Ode1
ocMA.OdeW1C21.greekmelody = he.h.m2_gr_GR_cog.heHE.DefteLaoi.greekname
ocMA.OdeW1C21.melody = he.h.m2_gr_GR_cog.heHE.DefteLaoi.name
ocMA.OdeW1C21.incipit = "Τὸν εὐσεβῶς, σὲ Θεοτόκον κυρίως…"
ocMA.OdeW1C21.text = "Τὸν εὐσεβῶς, σὲ Θεοτόκον κυρίως ἁγνή, ὁμολογοῦντα Ἄχραντε, ψυχῇ καὶ στόματι, χαλεπῶν ἐκ κινδύνων, καὶ νόσων καὶ πταισμάτων, νῦν με διάσωσον."

ocMA.OdeW1C22.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW1C22.ode = miscellanea_gr_GR_cog.misc.Ode1
ocMA.OdeW1C22.melody = he.h.m2_gr_GR_cog.heHE.DefteLaoi.name
ocMA.OdeW1C22.incipit = "Ὅλην ἐν σοί, τῶν χαρισμάτων…"
ocMA.OdeW1C22.text = "Ὅλην ἐν σοί, τῶν χαρισμάτων τὴν ἄβυσσον, ἀποκειμένην ἔγνωμεν· διὸ προσφεύγοντες, Θεοτόκε προθύμως, τῇ σκέπῃ σου τῇ θείᾳ διασῳζόμεθα."

ocMA.OdeW1C23.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW1C23.ode = miscellanea_gr_GR_cog.misc.Ode1
ocMA.OdeW1C23.melody = he.h.m2_gr_GR_cog.heHE.DefteLaoi.name
ocMA.OdeW1C23.incipit = "Ὑπὲρ ἡμῶν, τῶν σὲ ὑμνούντων…"
ocMA.OdeW1C23.text = "Ὑπὲρ ἡμῶν, τῶν σὲ ὑμνούντων δυσώπησον, τὸν σαρκωθέντα Πάναγνε, ἐκ τῶν ἀχράντων σου, καὶ τιμίων αἱμάτων, ῥυσθῆναι ἐκ πταισμάτων, καὶ χαλεπῶν πειρασμῶν."

ocMA.OdeW1C24.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW1C24.ode = miscellanea_gr_GR_cog.misc.Ode1
ocMA.OdeW1C24.melody = he.h.m2_gr_GR_cog.heHE.DefteLaoi.name
ocMA.OdeW1C24.incipit = "Σὲ οἱ πιστοί, καύχημα πάντες…"
ocMA.OdeW1C24.text = "Σὲ οἱ πιστοί, καύχημα πάντες κεκτήμεθα, καταφυγὴν καὶ στήριγμα, καὶ ἀγαλλίαμα, καὶ ψυχῶν σωτηρίαν, ἐλπίδα τε καὶ τεῖχος, θεοχαρίτωτε."

ocMA.OdeW3C1H.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW3C1H.ode = miscellanea_gr_GR_cog.misc.Ode3
ocMA.OdeW3C1H.greekmelody = he.h.m2_gr_GR_cog.heHE.EnPetraMeTisPisteos.greekname
ocMA.OdeW3C1H.melody = he.h.m2_gr_GR_cog.heHE.EnPetraMeTisPisteos.melody
ocMA.OdeW3C1H.incipit = he.h.m2_gr_GR_cog.heHE.EnPetraMeTisPisteos.incipit
ocMA.OdeW3C1H.text = he.h.m2_gr_GR_cog.heHE.EnPetraMeTisPisteos.text

ocMA.OdeW3C11.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW3C11.ode = miscellanea_gr_GR_cog.misc.Ode3
ocMA.OdeW3C11.melody = he.h.m2_gr_GR_cog.heHE.EnPetraMeTisPisteos.name
ocMA.OdeW3C11.incipit = "Ὁ βότρυς ἐπὶ ξύλου τῆς…"
ocMA.OdeW3C11.text = "Ὁ βότρυς ἐπὶ ξύλου τῆς ἀφθαρσίας, κρεμάμενος, ἀπέσταξε θεῖον γλεῦκος, καρδίας κατευφραῖνον τὰς τῶν ἀνθρώπων, κακίας παῦον δέ, τὴν μέθην πάντοτε, Ἰησοῦς ἡ λύτρωσις τῶν ψυχῶν ἡμῶν."

ocMA.OdeW3C12.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW3C12.ode = miscellanea_gr_GR_cog.misc.Ode3
ocMA.OdeW3C12.melody = he.h.m2_gr_GR_cog.heHE.EnPetraMeTisPisteos.name
ocMA.OdeW3C12.incipit = "Ὑψώθης ἐπὶ ξύλου ἐθελουσίως, καὶ…"
ocMA.OdeW3C12.text = "Ὑψώθης ἐπὶ ξύλου ἐθελουσίως, καὶ πᾶσαν διαβόλου κακοπραγίαν, κατέῤῥαξας, πεσόντας δὲ τοὺς ἀνθρώπους, εἰς τὴν ἀπώλειαν, γνώμης στρεβλότητι, Ἰησοῦ ἀνύψωσας, πολυέλεε."

ocMA.OdeW3C13.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW3C13.ode = miscellanea_gr_GR_cog.misc.Ode3
ocMA.OdeW3C13.melody = he.h.m2_gr_GR_cog.heHE.EnPetraMeTisPisteos.name
ocMA.OdeW3C13.incipit = "Πυρὶ τοῦ θείου πόθου προσαναφθέντες…"
ocMA.OdeW3C13.text = "Πυρὶ τοῦ θείου πόθου προσαναφθέντες, τὸ πῦρ οὐκ ἐδειλίασαν οἱ γενναῖοι, οὐκ ἔπτηξαν τὸν θάνατον ἀθανάτους, ἀποσκοπούμενοι δωρεὰς λήψεσθαι, καὶ χαρὰν μὴ λήγουσαν, καὶ φῶς ἄδυτον."

ocMA.OdeW3C14.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW3C14.ode = miscellanea_gr_GR_cog.misc.Ode3
ocMA.OdeW3C14.melody = he.h.m2_gr_GR_cog.heHE.EnPetraMeTisPisteos.name
ocMA.OdeW3C14.incipit = "Αἱμάτων ἐξ οἰκείων οἱ Ἀθλοφόροι…"
ocMA.OdeW3C14.text = "Αἱμάτων ἐξ οἰκείων οἱ Ἀθλοφόροι, ὑπέρλαμπρον ἐπέχρωσαν πορφυρίδα, καὶ ταύτην στολισάμενοι δεξιᾷ τε, ὡς σκῆπτρον φέροντες, χειρὶ τὸν τῷ Κυρίῳ πάντοτε θεῖον Σταυρόν, συμβασιλεύουσιν."

ocMA.OdeW3C1T.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW3C1T.ode = miscellanea_gr_GR_cog.misc.Ode3
ocMA.OdeW3C1T.melody = he.h.m2_gr_GR_cog.heHE.EnPetraMeTisPisteos.name
ocMA.OdeW3C1T.incipit = "Γεραίρουσί σε τάξεις τῶν Ἀσωμάτων…"
ocMA.OdeW3C1T.text = "Γεραίρουσί σε τάξεις τῶν Ἀσωμάτων· τὸν πάντων γὰρ Δεσπότην σωματοφόρον, ἐκύησας τὸν λύσαντα διὰ ξύλου, δεσμίους ἅπαντας, Κόρη Θεόνυμφε, καὶ πιστοὺς συνδήσαντα τῇ στοργῇ αὐτοῦ."

ocMA.OdeW3C21.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW3C21.ode = miscellanea_gr_GR_cog.misc.Ode3
ocMA.OdeW3C21.greekmelody = he.h.m2_gr_GR_cog.heHE.StereosonImasEnSoiKyrie.greekname
ocMA.OdeW3C21.melody = he.h.m2_gr_GR_cog.heHE.StereosonImasEnSoiKyrie.name
ocMA.OdeW3C21.incipit = "Πρεσβείαις σου Ἁγνὴ Θεὸν ὃν…"
ocMA.OdeW3C21.text = "Πρεσβείαις σου Ἁγνὴ Θεὸν ὃν ἔτεκες, παράσχου εὐΐλατον σοῖς ἱκέταις, τοῖς προστρέχουσι τῇ σκέπῃ σου, καὶ πιστῶς προσκυνοῦσί σου τὴν κύησιν."

ocMA.OdeW3C22.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW3C22.ode = miscellanea_gr_GR_cog.misc.Ode3
ocMA.OdeW3C22.melody = he.h.m2_gr_GR_cog.heHE.StereosonImasEnSoiKyrie.name
ocMA.OdeW3C22.incipit = "Ῥημάτων τῆς ἐμῆς ψυχῆς ἐπάκουσον…"
ocMA.OdeW3C22.text = "Ῥημάτων τῆς ἐμῆς ψυχῆς ἐπάκουσον, Παρθένε δεινῶς ἐκταραττομένης, ἣν εἰρήνευσον Πανύμνητε, ἀεὶ ταύτην κινδύνων διασῴζουσα."

ocMA.OdeW3C23.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW3C23.ode = miscellanea_gr_GR_cog.misc.Ode3
ocMA.OdeW3C23.melody = he.h.m2_gr_GR_cog.heHE.StereosonImasEnSoiKyrie.name
ocMA.OdeW3C23.incipit = "Ὅλην μου τὴν ζωὴν διακυβέρνησον…"
ocMA.OdeW3C23.text = "Ὅλην μου τὴν ζωὴν διακυβέρνησον, Παρθένε ἐλπίς μου καὶ προστασία, πειρασμῶν καὶ περιστάσεων, λυτρουμένη δεινῶν με Θεονύμφευτε."

ocMA.OdeW3C24.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW3C24.ode = miscellanea_gr_GR_cog.misc.Ode3
ocMA.OdeW3C24.melody = he.h.m2_gr_GR_cog.heHE.StereosonImasEnSoiKyrie.name
ocMA.OdeW3C24.incipit = "Σοφίαν τοῦ Θεοῦ τὴν ἐνυπόστατον…"
ocMA.OdeW3C24.text = "Σοφίαν τοῦ Θεοῦ τὴν ἐνυπόστατον, ἀγκάλαις ὡς φέρουσα Θεομῆτορ, ἐξ ἀγνοίας καὶ πωρώσεως, λυτρωθῆναι ἡμᾶς νῦν καθικέτευε."

ocMA.OdeW4C1H.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW4C1H.ode = miscellanea_gr_GR_cog.misc.Ode4
ocMA.OdeW4C1H.greekmelody = he.h.m2_gr_GR_cog.heHE.ElilythasEkParthenou.greekname
ocMA.OdeW4C1H.melody = he.h.m2_gr_GR_cog.heHE.ElilythasEkParthenou.melody
ocMA.OdeW4C1H.incipit = he.h.m2_gr_GR_cog.heHE.ElilythasEkParthenou.incipit
ocMA.OdeW4C1H.text = he.h.m2_gr_GR_cog.heHE.ElilythasEkParthenou.text

ocMA.OdeW4C11.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW4C11.ode = miscellanea_gr_GR_cog.misc.Ode4
ocMA.OdeW4C11.melody = he.h.m2_gr_GR_cog.heHE.ElilythasEkParthenou.name
ocMA.OdeW4C11.incipit = "Ἐκρέμασο ἐπὶ ξύλου, κρεμάσας ἐν…"
ocMA.OdeW4C11.text = "Ἐκρέμασο ἐπὶ ξύλου, κρεμάσας ἐν ὕδασι, τὴν γῆν Παντοδύναμε, λόγχῃ πλευρὰν ὀρυττόμενος, αἷμά τε σὺν ὕδατι, ἐναποστάξας εἰς πάντων ἀπολύτρωσιν."

ocMA.OdeW4C12.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW4C12.ode = miscellanea_gr_GR_cog.misc.Ode4
ocMA.OdeW4C12.melody = he.h.m2_gr_GR_cog.heHE.ElilythasEkParthenou.name
ocMA.OdeW4C12.incipit = "Νυγείσης σου τῆς πλευρᾶς, ἰατρεύθη…"
ocMA.OdeW4C12.text = "Νυγείσης σου τῆς πλευρᾶς, ἰατρεύθη τὸ ἄλγος μου, χειρὶ ῥαπισθέντος σου, ἐλευθερίας ἐπέτυχον, χολῆς γευσαμένου δέ, τῆς ἐνηδόνου ἐῤῥύσθην Χριστὲ βρώσεως."

ocMA.OdeW4C13.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW4C13.ode = miscellanea_gr_GR_cog.misc.Ode4
ocMA.OdeW4C13.melody = he.h.m2_gr_GR_cog.heHE.ElilythasEkParthenou.name
ocMA.OdeW4C13.incipit = "Τοῖς τραύμασι τραυματίσαντες, ὄφιν τὸν…"
ocMA.OdeW4C13.text = "Τοῖς τραύμασι τραυματίσαντες, ὄφιν τὸν δόλιον, ἰᾶσθε τραύματα, τῶν καρδιῶν ἡμῶν πάντοτε, χάριν ἀναβλύζοντες, ἐκ τῶν πηγῶν τοῦ Σωτῆρος θεῖοι Μάρτυρες."

ocMA.OdeW4C14.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW4C14.ode = miscellanea_gr_GR_cog.misc.Ode4
ocMA.OdeW4C14.melody = he.h.m2_gr_GR_cog.heHE.ElilythasEkParthenou.name
ocMA.OdeW4C14.incipit = "Ὁλόσωμον τῷ ἐχθρῷ, τὴν πληγὴν…"
ocMA.OdeW4C14.text = "Ὁλόσωμον τῷ ἐχθρῷ, τὴν πληγὴν προσηνέγκατε, πληγαῖς αἱματούμενοι, ἐπὶ τροχοῦ τε τεινόμενοι, καὶ τυμπανιζόμενοι, θεοειδεῖς Ἀθλόφοροι πανσεβάσμιοι."

ocMA.OdeW4C1T.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW4C1T.ode = miscellanea_gr_GR_cog.misc.Ode4
ocMA.OdeW4C1T.melody = he.h.m2_gr_GR_cog.heHE.ElilythasEkParthenou.name
ocMA.OdeW4C1T.incipit = "Σεσάρκωται, ἐξ ἁγνῶν σου αἱμάτων…"
ocMA.OdeW4C1T.text = "Σεσάρκωται, ἐξ ἁγνῶν σου αἱμάτων ὁ Ὕψιστος, ὃν βλέπουσα Πάναγνε, ξύλῳ ἀδίκως ὑψούμενον, ἔστενες δακρύουσα, καὶ τὴν αὐτοῦ εὐσπλαγχνίαν ἐμεγάλυνες."

ocMA.OdeW4C21.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW4C21.ode = miscellanea_gr_GR_cog.misc.Ode4
ocMA.OdeW4C21.greekmelody = he.h.m4_gr_GR_cog.heHE.EisakikoaTinEndoxon.greekname
ocMA.OdeW4C21.melody = he.h.m4_gr_GR_cog.heHE.EisakikoaTinEndoxon.name
ocMA.OdeW4C21.incipit = "Τῶν τραυμάτων μου Δέσποινα τῶν…"
ocMA.OdeW4C21.text = "Τῶν τραυμάτων μου Δέσποινα τῶν τῆς ψυχῆς, σαρκός τε νοσημάτων, ἡ Θεὸν τεκοῦσα, λύσιν παράσχου μοι."

ocMA.OdeW4C22.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW4C22.ode = miscellanea_gr_GR_cog.misc.Ode4
ocMA.OdeW4C22.melody = he.h.m4_gr_GR_cog.heHE.EisakikoaTinEndoxon.name
ocMA.OdeW4C22.incipit = "Ῥιπιζόμενον πάθεσι καὶ λογισμοῖς, βίου…"
ocMA.OdeW4C22.text = "Ῥιπιζόμενον πάθεσι καὶ λογισμοῖς, βίου τρικυμίαις, ἐλπίδι Παρθένε, καὶ πίστει βεβαίωσον."

ocMA.OdeW4C23.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW4C23.ode = miscellanea_gr_GR_cog.misc.Ode4
ocMA.OdeW4C23.melody = he.h.m4_gr_GR_cog.heHE.EisakikoaTinEndoxon.name
ocMA.OdeW4C23.incipit = "Ἐκ περιστάσεως ῥῦσαί με, Μῆτερ…"
ocMA.OdeW4C23.text = "Ἐκ περιστάσεως ῥῦσαί με, Μῆτερ Θεοῦ, καὶ ζάλης καὶ κινδύνων, ταῖς σαῖς ἱκεσίαις μόνη Πανύμνητε."

ocMA.OdeW4C24.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW4C24.ode = miscellanea_gr_GR_cog.misc.Ode4
ocMA.OdeW4C24.melody = he.h.m4_gr_GR_cog.heHE.EisakikoaTinEndoxon.name
ocMA.OdeW4C24.incipit = "Χειμαζόμενον κλύδωνι βιωτικῷ, ἐξάρπασον Παρθένε…"
ocMA.OdeW4C24.text = "Χειμαζόμενον κλύδωνι βιωτικῷ, ἐξάρπασον Παρθένε, πρὸς τὸν σὸν λιμένα καθοδηγοῦσά με."

ocMA.OdeW5C1H.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW5C1H.ode = miscellanea_gr_GR_cog.misc.Ode5
ocMA.OdeW5C1H.greekmelody = he.h.m2_gr_GR_cog.heHE.OAnthraxToIsaia.greekname
ocMA.OdeW5C1H.melody = he.h.m2_gr_GR_cog.heHE.OAnthraxToIsaia.melody
ocMA.OdeW5C1H.incipit = he.h.m2_gr_GR_cog.heHE.OAnthraxToIsaia.incipit
ocMA.OdeW5C1H.text = he.h.m2_gr_GR_cog.heHE.OAnthraxToIsaia.text

ocMA.OdeW5C11.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW5C11.ode = miscellanea_gr_GR_cog.misc.Ode5
ocMA.OdeW5C11.melody = he.h.m2_gr_GR_cog.heHE.OAnthraxToIsaia.name
ocMA.OdeW5C11.incipit = "Διʼ οἶκτον προσομιλήσας τῷ Σταυρῷ…"
ocMA.OdeW5C11.text = "Διʼ οἶκτον προσομιλήσας τῷ Σταυρῷ, Δέσποτα ἐκ τοῦ βυθοῦ με, τῶν κακῶν ἀνέσπασας, καὶ ἐτίμησας συνεδρίᾳ τῇ πατρικῇ, ἀτιμασθεὶς θελήματι."

ocMA.OdeW5C12.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW5C12.ode = miscellanea_gr_GR_cog.misc.Ode5
ocMA.OdeW5C12.melody = he.h.m2_gr_GR_cog.heHE.OAnthraxToIsaia.name
ocMA.OdeW5C12.incipit = "Ἀκάνθαις στεφανωθεὶς ὁ στεφανῶν, ἄνθεσι…"
ocMA.OdeW5C12.text = "Ἀκάνθαις στεφανωθεὶς ὁ στεφανῶν, ἄνθεσι πᾶσα τὴν γῆν, τὴν τῶν παθῶν μου ἄκανθαν, ἀποτέμνεις ῥιζόθεν Λόγε, καὶ ἐμφυτεύεις μοι τὴν σὴν ἐπίγνωσιν."

ocMA.OdeW5C13.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW5C13.ode = miscellanea_gr_GR_cog.misc.Ode5
ocMA.OdeW5C13.melody = he.h.m2_gr_GR_cog.heHE.OAnthraxToIsaia.name
ocMA.OdeW5C13.incipit = "Ἰσχύϊ τοῦ τὴν ἀσθένειαν ἡμῶν…"
ocMA.OdeW5C13.text = "Ἰσχύϊ τοῦ τὴν ἀσθένειαν ἡμῶν Ἅγιοι, ἐθελοντὶ περιθεμένου Μάρτυρες, δυναμούμενοι, τῶν δαιμόνων, τὴν δυναστείαν παντελῶς ὠλέσατε."

ocMA.OdeW5C14.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW5C14.ode = miscellanea_gr_GR_cog.misc.Ode5
ocMA.OdeW5C14.melody = he.h.m2_gr_GR_cog.heHE.OAnthraxToIsaia.name
ocMA.OdeW5C14.incipit = "Μεγάλως ἀγωνισάμενοι ἐν γῇ Ἅγιοι…"
ocMA.OdeW5C14.text = "Μεγάλως ἀγωνισάμενοι ἐν γῇ Ἅγιοι, ἐν οὐρανοῖς μεγίστην δόξαν εὕρατε, καὶ μεγίστων ἡμᾶς κινδύνων, ἀπολυτροῦσθε τοὺς ὑμᾶς γεραίροντας."

ocMA.OdeW5C1T.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW5C1T.ode = miscellanea_gr_GR_cog.misc.Ode5
ocMA.OdeW5C1T.melody = he.h.m2_gr_GR_cog.heHE.OAnthraxToIsaia.name
ocMA.OdeW5C1T.incipit = "Ὁ νώτοις Χερουβικοῖς ἐν οὐρανοῖς…"
ocMA.OdeW5C1T.text = "Ὁ νώτοις Χερουβικοῖς ἐν οὐρανοῖς, θεοπρεπῶς ὡς ἀληθῶς ὀχούμενος, ταῖς ἀγκάλαις σου ἐποχεῖται, καὶ σταυρωθεὶς πάντας φθορᾶς διέσωσε."

ocMA.OdeW5C21.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW5C21.ode = miscellanea_gr_GR_cog.misc.Ode5
ocMA.OdeW5C21.greekmelody = he.h.m2_gr_GR_cog.heHE.OTouFotosChorigos.greekname
ocMA.OdeW5C21.melody = he.h.m2_gr_GR_cog.heHE.OTouFotosChorigos.name
ocMA.OdeW5C21.incipit = "Ὅπλον ἀήττητον, κατὰ ποικίλων πειρασμῶν…"
ocMA.OdeW5C21.text = "Ὅπλον ἀήττητον, κατὰ ποικίλων πειρασμῶν ἔχοντες, τῶν δυσμενῶν πάσης ἐπηρείας, λυτρούμεθα σαφῶς, οἱ σὲ Θεοτόκον ἀεὶ ἐπιστάμενοι."

ocMA.OdeW5C22.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW5C22.ode = miscellanea_gr_GR_cog.misc.Ode5
ocMA.OdeW5C22.melody = he.h.m2_gr_GR_cog.heHE.OTouFotosChorigos.name
ocMA.OdeW5C22.incipit = "Νόμου τὸ πλήρωμα, ἡ ἀνωτέρα…"
ocMA.OdeW5C22.text = "Νόμου τὸ πλήρωμα, ἡ ἀνωτέρα Χερουβεὶμ τέτοκας, τὸν τοῦ Θεοῦ ἐκ σοῦ σαρκωθέντα, Υἱὸν μονογενῆ, ὃν ὑπὲρ τῶν δούλων τῶν σῶν ἐκδυσώπησον."

ocMA.OdeW5C23.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW5C23.ode = miscellanea_gr_GR_cog.misc.Ode5
ocMA.OdeW5C23.melody = he.h.m2_gr_GR_cog.heHE.OTouFotosChorigos.name
ocMA.OdeW5C23.incipit = "Τὸν τοῦ παντὸς ποιητήν, ἐν…"
ocMA.OdeW5C23.text = "Τὸν τοῦ παντὸς ποιητήν, ἐν ταῖς ἀγκάλαις σου Ἁγνὴ φέρουσα, τοῦτον ἡμῖν ταῖς σαῖς ἱκεσίαις, εὐΐλατον δίδου, τοῖς νῦν ἐκ καρδίας, πρὸς σὲ καταφεύγουσιν."

ocMA.OdeW5C24.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW5C24.ode = miscellanea_gr_GR_cog.misc.Ode5
ocMA.OdeW5C24.melody = he.h.m2_gr_GR_cog.heHE.OTouFotosChorigos.name
ocMA.OdeW5C24.incipit = "Ἀπὸ στενώσεως, καὶ κατωδύνου μου…"
ocMA.OdeW5C24.text = "Ἀπὸ στενώσεως, καὶ κατωδύνου μου ψυχῆς δέησιν, τῶν οἰκτιρμῶν μόνῃ τῇ τεκούσῃ, τὸν αἴτιον Λόγον, προσφέρω ὁ τάλας, οἰκτείρασα σῶσόν με."

ocMA.OdeW6C1H.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW6C1H.ode = miscellanea_gr_GR_cog.misc.Ode6
ocMA.OdeW6C1H.greekmelody = he.h.m2_gr_GR_cog.heHE.IchouRimatonDeiseos.greekname
ocMA.OdeW6C1H.melody = he.h.m2_gr_GR_cog.heHE.IchouRimatonDeiseos.melody
ocMA.OdeW6C1H.incipit = he.h.m2_gr_GR_cog.heHE.IchouRimatonDeiseos.incipit
ocMA.OdeW6C1H.text = he.h.m2_gr_GR_cog.heHE.IchouRimatonDeiseos.text

ocMA.OdeW6C11.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW6C11.ode = miscellanea_gr_GR_cog.misc.Ode6
ocMA.OdeW6C11.melody = he.h.m2_gr_GR_cog.heHE.IchouRimatonDeiseos.name
ocMA.OdeW6C11.incipit = "Νῶτον παρέχων εἰς μάστιγας, καὶ…"
ocMA.OdeW6C11.text = "Νῶτον παρέχων εἰς μάστιγας, καὶ σιαγόνας σου εἰς ῥαπισμόν, πρόσωπον δὲ Σῶτερ, εἰς ἐμπτύσματα, ἔσωσάς με τὸν πολλά, ἀφρόνως σοι πταίσαντα."

ocMA.OdeW6C12.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW6C12.ode = miscellanea_gr_GR_cog.misc.Ode6
ocMA.OdeW6C12.melody = he.h.m2_gr_GR_cog.heHE.IchouRimatonDeiseos.name
ocMA.OdeW6C12.incipit = "Ὥσπερ ἀρνίον τοῦ θύεσθαι, ἦλθες…"
ocMA.OdeW6C12.text = "Ὥσπερ ἀρνίον τοῦ θύεσθαι, ἦλθες Χριστέ μου, λύκου νοητοῦ, ἰοβόλῳ δήγματι, τοὺς θνήξαντας ἐπανάγων πρὸς ζωήν· Δόξα τῇ σταυρώσει σου."

ocMA.OdeW6C13.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW6C13.ode = miscellanea_gr_GR_cog.misc.Ode6
ocMA.OdeW6C13.melody = he.h.m2_gr_GR_cog.heHE.IchouRimatonDeiseos.name
ocMA.OdeW6C13.incipit = "Νόμους τηροῦντες οἱ Μάρτυρες, τοὺς…"
ocMA.OdeW6C13.text = "Νόμους τηροῦντες οἱ Μάρτυρες, τοὺς τοῦ Δεσπότου, ἄνομον βουλήν, παρανόμων παντελῶς ἐξέκλιναν, καὶ θανόντες, τῆς ζωῆς τῆς μελλούσης ἔτυχον."

ocMA.OdeW6C14.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW6C14.ode = miscellanea_gr_GR_cog.misc.Ode6
ocMA.OdeW6C14.melody = he.h.m2_gr_GR_cog.heHE.IchouRimatonDeiseos.name
ocMA.OdeW6C14.incipit = "Παραταξάμενοι Ἅγιοι, πρὸς ἐναντίας χαίροντες…"
ocMA.OdeW6C14.text = "Παραταξάμενοι Ἅγιοι, πρὸς ἐναντίας χαίροντες ἀρχάς, θείοις ὅπλοις, ταύτας ἐτροπώσασθε, καὶ στεφάνους ἐκ Θεοῦ, νίκης ἐκομίσασθε."

ocMA.OdeW6C1T.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW6C1T.ode = miscellanea_gr_GR_cog.misc.Ode6
ocMA.OdeW6C1T.melody = he.h.m2_gr_GR_cog.heHE.IchouRimatonDeiseos.name
ocMA.OdeW6C1T.incipit = "Ἵνα θεώσῃ τὸν ἄνθρωπον, ἐκ…"
ocMA.OdeW6C1T.text = "Ἵνα θεώσῃ τὸν ἄνθρωπον, ἐκ σοῦ Παρθένε τίκτεται Θεός, καὶ σταυροῦται, καὶ θανάτου γεύεται, ἀποκτείνων τὸν ἐμέ, τὸ πρὶν ἀποκτείναντα."

ocMA.OdeW6C21.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW6C21.ode = miscellanea_gr_GR_cog.misc.Ode6
ocMA.OdeW6C21.greekmelody = he.h.m2_gr_GR_cog.heHE.EnAvyssoPtaismaton.greekname
ocMA.OdeW6C21.melody = he.h.m2_gr_GR_cog.heHE.EnAvyssoPtaismaton.name
ocMA.OdeW6C21.incipit = "Σωτηρίας λιμένα γινώσκων σε, πλέων…"
ocMA.OdeW6C21.text = "Σωτηρίας λιμένα γινώσκων σε, πλέων πολυφρόντιδος βίου τὴν θάλασσαν, ἐπικαλοῦμαι Δέσποινα, κυβερνῆτιν ψυχῆς μου γενέσθαι σε."

ocMA.OdeW6C22.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW6C22.ode = miscellanea_gr_GR_cog.misc.Ode6
ocMA.OdeW6C22.melody = he.h.m2_gr_GR_cog.heHE.EnAvyssoPtaismaton.name
ocMA.OdeW6C22.incipit = "Ἐνεδύθην στολὴν κακουχούμενος, Μήτηρ Ἀειπάρθενε…"
ocMA.OdeW6C22.text = "Ἐνεδύθην στολὴν κακουχούμενος, Μήτηρ Ἀειπάρθενε, τῆς ἀφροσύνης μου, ἀλλʼ ἡ Θεὸν κυήσασα, τῆς χαρᾶς τὸν χιτῶνα ἀντίδος μοι."

ocMA.OdeW6C23.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW6C23.ode = miscellanea_gr_GR_cog.misc.Ode6
ocMA.OdeW6C23.melody = he.h.m2_gr_GR_cog.heHE.EnAvyssoPtaismaton.name
ocMA.OdeW6C23.incipit = "Ἱερᾶς πολιτείας ἐκπέπτωκα, ἀθυμῶν ὁ…"
ocMA.OdeW6C23.text = "Ἱερᾶς πολιτείας ἐκπέπτωκα, ἀθυμῶν ὁ δείλαιος, ἀλλʼ ἐπανάγαγε, εὐλογημένη Δέσποινα, προσταγαῖς τοῦ Υἱοῦ σου ῥυθμίζουσα."

ocMA.OdeW6C24.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW6C24.ode = miscellanea_gr_GR_cog.misc.Ode6
ocMA.OdeW6C24.melody = he.h.m2_gr_GR_cog.heHE.EnAvyssoPtaismaton.name
ocMA.OdeW6C24.incipit = "Συμπαθείας με θείας ἀξίωσον, ἡ…"
ocMA.OdeW6C24.text = "Συμπαθείας με θείας ἀξίωσον, ἡ τὸν συμπαθέστατον Λόγον κυήσασα, τὸν τῷ ἰδίῳ αἵματι, ἐκ φθορᾶς τοὺς ἀνθρώπους ῥυσάμενον."

ocMA.OdeW7C1H.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW7C1H.ode = miscellanea_gr_GR_cog.misc.Ode7
ocMA.OdeW7C1H.greekmelody = he.h.m2_gr_GR_cog.heHE.AntitheonProstagma.greekname
ocMA.OdeW7C1H.melody = he.h.m2_gr_GR_cog.heHE.AntitheonProstagma.melody
ocMA.OdeW7C1H.incipit = he.h.m2_gr_GR_cog.heHE.AntitheonProstagma.incipit
ocMA.OdeW7C1H.text = he.h.m2_gr_GR_cog.heHE.AntitheonProstagma.text

ocMA.OdeW7C11.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW7C11.ode = miscellanea_gr_GR_cog.misc.Ode7
ocMA.OdeW7C11.melody = he.h.m2_gr_GR_cog.heHE.AntitheonProstagma.name
ocMA.OdeW7C11.incipit = "Πτωθέντα με ἤγειρας τῇ παραβάσει…"
ocMA.OdeW7C11.text = "Πτωθέντα με ἤγειρας τῇ παραβάσει, Σταυρῷ ἀνυψούμενος, ἡ πάντων ἐξανάστασις, καὶ πτώσας τὸν πτώσαντα, Λόγε πολέμιον, ὅλον ἀνενέργητον, νεκρὸν ὑποδεικνύεις. Δόξα τῷ κράτει σου."

ocMA.OdeW7C12.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW7C12.ode = miscellanea_gr_GR_cog.misc.Ode7
ocMA.OdeW7C12.melody = he.h.m2_gr_GR_cog.heHE.AntitheonProstagma.name
ocMA.OdeW7C12.incipit = "Τοῖς ἥλοις ἐξήλωσας τὴν ἁμαρτίαν…"
ocMA.OdeW7C12.text = "Τοῖς ἥλοις ἐξήλωσας τὴν ἁμαρτίαν, Χριστὲ τοῦ προπάτορος, καλάμῳ δὲ τυπτόμενος, ὑπέγραψας ἅπασιν ἐλευθερίαν βροτοῖς. Δόξα τοῖς παθήμασι τοῖς σοῖς, διʼ ὧν μυρίων παθῶν ἐῤῥύσθημεν."

ocMA.OdeW7C13.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW7C13.ode = miscellanea_gr_GR_cog.misc.Ode7
ocMA.OdeW7C13.melody = he.h.m2_gr_GR_cog.heHE.AntitheonProstagma.name
ocMA.OdeW7C13.incipit = "Ἐτέμνοντο σώματα τῶν μιαιφόνων, χερσὶν…"
ocMA.OdeW7C13.text = "Ἐτέμνοντο σώματα τῶν μιαιφόνων, χερσὶν οἱ καλλίνικοι, Χριστοῦ Μεγαλομάρτυρες, καὶ ἔμενον Πνεύματι Θεοῦ, ἀχώριστοι, τέμνοντες τὸν δόλιον ἐχθρόν, ἀνδρείας ξίφει, καὶ κατασφάττοντες."

ocMA.OdeW7C14.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW7C14.ode = miscellanea_gr_GR_cog.misc.Ode7
ocMA.OdeW7C14.melody = he.h.m2_gr_GR_cog.heHE.AntitheonProstagma.name
ocMA.OdeW7C14.incipit = "Ἰσχὺν ἀπροσμάχητον τὸν σταυρωθέντα, Χριστὸν…"
ocMA.OdeW7C14.text = "Ἰσχὺν ἀπροσμάχητον τὸν σταυρωθέντα, Χριστὸν ἐπαγόμενος, στρατὸς ὁ ἀκατάπληκτος, ὀλέθριον στράτευμα κατεξωλόθρευσεν, ἔπαθλον ληψόμενος, ζωὴν τὴν μακαρίαν τε καὶ ἀνώλεθρον."

ocMA.OdeW7C1T.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW7C1T.ode = miscellanea_gr_GR_cog.misc.Ode7
ocMA.OdeW7C1T.melody = he.h.m2_gr_GR_cog.heHE.AntitheonProstagma.name
ocMA.OdeW7C1T.incipit = "Παλάτιον ἔμψυχον τοῦ Βασιλέως, καὶ…"
ocMA.OdeW7C1T.text = "Παλάτιον ἔμψυχον τοῦ Βασιλέως, καὶ θρόνος πυρίμορφος, Παρθένε ἀναδέδειξαι, ἐν ᾧ καθεζόμενος πάντας ἐξήγειρε, τοῦ προτέρου πτώματος βροτούς, καὶ συνεδρίᾳ Πατρὸς ἐτίμησε."

ocMA.OdeW7C21.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW7C21.ode = miscellanea_gr_GR_cog.misc.Ode7
ocMA.OdeW7C21.greekmelody = he.h.m2_gr_GR_cog.heHE.EikonosChrysis.greekname
ocMA.OdeW7C21.melody = he.h.m2_gr_GR_cog.heHE.EikonosChrysis.name
ocMA.OdeW7C21.incipit = "Σταυρῷ προσπαγείς, ὁ ἐκ σοῦ…"
ocMA.OdeW7C21.text = "Σταυρῷ προσπαγείς, ὁ ἐκ σοῦ, σωματωθεὶς Θεογεννήτρια, Ἀδὰμ διέῤῥηξε τὸ χειρόγραφον, ὃν νῦν Παρθένε δυσώπησον, πάσης πονηρίας ῥυσθῆναι, τοὺς ἐν πίστει κραυγάζοντας· Εὐλογητὸς εἶ ὁ Θεός, ὁ τῶν Πατέρων ἡμῶν."

ocMA.OdeW7C22.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW7C22.ode = miscellanea_gr_GR_cog.misc.Ode7
ocMA.OdeW7C22.melody = he.h.m2_gr_GR_cog.heHE.EikonosChrysis.name
ocMA.OdeW7C22.incipit = "Ἐλπὶς ἀγαθή, προστασία φοβερὰ ὑπάρχεις…"
ocMA.OdeW7C22.text = "Ἐλπὶς ἀγαθή, προστασία φοβερὰ ὑπάρχεις Δέσποινα, καὶ νῦν δεόμεθά σου δωρήσασθαι, τῆς συμπαθείας τὸ πέλαγος, πᾶσι τοῖς εἰς σὲ πεποιθόσι, καὶ τῷ Υἱῷ σου κραυγάζουσιν· Εὐλογητὸς εἶ ὁ Θεός, ὁ τῶν Πατέρων ἡμῶν."

ocMA.OdeW7C23.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW7C23.ode = miscellanea_gr_GR_cog.misc.Ode7
ocMA.OdeW7C23.melody = he.h.m2_gr_GR_cog.heHE.EikonosChrysis.name
ocMA.OdeW7C23.incipit = "Σκοτίᾳ δεινῇ, τῇ τοῦ βίου…"
ocMA.OdeW7C23.text = "Σκοτίᾳ δεινῇ, τῇ τοῦ βίου συσχεθεὶς τὸν συλλυπούμενον, καὶ συναλγοῦντα οὐ κατέλαβον, τῇ σῇ Παρθένε λαμπρότητι, λῦσον τὴν ἀχλὺν τῶν πταισμάτων, καὶ καταύγασον ψάλλοντα· Εὐλογητὸς εἶ ὁ Θεός, ὁ τῶν Πατέρων ἡμῶν."

ocMA.OdeW7C24.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW7C24.ode = miscellanea_gr_GR_cog.misc.Ode7
ocMA.OdeW7C24.melody = he.h.m2_gr_GR_cog.heHE.EikonosChrysis.name
ocMA.OdeW7C24.incipit = "Ὡραίαν στολήν, σωτηρίων ἐντολῶν διὰ…"
ocMA.OdeW7C24.text = "Ὡραίαν στολήν, σωτηρίων ἐντολῶν διὰ Βαπτίσματος, ἀμφιεσάμενος ἀπημαύρωσα, διʼ ἀμελείας ὁ δείλαιος, νῦν δέ σοι προστρέχω Παρθένε, τὸν χιτῶνα αἰτούμενος, τῆς σωφροσύνης διὰ σοῦ, πάλιν ἐνδύσασθαι."

ocMA.OdeW7C25.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW7C25.ode = miscellanea_gr_GR_cog.misc.Ode7
ocMA.OdeW7C25.melody = he.h.m2_gr_GR_cog.heHE.EikonosChrysis.name
ocMA.OdeW7C25.incipit = "Ζωῆς ἐκπεσών, ἀκηράτου μὲν τὸ…"
ocMA.OdeW7C25.text = "Ζωῆς ἐκπεσών, ἀκηράτου μὲν τὸ πρὶν ὁ Ἀδὰμ σέσωσται, διὰ τοῦ Τόκου σου, νῦν δὲ Πάναγνε, τὸν ἀῤῥωστίᾳ κρατούμενον, σῶσον ἐπιῤῥώσασα Κόρη, καὶ ἀξίωσον ψάλλειν σοι· Εὐλογημένη ἡ Θεόν, σαρκὶ κυήσασα."

ocMA.OdeW8C1H.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW8C1H.ode = miscellanea_gr_GR_cog.misc.Ode8
ocMA.OdeW8C1H.greekmelody = he.h.m2_gr_GR_cog.heHE.IndalmatosChrysou.greekname
ocMA.OdeW8C1H.melody = he.h.m2_gr_GR_cog.heHE.IndalmatosChrysou.melody
ocMA.OdeW8C1H.incipit = he.h.m2_gr_GR_cog.heHE.IndalmatosChrysou.incipit
ocMA.OdeW8C1H.text = he.h.m2_gr_GR_cog.heHE.IndalmatosChrysou.text

ocMA.OdeW8C11.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW8C11.ode = miscellanea_gr_GR_cog.misc.Ode8
ocMA.OdeW8C11.melody = he.h.m2_gr_GR_cog.heHE.IndalmatosChrysou.name
ocMA.OdeW8C11.incipit = "Λαὸς ὁ ἀπειθής, ἀδεῶς τε…"
ocMA.OdeW8C11.text = "Λαὸς ὁ ἀπειθής, ἀδεῶς τε τὰ παράνομα πάντα πράττων, μέσον ἀνόμων Οἰκτίρμον, τὸν δικαιοῦντά σε παρανομοῦντας, ξύλῳ ἀνυψοῖ σταυρούμενον, ὃν πᾶσα κτίσις δοξάζει ὡς Κύριον, ἀνυμνολογοῦσα τὴν σὴν μακροθυμίαν."

ocMA.OdeW8C12.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW8C12.ode = miscellanea_gr_GR_cog.misc.Ode8
ocMA.OdeW8C12.melody = he.h.m2_gr_GR_cog.heHE.IndalmatosChrysou.name
ocMA.OdeW8C12.incipit = "Αἱματώσας Χριστέ, τοὺς δακτύλους σου…"
ocMA.OdeW8C12.text = "Αἱματώσας Χριστέ, τοὺς δακτύλους σου ἐν ξύλῳ προσηλωμένος, τὸ δαίμοσιν αἷμα προσαγόμενον τοῖς πάλαι, ἐπʼ ἀπωλείᾳ τῶν προσαγομένων ἔπαυσας· ὅθεν σε δοξάζει, ἡ σύμπασα κτίσις, Θεὲ τῶν ὅλων, ἀνυμνολογοῦσα τὴν σὴν μακροθυμίαν."

ocMA.OdeW8C13.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW8C13.ode = miscellanea_gr_GR_cog.misc.Ode8
ocMA.OdeW8C13.melody = he.h.m2_gr_GR_cog.heHE.IndalmatosChrysou.name
ocMA.OdeW8C13.incipit = "Ναμάτων ζωτικῶν, ὄντες ἔμπλεοι οἱ…"
ocMA.OdeW8C13.text = "Ναμάτων ζωτικῶν, ὄντες ἔμπλεοι οἱ Μάρτυρες τοὺς χειμάῤῥους, τοὺς τῆς ἀπάτης ἀπεξήραναν, θείοις ῥείθροις τοῖς τῶν αἱμάτων, πίστει ἀκλινεῖ κραυγάζοντες· Ἡ οὐσιωθεῖσα ὑμνείτω τὸν Κύριον πᾶσα κτίσις, καὶ ὑπερυψούτω εἰς πάντας τοὺς αἰῶνας."

ocMA.OdeW8C14.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW8C14.ode = miscellanea_gr_GR_cog.misc.Ode8
ocMA.OdeW8C14.melody = he.h.m2_gr_GR_cog.heHE.IndalmatosChrysou.name
ocMA.OdeW8C14.incipit = "Ἡ ἄπειρος πληθύς, τῶν αἱμάτων…"
ocMA.OdeW8C14.text = "Ἡ ἄπειρος πληθύς, τῶν αἱμάτων ὑμῶν ἅγιοι, ἀσεβείας τὸ πῦρ κατέσβεσε, καὶ Ἑλλήνων τὴν πολύθεον ἔλυσε πλάνην, πάντας δὲ πιστοὺς ἐφώτισε μελῳδοῦντας· Ὑμνείτω τὸν Κύριον πᾶσα κτίσις, καὶ ὑπερυψούτω εἰς πάντας τοὺς αἰῶνας."

ocMA.OdeW8C1T.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW8C1T.ode = miscellanea_gr_GR_cog.misc.Ode8
ocMA.OdeW8C1T.melody = he.h.m2_gr_GR_cog.heHE.IndalmatosChrysou.name
ocMA.OdeW8C1T.incipit = "Ἡ ἄμωμος ἀμνάς, Προφητῶν τε…"
ocMA.OdeW8C1T.text = "Ἡ ἄμωμος ἀμνάς, Προφητῶν τε καὶ Μαρτύρων ἡ ὡραιότης, ὥσπερ ἀμνόν σε ἀναρτώμενον ἐπὶ ξύλου θεασαμένη, ἔκλαιε πικρῶς καὶ ἔλεγεν· Ἡ οὐσιωθεῖσα ὑμνείτω τὸν Κύριον πᾶσα κτίσις, καὶ ὑπερυψούτω εἰς πάντας τοὺς αἰῶνας."

ocMA.OdeW8C21.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW8C21.ode = miscellanea_gr_GR_cog.misc.Ode8
ocMA.OdeW8C21.greekmelody = he.h.m2_gr_GR_cog.heHE.TonEnKaminoTouPyros.greekname
ocMA.OdeW8C21.melody = he.h.m2_gr_GR_cog.heHE.TonEnKaminoTouPyros.name
ocMA.OdeW8C21.incipit = "Ἐξασθενεῖ μου νῦν ὁ νοῦς…"
ocMA.OdeW8C21.text = "Ἐξασθενεῖ μου νῦν ὁ νοῦς, εἰς τὰ βάθη ἐμπεσὼν τῆς ἀτιμίας, ὡς ἐντεῦθεν ποικίλοις περιπαρῆναι κακοῖς· ἀλλὰ σύ με Παρθένε θεράπευσον, τῷ τῆς ἀπαθείας φωτὶ περιβαλοῦσα."

ocMA.OdeW8C22.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW8C22.ode = miscellanea_gr_GR_cog.misc.Ode8
ocMA.OdeW8C22.melody = he.h.m2_gr_GR_cog.heHE.TonEnKaminoTouPyros.name
ocMA.OdeW8C22.incipit = "Πύργον ἰσχύος ἀσφαλῆ, καὶ κρηπῖδα…"
ocMA.OdeW8C22.text = "Πύργον ἰσχύος ἀσφαλῆ, καὶ κρηπῖδα καὶ φρουρὰν καὶ προστασίαν, κεκτημένοι σε πάντες, διασῳζόμεθα νῦν, ὑμνοῦντες τὸν τόκον, σου Πάναγνε, καὶ ὑπερυψοῦντες εἰς πάντας τοὺς αἰῶνας."

ocMA.OdeW8C23.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW8C23.ode = miscellanea_gr_GR_cog.misc.Ode8
ocMA.OdeW8C23.melody = he.h.m2_gr_GR_cog.heHE.TonEnKaminoTouPyros.name
ocMA.OdeW8C23.incipit = "Ἀθανασίας διαυγῆ, ἐπιστάμεθα πηγήν σε…"
ocMA.OdeW8C23.text = "Ἀθανασίας διαυγῆ, ἐπιστάμεθα πηγήν σε Θεοτόκε, ὡς τεκοῦσαν τὸν Λόγον τοῦ ἀθανάτου Πατρός, τὸν πάντας θανάτου λυτρούμενον, τοὺς ὑπερυψοῦντας αὐτὸν εἰς τοὺς αἰῶνας."

ocMA.OdeW8C24.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW8C24.ode = miscellanea_gr_GR_cog.misc.Ode8
ocMA.OdeW8C24.melody = he.h.m2_gr_GR_cog.heHE.TonEnKaminoTouPyros.name
ocMA.OdeW8C24.incipit = "Ῥεῖθρον ἰάσεων ἡμῖν, ἀενάως τοῖς…"
ocMA.OdeW8C24.text = "Ῥεῖθρον ἰάσεων ἡμῖν, ἀενάως τοῖς πιστοῖς ἁγνὴ πηγάζεις, οὗ τὴν ἄφθονον χάριν ἀπαρυόμενοι, σοῦ ὑμνοῦμεν τὸν τόκον Πανάχραντε, καὶ ὑπερυψοῦμεν εἰς πάντας τοὺς αἰῶνας."

ocMA.OdeW9C1H.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW9C1H.ode = miscellanea_gr_GR_cog.misc.Ode9
ocMA.OdeW9C1H.greekmelody = he.h.m2_gr_GR_cog.heHE.AporeiPasaGlossa.greekname
ocMA.OdeW9C1H.melody = he.h.m2_gr_GR_cog.heHE.AporeiPasaGlossa.melody
ocMA.OdeW9C1H.incipit = he.h.m2_gr_GR_cog.heHE.AporeiPasaGlossa.incipit
ocMA.OdeW9C1H.text = he.h.m2_gr_GR_cog.heHE.AporeiPasaGlossa.text

ocMA.OdeW9C11.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW9C11.ode = miscellanea_gr_GR_cog.misc.Ode9
ocMA.OdeW9C11.melody = he.h.m2_gr_GR_cog.heHE.AporeiPasaGlossa.name
ocMA.OdeW9C11.incipit = "Ἴνα τυπώσῃ πάλαι, Ἰσαὰκ σοῦ…"
ocMA.OdeW9C11.text = "Ἴνα τυπώσῃ πάλαι, Ἰσαὰκ σοῦ τὸ πάθος, δεσμεῖται Λόγε, λύεται δεθέντος δέ, προβάτου ἀντιτύπου, ἐν φυτῷ Σαβὲκ ἀφέσεως, καὶ ἀπελύθη, τῆς ἀκουσίως τότε, θυσίας ἀληθοῦς, σοῦ δὲ ἐθελουσίως τυθέντος, κακῶν ἐῤῥύσθημεν."

ocMA.OdeW9C12.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW9C12.ode = miscellanea_gr_GR_cog.misc.Ode9
ocMA.OdeW9C12.melody = he.h.m2_gr_GR_cog.heHE.AporeiPasaGlossa.name
ocMA.OdeW9C12.incipit = "Ὡραῖος κάλλει, παρὰ τοὺς υἱοὺς…"
ocMA.OdeW9C12.text = "Ὡραῖος κάλλει, παρὰ τοὺς υἱοὺς τῶν ἀνθρώπων, Χριστὲ ὑπάρχων, κάλλος οὐκ ἐκέκτησο, οὐδὲ εἶδος ἐν τῷ πάσχειν, ξύλῳ τοῦ Σταυροῦ κρεμάμενος, καὶ καλλωπίζων, τὴν ἀμορφίαν πᾶσαν τοῦ γένους τῶν βροτῶν. Δόξα τῇ εὐσπλαγχνίᾳ σου μόνε, εὔσπλαγχνε Κύριε."

ocMA.OdeW9C13.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW9C13.ode = miscellanea_gr_GR_cog.misc.Ode9
ocMA.OdeW9C13.melody = he.h.m2_gr_GR_cog.heHE.AporeiPasaGlossa.name
ocMA.OdeW9C13.incipit = "Σιὼν τῆς ἄνω ὤφθητε, οἰκήτορες…"
ocMA.OdeW9C13.text = "Σιὼν τῆς ἄνω ὤφθητε, οἰκήτορες θεῖοι, καὶ τῶν Ἀγγέλων Μάρτυρες ἰσάγγελοι, ὡς ὄντες συμπολῖται, καὶ τὴν Ἐκκλησίαν Ἅγιοι τῶν πρωτοτόκων, περιφανῶς φαιδρύνετε, λάμποντες φωτὶ θείῳ, καὶ μαρτυρίου στεφάνῳ, καλλωπιζόμενοι."

ocMA.OdeW9C14.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW9C14.ode = miscellanea_gr_GR_cog.misc.Ode9
ocMA.OdeW9C14.melody = he.h.m2_gr_GR_cog.heHE.AporeiPasaGlossa.name
ocMA.OdeW9C14.incipit = "Ἠγαπημένοι φίλοι, τοῦ ἡμᾶς παραδόξως…"
ocMA.OdeW9C14.text = "Ἠγαπημένοι φίλοι, τοῦ ἡμᾶς παραδόξως πεφιληκότος, ῥύσασθε φιλίας με δολίας, τῆς πρὸς σάρκα, ἅγιοι Κυρίου Μάρτυρες, ἁγιασμόν τε καὶ φωτισμόν, καὶ λύσιν πταισμάτων χαλεπῶν, πᾶσι τοῖς ἐκτελοῦσι τὴν μνήμην ὑμῶν αἰτήσασθε."

ocMA.OdeW9C1T.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW9C1T.ode = miscellanea_gr_GR_cog.misc.Ode9
ocMA.OdeW9C1T.melody = he.h.m2_gr_GR_cog.heHE.AporeiPasaGlossa.name
ocMA.OdeW9C1T.incipit = "Φωστήρων κάλλος δρόμου, τοῦ συνήθους…"
ocMA.OdeW9C1T.text = "Φωστήρων κάλλος δρόμου, τοῦ συνήθους ἀπέστη, ὅτε σε εἶδεν Ἥλιε τῆς δικαιοσύνης, ἐπηρμένον ἐπὶ τοῦ Σταυροῦ θελήματι, ἡ δὲ Παρθένος, σὺν μαθητῇ παρθένῳ, ἠλάλαζε πικρῶς· Οἴμοι! ἀναβοῶσα, τί τοῦτο τὸ ξένον θέαμα;"

ocMA.OdeW9C21.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW9C21.ode = miscellanea_gr_GR_cog.misc.Ode9
ocMA.OdeW9C21.greekmelody = he.h.m2_gr_GR_cog.heHE.TonEkTheouTheonLogon.greekname
ocMA.OdeW9C21.melody = he.h.m2_gr_GR_cog.heHE.TonEkTheouTheonLogon.name
ocMA.OdeW9C21.incipit = "Θεομακάριστε Κόρη, τὴν ἐλπίδα μου…"
ocMA.OdeW9C21.text = "Θεομακάριστε Κόρη, τὴν ἐλπίδα μου πᾶσαν, προθύμως ἀνατίθημι ἐν σοί, σῶσόν με Μήτηρ τῆς ὄντως ζωῆς, καὶ τρυφῆς ἀϊδίου, πλησθῆναι καθικέτευσον Ἁγνή, τὸν ἐν πίστει καὶ πόθῳ, σὲ ὕμνοις μεγαλύνοντα."

ocMA.OdeW9C22.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW9C22.ode = miscellanea_gr_GR_cog.misc.Ode9
ocMA.OdeW9C22.melody = he.h.m2_gr_GR_cog.heHE.TonEkTheouTheonLogon.name
ocMA.OdeW9C22.incipit = "Ἐπιφανεῖσα Παρθένε, τῆς ψυχῆς μου…"
ocMA.OdeW9C22.text = "Ἐπιφανεῖσα Παρθένε, τῆς ψυχῆς μου τὸν ζόφον, φωτός σου ταῖς ἀΰλοις ἀστραπαῖς, αὔγασον, πύλη τοῦ θείου φωτός, καὶ τρυφῆς αἰωνίου, πλησθῆναι καταξίωσον Ἁγνή, τὸν ἐν πίστει καὶ πόθῳ, σὲ ὕμνοις μεγαλύνοντα."

ocMA.OdeW9C23.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW9C23.ode = miscellanea_gr_GR_cog.misc.Ode9
ocMA.OdeW9C23.melody = he.h.m2_gr_GR_cog.heHE.TonEkTheouTheonLogon.name
ocMA.OdeW9C23.incipit = "Νενοσηκότας ὁρῶσα, καὶ ψυχὴν καὶ…"
ocMA.OdeW9C23.text = "Νενοσηκότας ὁρῶσα, καὶ ψυχὴν καὶ τὸ σῶμα, καὶ βεβλημένους πάθεσι δεινοῖς, τῇ εὐσπλαγχνίᾳ σου Δέσποινα, θεραπεύσασα λῦσον, τῶν νῦν παρενοχλούντων λυπηρῶν, ἵνα πάντες σε ὕμνοις, ἀπαύστως μεγαλύνομεν."

ocMA.OdeW9C24.mode = miscellanea_gr_GR_cog.misc.Mode2
ocMA.OdeW9C24.ode = miscellanea_gr_GR_cog.misc.Ode9
ocMA.OdeW9C24.melody = he.h.m2_gr_GR_cog.heHE.TonEkTheouTheonLogon.name
ocMA.OdeW9C24.incipit = "Ἐν τῇ γαστρί σου σκηνώσας…"
ocMA.OdeW9C24.text = "Ἐν τῇ γαστρί σου σκηνώσας, ἐκ γαστρὸς πρὸ αἰώνων, Πατὴρ ὅνπερ ἐγέννησεν, Υἱὸς τέλειος γέγονεν ἄνθρωπος, καὶ πηγὴν χαρισμάτων, σὲ Θεομῆτορ, ἔδειξεν ἡμῖν, τοῖς πιστῶς προσκυνοῦσι, τὴν ἄφραστόν σου γέννησιν."

