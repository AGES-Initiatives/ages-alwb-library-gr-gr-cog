A_Resource_Whose_Name = he.a.m6_gr_GR_cog

heAU.title = "Αὐτόμελα τοῦ πλ. Βʹ Ἦχου"

heAU.AiAngelikai.greekname = " "
heAU.AiAngelikai.melody = miscellanea_gr_GR_cog.misc.Automelon
heAU.AiAngelikai.mode = miscellanea_gr_GR_cog.misc.Mode6
heAU.AiAngelikai.name = "Αἱ Ἀγγελικαί."
heAU.AiAngelikai.incipit = "Αἱ Ἀγγελικαί…"
heAU.AiAngelikai.text = "Αἱ Ἀγγελικαί, προπορεύεσθε Δυνάμεις, οἱ ἐν Βηθλεέμ, ἐτοιμάσατε τὴν Φάτνην· ὁ λόγος γὰρ γεννᾶται, ἡ σοφία προέρχεται, δέχου ἀσπασμὸν ἡ Ἐκκλησία, εἰς τὴν χαρὰν τῆς Θεοτόκου, λαοὶ εἴπωμεν· Εὐλογημένος ὁ ἐλθών, Θεὸς ἡμῶν δόξα σοι."

heAU.AngelikaiDynameis.greekname = " "
heAU.AngelikaiDynameis.melody = miscellanea_gr_GR_cog.misc.Automelon
heAU.AngelikaiDynameis.mode = miscellanea_gr_GR_cog.misc.Mode6
heAU.AngelikaiDynameis.name = "Ἀγγελικαὶ Δυνάμεις."
heAU.AngelikaiDynameis.incipit = "Ἀγγελικαὶ Δυνάμεις…"
heAU.AngelikaiDynameis.text = "Ἀγγελικαὶ Δυνάμεις ἐπὶ τὸ μνῆμά σου, καὶ οἱ φυλάσσοντες ἀπενεκρώθησαν· καὶ ἵστατο Μαρία ἐν τῷ τάφῳ ζητοῦσα τὸ ἄχραντόν σου σῶμα. Ἐσκύλευσας τὸν Ἅδην, μὴ πειρασθεὶς ὑπʼ αὐτοῦ· ὑπήντησας τῇ Παρθένῳ, δωρούμενος τὴν ζωήν. Ὁ ἀναστὰς ἐκ τῶν νεκρῶν, Κύριε δόξα σοι."

heAU.EkGastrosEtechthis.greekname = " "
heAU.EkGastrosEtechthis.melody = miscellanea_gr_GR_cog.misc.Automelon
heAU.EkGastrosEtechthis.mode = miscellanea_gr_GR_cog.misc.Mode6
heAU.EkGastrosEtechthis.name = "Ἐκ γαστρὸς ἐτέχθης."
heAU.EkGastrosEtechthis.incipit = "Ἐκ γαστρὸς ἐτέχθης…"
heAU.EkGastrosEtechthis.text = "Ἐκ γαστρὸς ἐτέχθης πρὸ ἑωσφόρου, ἐκ Πατρὸς ἀμήτωρ πρὸ τῶν αἰώνων, κἂν Ἄρειος κτίσμα σε, καὶ οὐ Θεὸν δοξάζει, τόλμη συνάπτων σε τὸν κτίστην, τοῖς κτίσμασιν ἀφρόνως, ὕλην πυρὸς τοῦ αἰωνίου, ἑαυτῷ θησαυρίζων· ἀλλʼ ἡ Σύνοδος ἡ ἐν Νικαίᾳ, Υἱὸν Θεοῦ σε ἀνεκήρυξε Κύριε, Πατρὶ καὶ Πνεύματι σύνθρονον."

heAU.ElpisTouKosmouAgathi.greekname = " "
heAU.ElpisTouKosmouAgathi.melody = miscellanea_gr_GR_cog.misc.Automelon
heAU.ElpisTouKosmouAgathi.mode = miscellanea_gr_GR_cog.misc.Mode6
heAU.ElpisTouKosmouAgathi.name = "Ἐλπὶς τοῦ κόσμου ἀγαθή."
heAU.ElpisTouKosmouAgathi.incipit = "Ἐλπὶς τοῦ κόσμου ἀγαθή…"
heAU.ElpisTouKosmouAgathi.text = "Ἐλπὶς τοῦ κόσμου ἀγαθή, Θεοτόκε Παρθένε, τὴν σὴν καὶ μόνην φοβερὰν προστασίαν αἰτοῦμεν, σπλαγχνίσθητι εἰς ἀπροστάτευτον λαόν, δυσώπησον τὸν ἐλεήμονα Θεόν, ῥυσθῆναι τὰς ψυχὰς ἡμῶν, ἐκ πάσης ἀπειλῆς, μόνη εὐλογημένη."

heAU.IApegnosmeni.greekname = " "
heAU.IApegnosmeni.melody = miscellanea_gr_GR_cog.misc.Automelon
heAU.IApegnosmeni.mode = miscellanea_gr_GR_cog.misc.Mode6
heAU.IApegnosmeni.name = "Ἡ ἀπεγνωσμένη."
heAU.IApegnosmeni.incipit = "Ἡ ἀπεγνωσμένη…"
heAU.IApegnosmeni.text = "Ἡ ἀπεγνωσμένη διὰ τὸν βίον, καὶ ἐπεγνωσμένη διὰ τὸν τρόπον, τὸ μύρον βαστάζουσα, προσῆλθέ σοι βοῶσα· Μή με τὴν πόρνην ἀποῤῥίψῃς, ὁ τεχθεὶς ἐκ Παρθένου, μή μου τὰ δάκρυα παρίδῃς, ἡ χαρὰ τῶν Ἀγγέλων, ἀλλὰ δέξαι με μετανοοῦσαν, ἣν οὐκ ἀπώσω ἁμαρτάνουσαν Κύριε, διὰ τὸ μέγα σου ἔλεος."

heAU.KyrieEleisonImas.greekname = " "
heAU.KyrieEleisonImas.melody = miscellanea_gr_GR_cog.misc.Automelon
heAU.KyrieEleisonImas.mode = miscellanea_gr_GR_cog.misc.Mode6
heAU.KyrieEleisonImas.name = "Κύριε ἐλέησον ἡμᾶς."
heAU.KyrieEleisonImas.incipit = "Κύριε ἐλέησον ἡμᾶς…"
heAU.KyrieEleisonImas.text = "Κύριε ἐλέησον ἡμᾶς· ἐπὶ σοὶ γὰρ πεποίθαμεν· μὴ ὀργισθῇς ἡμῖν σφόδρα, μηδὲ μνησθῇς τῶν ἀνομιῶν ἡμῶν· ἀλλʼ ἐπίβλεψον καὶ νῦν ὡς εὔσπλαγχνος, καὶ λύτρωσαι ἡμᾶς ἐκ τῶν ἐχθρῶν ἡμῶν· σὺ γὰρ εἶ Θεὸς ἡμῶν, καὶ ἡμεῖς λαός σου· πάντες ἔργα χειρῶν σου, καὶ τὸ ὄνομά σου ἐπικεκλήμεθα."

heAU.OlinApothemenoi.greekname = " "
heAU.OlinApothemenoi.melody = miscellanea_gr_GR_cog.misc.Automelon
heAU.OlinApothemenoi.mode = miscellanea_gr_GR_cog.misc.Mode6
heAU.OlinApothemenoi.name = "Ὅλην ἀποθέμενοι."
heAU.OlinApothemenoi.incipit = "Ὅλην ἀποθέμενοι…"
heAU.OlinApothemenoi.text = "Ὅλην ἀποθέμενοι, ἐν οὐρανοῖς τὴν ἐλπίδα, θησαυρὸν ἀσύλητον, ἑαυτοῖς οἱ Ἅγιοι ἐθησαύρισαν, δωρεὰν ἔλαβον, δωρεὰν διδοῦσι, τοῖς νοσοῦσι τὰ ἰάματα, χρυσὸν ἢ ἄργυρον, εὐαγγελικῶς οὐκ ἐκτήσαντο, ἀνθρώποις τε καὶ κτήνεσι, τὰς εὐεργεσίας μετέδωκαν, ἵνα διὰ πάντων, ὑπήκοοι γενόμενοι Χριστῷ, ἐν παῤῥησίᾳ πρεσβεύωσιν, ὑπὲρ τῶν ψυχῶν ἡμῶν."

heAU.TinEnPresveiais.greekname = " "
heAU.TinEnPresveiais.melody = miscellanea_gr_GR_cog.misc.Automelon
heAU.TinEnPresveiais.mode = miscellanea_gr_GR_cog.misc.Mode6
heAU.TinEnPresveiais.name = "Τὴν ἐν πρεσβείαις."
heAU.TinEnPresveiais.incipit = "Τὴν ἐν πρεσβείαις…"
heAU.TinEnPresveiais.text = "Τὴν ἐν πρεσβείαις ἀκοίμητον Θεοτόκον, καὶ προστασίαις ἀμετάθετον ἐλπίδα, τάφος καὶ νέκρωσις οὐκ ἐκράτησεν· ὡς γὰρ ζωῆς Μητέρα, πρὸς τὴν ζωὴν μετέστησεν, ὁ μήτραν οἰκήσας ἀειπάρθενον."

heAU.TinYperImon.greekname = " "
heAU.TinYperImon.melody = miscellanea_gr_GR_cog.misc.Automelon
heAU.TinYperImon.mode = miscellanea_gr_GR_cog.misc.Mode6
heAU.TinYperImon.name = "Τὴν ὑπὲρ ἡμῶν."
heAU.TinYperImon.incipit = "Τὴν ὑπὲρ ἡμῶν…"
heAU.TinYperImon.text = "Τὴν ὑπὲρ ἡμῶν πληρώσας οἰκονομίαν, καὶ τὰ ἐπὶ γῆς ἑνώσας τοῖς οὐρανίοις, ἀνελήφθης ἐν δόξῃ, Χριστε ὁ Θεὸς ἡμῶν, οὐδαμόθεν χωριζόμενος, ἀλλὰ μένων ἀδιάστατος, καὶ βοῶν τοῖς ἀγαπῶσί σε· Ἐγώ εἰμι μεθʼ ὑμῶν, καὶ οὐδεὶς καθʼ ὑμῶν."

heAU.TriimerosAnestis.greekname = " "
heAU.TriimerosAnestis.melody = miscellanea_gr_GR_cog.misc.Automelon
heAU.TriimerosAnestis.mode = miscellanea_gr_GR_cog.misc.Mode6
heAU.TriimerosAnestis.name = "Τριήμερος ἀνέστης."
heAU.TriimerosAnestis.incipit = "Τριήμερος ἀνέστης…"
heAU.TriimerosAnestis.text = "Τριήμερος ἀνέστης Χριστέ, ἐκ τάφου, καθὼς γέγραπται, συνεγείρας τὸν προπάτορα ἡμῶν· διό σε καὶ δοξάζει, τὸ γένος τῶν ἀνθρώπων, καὶ ἀνυμνεῖ σου τὴν ἀνάστασιν."
