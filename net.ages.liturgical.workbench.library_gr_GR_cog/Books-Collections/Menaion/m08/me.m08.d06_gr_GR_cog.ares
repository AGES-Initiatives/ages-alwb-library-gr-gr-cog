meMA.C1.acrostic.notice1 = "οὗ ἡ Ἀκροστιχίς·"
meMA.C1.acrostic.text = "Χριστὸς ἐνὶ σκοπιῇ σέλας ἄπλετον εἴδεος ἧκε."
