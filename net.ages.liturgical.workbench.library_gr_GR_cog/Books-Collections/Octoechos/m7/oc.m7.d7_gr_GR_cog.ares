ocMA.ApolTheotokion.text = oc.m7.d7_gr_GR_cog.ocMA.Kathisma23.text

ocLI.Beatitudes1.mode = miscellanea_gr_GR_cog.misc.Mode7
ocLI.Beatitudes1.melody = ""
ocLI.Beatitudes1.incipit = oc.m7.d1_gr_GR_cog.ocLI.Beatitudes1.incipit
ocLI.Beatitudes1.text = oc.m7.d1_gr_GR_cog.ocLI.Beatitudes1.text

ocLI.Beatitudes2.mode = miscellanea_gr_GR_cog.misc.Mode7
ocLI.Beatitudes2.melody = ""
ocLI.Beatitudes2.incipit = "Τελέσαντες τὸν καλὸν ἀγῶνα, καὶ…"
ocLI.Beatitudes2.text = "Τελέσαντες τὸν καλὸν ἀγῶνα, καὶ τὴν πίστιν τηρήσαντες, στεφάνους ἀφθαρσίας παρὰ Θεοῦ, ἐδέξασθε καὶ δόξης, καταξιοῦσθε τῆς αὐτοῦ, Μάρτυρες πανεύφημοι, τῶν Ἀγγέλων σύσκηνοι."

ocLI.Beatitudes3.mode = miscellanea_gr_GR_cog.misc.Mode7
ocLI.Beatitudes3.melody = ""
ocLI.Beatitudes3.incipit = "Ἱερουργῶν καὶ Ὁσίων δῆμοι, Προφητῶν…"
ocLI.Beatitudes3.text = "Ἱερουργῶν καὶ Ὁσίων δῆμοι, Προφητῶν καὶ σεπτῶν Γυναικῶν, κατάλογος τιμάσθω ὁ ἱερός, σκηναῖς τῶν πρωτοτόκων, νῦν κατοικοῦντες ἐν χαρᾷ, καὶ συναγελάζοντες, Ἀσωμάτων τάξεσι."

ocLI.Beatitudes4.mode = miscellanea_gr_GR_cog.misc.Mode7
ocLI.Beatitudes4.melody = ""
ocLI.Beatitudes4.incipit = "Κατάταξον ἐν σκηναῖς Δικαίων, ἐξ…"
ocLI.Beatitudes4.text = "Κατάταξον ἐν σκηναῖς Δικαίων, ἐξ ἡμῶν οὓς μετέστησας, τὰ τούτων παραβλέψας Λόγε Θεοῦ, ἐν γνώσει καὶ ἀγνοίᾳ, πεπραγμένα ἐπὶ γῆς, ψυχικὰ ἐγκλήματα, διὰ μέγα ἔλεος."

ocLI.BeatitudesTr.mode = miscellanea_gr_GR_cog.misc.Mode7
ocLI.BeatitudesTr.melody = ""
ocLI.BeatitudesTr.incipit = "Ἱκέται σοι, ὦ Τριὰς ἁγία…"
ocLI.BeatitudesTr.text = "Ἱκέται σοι, ὦ Τριὰς ἁγία, οἱ θανέντες προσέρχονται, ῥυσθῆναι ἐξαιτοῦντες ἐκ φλογός, τῆς φοβερᾶς ἐκείνης, καὶ τυχεῖν τῶν ἀγαθῶν, δόξης τῆς ἁγίας σου, ἐν ἡμέρᾳ κρίσεως."

ocLI.BeatitudesT.mode = miscellanea_gr_GR_cog.misc.Mode7
ocLI.BeatitudesT.melody = ""
ocLI.BeatitudesT.incipit = "Ἐσκήνωσεν ἐν τῇ σῇ νηδύϊ…"
ocLI.BeatitudesT.text = "Ἐσκήνωσεν ἐν τῇ σῇ νηδύϊ, τοῦ Πατρὸς τὸ ἀπαύγασμα, καὶ τοὺς καθυπαχθέντας τοῦ ἐχθροῦ, κακίστῃ συμβουλίᾳ, καὶ ἐμπεσόντας εἰς φθοράν, Παναγία Ἄχραντε, πάλιν ἀνεκαίνισε."

ocLI.MM.Beatitudes1234TrTas78910GB.text = properties_gr_GR_cog.media.key

ocBK.end.title = "ΤΕΛΟΣ ΤΟΥ ΒΑΡΕΟΣ ΗΧΟΥ"
ocBK.end.verse1.text = "Ὁπλιτικῆς φάλαγγος οἰκεῖον μέλος,"
ocBK.end.verse2.text = "Ὁ τοῦ βάρους, σύ, κλῆσιν εἰληφώς, φέρεις."
ocBK.end.verse3.text = "Ἦχον τὸν ἁπλοῦν, τὸν βάρους ἐπώνυμον,"
ocBK.end.verse4.text = "Ὁ τοὺς λογισμοὺς ἐν βοαῖς μισῶν, φιλεῖ."
ocBK.end.verse5.text = "Ἀνδρῶν δὲ ᾆσμα δευτερότριτε βρέμεις,"
ocBK.end.verse6.text = "Ποικίλος δὲ ὤν ὁ ἁπλοῦς ἔχεις φίλους."

//Octoechos in Triodion

