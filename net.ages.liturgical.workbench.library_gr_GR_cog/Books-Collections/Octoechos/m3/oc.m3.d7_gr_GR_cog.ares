ocLI.Beatitudes1.text = oc.m3.d1_gr_GR_cog.ocLI.Beatitudes1.text

ocLI.Beatitudes2.mode = misc_gr_GR_cog.Mode3
ocLI.Beatitudes2.melody = ""
ocLI.Beatitudes2.incipit = "Τῷ πυρὶ τῶν αἰκισμῶν, προσομιλοῦντες,"
ocLI.Beatitudes2.text = "Τῷ πυρὶ τῶν αἰκισμῶν, προσομιλοῦντες, Ἀθλοφόροι τοῦ Χριστοῦ, δρόσον οὐράνιον εὕρατε, ἐπαναψυχοῦσάν τε, καὶ δυναμοῦσαν φέρειν τῆς σαρκός, τὰς ὀδύνας· ὅθεν κουφίζετε, πάντα πόνον ἀεί, ἀπὸ τῶν ψυχῶν ἡμῶν."

ocLI.Beatitudes3.mode = misc_gr_GR_cog.Mode3
ocLI.Beatitudes3.melody = ""
ocLI.Beatitudes3.incipit = "Ἱεράρχαι ἱεροί, Προφῆται ἔνδοξοι, Ὁσίων"
ocLI.Beatitudes3.text = "Ἱεράρχαι ἱεροί, Προφῆται ἔνδοξοι, Ὁσίων ἡ πληθύς, Γυναίων θείων ὁμήγυρις, ἐναθλησάντων πίστει, καὶ καθελόντων πλάνην τοῦ ἐχθροῦ, οὐρανίου δόξης ἐπέτυχον, ὧν πρεσβείαις Σωτήρ, οἴκτειρον τοὺς δούλους σου."

ocLI.Beatitudes4.mode = misc_gr_GR_cog.Mode3
ocLI.Beatitudes4.melody = ""
ocLI.Beatitudes4.incipit = "Ἔνθα λάμπει σου τὸ φῶς,"
ocLI.Beatitudes4.text = "Ἔνθα λάμπει σου τὸ φῶς, ἐν ταῖς σκηναῖς, τῶν ἐκλεκτῶν σου ὁ Θεός, ὅπου τρυφὴ ἡ ἀέναος, τοὺς ἐξ ἡμῶν ἐν πίστει, μετατεθέντας τάξον Ἰησοῦ, παρορῶν τὰ τούτων ἐγκλήματα, ἵνα σε ἐκτενῶς, Δέσποτα δοξάζωμεν."

ocLI.BeatitudesTr.mode = misc_gr_GR_cog.Mode3
ocLI.BeatitudesTr.melody = ""
ocLI.BeatitudesTr.incipit = "Τὴν προσώποις ἐν τρισί, προσκυνουμένην,"
ocLI.BeatitudesTr.text = "Τὴν προσώποις ἐν τρισί, προσκυνουμένην, μίαν ἄναρχον πιστοί, θείαν Μονάδα δοξάσωμεν, ὅπως ἀξιωθῶμεν, ἁμαρτιῶν ἀφέσεως τυχεῖν, καὶ τῆς Βασιλείας τευξώμεθα, ἀνυμνοῦντες αὐτῆς, τὸ ἄπειρον ἔλεος."

ocLI.BeatitudesT.mode = misc_gr_GR_cog.Mode3
ocLI.BeatitudesT.melody = ""
ocLI.BeatitudesT.incipit = "Φωτισμὸς καὶ ἱλασμός, ἐκ σοῦ"
ocLI.BeatitudesT.text = "Φωτισμὸς καὶ ἱλασμός, ἐκ σοῦ ἐπέφανεν ὁ πάντων Λυτρωτής, θεοχαρίτωτε Δέσποινα, ὃν ἐκτενῶς δυσώπει, ἐν τῇ μελλούσῃ δίκῃ φοβερᾷ, καταδίκης πάσης λυτρώσασθαι, Ἄχραντε τοὺς πιστῶς, πάντοτε ὑμνοῦντάς σε."

ocLI.MM.Beatitudes1234TrTas78910GB.text = properties_gr_GR_cog.media.key

ocBK.end.title = "ΤΕΛΟΣ ΤΟΥ ΤΡΙΤΟΥ ΗΧΟΥ"
ocBK.end.verse1.text = "Εἰ καὶ τρίτος σύ, πλὴν πρὸς ἀνδρικοὺς πόνους,"
ocBK.end.verse2.text = "Σύνεγγυς εἶ πως, τοῦ προάρχοντος Τρίτε."
ocBK.end.verse3.text = "Ἄκομψος, ἁπλοῦς, ἀνδρικὸς πάνυ Τρίτε,"
ocBK.end.verse4.text = "Πέφηνας ὄντως, καὶ σὲ τιμῶμεν Τρίτε."
ocBK.end.verse5.text = "Πλήθους κατάρχων, ἰσαρίθμου σοὶ Τρίτε,"
ocBK.end.verse6.text = "Πλήθει προσήκεις προσφυῶς ἡρμοσμένῳ."

