meMA.LaudsBoth.text = me.m01.d06_gr_GR_cog.meMA.LaudsBoth.text

meLI.Kontakion1.mode = me.m01.d11_gr_GR_cog.meMA.Kontakion1.mode
meLI.Kontakion1.greekmelody = me.m01.d11_gr_GR_cog.meMA.Kontakion1.greekmelody
meLI.Kontakion1.melody = me.m01.d11_gr_GR_cog.meMA.Kontakion1.melody
