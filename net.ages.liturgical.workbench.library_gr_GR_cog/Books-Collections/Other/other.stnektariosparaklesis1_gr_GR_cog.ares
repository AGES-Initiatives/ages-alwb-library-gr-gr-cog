A_Resource_Whose_Name = other.stnektariosparaklesis1_gr_GR_cog

hoCA.title = "ΠΑΡΑΚΛΗΤΙΚΟΣ ΚΑΝΩΝ"
hoCA.title2 = "ΑΓΙΟΥ ΝΕΚΤΑΡΙΟΥ"
hoCA.poet = poets_gr_GR_cog.po.GerasimouMonachouMikragiannanitou
hoCA.translator = ""
hoCA.Insert1 = "τοῦ ἐν ἁγίοις Πατρὸς ἡμῶν Νεκταρίου, Μητροπολίτου Πενταπόλεως, τοῦ Θαυματουργοῦ,"

hoCA.media.text = ""

hoCA.Apolytikion1.mode = miscellanea_gr_GR_cog.misc.Mode4
hoCA.Apolytikion1.melody = he.a.m4_gr_GR_cog.heAU.OYpsotheisEnToStavro.name
hoCA.Apolytikion1.incipit = "Τω Ιεράρχη του Χριστού Νεκταρίω…"
hoCA.Apolytikion1.text = "Τω Ιεράρχη του Χριστού Νεκταρίω, τω δοξασθέντι δωρεάς ουρανίας, προσπέσωμεν κραυγάζοντες εκ βάθους ψυχής, άγιε Νεκτάριε, Ορθοδόξων προστάτα, πάσης ημάς λύτρωσαι, συμφοράς και ανάγκης, και πειρασμών και νόσων χαλεπών, τους καταφεύγοντας, Πάτερ τη σκέπη σου."

hoCA.Apolytikion2.mode = miscellanea_gr_GR_cog.misc.Mode4
hoCA.Apolytikion2.melody = he.a.m4_gr_GR_cog.heAU.OYpsotheisEnToStavro.name
hoCA.Apolytikion2.incipit = "Ως ειληφώς παρά Θεού εξουσίαν…"
hoCA.Apolytikion2.text = "Ως ειληφώς παρά Θεού εξουσίαν, του θεραπεύειν τας δεινάς καχεξίας τῶν προσιόντων, Πάτερ, τοις λειψάνοις σου, ίασαι δεόμεθα, τούς δεινώς θλιβομένους, νόσοις και παθήμασι, και πικραίς αλγηδόσι, και εν ειρήνη φύλαττε ημάς, ταις σαις πρεσβείαις Νεκτάριε όσιε."

hoCA.acrostic = "οὗ ἡ Ἀκροστιχίς· «Παθῶν, Νεκτάριε, ἡμᾶς ῥῦσαι. Γερασίμου»."
hoCA.verse = prayers_gr_GR_cog.pr.verse_SaintM.text

hoCA.Ode11.ode = miscellanea_gr_GR_cog.misc.Ode1
hoCA.Ode11.mode = miscellanea_gr_GR_cog.misc.Mode8
hoCA.Ode11.melody = he.h.m8_gr_GR_cog.heHE.YgranDiodefsas.name
hoCA.Ode11.incipit = "Πληγείς τη κακία του δυσμενούς…"
hoCA.Ode11.text = "Πληγείς τη κακία του δυσμενούς, τη ση αντιλήψει, καταφεύγω αναβοών. Ειρήνευσον Πάτερ την ζωήν μου και την κατ’ άμφω υγείαν μου δώρησαι."

hoCA.Ode12.ode = miscellanea_gr_GR_cog.misc.Ode1
hoCA.Ode12.mode = miscellanea_gr_GR_cog.misc.Mode8
hoCA.Ode12.melody = he.h.m8_gr_GR_cog.heHE.YgranDiodefsas.name
hoCA.Ode12.incipit = "Αγίως ανύσας σου την ζωήν…"
hoCA.Ode12.text = "Αγίως ανύσας σου την ζωήν, αεί αγιάζεις, και λυτρούσαι παντός κακού, Νεκτάριε Πάτερ θεοφόρε, τους προσιόντας τοις θείοις λειψάνοις σου."

hoCA.Ode13.ode = miscellanea_gr_GR_cog.misc.Ode1
hoCA.Ode13.mode = miscellanea_gr_GR_cog.misc.Mode8
hoCA.Ode13.melody = he.h.m8_gr_GR_cog.heHE.YgranDiodefsas.name
hoCA.Ode13.incipit = "Θαυμάτων δυνάμεις ως ενεργών, θεράπευσον…"
hoCA.Ode13.text = "Θαυμάτων δυνάμεις ως ενεργών, θεράπευσον Πάτερ, ασθενείας οδυνηράς, τῶν την σην βοήθειαν ζητούντων, θαυματουργέ Ιεράρχα Νεκτάριε."

hoCA.Ode1T.ode = miscellanea_gr_GR_cog.misc.Ode1
hoCA.Ode1T.mode = miscellanea_gr_GR_cog.misc.Mode8
hoCA.Ode1T.melody = he.h.m8_gr_GR_cog.heHE.YgranDiodefsas.name
hoCA.Ode1T.incipit = "Ως Μήτηρ φιλεύσπλαχνος του Θεού…"
hoCA.Ode1T.text = "Ως Μήτηρ φιλεύσπλαχνος του Θεού, φιλάγαθε Κόρη, ευσπλαχνίσθητι επ’ εμοί, και βλάβης με πάσης και μανίας, του αοράτου απάλλαξον όφεως."

hoCA.Ode31.ode = miscellanea_gr_GR_cog.misc.Ode3
hoCA.Ode31.mode = miscellanea_gr_GR_cog.misc.Mode8
hoCA.Ode31.melody = he.h.m8_gr_GR_cog.heHE.OuraniasApsidos.name
hoCA.Ode31.incipit = "Νοσημάτων παντοίων, χαλεπών θλίψεων, και…"
hoCA.Ode31.text = "Νοσημάτων παντοίων, χαλεπών θλίψεων, και επηρειών ολεθρίων, και επιθέσεων του αρχεκάκου εχθρού, ως συμπαθής Ιεράρχης, ασινείς διάσωζε, τους σε γεραίροντας."

hoCA.Ode32.ode = miscellanea_gr_GR_cog.misc.Ode3
hoCA.Ode32.mode = miscellanea_gr_GR_cog.misc.Mode8
hoCA.Ode32.melody = he.h.m8_gr_GR_cog.heHE.OuraniasApsidos.name
hoCA.Ode32.incipit = "Νέκταρ άυλον θείον, της θεικής…"
hoCA.Ode32.text = "Νέκταρ άυλον θείον, της θεικής χάριτος, ής παρά Θεού εκομίσω, βλύζει εκάστοτε, η θεία κάρα σου, και τῶν παθών την πικρίαν, απελαύνει άγιε, εκ τῶν ψυχών ημών."

hoCA.Ode33.ode = miscellanea_gr_GR_cog.misc.Ode3
hoCA.Ode33.mode = miscellanea_gr_GR_cog.misc.Mode8
hoCA.Ode33.melody = he.h.m8_gr_GR_cog.heHE.OuraniasApsidos.name
hoCA.Ode33.incipit = "Εν οδύναις και πόνοις, και…"
hoCA.Ode33.text = "Εν οδύναις και πόνοις, και θλιβεραίς μάστιξι, Πάτερ την ζωήν μου ανύων, προς σε κατέφυγον, μη ούν παρίδης με, αλλά τη ση επισκέψει, εκ τῶν συνεχόντων με, δεινών απάλλαξον."

hoCA.Ode3T.ode = miscellanea_gr_GR_cog.misc.Ode3
hoCA.Ode3T.mode = miscellanea_gr_GR_cog.misc.Mode8
hoCA.Ode3T.melody = he.h.m8_gr_GR_cog.heHE.OuraniasApsidos.name
hoCA.Ode3T.incipit = "Καταφύγιον κόσμου, και αῤῥαγές στήριγμα…"
hoCA.Ode3T.text = "Καταφύγιον κόσμου, και αῤῥαγές στήριγμα, Κεχαριτωμένη Παρθένε, τους καταφεύγοντας, υπό την σκέπην σου, τῶν πολυπλόκων σκανδάλων, του δολίου δράκοντος, σκέπε και φύλαττε."

hoCA.Troparion31.mode = miscellanea_gr_GR_cog.misc.Mode8
hoCA.Troparion31.melody = ""
hoCA.Troparion31.incipit = "Διάσωσον εκ πάσης βλάβης και…"
hoCA.Troparion31.text = "Διάσωσον εκ πάσης βλάβης και θλίψεως Ιεράρχα, τους εν πίστει τη ση πρεσβεία προστρέχοντας, και σε τιμώντας Νεκτάριε θεοφόρε."

hoCA.Troparion32.mode = miscellanea_gr_GR_cog.misc.Mode8
hoCA.Troparion32.melody = ""
hoCA.Troparion32.incipit = ho.ho23_gr_GR_cog.hoCA.Troparion32.incipit
hoCA.Troparion32.text = ho.ho23_gr_GR_cog.hoCA.Troparion32.text

hoCA.Kathisma11.mode = miscellanea_gr_GR_cog.misc.Mode2
hoCA.Kathisma11.melody = "Πρεσβεία θερμή." //he.a.m2_gr_GR_cog.heAU.TaAnoZiton.name
hoCA.Kathisma11.incipit = "Πηγή δαψιλής, ιάσεων εν Πνεύματι…"
hoCA.Kathisma11.text = "Πηγή δαψιλής, ιάσεων εν Πνεύματι, εδείχθη σοφέ, η θήκη τῶν λειψάνων σου, ιάται γαρ τους πάσχοντας, και υγείαν και ρώσιν χαρίζεται, τοις προσιούσιν εν πίστει θερμή, Νεκτάριε πάτερ ιερώτατε."

hoCA.Ode41.ode = miscellanea_gr_GR_cog.misc.Ode4
hoCA.Ode41.mode = miscellanea_gr_GR_cog.misc.Mode8
hoCA.Ode41.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
hoCA.Ode41.incipit = "Την Μονήν σου Νεκτάριε, πάσης…"
hoCA.Ode41.text = "Την Μονήν σου Νεκτάριε, πάσης επηρείας ανεπηρέαστον, του δολίου πολεμήτορος, Πάτερ διατήρει τη ση χάριτι."

hoCA.Ode42.ode = miscellanea_gr_GR_cog.misc.Ode4
hoCA.Ode42.mode = miscellanea_gr_GR_cog.misc.Mode8
hoCA.Ode42.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
hoCA.Ode42.incipit = "Ασθενείς τους προστρέχοντας, Πάτερ πανταχόθεν…"
hoCA.Ode42.text = "Ασθενείς τους προστρέχοντας, Πάτερ πανταχόθεν τη θεία Μάνδρα σου, θεραπείας καταξίωσον, και την λύπην τούτων διασκέδασον."

hoCA.Ode43.ode = miscellanea_gr_GR_cog.misc.Ode4
hoCA.Ode43.mode = miscellanea_gr_GR_cog.misc.Mode8
hoCA.Ode43.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
hoCA.Ode43.incipit = "Ρώσιν δίδου και ίασιν, την…"
hoCA.Ode43.text = "Ρώσιν δίδου και ίασιν, την κατά ψυχήν και σώμα Νεκτάριε, και πταισμάτων απολύτρωσιν, τους ειλικρινώς σε μακαρίζοουσι."

hoCA.Ode4T.ode = miscellanea_gr_GR_cog.misc.Ode4
hoCA.Ode4T.mode = miscellanea_gr_GR_cog.misc.Mode8
hoCA.Ode4T.melody = he.h.m8_gr_GR_cog.heHE.EisakikoaKyrie.name
hoCA.Ode4T.incipit = "ίθυνόν με Πανάμωμε, προς της…"
hoCA.Ode4T.text = "ίθυνόν με Πανάμωμε, προς της μετανοίας οδόν σωτήριον, και παθών μου τα σκιρτήματα, νέκρωσον εις τέλος και αφάνισον."

hoCA.Ode51.ode = miscellanea_gr_GR_cog.misc.Ode5
hoCA.Ode51.mode = miscellanea_gr_GR_cog.misc.Mode8
hoCA.Ode51.melody = he.h.m8_gr_GR_cog.heHE.FotisonImas.name
hoCA.Ode51.incipit = "Εύρε σε θερμόν, αντιλήπτορα η…"
hoCA.Ode51.text = "Εύρε σε θερμόν, αντιλήπτορα η Αίγινα, δια τούτο σου τη χάριτι αεί, καταφεύγει και λυτρούται πάσης θλίψεως."

hoCA.Ode52.ode = miscellanea_gr_GR_cog.misc.Ode5
hoCA.Ode52.mode = miscellanea_gr_GR_cog.misc.Mode8
hoCA.Ode52.melody = he.h.m8_gr_GR_cog.heHE.FotisonImas.name
hoCA.Ode52.incipit = "ήρεμον ζωήν, διανύειν και ατάραχον…"
hoCA.Ode52.text = "ήρεμον ζωήν, διανύειν και ατάραχον, από πάσης του εχθρού επιβουλής, καταξίωσον ημάς Πάτερ Νεκτάριε."

hoCA.Ode53.ode = miscellanea_gr_GR_cog.misc.Ode5
hoCA.Ode53.mode = miscellanea_gr_GR_cog.misc.Mode8
hoCA.Ode53.melody = he.h.m8_gr_GR_cog.heHE.FotisonImas.name
hoCA.Ode53.incipit = "Μέγαν σε φρουρόν, και προστάτην…"
hoCA.Ode53.text = "Μέγαν σε φρουρόν, και προστάτην Πάτερ όσιε, κεκτημένη η Μονή σου η σεπτή, εγκαυχάται τη ταχεία αντιλήψει σου."

hoCA.Ode5T.ode = miscellanea_gr_GR_cog.misc.Ode5
hoCA.Ode5T.mode = miscellanea_gr_GR_cog.misc.Mode8
hoCA.Ode5T.melody = he.h.m8_gr_GR_cog.heHE.FotisonImas.name
hoCA.Ode5T.incipit = "άχραντε Αγνή, του Θεού Μήτερ…"
hoCA.Ode5T.text = "άχραντε Αγνή, του Θεού Μήτερ απείρανδρε, τους ελπίζοντας τη σκέπη σου αεί, ανωτέρους πάσης βλάβης διατήρησον."

hoCA.Ode61.ode = miscellanea_gr_GR_cog.misc.Ode6
hoCA.Ode61.mode = miscellanea_gr_GR_cog.misc.Mode8
hoCA.Ode61.melody = he.h.m8_gr_GR_cog.heHE.TinDeisinEkcheo.name
hoCA.Ode61.incipit = "Σωμάτων, και τῶν ψυχών τας…"
hoCA.Ode61.text = "Σωμάτων, και τῶν ψυχών τας οδύνας, επικούφησον Νεκτάριε Πάτερ, τῶν προς την σην αφορώντων πρεσβείαν, και τας καρδίας ημών χαράς πλήρωσον, και αθυμίας χαλεπής, εξ ημών την ομίχλην διάλυσον."

hoCA.Ode62.ode = miscellanea_gr_GR_cog.misc.Ode6
hoCA.Ode62.mode = miscellanea_gr_GR_cog.misc.Mode8
hoCA.Ode62.melody = he.h.m8_gr_GR_cog.heHE.TinDeisinEkcheo.name
hoCA.Ode62.incipit = "Ρυσθήναι, πειρατηρίων παντοίων, και σκανδάλων…"
hoCA.Ode62.text = "Ρυσθήναι, πειρατηρίων παντοίων, και σκανδάλων του εχθρού ολεθρίων, και αναγκών και στενώσεων πλείστων, τον Πανοικτίρμονα Λόγον ικέτευε, Νεκτάριε θαυματουργέ, τους προστρέχοντας πίστει τη σκέπη σου."

hoCA.Ode63.ode = miscellanea_gr_GR_cog.misc.Ode6
hoCA.Ode63.mode = miscellanea_gr_GR_cog.misc.Mode8
hoCA.Ode63.melody = he.h.m8_gr_GR_cog.heHE.TinDeisinEkcheo.name
hoCA.Ode63.incipit = "Υψόθεν, ως συμπαθής εποπτεύων, και…"
hoCA.Ode63.text = "Υψόθεν, ως συμπαθής εποπτεύων, και απαύστως προστατεύων μη παύση, της ιεράς σου Μονής θεοφόρε, της κεκτημένης ως μέγα θυσαύρισμα, Νεκτάριε θαυματουργέ, την σορόν τῶν αγίων λειψάνων σου."

hoCA.Ode6T.ode = miscellanea_gr_GR_cog.misc.Ode6
hoCA.Ode6T.mode = miscellanea_gr_GR_cog.misc.Mode8
hoCA.Ode6T.melody = he.h.m8_gr_GR_cog.heHE.TinDeisinEkcheo.name
hoCA.Ode6T.incipit = "Σωτήρα, και Λυτρωτήν τῶν ανθρώπων…"
hoCA.Ode6T.text = "Σωτήρα, και Λυτρωτήν τῶν ανθρώπων, τον φιλάνθρωπον κυήσασα Λόγον, σώσον με Κόρη τη ση προστασία, της πονηράς τυραννίδος του χείρονος, και νεύρωσόν μου την ψυχήν, προς επίδοσιν βίου βελτίονος."

hoCA.Troparion61.mode = miscellanea_gr_GR_cog.misc.Mode8
hoCA.Troparion61.melody = ""
hoCA.Troparion61.incipit = hoCA.Troparion31.incipit
hoCA.Troparion61.text = hoCA.Troparion31.text

