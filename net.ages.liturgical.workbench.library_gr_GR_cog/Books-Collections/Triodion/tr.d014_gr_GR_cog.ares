trMA.Ode7C1H.text = he.h.m8_gr_GR_cog.OEnArchiTinGin.text

trMA.Ode7C11.ode = misc_gr_GR_cog.Ode7
trMA.Ode7C11.mode = misc_gr_GR_cog.Mode8
trMA.Ode7C11.melody = he.h.m8_gr_GR_cog.OEnArchiTinGin.name
trMA.Ode7C11.incipit = "Τῶν εὐσεβῶς, ἐξ αἰῶνος θανέντων…"
trMA.Ode7C11.text = "Τῶν εὐσεβῶς, ἐξ αἰῶνος θανέντων, μνημόσυνα ἐκτελοῦντες βοῶμεν· Εὐλογητὸς εἶ εἰς τοὺς αἰῶνας Κύριε, ὁ Θεὸς τῶν Πατέρων ἡμῶν."

trMA.Ode7C12.ode = misc_gr_GR_cog.Ode7
trMA.Ode7C12.mode = misc_gr_GR_cog.Mode8
trMA.Ode7C12.melody = he.h.m8_gr_GR_cog.OEnArchiTinGin.name
trMA.Ode7C12.incipit = "Τῶν εὐσεβῶς, ἐξαπίνης θανέντων, καὶ…"
trMA.Ode7C12.text = "Τῶν εὐσεβῶς, ἐξαπίνης θανέντων, καὶ ἐκβολῆς παντοδαπῆς ῥιφείσης, σιδήρου, ξύλου, παντοίου λίθου, ἀνάπαυσον ὁ Θεός, κεκοιμημένους πιστούς."

trMA.Ode7C13.ode = misc_gr_GR_cog.Ode7
trMA.Ode7C13.mode = misc_gr_GR_cog.Mode8
trMA.Ode7C13.melody = he.h.m8_gr_GR_cog.OEnArchiTinGin.name
trMA.Ode7C13.incipit = "Ἐν τῇ φρικτῇ, ἐλεύσει σου…"
trMA.Ode7C13.text = "Ἐν τῇ φρικτῇ, ἐλεύσει σου Οἰκτίρμον, ἐκ δεξιῶν τῶν προβάτων σου στῆσον, τοὺς ὀρθοδόξως σοι ἐν βίῳ λειτουργήσαντας Χριστέ, καὶ μεταστάντας πρὸς σέ."

trMA.Ode7C14.ode = misc_gr_GR_cog.Ode7
trMA.Ode7C14.mode = misc_gr_GR_cog.Mode8
trMA.Ode7C14.melody = he.h.m8_gr_GR_cog.OEnArchiTinGin.name
trMA.Ode7C14.incipit = "Ἐν τῷ χορῷ, Χριστὲ τῶν…"
trMA.Ode7C14.text = "Ἐν τῷ χορῷ, Χριστὲ τῶν ἐκλεκτῶν σου, κατάταξον τοὺς σοὺς δούλους βοᾷν σοι· Εὐλογητὸς εἶ εἰς τοὺς αἰῶνας Κύριε, ὁ Θεὸς τῶν Πατέρων ἡμῶν."

trMA.Ode7C15.ode = misc_gr_GR_cog.Ode7
trMA.Ode7C15.mode = misc_gr_GR_cog.Mode8
trMA.Ode7C15.melody = he.h.m8_gr_GR_cog.OEnArchiTinGin.name
trMA.Ode7C15.incipit = "Ὁ ἀπὸ γῆς, τὸν χοῦν…"
trMA.Ode7C15.text = "Ὁ ἀπὸ γῆς, τὸν χοῦν δημιουργήσας, τὸ τῆς σαρκός, καὶ Πνεύματι ζωώσας, Σωτὴρ οἰκτίρμων, οὓς προσελάβου ἀνάπαυσον ὁ Θεός, ἐν τῇ ἀγήρῳ ζωῇ."

trMA.Ode7C1Tr.ode = misc_gr_GR_cog.Ode7
trMA.Ode7C1Tr.mode = misc_gr_GR_cog.Mode8
trMA.Ode7C1Tr.melody = he.h.m8_gr_GR_cog.OEnArchiTinGin.name
trMA.Ode7C1Tr.incipit = "Οἷα τρισίν, ἡλίοις ἡ Θεότης…"
trMA.Ode7C1Tr.text = "Οἷα τρισίν, ἡλίοις ἡ Θεότης, μιᾷ φωτὸς συγκράσει ἀνυμνείσθω, Πατὴρ Υἱός τε, καὶ θεῖον Πνεῦμα, ἐν τῇ φύσει, ἀλλὰ τρία ὑποστάσεσι."

trMA.Ode7C1T.ode = misc_gr_GR_cog.Ode7
trMA.Ode7C1T.mode = misc_gr_GR_cog.Mode8
trMA.Ode7C1T.melody = he.h.m8_gr_GR_cog.OEnArchiTinGin.name
trMA.Ode7C1T.incipit = "Δαυϊτικόν, μελῳδοῦμέν σοι ᾆσμα, ὄρος…"
trMA.Ode7C1T.text = "Δαυϊτικόν, μελῳδοῦμέν σοι ᾆσμα, ὄρος Θεοῦ, σὲ καλοῦντες Παρθένε, ἐν ᾧ οἰκήσας σαρκὶ ὁ Λόγος, ἐθεούργησεν ἡμᾶς, πνευματικῶς ἐν αὐτῷ."

trMA.Ode7K.ode = misc_gr_GR_cog.Ode7
trMA.Ode7K.mode = misc_gr_GR_cog.Mode8
trMA.Ode7K.melody = misc_gr_GR_cog.Heirmos
trMA.Ode7K.incipit = he.h.m8_gr_GR_cog.OEnArchiTinGin.incipit
trMA.Ode7K.text = he.h.m8_gr_GR_cog.OEnArchiTinGin.text

trMA.Ode8C1H.ode = misc_gr_GR_cog.Ode8
trMA.Ode8C1H.mode = misc_gr_GR_cog.Mode8
trMA.Ode8C1H.melody = he.h.m8_gr_GR_cog.TonEnOreiAgio.melody
trMA.Ode8C1H.incipit = he.h.m8_gr_GR_cog.TonEnOreiAgio.incipit
trMA.Ode8C1H.text = he.h.m8_gr_GR_cog.TonEnOreiAgio.text

trMA.Ode8C11.ode = misc_gr_GR_cog.Ode8
trMA.Ode8C11.mode = misc_gr_GR_cog.Mode8
trMA.Ode8C11.melody = he.h.m8_gr_GR_cog.TonEnOreiAgio.name
trMA.Ode8C11.incipit = "Ὁ ἐκτρίψας, πρῴην σκιὰν θανάτου…"
trMA.Ode8C11.text = "Ὁ ἐκτρίψας, πρῴην σκιὰν θανάτου, ἀνατείλας, ὡς Ἥλιος ἐκ τάφου, υἱοὺς τῆς ἀναστάσεώς σου ποίησον, Κύριε τῆς δόξης, πάντας τοὺς θανέντας, ἐν πίστει εἰς αἰῶνας."

trMA.Ode8C12.ode = misc_gr_GR_cog.Ode8
trMA.Ode8C12.mode = misc_gr_GR_cog.Mode8
trMA.Ode8C12.melody = he.h.m8_gr_GR_cog.TonEnOreiAgio.name
trMA.Ode8C12.incipit = "Τῶν ἀδήλων, καὶ κρυφίων ὁ…"
trMA.Ode8C12.text = "Τῶν ἀδήλων, καὶ κρυφίων ὁ γνώστης, ὅταν μέλλῃς, ἐκκαλύπτειν τοῦ σκότους, τὰ ἔργα, καὶ βουλὰς τῶν καρδιῶν ἡμῶν, τότε μὴ συνάρῃς, λόγον μετὰ πάντων, τῶν πίστει κοιμηθέντων."

trMA.Ode8C13.ode = misc_gr_GR_cog.Ode8
trMA.Ode8C13.mode = misc_gr_GR_cog.Mode8
trMA.Ode8C13.melody = he.h.m8_gr_GR_cog.TonEnOreiAgio.name
trMA.Ode8C13.incipit = "Ὅταν μέλλῃς, καθίσαι ἐπὶ θρόνου…"
trMA.Ode8C13.text = "Ὅταν μέλλῃς, καθίσαι ἐπὶ θρόνου, καὶ κελεύσῃς, εἰς κρίσιν παραστῆναι, τοὺς ἐξ ἐσχάτων γῆς διὰ τῆς σάλπιγγος, συναθροιζομένους, τότε φεῖσαι πάντων, Χριστὲ ὡς ἐλεήμων."

trMA.Ode8C14.ode = misc_gr_GR_cog.Ode8
trMA.Ode8C14.mode = misc_gr_GR_cog.Mode8
trMA.Ode8C14.melody = he.h.m8_gr_GR_cog.TonEnOreiAgio.name
trMA.Ode8C14.incipit = "Τοὺς θανέντας, ἄφνω ἐκ συμπτωμάτων…"
trMA.Ode8C14.text = "Τοὺς θανέντας, ἄφνω ἐκ συμπτωμάτων, ἐκ βοῆς τε σφοδρᾶς καὶ τάχους δρόμου, ῥαπίσματος, πυγμῆς τε καὶ λακτίσεως, Κύριε τῆς δόξης, πίστει κοιμηθέντας, ἄνες εἰς τοὺς αἰῶνας."

trMA.Ode8C1Tr.ode = misc_gr_GR_cog.Ode8
trMA.Ode8C1Tr.mode = misc_gr_GR_cog.Mode8
trMA.Ode8C1Tr.melody = he.h.m8_gr_GR_cog.TonEnOreiAgio.name
trMA.Ode8C1Tr.incipit = "Ὡς Μονάδα, τῇ οὐσίᾳ ὑμνῶ…"
trMA.Ode8C1Tr.text = "Ὡς Μονάδα, τῇ οὐσίᾳ ὑμνῶ σε, ὡς Τριάδα, τοῖς προσώποις σε, σέβω, Πάτερ Υἱὲ καὶ Πνεῦμα τὸ πανάγιον, ἄναρχον τὸ κράτος, τῆς σῆς βασιλείας, δοξάζω εἰς αἰῶνας."

trMA.Ode8C1T.ode = misc_gr_GR_cog.Ode8
trMA.Ode8C1T.mode = misc_gr_GR_cog.Mode8
trMA.Ode8C1T.melody = he.h.m8_gr_GR_cog.TonEnOreiAgio.name
trMA.Ode8C1T.incipit = "Ῥείθρου ζῶντος, πηγὴ ἐσφραγισμένη, ἀνεδείχθης…"
trMA.Ode8C1T.text = "Ῥείθρου ζῶντος, πηγὴ ἐσφραγισμένη, ἀνεδείχθης, Θεοτόκε Παρθένε· ἀνάνδρως, γὰρ τὸν Κύριον γεννήσασα, τῆς ἀθανασίας, τοὺς πιστοὺς ποτίζεις, τὸ νᾶμα εἰς αἰῶνας."

trMA.Ode8K.ode = misc_gr_GR_cog.Ode8
trMA.Ode8K.mode = misc_gr_GR_cog.Mode8
trMA.Ode8K.melody = misc_gr_GR_cog.Heirmos
trMA.Ode8K.incipit = he.h.m8_gr_GR_cog.TonEnOreiAgio.incipit
trMA.Ode8K.text = he.h.m8_gr_GR_cog.TonEnOreiAgio.text

trMA.Ode9C1H.ode = misc_gr_GR_cog.Ode9
trMA.Ode9C1H.mode = misc_gr_GR_cog.Mode8
trMA.Ode9C1H.melody = he.h.m8_gr_GR_cog.TonProdilothenta.melody
trMA.Ode9C1H.incipit = he.h.m8_gr_GR_cog.TonProdilothenta.incipit
trMA.Ode9C1H.text = he.h.m8_gr_GR_cog.TonProdilothenta.text

trMA.Ode9C11.ode = misc_gr_GR_cog.Ode9
trMA.Ode9C11.mode = misc_gr_GR_cog.Mode8
trMA.Ode9C11.melody = he.h.m8_gr_GR_cog.TonProdilothenta.name
trMA.Ode9C11.incipit = "Ἔνθα εὐφραινομένων ἐστὶν ἡ κατοικία…"
trMA.Ode9C11.text = "Ἔνθα εὐφραινομένων ἐστὶν ἡ κατοικία, τῶν Ἁγίων σου Κύριε, πάντας τοὺς ἀπʼ αἰῶνος, κεκοιμημένους ἐν πίστει τε καὶ ἐλπίδι, ἀγαλλιασθῆναι καταξίωσον."

trMA.Ode9C12.ode = misc_gr_GR_cog.Ode9
trMA.Ode9C12.mode = misc_gr_GR_cog.Mode8
trMA.Ode9C12.melody = he.h.m8_gr_GR_cog.TonProdilothenta.name
trMA.Ode9C12.incipit = "Τοὺς θεομηνίᾳ, θανατικῶν ἐκτριβέντας, κεραυνῶν…"
trMA.Ode9C12.text = "Τοὺς θεομηνίᾳ, θανατικῶν ἐκτριβέντας, κεραυνῶν παντοίων, ἐξ οὐρανοῦ ἐνεχθέντων, γῆς σχισθείσης, ἐπικλυσάσης θαλάσσης πάντας τοὺς πιστούς, Χριστὲ ἀνάπαυσον."

trMA.Ode9C13.ode = misc_gr_GR_cog.Ode9
trMA.Ode9C13.mode = misc_gr_GR_cog.Mode8
trMA.Ode9C13.melody = he.h.m8_gr_GR_cog.TonProdilothenta.name
trMA.Ode9C13.incipit = "Πᾶσαν ἡλικίαν, πρεσβύτας καὶ νεανίσκους…"
trMA.Ode9C13.text = "Πᾶσαν ἡλικίαν, πρεσβύτας καὶ νεανίσκους, νέους καὶ ἐφήβους, παῖδας, καὶ τὰ ἄωρα βρέφη, ἀῤῥενικὴν φύσιν τε καὶ θηλείαν, ἀνάπαυσον ὁ Θεός, οὓς προσελάβου πιστούς."

trMA.Ode9C14.ode = misc_gr_GR_cog.Ode9
trMA.Ode9C14.mode = misc_gr_GR_cog.Mode8
trMA.Ode9C14.melody = he.h.m8_gr_GR_cog.TonProdilothenta.name
trMA.Ode9C14.incipit = "Τοὺς ἐξ ἰοβόλων δηγμάτων κεκοιμημένους…"
trMA.Ode9C14.text = "Τοὺς ἐξ ἰοβόλων δηγμάτων κεκοιμημένους, καταπόσεως ὄφεων, συμπατήσεως ἵππων καὶ ἐκ πνιγμοῦ, καὶ ἀγχόνης τοῦ πέλας, πίστει σοι λατρεύσαντας ἀνάπαυσον."

trMA.Ode9C15.ode = misc_gr_GR_cog.Ode9
trMA.Ode9C15.mode = misc_gr_GR_cog.Mode8
trMA.Ode9C15.melody = he.h.m8_gr_GR_cog.TonProdilothenta.name
trMA.Ode9C15.incipit = "Ἕκαστον κατʼ ὄνομα, τῶν ἐν…"
trMA.Ode9C15.text = "Ἕκαστον κατʼ ὄνομα, τῶν ἐν πίστει θανέντων, ἀπὸ τοῦ αἰῶνος, καὶ γενεῶν καὶ γενεᾶς, ἀκατακρίτως παρασταθῆναί σοι τότε, ἐν τῇ παρουσία σου ἀξίωσον."

trMA.Ode9C1Tr.ode = misc_gr_GR_cog.Ode9
trMA.Ode9C1Tr.mode = misc_gr_GR_cog.Mode8
trMA.Ode9C1Tr.melody = he.h.m8_gr_GR_cog.TonProdilothenta.name
trMA.Ode9C1Tr.incipit = "Ὁ εἷς ἐν Τριάδι Θεός…"
trMA.Ode9C1Tr.text = "Ὁ εἷς ἐν Τριάδι Θεός, δόξα σοι ἀπαύστως· εἰ γὰρ καὶ Θεὸς ἕκαστον, ἀλλʼ εἷς τῇ φύσει πέλει, ὁ Πατὴρ ὁ Υἱὸς καὶ τὸ Πνεῦμα, τοῖς τρισοφαέσιν ἰδιώμασι."

trMA.Ode9C1T.ode = misc_gr_GR_cog.Ode9
trMA.Ode9C1T.mode = misc_gr_GR_cog.Mode8
trMA.Ode9C1T.melody = he.h.m8_gr_GR_cog.TonProdilothenta.name
trMA.Ode9C1T.incipit = "Ὑπὲρ νοῦν ὁ τόκος σου…"
trMA.Ode9C1T.text = "Ὑπὲρ νοῦν ὁ τόκος σου· γεννᾷς γὰρ τὸν προόντα, καὶ γαλουχεῖς ἀφράστως, τὸν τροφοδότην τοῦ κόσμου, ἀνακλίνεις τὸν τοῦ παντὸς συνοχέα, Χριστὸν μόνον λυτρωτήν ἡμῶν Πανάμωμε."

trMA.Ode9K.ode = misc_gr_GR_cog.Ode9
trMA.Ode9K.mode = misc_gr_GR_cog.Mode8
trMA.Ode9K.melody = misc_gr_GR_cog.Heirmos
trMA.Ode9K.incipit = he.h.m8_gr_GR_cog.TonProdilothenta.incipit
trMA.Ode9K.text = he.h.m8_gr_GR_cog.TonProdilothenta.text

trMA.Exaposteilarion1.mode = misc_gr_GR_cog.Mode3
trMA.Exaposteilarion1.melody = he.a.m3_gr_GR_cog.OOuranonToisAstrois.name
trMA.Exaposteilarion1.incipit = "Ὁ καὶ νεκρῶν καὶ ζώντων…"
trMA.Exaposteilarion1.text = "Ὁ καὶ νεκρῶν καὶ ζώντων, ἐξουσιάζων ὡς Θεός, ἀνάπαυσον τοὺς σοὺς δούλους, ἐν ταῖς σκηναῖς τῶν ἐκλεκτῶν· εἰ γὰρ καὶ ἥμαρτον Σῶτερ, ἀλλʼ οὐκ ἀπέστησαν ἐκ σοῦ."

trMA.Exaposteilarion2.mode = misc_gr_GR_cog.Mode2
trMA.Exaposteilarion2.melody = he.a.m2_gr_GR_cog.GynaikesAkoutisthite.name
trMA.Exaposteilarion2.incipit = "Ἀνάπαυσον τοὺς δούλους σου, ἐν…"
trMA.Exaposteilarion2.text = "Ἀνάπαυσον τοὺς δούλους σου, ἐν χώρᾳ ζώντων Κύριε, ἔνθα ἀπέδρα ὀδύνη, λύπη ὁμοῦ καὶ στεναγμός, ἵλαθι ὡς φιλάνθρωπος, ἅπερ ἐν βίῳ ἥμαρτον· μόνος γὰρ ἀναμάρτητος, ὑπάρχεις καὶ ἐλεήμων, νεκρῶν καὶ ζώντων Δεσπότης."

trMA.ExapTheotokion.mode = misc_gr_GR_cog.Mode2
trMA.ExapTheotokion.melody = he.a.m2_gr_GR_cog.GynaikesAkoutisthite.name
trMA.ExapTheotokion.incipit = "Μαρία θεονύμφευτε, Χριστὸν ἀπαύστως πρέσβευε…"
trMA.ExapTheotokion.text = "Μαρία θεονύμφευτε, Χριστὸν ἀπαύστως πρέσβευε, ὑπερ ἡμῶν τῶν σῶν δούλων, σὺν θεηγόροις Προφήταις, καὶ τῶν Μαρτύρων δήμοις τε, Ἱεραρχῶν Ὁσίων τε, καὶ πάντων τε τῶν Δικαίων, συγκληρονόμους γενέσθαι, τῆς οὐρανῶν βασιλείας."

trMA.Lauds1.mode = misc_gr_GR_cog.Mode8
trMA.Lauds1.melody = he.a.m8_gr_GR_cog.OEnEdemParadeisos.name
trMA.Lauds1.incipit = "Δεῦτε πρὸ τέλους πάντες Ἀδελφοί…"
trMA.Lauds1.text = "Δεῦτε πρὸ τέλους πάντες Ἀδελφοί, τὸν χοῦν ἡμῶν βλέποντες, καὶ τῆς φύσεως ἡμῶν τὸ ἀσθενές, καὶ τὴν εὐτέλειαν ἡμῶν, καὶ τὸ τέλος ὀψώμεθα, καὶ τὰ ὄργανα τοῦ σκεύους τῆς σαρκός, καὶ ὅτι κόνις ὁ ἄνθρωπος, βρῶμα σκωλήκων καὶ φθορά· ὅτι ξηρὰ τὰ ὀστᾶ ἡμῶν, ὅλως μὴ ἔχοντα πνοήν· τοὺς τάφους κατίδωμεν, ποῦ ἡ δόξα; ποῦ τὸ κάλλος τῆς μορφῆς; ποῦ ἡ εὔλαλος γλῶσσα; ποῦ ἡ ὀφρύς; ἢ ποῦ ὁ ὀφθαλμός; πάντα κόνις καὶ σκιά· διὸ φεῖσαι Σωτὴρ πάντων ἡμῶν."

trMA.Lauds2.mode = misc_gr_GR_cog.Mode8
trMA.Lauds2.melody = he.a.m8_gr_GR_cog.OEnEdemParadeisos.name
trMA.Lauds2.incipit = "Τί ἀπατᾶται ἄνθρωπος αὐχῶν, τί…"
trMA.Lauds2.text = "Τί ἀπατᾶται ἄνθρωπος αὐχῶν, τί μάτην ταράττεται, ὁ πηλός, καὶ μετʼ ὀλίγον ὁ αὐτός; τί οὐ λογίζεται ὁ χοῦς, ὅτι κόνις τὸ φύραμα, καὶ σαπρίας καὶ φθορᾶς ἀποβολή; Εἰ οὖν πηλὸς ἐσμὲν ἄνθρωποι, τί προστετήκαμεν τῇ γῆ; καὶ εἰ Χριστοῦ ἐσμὲν σύμφυτοι, τί οὐ προστρέχομεν αὐτῷ, καὶ ὅλην ἀφέμενοι τὴν ἐπίκηρον καὶ ῥέουσαν ζωήν, τῇ ζωῇ τῇ ἀφθάρτῳ, ἀκολουθοῦντες; ἥτις ἐστὶν ὁ Χριστός, ὁ φωτισμὸς τῶν ψυχῶν ἡμῶν."

trMA.Lauds3.mode = misc_gr_GR_cog.Mode8
trMA.Lauds3.melody = he.a.m8_gr_GR_cog.OEnEdemParadeisos.name
trMA.Lauds3.incipit = "Ὁ τῇ χειρί σου πλάσας…"
trMA.Lauds3.text = "Ὁ τῇ χειρί σου πλάσας τὸν Ἀδάμ, καὶ στήσας μεθόριον, ἀφθαρσίας, καὶ θνητότητος Σωτήρ, καὶ τῆς ἐν χάριτι ζωῆς, τῆς φθορᾶς ἀπαλλάξας τε, πρὸς τὴν πρώτην μεταθέμενος ζωήν, αὐτὸς τοὺς δούλους σου Δέσποτα, οὓς προσελάβου ἐξ ἡμῶν, μετὰ Δικαίων ἀνάπαυσον, καὶ ἐν χορῷ τῶν ἐκλεκτῶν, καὶ τούτων τὰ ὀνόματα, μεταγράψας ἐν βίβλῳ τῆς ζωῆς, ἐν φωνῇ Ἀρχαγγέλου, ἐξαναστήσας σάλπιγγος ἠχοῖ, καταξίωσον αὐτούς, τῆς οὐρανίου βασιλείας σου."

trMA.Lauds4.mode = misc_gr_GR_cog.Mode8
trMA.Lauds4.melody = he.a.m8_gr_GR_cog.OEnEdemParadeisos.name
trMA.Lauds4.incipit = "Χριστὸς ἀνέστη λύσας τῶν δεσμῶν…"
trMA.Lauds4.text = "Χριστὸς ἀνέστη λύσας τῶν δεσμῶν, Ἀδὰμ τὸν πρωτόπλαστον, καὶ τοῦ ᾍδου καταλύσας τὴν ἰσχύν, θαρσεῖτε πάντες οἱ νεκροί, ἐνεκρώθη ὁ θάνατος, ἐσκυλεύθη καὶ ὁ ᾍδης σὺν αὐτῷ, καὶ ὁ Χριστὸς ἐβασίλευσεν, ὁ σταυρωθεὶς καὶ ἀναστάς, αὐτὸς ἡμῖν ἐχαρίσατο, τὴν ἀφθαρσίαν τῆς σαρκός, αὐτὸς ἀνιστᾷ ἡμᾶς, καὶ δωρεῖται τὴν ἀνάστασιν ἡμῖν, καὶ τῆς δόξης ἐκείνης, μετʼ εὐφροσύνης πάντας ἀξιοῖ, τοὺς ἐν πίστει ἀκλινεῖ, πεπιστευκότας θερμῶς ἐπʼ αὐτῷ."

