ocMA.C1.acrostic.title = "Κανὼν εἰς Προφήτας, καὶ Μάρτυρας,"
ocMA.C1.acrostic.notice1 = "οὗ ἡ ἀκροστιχις ἐν τῇ θʹ ᾠδῇ·"
ocMA.C1.acrostic.text = "Ἰωσήφ."

ocMA.C2.acrostic.title = "Κανὼν εἰς Κοιμηθέντας,"
ocMA.C2.acrostic.notice1 = "οὗ ἡ ἀκροστιχίς·"
ocMA.C2.acrostic.text = "Πιστῶς θανοῦσιν πρῶτον ὕμνον προσφέρω."

ocMA.CA.notice = ""

