meMA.C2.acrostic.title = "Κανὼν δεύτερος,"
meMA.C2.acrostic.notice1 = "οὗ ἡ Ἀκροστιχίς·"
meMA.C2.acrostic.text = "Ἰσχὺν με τὴν σὴν ζῶσον, ἁγνὴ παρθένε. Ἰωσήφ."
meMA.C2.title = titles_gr_GR_cog.ti.Canon2Theotokos
meMA.C2.verse = prayers_gr_GR_cog.pr.verse_Theotokos.text

