meVE.GeneralApolytikion1.mode = miscellanea_gr_GR_cog.misc.Mode1
meVE.GeneralApolytikion1.melody = he.ga_gr_GR_cog.heGA.TisErimouPolitis.name
meVE.GeneralApolytikion1.name = "Κυριακέ·"
