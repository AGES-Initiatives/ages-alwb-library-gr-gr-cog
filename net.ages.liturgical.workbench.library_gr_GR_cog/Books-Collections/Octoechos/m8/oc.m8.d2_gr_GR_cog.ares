octrMA.Photagogic.mode = miscellanea_gr_GR_cog.misc.Mode8
octrMA.Photagogic.incipit = "Φῶς ὑπάρχων Χριστέ, φώτισόν με…"
octrMA.Photagogic.text = "Φῶς ὑπάρχων Χριστέ, φώτισόν με ἐν σοί,"
