meMA.Ode8C21.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C21.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode8C21.greekmelody = he.h.m1_gr_GR_cog.heHE.MitranAflektos.greekname
meMA.Ode8C21.melody = he.h.m1_gr_GR_cog.heHE.MitranAflektos.name
meMA.Ode8C21.incipit = "Λύμην φυγοῦσα…"
meMA.Ode8C21.text = "Λύμην φυγοῦσα, τοῦ θεοῦσθαι τῇ πλάνῃ * Ἄληκτον ὑμνεῖ, τὸν κενούμενον Λόγον * Νεανικῶς ἅπασα, σὺν τρόμῳ κτίσις. * Ἄδοξον εὖχος, δειματουμένη φέρειν * Ῥευστὴ γεγῶσα, κἂν σοφῶς ἐκαρτέρει."
meMA.Ode8C211.text = "Λύμην φυγοῦσα, τοῦ θεοῦσθαι τῇ πλάνῃ"
meMA.Ode8C211a.text = "Λ"
meMA.Ode8C211b.text = "ύμην φυγοῦσα, τοῦ θεοῦσθαι τῇ πλάνῃ"
meMA.Ode8C212.text = "Ἄληκτον ὑμνεῖ, τὸν κενούμενον Λόγον"
meMA.Ode8C212a.text = "Ἄ"
meMA.Ode8C212b.text = "ληκτον ὑμνεῖ, τὸν κενούμενον Λόγον"
meMA.Ode8C213.text = "Νεανικῶς ἅπασα, σὺν τρόμῳ κτίσις."
meMA.Ode8C213a.text = "Ν"
meMA.Ode8C213b.text = "εανικῶς ἅπασα, σὺν τρόμῳ κτίσις."
meMA.Ode8C214.text = "Ἄδοξον εὖχος, δειματουμένη φέρειν"
meMA.Ode8C214a.text = "Ἄ"
meMA.Ode8C214b.text = "δοξον εὖχος, δειματουμένη φέρειν"
meMA.Ode8C215.text = "Ῥευστὴ γεγῶσα, κἂν σοφῶς ἐκαρτέρει."
meMA.Ode8C215a.text = "Ῥ"
meMA.Ode8C215b.text = "ευστὴ γεγῶσα, κἂν σοφῶς ἐκαρτέρει."

meMA.Ode8C22.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C22.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode8C22.melody = he.h.m1_gr_GR_cog.heHE.MitranAflektos.name
meMA.Ode8C22.incipit = "Ἥκεις πλανῆτιν…"
meMA.Ode8C22.text = "Ἥκεις πλανῆτιν, πρὸς νομὴν ἐπιστρέφων * Τὴν ἀνθοποιόν, ἐξ ἐρημαίων λόφων * Ἡ τῶν ἐθνῶν ἔγερσις, ἀνθρώπων φύσιν· * Ῥώμην βιαίαν, τοῦ βροτοκτόνου σβέσαι * Ἀνὴρ φανείς τε, καὶ Θεὸς προμηθείᾳ."
meMA.Ode8C221.text = "Ἥκεις πλανῆτιν, πρὸς νομὴν ἐπιστρέφων"
meMA.Ode8C221a.text = "Ἥ"
meMA.Ode8C221b.text = "κεις πλανῆτιν, πρὸς νομὴν ἐπιστρέφων"
meMA.Ode8C222.text = "Τὴν ἀνθοποιόν, ἐξ ἐρημαίων λόφων"
meMA.Ode8C222a.text = "Τ"
meMA.Ode8C222b.text = "ὴν ἀνθοποιόν, ἐξ ἐρημαίων λόφων"
meMA.Ode8C223.text = "Ἡ τῶν ἐθνῶν ἔγερσις, ἀνθρώπων φύσιν·"
meMA.Ode8C223a.text = "Ἡ"
meMA.Ode8C223b.text = " τῶν ἐθνῶν ἔγερσις, ἀνθρώπων φύσιν·"
meMA.Ode8C224.text = "Ῥώμην βιαίαν, τοῦ βροτοκτόνου σβέσαι"
meMA.Ode8C224a.text = "Ῥ"
meMA.Ode8C224b.text = "ώμην βιαίαν, τοῦ βροτοκτόνου σβέσαι"
meMA.Ode8C225.text = "Ἀνὴρ φανείς τε, καὶ Θεὸς προμηθείᾳ."
meMA.Ode8C225a.text = "Ἀ"
meMA.Ode8C225b.text = "νὴρ φανείς τε, καὶ Θεὸς προμηθείᾳ."

meMA.Ode9C1M01.incipit = me.m01.d30_gr_GR_cog.meMA.Ode9C2MTh.incipit
meMA.Ode9C1M01.text = me.m01.d30_gr_GR_cog.meMA.Ode9C2MTh.text
meMA.Ode9C1M02.incipit = "Μεγάλυνον ψυχή μου, τὸν ἐκ τῆς Παρθένου…"
meMA.Ode9C1M02.text = "Μεγάλυνον ψυχή μου, τὸν ἐκ τῆς Παρθένου, Θεὸν σαρκὶ τεχθέντα."
meMA.Ode9C1M03.incipit = "Μεγάλυνον ψυχή μου, τὸν ἐν τῷ Σπηλαίῳ…"
meMA.Ode9C1M03.text = "Μεγάλυνον ψυχή μου, τὸν ἐν τῷ Σπηλαίῳ, τεχθέντα Βασιλέα."
meMA.Ode9C1M04.incipit = "Μεγάλυνον ψυχή μου, τῶν ὑπὸ τῶν Μάγων…"
meMA.Ode9C1M04.text = "Μεγάλυνον ψυχή μου, τῶν ὑπὸ τῶν Μάγων, Θεὸν προσκυνηθέντα."
meMA.Ode9C1M05.incipit = "Μεγάλυνον ψυχή μου, τὸν ὑπὸ Ἀστέρος…"
meMA.Ode9C1M05.text = "Μεγάλυνον ψυχή μου, τὸν ὑπὸ Ἀστέρος, τοῖς Μάγοις μηνυθέντα."
meMA.Ode9C1M06.incipit = "Μεγάλυνον ψυχή μου, τὴν ἁγνὴν Παρθένον…"
meMA.Ode9C1M06.text = "Μεγάλυνον ψυχή μου, τὴν ἁγνὴν Παρθένον, τὴν γεννησαμένην, Χριστὸν τὸν Βασιλέα."
meMA.Ode9C1M07.incipit = "Μάγοι καὶ Ποιμένες, ἦλθον προσκυνῆσα…"
meMA.Ode9C1M07.text = "Μάγοι καὶ Ποιμένες, ἦλθον προσκυνῆσαι, Χριστὸν τὸν γεννηθέντα, ἐν Βηθλεὲμ τῇ πόλει."

meMA.Ode9C2M01.incipit = "Σήμερον ἡ Παρθένος, τίκτε…"
meMA.Ode9C2M01.text = "Σήμερον ἡ Παρθένος, τίκτει τὸν Δεσπότην, ἔνδον ἐν τῷ Σπηλαίῳ."
meMA.Ode9C2M02.incipit = "Σήμερον ὁ Δεσπότης, τίκτεται…"
meMA.Ode9C2M02.text = "Σήμερον ὁ Δεσπότης, τίκτεται ὡς βρέφος, ὑπὸ Μητρὸς Παρθένου."
meMA.Ode9C2M03.incipit = "Σήμερον οἱ Ποιμένες, βλέπουσι…"
meMA.Ode9C2M03.text = "Σήμερον οἱ Ποιμένες, βλέπουσι τὸν Σωτῆρα, σπαργάνοις εἱλημένον, καὶ κείμενον ἐν Φάτνῃ."
meMA.Ode9C2M04.incipit = "Σήμερον ὁ Δεσπότης, ῥάκει…"
meMA.Ode9C2M04.text = "Σήμερον ὁ Δεσπότης, ῥάκει σπαργανοῦται, ὁ ἀναφὴς ὡς βρέφος."
meMA.Ode9C2M05.incipit = "Σήμερον πᾶσα κτίσις…"
meMA.Ode9C2M05.text = "Σήμερον πᾶσα κτίσις, ἀγάλλεται καὶ χαίρει, ὅτι Χριστὸς ἐτέχθη, ἐκ τῆς Παρθένου Κόρης."
meMA.Ode9C2M06.incipit = "Οὐράνιαι Δυνάμεις…"
meMA.Ode9C2M06.text = "Οὐράνιαι Δυνάμεις, τεχθέντα τὸν Σωτῆρα, Κύριον καὶ Δεσπότην, μηνύουσι τῷ κόσμῳ."
