meMA.Ode3C11.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C11.greekmelody = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.greekname
meMA.Ode3C11.melody = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.name
meMA.Ode3C11.incipit = "Ἀνύστακτον τὸν πυρσόν, τὸν τῆς…"
meMA.Ode3C11.text = "Ἀνύστακτον τὸν πυρσόν, τὸν τῆς ψυχῆς διατηρῶν πνεύματι, τῆς νοητῆς ἔνδοθεν, Ὅσιε παστάδος ἐσκήνωσας."

meMA.Ode3C12.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C12.melody = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.name
meMA.Ode3C12.incipit = "Ῥεόντων παραδραμών, εὐσεβοφρόνως τὰ τερπνὰ…"
meMA.Ode3C12.text = "Ῥεόντων παραδραμών, εὐσεβοφρόνως τὰ τερπνὰ ἔσπευσας, τὰ μηδαμῶς ῥέοντα, Μαρτινιανὲ ἐγκολπώσασθαι."

meMA.Ode3C13.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C13.melody = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.name
meMA.Ode3C13.incipit = "Στενούμενος πλατυσμῷ, ἀσκητικῆς διαγωγῆς Ὅσιε…"
meMA.Ode3C13.text = "Στενούμενος πλατυσμῷ, ἀσκητικῆς διαγωγῆς Ὅσιε, πλάτος τερπνὸν ἔφθασας, τὸ τοῦ Παραδείσου γηθόμενος."

meMA.Ode3C1T.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C1T.melody = he.h.m4_gr_GR_cog.heHE.EffrainetaiEpiSoi.name
meMA.Ode3C1T.incipit = "Οἰκήσας σου τὴν νηδύν, ὁ…"
meMA.Ode3C1T.text = "Οἰκήσας σου τὴν νηδύν, ὁ κατοικῶν τὸν οὐρανὸν ἄλλον σε, Μήτηρ Θεοῦ ἔμψυχον, ὄντως οὐρανὸν ἀπειργάσατο."

meMA.Ode3Kathisma1.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode3Kathisma1.greekmelody = he.a.m1_gr_GR_cog.heAU.TouLithouSfragisthentos.greekname
meMA.Ode3Kathisma1.melody = he.a.m1_gr_GR_cog.heAU.TouLithouSfragisthentos.name
meMA.Ode3Kathisma1.incipit = "Ἐν πέτρᾳ εὐσεβείας προσερείσας τὸν…"
meMA.Ode3Kathisma1.text = "Ἐν πέτρᾳ εὐσεβείας προσερείσας τὸν πόδα σου, ἐχθροῦ ταῖς μεθοδείαις ἀπερίτρεπτος ἔμεινας, πυρὶ δὲ θερμανθεὶς τῶν ἡδονῶν, εἰς πῦρ σαυτὸν ἐνέβαλες δειχθείς, ἐθελούσιος Μαρτυς, ἐπισκοπῇ τῇ θείᾳ δροσιζόμενος. Δόξα τῷ δεδωκότι σοι ἰσχύν, δόξα τῷ σὲ στεφανώσαντι, δόξα τῷ ἐνεργοῦντι διὰ σοῦ πᾶσιν ἰάματα."

meMA.Ode3KathismaT.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode3KathismaT.melody = he.a.m1_gr_GR_cog.heAU.TouLithouSfragisthentos.name
meMA.Ode3KathismaT.incipit = "Τὰς χεῖράς σου τὰς θείας…"
meMA.Ode3KathismaT.text = "Τὰς χεῖράς σου τὰς θείας, αἷς τὸν Κτίστην ἐβάστασας, Παρθένε Παναγία, σαρκωθέντα χρηστότητι, προτείνουσα δυσώπησον αὐτόν, λυτρώσασθαι ἡμᾶς ἐκ πειρασμῶν, καὶ παθῶν καὶ τῶν κινδύνων, τοὺς εὐφημοῦντάς σε πόθῳ καὶ βοῶντάς σοι· Δόξα τῷ ἐνοικήσαντι ἐν σοί· δόξα τῷ προελθόντι ἐκ σοῦ· δόξα τῷ ἐλευθερώσαντι ἡμᾶς διὰ τοῦ τόκου σου."

meMA.Ode3KathismaSt.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode3KathismaSt.melody = he.a.m1_gr_GR_cog.heAU.TouLithouSfragisthentos.name
meMA.Ode3KathismaSt.incipit = "Σταυρῷ σου προσπαγέντος ὑπὸ τῶν…"
meMA.Ode3KathismaSt.text = "Σταυρῷ σου προσπαγέντος ὑπὸ τῶν παρανόμων, καὶ στρατιωτῶν Σῶτερ λόγχῃ, τὴν πλευρὰν ὀρυγέντος, ἡ Πάναγνος ὠδύρετο πικρῶς, τὰ σπλάγχνα κοπτομένη μητρικῶς, καὶ τὸ πολύ σου καὶ φρικτὸν τῆς ἀνοχῆς, ἐξίστατο βοῶσα· Δόξα τῇ πρὸς ἀνθρώπους σου στοργῇ, δόξα τῇ σῇ χρηστότητι, δόξα τῷ ἐν τῷ θανάτῳ σου, βροτοὺς ἀθανατίζοντι."

meMA.Ode4C1H.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C1H.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C1H.greekmelody = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.greekname
meMA.Ode4C1H.melody = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.melody
meMA.Ode4C1H.incipit = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.incipit
meMA.Ode4C1H.text = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.text

meMA.Ode4C11.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C11.greekmelody = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.greekname
meMA.Ode4C11.melody = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.name
meMA.Ode4C11.incipit = "Ὑπωπιάσας τὴν σάρκα σκληραγωγίαις, τῇ…"
meMA.Ode4C11.text = "Ὑπωπιάσας τὴν σάρκα σκληραγωγίαις, τῇ καθαρᾷ καρδίᾳ σου, φέγγος ὑπεδέξω Πάτερ τὸ τοῦ Πνεύματος, καὶ χαίρων ἐκραύγαζες· Δόξα τῇ δυνάμει σου Κύριε."

meMA.Ode4C12.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C12.melody = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.name
meMA.Ode4C12.incipit = "Ταῖς τοῦ πνεύματος λαμπόμενος φωταυγείαις…"
meMA.Ode4C12.text = "Ταῖς τοῦ πνεύματος λαμπόμενος φωταυγείαις, τὸ σκοτεινὸν παρέδραμες, νέφος τῶν δαιμόνων, μέλπων ἐμμελέστατα, τῷ σὲ δυναμώσαντι· Δόξα τῇ δυνάμει σου Κύριε."

meMA.Ode4C13.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C13.melody = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.name
meMA.Ode4C13.incipit = "Οὐκ ἐσάλευσε τὸν πύργον τῆς…"
meMA.Ode4C13.text = "Οὐκ ἐσάλευσε τὸν πύργον τῆς σῆς καρδίας, ὁ δυσμενὴς προσρήξας σοι, κύματα κακίας· πέτρα γὰρ ἐστήρικτο, Θεοῦ ἀγαπήσεως, Μαρτινιανὲ ἀξιάγαστε."

meMA.Ode4C1T.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C1T.melody = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.name
meMA.Ode4C1T.incipit = "Ὑπερτέρα τῶν Ἀγγέλων ἐδείχθης μόνη…"
meMA.Ode4C1T.text = "Ὑπερτέρα τῶν Ἀγγέλων ἐδείχθης μόνη, βουλῆς μεγάλης Ἀγγελον· τέτοκας γὰρ Λόγον πάντας τὸν φωτίζοντα, Παρθένε τοὺς κραυγάζοντας· Δόξα τῇ δυνάμει σου Κύριε."

meMA.Ode5C1H.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C1H.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C1H.greekmelody = he.h.m4_gr_GR_cog.heHE.SyKyrieMouFos.greekname
meMA.Ode5C1H.melody = he.h.m4_gr_GR_cog.heHE.SyKyrieMouFos.melody
meMA.Ode5C1H.incipit = he.h.m4_gr_GR_cog.heHE.SyKyrieMouFos.incipit
meMA.Ode5C1H.text = he.h.m4_gr_GR_cog.heHE.SyKyrieMouFos.text

meMA.Ode5C11.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C11.greekmelody = he.h.m4_gr_GR_cog.heHE.SyKyrieMouFos.greekname
meMA.Ode5C11.melody = he.h.m4_gr_GR_cog.heHE.SyKyrieMouFos.name
meMA.Ode5C11.incipit = "Τίς δύναται τὰ σά, ἐξειπεῖν…"
meMA.Ode5C11.text = "Τίς δύναται τὰ σά, ἐξειπεῖν κατορθώματα, ἰσάγγελον πολιτείαν· ἐπὶ γῆς γὰρ ἀσκήσας, ὁσίως ὑπερήθλησας."

meMA.Ode5C12.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C12.melody = he.h.m4_gr_GR_cog.heHE.SyKyrieMouFos.name
meMA.Ode5C12.incipit = "Ὄργανον τῆς φθορᾶς, χρηματίσας ὁ…"
meMA.Ode5C12.text = "Ὄργανον τῆς φθορᾶς, χρηματίσας ὁ τύραννος, προσβάλλει σου δελεάσαι, ἡδονῇ· ἀλλʼ ἡττήθη, ταῖς σαῖς Πάτερ ἐνστάσεσιν."

meMA.Ode5C13.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C13.melody = he.h.m4_gr_GR_cog.heHE.SyKyrieMouFos.name
meMA.Ode5C13.incipit = "Ὑπῆρξας σεαυτοῦ, δικαστὴς ἐθελούσιος· τὴν…"
meMA.Ode5C13.text = "Ὑπῆρξας σεαυτοῦ, δικαστὴς ἐθελούσιος· τὴν φλόγα γὰρ ὑπεισδύνων, τῶν παθῶν τὰς καμίνους, Μακάριε κατέσβεσας."

meMA.Ode5C1T.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C1T.melody = he.h.m4_gr_GR_cog.heHE.SyKyrieMouFos.name
meMA.Ode5C1T.incipit = "Σὲ ὅπλον ἀῤῥαγές, κατʼ ἐχθρῶν…"
meMA.Ode5C1T.text = "Σὲ ὅπλον ἀῤῥαγές, κατʼ ἐχθρῶν προβαλλόμεθα, σὲ ἄγκυραν καὶ ἐλπίδα, τῆς ἡμῶν σωτηρίας, Θεόνυμφε κεκτήμεθα."

meMA.Ode6C1H.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C1H.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C1H.greekmelody = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.greekname
meMA.Ode6C1H.melody = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.melody
meMA.Ode6C1H.incipit = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.incipit
meMA.Ode6C1H.text = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.text

meMA.Ode6C11.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C11.greekmelody = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.greekname
meMA.Ode6C11.melody = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.name
meMA.Ode6C11.incipit = "Δολίαις, γυναικὸς λαλιαῖς σοι προσέβαλεν…"
meMA.Ode6C11.text = "Δολίαις, γυναικὸς λαλιαῖς σοι προσέβαλεν, ὡς τῷ Προπάτορι πάλαι, δυσμενὴς ὁ ὄφις, ἀλλʼ ἐπινοίᾳ, τῇ σοφῇ σου, κατηργήθη αὐτοῦ τὰ σοφίσματα."

meMA.Ode6C12.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C12.melody = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.name
meMA.Ode6C12.incipit = "Ῥαδίως, διοδεύσας τὴν τρίβον τὴν…"
meMA.Ode6C12.text = "Ῥαδίως, διοδεύσας τὴν τρίβον τὴν φέρουσαν, πρὸς τὰς ἐκεῖ καταπαύσεις, οὐκ ἐπαύσω Πάτερ διέρχεσθαι, τὰς ἐρήμους, καὶ τὰς πόλεις πολλοῖς ἀγωνίσμασιν."

meMA.Ode6C13.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C13.melody = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.name
meMA.Ode6C13.incipit = "Ὄρεσι, καὶ θαλάσσαις σαυτὸν ἀπεμάκρυνας…"
meMA.Ode6C13.text = "Ὄρεσι, καὶ θαλάσσαις σαυτὸν ἀπεμάκρυνας, τὸ τῆς ἁγνείας καλόν σου, ἱεραῖς μελέταις καρπούμενος, καὶ στεφάνων, λαμπροτάτων Σοφὲ ἀξιούμενος."

meMA.Ode6C14.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C14.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C14.melody = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.name
meMA.Ode6C14.incipit = "Μέλη σου, τῇ πυρᾷ καταφλέγεσθαι…"
meMA.Ode6C14.text = "Μέλη σου, τῇ πυρᾷ καταφλέγεσθαι δέδωκας, τὰς ὑπεκκαύσεις σβεννύων, ἡδονῶν τῇ δρόσῳ τοῦ Πνεύματος, θεοφόρε, μοναστῶν ἀκλινέστατον ἔρεισμα."

meMA.Ode6C1T.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C1T.melody = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.name
meMA.Ode6C1T.incipit = "Ὅλην σε, καθηγίασε Λόγος ὁ…"
meMA.Ode6C1T.text = "Ὅλην σε, καθηγίασε Λόγος ὁ Ἅγιος, σοῦ κατοικήσας τὴν μήτραν, τὴν ἡγιασμένην ἀῤῥήτῳ λόγῳ, ὃν δυσώπει, ἐκτενῶς τοῦ σωθῆναι τοὺς δούλους σου."

meMA.Kontakion1.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Kontakion1.greekmelody = he.a.m2_gr_GR_cog.heAU.TousAsfaleis.greekname
meMA.Kontakion1.melody = he.a.m2_gr_GR_cog.heAU.TousAsfaleis.name
meMA.Kontakion1.incipit = "Ὡς ἀσκητὴν τῆς εὐσεβείας δόκιμον…"
meMA.Kontakion1.text = "Ὡς ἀσκητὴν τῆς εὐσεβείας δόκιμον, καὶ ἀθλητὴν τῇ προαιρέσει τίμιον, καὶ ἐρήμου καρτερόψυχον, πολίτην ἅμα καὶ συνίστορα, ἐν ὕμνοις ἐπαξίως εὐφημήσωμεν, Μαρτινιανόν τὸν ἀεισέβαστον· αὐτὸς γὰρ τὸν ὄφιν κατεπάτησεν."

meMA.Oikos1.text = "Ἀπὸ περάτων ἕως περάτων διεξῆλθεν ὁ φθόγγος τῶν τερπνῶν ἀρετῶν, καὶ τῶν ἐνθέων ἀγώνων σου· νέος γὰρ ὑπάρχων τῇ ἡλικίᾳ, ἐρήμοις ἐπόθησας συνδιαιτᾶσθαι, ὕμνους Χριστῷ, ψαλμῳδίας, εὐχὰς ἀναπέμπων ἀεί· ἡμέραν τε καὶ νύκτα συναύξων ἔν τε πόνοις καὶ δάκρυσιν, ἁγνῶς τὸν βίον ἐτέλεσας, καὶ σοφῶς τὸν ἀρχέκακον ᾔσχυνας. Αὐτὸς γὰρ τὸν ὄφιν κατεπάτησας."

meMA.Ode7C1H.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C1H.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C1H.greekmelody = he.h.m4_gr_GR_cog.heHE.EnTiKaminoAvramiaioi.greekname
meMA.Ode7C1H.melody = he.h.m4_gr_GR_cog.heHE.EnTiKaminoAvramiaioi.melody
meMA.Ode7C1H.incipit = he.h.m4_gr_GR_cog.heHE.EnTiKaminoAvramiaioi.incipit
meMA.Ode7C1H.text = he.h.m4_gr_GR_cog.heHE.EnTiKaminoAvramiaioi.text

meMA.Ode7C11.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C11.greekmelody = he.h.m4_gr_GR_cog.heHE.EnTiKaminoAvramiaioi.greekname
meMA.Ode7C11.melody = he.h.m4_gr_GR_cog.heHE.EnTiKaminoAvramiaioi.name
meMA.Ode7C11.incipit = "Ὑπὸ τῆς θείας, κυβερνώμενος Πάτερ…"
meMA.Ode7C11.text = "Ὑπὸ τῆς θείας, κυβερνώμενος Πάτερ χειρός, ὥσπερ, Ἰωνᾶς ἀπέῤῥιψας σεαυτόν, εἰς βυθὸν θαλάσσης Ὅσιε, θηρσὶν ὀχούμενος, καὶ τῇ χέρσῳ λαμπρὸς ἐκδιδόμενος."

meMA.Ode7C12.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C12.melody = he.h.m4_gr_GR_cog.heHE.EnTiKaminoAvramiaioi.name
meMA.Ode7C12.incipit = "Συμπαθεστάτῃ, προαιρέσει Πάτερ σῴζεις σαφῶς…"
meMA.Ode7C12.text = "Συμπαθεστάτῃ, προαιρέσει Πάτερ σῴζεις σαφῶς κόρην πικροτάτης ζάλης καὶ ἀκλινῆ ἐπὶ πέτραν θείας γνώσεως, στηρίζεις ψάλλουσαν, καὶ Θεῷ εὐαρέστως λατρεύουσαν."

meMA.Ode7C13.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C13.melody = he.h.m4_gr_GR_cog.heHE.EnTiKaminoAvramiaioi.name
meMA.Ode7C13.incipit = "Θεοῦ προστάξει, θαλαττίου ῥυσθεὶς ἐκ…"
meMA.Ode7C13.text = "Θεοῦ προστάξει, θαλαττίου ῥυσθεὶς ἐκ κλύδωνος, ταύτην μακαρίαν δέδωκας ἀμοιβήν, τὴν τοῦ σώματός σου νέκρωσιν, καὶ τὴν ὁλόκληρον, πρὸς τὸν πάντων Δεσπότην διάθεσιν."

meMA.Ode7C1T.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C1T.melody = he.h.m4_gr_GR_cog.heHE.EnTiKaminoAvramiaioi.name
meMA.Ode7C1T.incipit = "Ἐκ σοῦ τῆς δόξης, Θεογεννῆτορ…"
meMA.Ode7C1T.text = "Ἐκ σοῦ τῆς δόξης, Θεογεννῆτορ λάμψας Ἥλιος, πάντων τῶν πιστῶν ἐφώτισε τὰς ψυχάς, μελῳδούντων θείῳ Πνεύματι· Εὐλογημένος εἶ ἐν τῷ ναῷ τῆς δόξης σου Κύριε."

meMA.Ode8C1H.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C1H.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C1H.greekmelody = he.h.m4_gr_GR_cog.heHE.CheirasEkpetasasDaniil.greekname
meMA.Ode8C1H.melody = he.h.m4_gr_GR_cog.heHE.CheirasEkpetasasDaniil.melody
meMA.Ode8C1H.incipit = he.h.m4_gr_GR_cog.heHE.CheirasEkpetasasDaniil.incipit
meMA.Ode8C1H.text = he.h.m4_gr_GR_cog.heHE.CheirasEkpetasasDaniil.text

meMA.Ode8C11.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C11.greekmelody = he.h.m4_gr_GR_cog.heHE.CheirasEkpetasasDaniil.greekname
meMA.Ode8C11.melody = he.h.m4_gr_GR_cog.heHE.CheirasEkpetasasDaniil.name
meMA.Ode8C11.incipit = "Ἰσχύς μου καὶ ὕμνησις αὐτός…"
meMA.Ode8C11.text = "Ἰσχύς μου καὶ ὕμνησις αὐτός, ὑπάρχεις Κύριε, μερὶς καὶ κλῆρός μου, σοῦ τὴν ἑκούσιον νέκρωσιν, περιφέρων περιέρχομαι, πόλεις καὶ χώρας ἐμμελῶς, Πάτερ ἐκραύγαζες· Εὐλογεῖτε, πάντα τὰ ἔργα Κυρίου τὸν Κύριον."

meMA.Ode8C12.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C12.melody = he.h.m4_gr_GR_cog.heHE.CheirasEkpetasasDaniil.name
meMA.Ode8C12.incipit = "Οὐ ψύχει χαυνούμενος τὸν νοῦν…"
meMA.Ode8C12.text = "Οὐ ψύχει χαυνούμενος τὸν νοῦν, ἀλλʼ οὐδὲ καύσωνι, ψυχὴν φλεγόμενος, ὅλως ἐνέδωκας θλίβων σου, τὸ σαρκίον, ἀλλʼ ὑπέφερες, τὴν τοῖς δικαίοις ἐννοῶν μακαριότητα, καὶ κραυγάζων· Πάντα τὰ ἔργα ὑμνεῖτε τὸν Κύριον."

meMA.Ode8C13.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C13.melody = he.h.m4_gr_GR_cog.heHE.CheirasEkpetasasDaniil.name
meMA.Ode8C13.incipit = "Ἱστίῳ πτερούμενος σοφέ, τοῦ θείου…"
meMA.Ode8C13.text = "Ἱστίῳ πτερούμενος σοφέ, τοῦ θείου Πνεύματος, κούφως διέπλασας, βίου τὴν θάλασσαν Ὅσιε, καὶ λιμένι προσεπέλασας, τῆς βασιλείας τοῦ Θεοῦ, μέλπων γηθόμενος· Εὐλογεῖτε, πάντα τὰ ἔργα Κυρίου τὸν Κύριον."

meMA.Ode8C14.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C14.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C14.melody = he.h.m4_gr_GR_cog.heHE.CheirasEkpetasasDaniil.name
meMA.Ode8C14.incipit = "Σκηναῖς ἐναυλίζῃ φωτειναῖς, ἀποδυσάμενος, νέφος…"
meMA.Ode8C14.text = "Σκηναῖς ἐναυλίζῃ φωτειναῖς, ἀποδυσάμενος, νέφος τοῦ σώματος, στολαῖς φαιδρύνῃ δὲ Ὅσιε, ἐξ ἱδρώτων ὑφανθείσαις σου, ἀσκητικῶν, καὶ μελῳδεῖς καθαρωτάτῃ ψυχῇ· Εὐλογεῖτε, πάντα τὰ ἔργα Κυρίου τὸν Κύριον."

meMA.Ode8C1T.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C1T.melody = he.h.m4_gr_GR_cog.heHE.CheirasEkpetasasDaniil.name
meMA.Ode8C1T.incipit = "Λυτρούμενος κόσμον τῆς φθορᾶς, ἐκ…"
meMA.Ode8C1T.text = "Λυτρούμενος κόσμον τῆς φθορᾶς, ἐκ σοῦ σεσάρκωται ὁ ὑπερούσιος, θεοχαρίτωτε Δέσποινα, ἐνεργείαις καὶ θελήσεσι, διπλοῦς ὁρώμενος, μιᾷ τῇ ὑποστάσει δέ, ᾧ βοῶμεν· Πάντα τὰ ἔργα ὑμνεῖτε τὸν Κύριον."

