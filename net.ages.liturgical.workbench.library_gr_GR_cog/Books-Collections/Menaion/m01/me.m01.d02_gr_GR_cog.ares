meCO.Ode1C11.ode = misc_gr_GR_cog.Ode1
meCO.Ode1C11.mode = misc_gr_GR_cog.Mode2
meCO.Ode1C11.melody = he.h.m2_gr_GR_cog.ToTinAvaton.name
meCO.Ode1C11.incipit = "Ἡ ἀπόῤῥητος, τοῦ Κτίστου συγκατάβασις…"
meCO.Ode1C11.text = "Ἡ ἀπόῤῥητος, τοῦ Κτίστου συγκατάβασις, πρὶν ἐκ Παρθένου λάμψασα, κόσμον κατηύγασεν, αὐτὸς πάλιν ὁ Χριστός, ἄλλο νῦν μυστήριον, τελέσων ἔρχεται, ἐν Ἰορδάνῃ βροτῶν, εἰς ξένην ἀναγέννησιν."

meCO.Ode1C12.ode = misc_gr_GR_cog.Ode1
meCO.Ode1C12.mode = misc_gr_GR_cog.Mode2
meCO.Ode1C12.melody = he.h.m2_gr_GR_cog.ToTinAvaton.name
meCO.Ode1C12.incipit = "Διακονήσων, ἔργῳ φρικτῷ προέρχεται, πρὸς…"
meCO.Ode1C12.text = "Διακονήσων, ἔργῳ φρικτῷ προέρχεται, πρὸς Ἰορδάνην Πρόδρομος, νῦν ἡ τοῦ Λόγου φωνή, τὸν πτωχεύσαντα Ἀδάμ, ὁ πλουτῶν θεότητι· διὰ τοῦ Πνεύματος γάρ, πλουτήσων ἥκει Χριστός, τῷ δώρῳ τοῦ Βαπτίσματος."

meCO.Ode8C1H.ode = misc_gr_GR_cog.Ode8
meCO.Ode8C1H.mode = misc_gr_GR_cog.Mode2
meCO.Ode8C1H.melody = misc_gr_GR_cog.Heirmos
meCO.Ode8C1H.incipit = he.h.m2_gr_GR_cog.EfrixePaidonEvagon.incipit
meCO.Ode8C1H.text = he.h.m2_gr_GR_cog.EfrixePaidonEvagon.text

meCO.Ode8C11.ode = misc_gr_GR_cog.Ode8
meCO.Ode8C11.mode = misc_gr_GR_cog.Mode2
meCO.Ode8C11.melody = he.h.m2_gr_GR_cog.EfrixePaidonEvagon.name
meCO.Ode8C11.incipit = "Ὑμᾶς μου σκεύη ἐκλεκτά, ἐπιγνώσομαι…"
meCO.Ode8C11.text = "Ὑμᾶς μου σκεύη ἐκλεκτά, ἐπιγνώσομαι, εἰ τῆς ἐμῆς δωρεᾶς, σφραγὶς ἐνσημανθῇ, ἐν ὑμῖν τρισσὸν φῶς ἀπαστράπτουσα, πρὸς τὸ Βάπτισμα Χριστός, ὁ Λόγος φησί, τοῖς ῥᾳθυμίας ὄκνῳ διαμένουσιν· ἐν τούτῳ γὰρ νέαν ὁδὸν σωτηρίας, καὶ ἐλευθερίας, ζωὴν ἥκω δωρούμενος."

meCO.Ode8C12.ode = misc_gr_GR_cog.Ode8
meCO.Ode8C12.mode = misc_gr_GR_cog.Mode2
meCO.Ode8C12.melody = he.h.m2_gr_GR_cog.EfrixePaidonEvagon.name
meCO.Ode8C12.incipit = "Τάξεως ἔμπαλιν ἐν σοί, φυσικῆς…"
meCO.Ode8C12.text = "Τάξεως ἔμπαλιν ἐν σοί, φυσικῆς ἐστι πρᾶγμα τελούμενον νῦν, τὸ ἔλαττον καὶ γάρ, γνώμῃ εὐλογεῖται τοῦ κρείττονος· ὅθεν δέδοικα σὴν χειραπτῆσαι κάραν, τῷ Λυτρωτῇ ἐβόησεν ὁ Πρόδρομος· οὐ θέμις γὰρ χόρτον πυρὶ προσεγγίζειν, σὺ με μᾶλλον, ἁγίασον σὸν δοῦλον."

meCO.Ode8C21.ode = misc_gr_GR_cog.Ode8
meCO.Ode8C21.mode = misc_gr_GR_cog.Mode2
meCO.Ode8C21.melody = he.h.m2_gr_GR_cog.ToDogmatiToTyranniko.name
meCO.Ode8C21.incipit = "Ῥᾳθυμίας ὕπνον ψυχικόν, τινάξαντες καρδίας…"
meCO.Ode8C21.text = "Ῥᾳθυμίας ὕπνον ψυχικόν, τινάξαντες καρδίας ἐγρηγόρσει, τῷ ἐρχομένῳ καθᾶραι ψυχὰς ἐν ῥείθροις βοήσωμεν· Εὐλογεῖτε τὰ ἔργα, Κυρίου τὸν Κύριον."

meCO.Ode8C22.ode = misc_gr_GR_cog.Ode8
meCO.Ode8C22.mode = misc_gr_GR_cog.Mode2
meCO.Ode8C22.melody = he.h.m2_gr_GR_cog.ToDogmatiToTyranniko.name
meCO.Ode8C22.incipit = "Ἱκανούσθω πρᾶξις ἐν ἡμῖν, ψυχῆς…"
meCO.Ode8C22.text = "Ἱκανούσθω πρᾶξις ἐν ἡμῖν, ψυχῆς ἐν ὑψηλῇ θεωρίᾳ ὅπως κατʼ ἄμφω φανέντες φαιδροί, τῷ πάντας καθαίροντι· Εὐλογεῖτε τὰ ἔργα, βοῶμεν τὸν Κύριον."

meCO.Ode8C23.ode = misc_gr_GR_cog.Ode8
meCO.Ode8C23.mode = misc_gr_GR_cog.Mode2
meCO.Ode8C23.melody = he.h.m2_gr_GR_cog.ToDogmatiToTyranniko.name
meCO.Ode8C23.incipit = "Τὸ τάλαντον λόγῳ πρακτικῷ, κοσμήσαντες…"
meCO.Ode8C23.text = "Τὸ τάλαντον λόγῳ πρακτικῷ, κοσμήσαντες ἐν διπλῇ ἐργασίᾳ, πίστει τῷ δόντι Δεσπότῃ πιστοί, ὡς δοῦλοι προσάξωμεν, βαπτισθῆναι μολοῦντι, ψυχῶν εἰς ἀνάπλασιν."

