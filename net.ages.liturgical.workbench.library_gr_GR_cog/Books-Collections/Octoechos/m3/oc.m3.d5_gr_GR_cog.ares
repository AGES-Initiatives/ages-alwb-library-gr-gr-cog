A_Resource_Whose_Name = oc.m3.d5_gr_GR_cog

ocVE.Stichera1.mode = misc_gr_GR_cog.Mode3
ocVE.Stichera1.melody = he.a.m3_gr_GR_cog.MegaliTonMartyronSou.incipit
ocVE.Stichera1.incipit = "Πρεσβείαις τῶν σεπτῶν καὶ θείων"
ocVE.Stichera1.text = "Πρεσβείαις τῶν σεπτῶν καὶ θείων Ἀποστόλων σου, φιλάνθρωπε Οἰκτίρμον, καὶ μόνε ἐλεῆμον, τὴν εἰρήνην σου παράσχου τῷ λαῷ σου, καὶ τῶν δεινῶν λύτρωσαι, τοὺς ἀνυμνοῦντάς σε, καὶ πιστῶς προσκυνοῦντας."

ocVE.Stichera2.mode = misc_gr_GR_cog.Mode3
ocVE.Stichera2.melody = he.a.m3_gr_GR_cog.MegaliTonMartyronSou.incipit
ocVE.Stichera2.incipit = "Βελῶν με ὡς Θεοῦ αὐτόπται"
ocVE.Stichera2.text = "Βελῶν με ὡς Θεοῦ αὐτόπται τοῦ ἀλάστορος, λυτρώσασθε σβεννῦντες, αὐτοῦ τὰς πανουργίας, καὶ δροσίσατέ με πνεύματος τῇ δρόσῳ, ἁμαρτιῶν τῷ καύσωνι, ἐκτηκόμενον, εὐεργέται μοι θεῖοι."

ocVE.Stichera3.mode = misc_gr_GR_cog.Mode3
ocVE.Stichera3.melody = he.a.m3_gr_GR_cog.MegaliTonMartyronSou.incipit
ocVE.Stichera3.incipit = "Εἰς πᾶσαν οἰκουμένην ὄντως ἐξελήλυθεν,"
ocVE.Stichera3.text = "Εἰς πᾶσαν οἰκουμένην ὄντως ἐξελήλυθεν, ὁ θεῖος ὑμῶν φθόγγος, Ἀπόστολοι Κυρίου, καὶ ἐφώτισεν ἐν σκότει τοὺς κειμένους, καὶ τῷ Θεῷ προσήγαγεν, ὡς θυμίαμα, τοὺς αὐτὸν ἐγνωκότας."

ocVE.Stichera4.mode = misc_gr_GR_cog.Mode3
ocVE.Stichera4.melody = he.a.m3_gr_GR_cog.MegaliTonMartyronSou.incipit
ocVE.Stichera4.incipit = "Μεγίστη σωτηρία πᾶσιν ἀνεδείχθης ἡμῖν,"
ocVE.Stichera4.text = "Μεγίστη σωτηρία πᾶσιν ἀνεδείχθης ἡμῖν, Νικόλαε τρισμάκαρ· λυτροῦσαι γὰρ παντοιων, τοὺς οἰκέτας σου κινδύνων, καὶ ἀνάγκης καὶ πειρασμῶν, καὶ νόσων, καὶ περιστάσεων, καὶ ἐχθρῶν ἀοράτων."

ocVE.Stichera5.mode = misc_gr_GR_cog.Mode3
ocVE.Stichera5.melody = he.a.m3_gr_GR_cog.MegaliTonMartyronSou.incipit
ocVE.Stichera5.incipit = "Ἐπλήγην ἰοβόλῳ δήγματι ὁ ἄσωτος,"
ocVE.Stichera5.text = "Ἐπλήγην ἰοβόλῳ δήγματι ὁ ἄσωτος, καὶ κεῖμαι ἐῤῥιμμένος καὶ ἐξηπορημένος, τῇ ἀγρύπνῳ σου δεήσει Ἱεράρχα, ἐν τάχει ἀνάστησον, ἵνα δοξάζω σου, τὴν ταχύκοον χάριν."

ocVE.Stichera6.mode = misc_gr_GR_cog.Mode3
ocVE.Stichera6.melody = he.a.m3_gr_GR_cog.MegaliTonMartyronSou.incipit
ocVE.Stichera6.incipit = "Ἐν ὥρᾳ τῇ φρικτῇ, ἐν"
ocVE.Stichera6.text = "Ἐν ὥρᾳ τῇ φρικτῇ, ἐν ᾗ μέλλω παρίστασθαι, τὸ βῆμα τὸ φρικῶδες, καὶ πέμπεσθαι κολάσει, βοηθόν σε εὕροιμι Ἱεράρχα, ῥυόμενόν με σκότους, καθικετεύω σε, παῤῥησίαν ὡς ἔχεις."

ocVE.SticTheotokion.mode = misc_gr_GR_cog.Mode3
ocVE.SticTheotokion.melody = he.a.m3_gr_GR_cog.MegaliTonMartyronSou.incipit
ocVE.SticTheotokion.incipit = "Ἁπάντων κυριεύουσα τῶν κτισμάτων Δέσποινα,"
ocVE.SticTheotokion.text = "Ἁπάντων κυριεύουσα τῶν κτισμάτων Δέσποινα, δεινῶς κυριευθεῖσαν, τοῖς πάθεσι Θεόνυμφε, τὴν ψυχήν μου ἐλευθέρωσον ἐν τάχει, φαρμάκῳ σῆς πρεσβείας, καὶ καταδούλωσον τῷ Υἱῷ καὶ Θεῷ σου."

ocVE.Aposticha1.mode = misc_gr_GR_cog.Mode3
ocVE.Aposticha1.melody = ""
ocVE.Aposticha1.incipit = "Εἰς πᾶσαν τὴν γῆν ἐξῆλθεν"
ocVE.Aposticha1.text = "Εἰς πᾶσαν τὴν γῆν ἐξῆλθεν ὁ φθόγγος ὑμῶν, ἅγιοι Ἀπόστολοι, εἰδώλων πλάνην ἐλύσατε, θεογνωσίαν κηρύξαντες, οὗτος ὁ καλὸς ἀγὼν ὑμῶν ὑπάρχει Μακάριοι· διὸ ὑμνοῦμεν καὶ δοξολογοῦμεν, ὑμῶν τὰ μνημόσυνα."

ocVE.Aposticha2.mode = misc_gr_GR_cog.Mode3
ocVE.Aposticha2.melody = ""
ocVE.Aposticha2.incipit = "Τὰς τοῦ Χριστοῦ παραγγελίας, ἀμέμπτως"
ocVE.Aposticha2.text = "Τὰς τοῦ Χριστοῦ παραγγελίας, ἀμέμπτως φυλάξαντες, ἅγιοι Ἀπόστολοι, δωρεὰν ἐλάβετε, δωρεὰν παρέχετε, ἰατρεύοντες τὰ πάθη, τῶν ψυχῶν καὶ τῶν σωμάτων ἡμῶν· διὸ ἔχοντες παῤῥησίαν, αὐτὸν ἱκετεύσατε, ἐλεηθῆναι τὰς ψυχὰς ἡμῶν."

ocVE.Aposticha3.mode = misc_gr_GR_cog.Mode3
ocVE.Aposticha3.melody = ""
ocVE.Aposticha3.incipit = "Οἱ Μάρτυρες σου Κύριε, πίστει"
ocVE.Aposticha3.text = "Οἱ Μάρτυρες σου Κύριε, πίστει στηριχθέντες, ἐλπίδι βεβαιωθέντες, τῇ ἀγάπῃ τοῦ Σταυροῦ σου, ψυχικῶς ἑνωθέντες, τοῦ ἐχθροῦ τὴν τυραννίδα ὤλεσαν, καὶ τυχόντες τῶν στεφάνων, μετὰ τῶν Ἀσωμάτων πρεσβεύουσιν, ὑπὲρ τῶν ψυχῶν ἡμῶν."

ocVE.AposTheotokion.mode = misc_gr_GR_cog.Mode3
ocVE.AposTheotokion.melody = he.a.m3_gr_GR_cog.MegaliTonMartyronSou.incipit
ocVE.AposTheotokion.incipit = "Ἀγγέλων χαρμονή, τῶν θείων Κόρη"
ocVE.AposTheotokion.text = "Ἀγγέλων χαρμονή, τῶν θείων Κόρη πέφυκας, τῶν Ἀποστόλων δόξα, καὶ προφητῶν ἐκσφράγισμα, τῶν ἀνθρώπων τε πιστῶν ἡ προστασία, καὶ ὁδηγὸς σωτήριος· διὰ τοῦτό σε, προσκυνοῦμεν Παρθένε."

ocVE.ApolTheotokionVM.mode = misc_gr_GR_cog.Mode3
ocVE.ApolTheotokionVM.melody = ""
ocVE.ApolTheotokionVM.incipit = "Προφῆται προεκήρυξαν, Ἀπόστολοι ἐδίδαξαν, Μάρτυρες"
ocVE.ApolTheotokionVM.text = "Προφῆται προεκήρυξαν, Ἀπόστολοι ἐδίδαξαν, Μάρτυρες ὡμολόγησαν, καὶ ἡμεῖς ἐπιστεύσαμεν, Θεοτόκον σε κυρίως ὑπάρχουσαν· διὸ καὶ μεγαλύνομεν, τὸν τόκον σου τὸν ἄφραστον."

ocMA.Kathisma11.mode = misc_gr_GR_cog.Mode3
ocMA.Kathisma11.melody = he.a.m3_gr_GR_cog.TinOraiotita.incipit
ocMA.Kathisma11.incipit = "Τῶν Ἀποστόλων σου, τὴν μνήμην"
ocMA.Kathisma11.text = "Τῶν Ἀποστόλων σου, τὴν μνήμην Κύριε, ὑπερεφαίδρυνας, ὡς παντοδύναμος, ὅτι ἐνίσχυσας αὐτούς, τὰ πάθη σου μιμήσασθαι· ἐνίκησαν ἀνδρείως γάρ, τοῦ Βελίαρ τὴν δύναμιν· ὅθεν καὶ ἀπέλαβον, ἰαμάτων χαρίσματα. Αὐτῶν ταῖς ἱκεσίαις φιλάνθρωπε, εἰρήνην παράσχου τῷ λαῷ σου."

ocMA.Kathisma12.mode = misc_gr_GR_cog.Mode3
ocMA.Kathisma12.melody = he.a.m3_gr_GR_cog.TheiasPisteos.incipit
ocMA.Kathisma12.incipit = "Πᾶσαν ἤρδευσαν, τὴν γῆν τοῖς"
ocMA.Kathisma12.text = "Πᾶσαν ἤρδευσαν, τὴν γῆν τοῖς ῥείθροις, οἱ Ἀπόστολοι, τοῦ Θεοῦ Λόγου, καὶ ἀνεβλάστησεν ὁ στάχυς τῆς πίστεως, καὶ ἐπληρώθη τοῦ κόσμου τὰ πέρατα· τὰ γὰρ ζιζάνια πάντα ἐξέκοψαν· ὅθεν ἅπαντας Χριστῷ τῷ Θεῷ προσήγαγον, βαπτίζοντες αὐτούς, εἰς Τριάδα τὴν ἄκτιστον."

ocMA.Kathisma13.mode = misc_gr_GR_cog.Mode3
ocMA.Kathisma13.melody = he.a.m3_gr_GR_cog.TinOraiotita.incipit
ocMA.Kathisma13.incipit = "Τῆς συμπαθείας σου, πλοῦτον τὸν"
ocMA.Kathisma13.text = "Τῆς συμπαθείας σου, πλοῦτον τὸν ἄμετρον, καὶ δυναστείας σου, κράτος τὸ ἄμαχον, ἀναλαβόμενος εἰς νοῦν, προσέδραμον τῇ σκέπῃ σου, θλίψει συνεχόμενος, καὶ δεινῶς ἀπορούμενος, κράζων ἐκ βαθέων μου, τῆς καρδίας τοῖς δάκρυσι, Παρθένε Θεοτόκε βοῶ σοι· Σῶσόν με, σῶσον τὸν ἀνάξιον."

ocMA.Kathisma21.mode = misc_gr_GR_cog.Mode3
ocMA.Kathisma21.melody = he.a.m3_gr_GR_cog.TheiasPisteos.incipit
ocMA.Kathisma21.incipit = "Δεῦτε ἅπαντες, τοὺς Ἀποστόλους, εὐφημήσωμεν,"
ocMA.Kathisma21.text = "Δεῦτε ἅπαντες, τοὺς Ἀποστόλους, εὐφημήσωμεν, ᾄσμασι θείοις· τῶν εἰδώλων γὰρ τὴν πλάνην βυθίσαντες, πρὸς θεῖον φῶς τοὺς βροτοὺς ἐπανήγαγον, καὶ τῇ Τριάδι πιστεύειν ἐδίδαξαν· ὅθεν σήμερον, αὐτοὺς εὐσεβῶς γεραίροντες, δοξάσωμεν Χριστὸν τὸν Θεὸν ἡμῶν."

ocMA.Kathisma22.mode = misc_gr_GR_cog.Mode3
ocMA.Kathisma22.melody = he.a.m3_gr_GR_cog.TheiasPisteos.incipit
ocMA.Kathisma22.incipit = "Ἔργοις λάμψαντες, ὀρθοδοξίας, πλάνην ἔσβεσαν"
ocMA.Kathisma22.text = "Ἔργοις λάμψαντες, ὀρθοδοξίας, πλάνην ἔσβεσαν κακοδοξίας, οἱ εὐκλεεῖς καὶ θεόπται Ἀπόστολοι, καὶ εὐσεβείᾳ τὸν κόσμον πλουτίσαντες, τὰς Ἐκκλησίας φαιδρῶς κατεκόσμησαν, ἐξαιτούμενοι, Χριστῷ τῷ Θεῷ, δωρήσασθαι, εἰρήνην τοῖς πιστοῖς, καὶ μέγα ἔλεος."

ocMA.Kathisma23.mode = misc_gr_GR_cog.Mode3
ocMA.Kathisma23.melody = he.a.m3_gr_GR_cog.TheiasPisteos.incipit
ocMA.Kathisma23.incipit = "Βυθιζόμενος, παθῶν θαλάσσῃ, εἰς τὸ"
ocMA.Kathisma23.text = "Βυθιζόμενος, παθῶν θαλάσσῃ, εἰς τὸ πέλαγος τῶν οἰκτιρμῶν σου, καταφεύγω, Θεοτόκε βοήθει μοι, ἔκτεινον Πανάχραντε, δεξιάν μοι τῷ δούλῳ σου, σῶσον ὡς τὸν Πέτρον πρίν, ὁ Υἱός σου διέσωσεν, ἐλέησον ψυχὴν ποντουμένην, σάλῳ πολλῶν παραπτώσεων."

ocMA.Kathisma31.mode = misc_gr_GR_cog.Mode3
ocMA.Kathisma31.melody = he.a.m3_gr_GR_cog.TheiasPisteos.incipit
ocMA.Kathisma31.incipit = "Σὲ τὸν κήρυκα, τῆς ἀληθείας,"
ocMA.Kathisma31.text = "Σὲ τὸν κήρυκα, τῆς ἀληθείας, καὶ ὑπέρλαμπρον, φωστῆρα κόσμου, εὐφημοῦντες ἀνυμνοῦμεν Νικόλαε, καὶ ἐκβοῶμεν ἐν πίστει δεόμενοι. Ὡς τοὺς ἀθώους θανάτου ἐξήρπασας, οὕτως Ἅγιε, κινδύνων ἡμᾶς καὶ θλίψεων, καὶ πάσης ἐξελοῦ δεινῆς κακώσεως."

ocMA.Kathisma32.mode = misc_gr_GR_cog.Mode3
ocMA.Kathisma32.melody = ""
ocMA.Kathisma32.incipit = "Ἐκλάμπετε διὰ τῆς πίστεως, ὑπέρλαμπροι"
ocMA.Kathisma32.text = "Ἐκλάμπετε διὰ τῆς πίστεως, ὑπέρλαμπροι φωστῆρες Ἅγιοι, τῶν ἀσθενούντων ἰατροί, Ἀθλοφόροι πανεύφημοι· τῶν τυράννων γὰρ τὰς αἰκίσεις μὴ δειλανδρήσαντες, τῶν εἰδώλων τὰς δυσφημίας κατηδαφίσατε, τρόπαιον ἔχοντες ἀήττητον, τὸν Σταυρὸν τῆς ἀληθείας."

ocMA.Kathisma33.mode = misc_gr_GR_cog.Mode3
ocMA.Kathisma33.melody = he.a.m3_gr_GR_cog.TheiasPisteos.incipit
ocMA.Kathisma33.incipit = "Θρόνος πάγχρυσος, τοῦ Βασιλέως, καὶ"
ocMA.Kathisma33.text = "Θρόνος πάγχρυσος, τοῦ Βασιλέως, καὶ Παράδεισος, διηνθισμένος, ἀνεδείχθης Θεοτόκε Πανάχραντε· τὸν γὰρ Θεὸν ἐν γαστρί σου βαστάσασα, εὐωδιάζεις ἡμᾶς θείαις χάρισιν· ὅθεν ἅπαντες, Θεοῦ ἀληθῶς Μητέρα σε, κηρύττομεν ἀεὶ καὶ μεγαλύνομεν."

ocMA.C2.acrostic = " οὗ ἡ ἀκροστιχίς· Τρίτ(ην δέη)σιν προσφέρω Νικολάῳ ἐγὼ ὁ Ἰωσήφ."

ocMA.CA.notice = ""

ocMA.OdeW1C1H.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW1C1H.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW1C1H.incipit = he.h.m3_gr_GR_cog.OTaYdata.incipit
ocMA.OdeW1C1H.text = he.h.m3_gr_GR_cog.OTaYdata.text

ocMA.OdeW1C11.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW1C11.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C11.melody = he.h.m3_gr_GR_cog.OTaYdata.incipit
ocMA.OdeW1C11.incipit = "Ἐκκλησίας τοὺς πύργους, τῷ θεμελίῳ,"
ocMA.OdeW1C11.text = "Ἐκκλησίας τοὺς πύργους, τῷ θεμελίῳ, τῆς Πίστεως εὐσεβῶς ἡδρασμένους, καὶ πιστοὺς ἅπαντας ἐπιστηρίζοντας, δεῦτε μακαρίσωμεν, τοὺς ἱεροὺς Ἀποστόλους, ὅπως ταῖς δεήσεσι, τούτων σωθείημεν."

ocMA.OdeW1C12.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW1C12.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C12.melody = he.h.m3_gr_GR_cog.OTaYdata.incipit
ocMA.OdeW1C12.incipit = "Ἀπερίτρεπτοι στῦλοι, τῆς εὐσεβείας, τῇ"
ocMA.OdeW1C12.text = "Ἀπερίτρεπτοι στῦλοι, τῆς εὐσεβείας, τῇ πλάνῃ με τοῦ ἐχθροῦ σκελισθέντα, καὶ εἰς γῆν κείμενον, κινδυνεύοντα, καὶ ἐξαπορούμενον, ἐπανορθώσατε, ὅπως εὕροιμι τὴν ἄφεσιν, ὧν ἐπλημμέλησα."

ocMA.OdeW1C13.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW1C13.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C13.melody = he.h.m3_gr_GR_cog.OTaYdata.incipit
ocMA.OdeW1C13.incipit = "Ἐμαυτὸν πρὸ τῆς δίκης, δικαιοκρῖτα,"
ocMA.OdeW1C13.text = "Ἐμαυτὸν πρὸ τῆς δίκης, δικαιοκρῖτα, ὁ ἄσωτος δεδοικὼς κατακρίνω· πονηρὰ ἔργα γάρ, ἄμετρα κέκτημαι· ὅθεν ἱκετεύω σε, ἀπεγνωσμένον με σῶσον, θείαις παρακλήσεσι, τῶν Ἀποστόλων σου."

ocMA.OdeW1C1T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW1C1T.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C1T.melody = he.h.m3_gr_GR_cog.OTaYdata.incipit
ocMA.OdeW1C1T.incipit = "Σὺν ἁγίοις Προφήταις, σὺν Ἀποστόλοις,"
ocMA.OdeW1C1T.text = "Σὺν ἁγίοις Προφήταις, σὺν Ἀποστόλοις, σὺν Μάρτυσι, τὸν ἐκ σοῦ σαρκωθέντα, ἐκτενῶς Κύριον, ἁγνὴ ἱκέτευε, ὅπως τῆς σαρκὸς ἡμῶν, πάντα τὰ πάθη νεκρώσῃ, καὶ ζωὴν αἰώνιον, ἡμῖν δωρήσηται."

ocMA.OdeW1C21.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW1C21.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C21.melody = he.h.m3_gr_GR_cog.AsomenToKyrio.incipit
ocMA.OdeW1C21.incipit = "Τὸν φαεινὸν λαμπτῆρα, καὶ ἀκοίμητον,"
ocMA.OdeW1C21.text = "Τὸν φαεινὸν λαμπτῆρα, καὶ ἀκοίμητον, ἐπὶ γῆς ὑπάρχοντα πυρσόν, καὶ πρὸς λιμένας θείους ἐμβιβάζοντα, τοῖς κύμασι τοῦ βίου, ἀεὶ τοὺς θαλαττεύοντας, Νικόλαον τιμήσωμεν, καὶ πόθῳ μακαρίσωμεν."

ocMA.OdeW1C22.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW1C22.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C22.melody = he.h.m3_gr_GR_cog.AsomenToKyrio.incipit
ocMA.OdeW1C22.incipit = "Ῥώμῃ Θεοῦ παμμάκαρ, δυναμούμενος, ζῆλον"
ocMA.OdeW1C22.text = "Ῥώμῃ Θεοῦ παμμάκαρ, δυναμούμενος, ζῆλον κατ' ἐπίγνωσιν, ἐκτήσω εὐσεβείας· ὅθεν θνῄσκειν μέλλοντας, ἀδίκως ἐλυτρώσω· διό σε ἱκετεύομεν, ἀδίκου πάσης ῥῦσαι ἡμᾶς, κακώσεως Νικόλαε."

ocMA.OdeW1C23.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW1C23.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C23.melody = he.h.m3_gr_GR_cog.AsomenToKyrio.incipit
ocMA.OdeW1C23.incipit = "Ἱκετηρίαν Πάτερ, ἀδιάλειπτον τῷ Κυρίῳ"
ocMA.OdeW1C23.text = "Ἱκετηρίαν Πάτερ, ἀδιάλειπτον τῷ Κυρίῳ πρόσφερε, ὅπως ἁμαρτημάτων, καὶ φλογὸς κολάσεως, διαιωνιζούσης, καὶ πειρασμῶν καὶ θλίψεων, ἡμᾶς ἀπολυτρώσηται, ὡς μόνος ὑπεράγαθος."

ocMA.OdeW1C2T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW1C2T.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C2T.melody = he.h.m3_gr_GR_cog.AsomenToKyrio.incipit
ocMA.OdeW1C2T.incipit = "Τὴν κιβωτὸν τὴν θείαν, τὴν"
ocMA.OdeW1C2T.text = "Τὴν κιβωτὸν τὴν θείαν, τὴν χωρήσασαν τὸν τοῦ νόμου πάροχον, ἡμῶν τὰς ἀνομίας, πάσας ἀφαιρούμενον, δι' εὐσπλαγχνίας θείας, ἀδιήγητον πέλαγος, Μαρίαν τὴν Πανάχραντον, συμφώνως μακαρίσωμεν."

ocMA.OdeW3C1H.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW3C1H.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW3C1H.incipit = he.h.m3_gr_GR_cog.OEkMiOnton.incipit
ocMA.OdeW3C1H.text = he.h.m3_gr_GR_cog.OEkMiOnton.text

ocMA.OdeW3C11.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW3C11.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C11.melody = he.h.m3_gr_GR_cog.OEkMiOnton.incipit
ocMA.OdeW3C11.incipit = "Οἱ τὰ τοῦ κόσμου πληρώματα,"
ocMA.OdeW3C11.text = "Οἱ τὰ τοῦ κόσμου πληρώματα, διδαχῆς ἐνθέου πληρώσαντες, τὴν ψυχήν μου πληρώσατε, ἐναρέτων πράξεων, καὶ λογισμῶν ἀγαθῶν, Χριστοῦ αὐτόπται."

ocMA.OdeW3C12.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW3C12.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C12.melody = he.h.m3_gr_GR_cog.OEkMiOnton.incipit
ocMA.OdeW3C12.incipit = "Ἀπὸ σκανδάλων μυρίων τοῦ πονηροῦ,"
ocMA.OdeW3C12.text = "Ἀπὸ σκανδάλων μυρίων τοῦ πονηροῦ, ἀπὸ πάσης θλίψεως, ἀπὸ πάσης στενώσεως, ἀπὸ περιστάσεως, ὦ Μαθηταὶ Χριστοῦ, λυτρώσασθέ με."

ocMA.OdeW3C13.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW3C13.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C13.melody = he.h.m3_gr_GR_cog.OEkMiOnton.incipit
ocMA.OdeW3C13.incipit = "Οἴμοι ψυχή μου ἀθλία καὶ"
ocMA.OdeW3C13.text = "Οἴμοι ψυχή μου ἀθλία καὶ ταπεινή, συχνῶς ἁμαρτάνουσα, καὶ Θεὸν παροργίζουσα! πῶς αἰτεῖς συγχώρησιν, τὴν ἀποχὴν τῶν κακῶν, μὴ ποιουμένη;"

ocMA.OdeW3C1T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW3C1T.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C1T.melody = he.h.m3_gr_GR_cog.OEkMiOnton.incipit
ocMA.OdeW3C1T.incipit = "Ἡ θεία κλῖμαξ ἣν εἶδεν"
ocMA.OdeW3C1T.text = "Ἡ θεία κλῖμαξ ἣν εἶδεν ὁ Ἰακώβ, δι' ἧς καταβέβηκεν, ὁ Θεὸς ἀνυψῶν ἡμᾶς, ἐκτενῶς ἱκέτευε, σὺν Ἀποστόλοις, ἡμᾶς οἰκτειρηθῆναι."

ocMA.OdeW3C21.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW3C21.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C21.melody = he.h.m3_gr_GR_cog.SteiraPsychi.incipit
ocMA.OdeW3C21.incipit = "Ἠκονημένη μάχαιρα, Πνεύματος ὤφθης σοφέ,"
ocMA.OdeW3C21.text = "Ἠκονημένη μάχαιρα, Πνεύματος ὤφθης σοφέ, τὰ πονηρὰ ζιζάνια, διακόπτουσα αἱρετιζόντων, καὶ τρίβον σωτήριον, πιστοῖς ὁμαλίζουσα, Πάτερ Νικόλαε."

ocMA.OdeW3C22.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW3C22.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C22.melody = he.h.m3_gr_GR_cog.SteiraPsychi.incipit
ocMA.OdeW3C22.incipit = "Σὲ τὴν πηγὴν τὴν βρύουσαν,"
ocMA.OdeW3C22.text = "Σὲ τὴν πηγὴν τὴν βρύουσαν, τῶν ἰαμάτων κρουνούς, ἐκδυσωποῦμεν Ἅγιε, τὰ βορβορώδη πάθη πάντων ἡμῶν, εὐχαῖς σου ἀπέλασον, κινδύνων καὶ θλίψεων, ἀπολυτρούμενος."

ocMA.OdeW3C23.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW3C23.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C23.melody = he.h.m3_gr_GR_cog.SteiraPsychi.incipit
ocMA.OdeW3C23.incipit = "Ἰσχυροτάτοις πόνοις σου, καταβαλὼν τὸν"
ocMA.OdeW3C23.text = "Ἰσχυροτάτοις πόνοις σου, καταβαλὼν τὸν ἐχθρόν, νῦν κατ'αὐτοῦ ἐνίσχυσον, τοὺς ἀσθενοῦντας λογισμοὺς ἡμῶν, παθῶν ἐπικλύσεσιν, ἁγίαις πρεσβείαις σου, Πάτερ Νικόλαε. "

ocMA.OdeW3C2T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW3C2T.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C2T.melody = he.h.m3_gr_GR_cog.SteiraPsychi.incipit
ocMA.OdeW3C2T.incipit = "Νοῦς ἐννοεῖν οὐ δύναται, τὸν"
ocMA.OdeW3C2T.text = "Νοῦς ἐννοεῖν οὐ δύναται, τὸν ὑπὲρ νοῦν τοκετὸν θαυμαστῆς λοχείας σου, Θεογεννῆτορ Μητροπάρθενε· διὸ ἱκετεύω σε, τὸν νοῦν μου χαρίτωσον, τοῦ ἀεὶ δοξάζειν σε."

ocMA.OdeW4C1H.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW4C1H.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW4C1H.incipit = he.h.m3_gr_GR_cog.EthouProsImas.incipit
ocMA.OdeW4C1H.text = he.h.m3_gr_GR_cog.EthouProsImas.text

ocMA.OdeW4C11.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW4C11.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C11.melody = he.h.m3_gr_GR_cog.EthouProsImas.incipit
ocMA.OdeW4C11.incipit = "Ὥσπερ ἀστραπάς, ὡς ἀκτῖνας ἄδυτε"
ocMA.OdeW4C11.text = "Ὥσπερ ἀστραπάς, ὡς ἀκτῖνας ἄδυτε Ἤλιε, σοῦ τοὺς Ἀποστόλους ἐφαπλῶν, εἰς τὰ τοῦ κόσμου Λόγε πληρώματα, πάντας κατεφώτισας, καὶ ἀγνωσίας ζόφον ἀπεδίωξας."

ocMA.OdeW4C12.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW4C12.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C12.melody = he.h.m3_gr_GR_cog.EthouProsImas.incipit
ocMA.OdeW4C12.incipit = "Θάλασσα δεινῆς, ἁμαρτίας Λόγε χειμάζει"
ocMA.OdeW4C12.text = "Θάλασσα δεινῆς, ἁμαρτίας Λόγε χειμάζει με, κύματα ἀτάκτων λογισμῶν, τὴν ταπεινὴν ψυχήν μου ταράττουσι. Κυβερνῆτα Κύριε, τῶν Μαθητῶν σου, σῶσόν με δεήσεσιν."

ocMA.OdeW4C13.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW4C13.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C13.melody = he.h.m3_gr_GR_cog.EthouProsImas.incipit
ocMA.OdeW4C13.incipit = "Φρίττω ἐννοῶν, τὴν φρικτήν σου"
ocMA.OdeW4C13.text = "Φρίττω ἐννοῶν, τὴν φρικτήν σου Δέσποτα ἔλευσιν· ἔχω γὰρ πρὸ δίκης τὴν ἐμήν, κατηγοροῦσαν ἔνδον συνείδησιν, καὶ προβασανίζουσαν, πρὸ τῆς βασάνου, τὴν ἀναισθησίαν μου."

ocMA.OdeW4C1T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW4C1T.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C1T.melody = he.h.m3_gr_GR_cog.EthouProsImas.incipit
ocMA.OdeW4C1T.incipit = "Λόγε τοῦ Θεοῦ, ὁ τεχθεὶς"
ocMA.OdeW4C1T.text = "Λόγε τοῦ Θεοῦ, ὁ τεχθεὶς ἐκ Κόρης θεόπαιδος, ταύτης μεσιτείαις ἱεραῖς, καὶ τῶν σοφῶν Σωτὴρ Ἀποστόλων σου, πάσης περιστάσεως, πάσης ἀνάγκης, λύτρωσαι τοὺς δούλους σου."

ocMA.OdeW4C21.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW4C21.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C21.melody = he.h.m3_gr_GR_cog.ToKataskionOros.incipit
ocMA.OdeW4C21.incipit = "Πρὸς Θεόν σε μεσίτην, ἅπας"
ocMA.OdeW4C21.text = "Πρὸς Θεόν σε μεσίτην, ἅπας πιστὸς καθ' ἑκάστην, Νικόλαε προβάλλεται· διό σε ἱκετεύομεν, ἐκ περιστάσεως χαλεπῆς, καὶ συμπτώσεως πταισμάτων, ἡμᾶς λύτρωσαι Πάτερ."

ocMA.OdeW4C22.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW4C22.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C22.melody = he.h.m3_gr_GR_cog.ToKataskionOros.incipit
ocMA.OdeW4C22.incipit = "Ῥεῖθρον ζῶν ἐν καρδίᾳ, Κυρίου"
ocMA.OdeW4C22.text = "Ῥεῖθρον ζῶν ἐν καρδίᾳ, Κυρίου ἔχων τὴν χάριν, δροσίζεις ἀενάως σοφέ, τοὺς καύσωνι τῶν θλίψεων, καὶ τῷ αὐχμῷ τῶν ἁμαρτιῶν, κινδυνεύοντας ἀθλίως, ἀπολέσθαι παμμάκαρ."

ocMA.OdeW4C23.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW4C23.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C23.melody = he.h.m3_gr_GR_cog.ToKataskionOros.incipit
ocMA.OdeW4C23.incipit = "Ὁ ῥυσάμενος πάλαι, ἀπαγομένους θανάτου,"
ocMA.OdeW4C23.text = "Ὁ ῥυσάμενος πάλαι, ἀπαγομένους θανάτου, ἀδίκου παμμακάριστε, καὶ νῦν ἡμᾶς περίσῳζε, ἀπὸ ἀνθρώπων φθοροποιῶν, ἐνοχλήσεως καὶ πάσης, τῶν δαιμόνων ἀπάτης."

ocMA.OdeW4C2T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW4C2T.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C2T.melody = he.h.m3_gr_GR_cog.ToKataskionOros.incipit
ocMA.OdeW4C2T.incipit = "Σαρκωθέντα τὸν Λόγον, ὑπερβολῇ εὐσπλαγχνίας,"
ocMA.OdeW4C2T.text = "Σαρκωθέντα τὸν Λόγον, ὑπερβολῇ εὐσπλαγχνίας, ἀῤῥήτως ἀπεκύησας. Αὐτὸν δυσώπει Ἄχραντε, τῶν σαρκικῶν πάντας μολυσμῶν, ἐκλυτρώσασθαι, καὶ πάντων, δυσχερῶν τῶν τοῦ βίου."

ocMA.OdeW5C1H.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW5C1H.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW5C1H.incipit = he.h.m3_gr_GR_cog.EpiTisGis.incipit
ocMA.OdeW5C1H.text = he.h.m3_gr_GR_cog.EpiTisGis.text

ocMA.OdeW5C11.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW5C11.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C11.melody = he.h.m3_gr_GR_cog.EpiTisGis.incipit
ocMA.OdeW5C11.incipit = "Ἐπὶ τῆς γῆς σαρκωθείς, ἀπειργάσω"
ocMA.OdeW5C11.text = "Ἐπὶ τῆς γῆς σαρκωθείς, ἀπειργάσω τοὺς Μαθητάς, οὐρανοὺς φθεγγομένους, Χριστὲ τὴν δόξαν σου· δι' αὐτῶν οὖν Κύριε, τὰς ψυχὰς ἡμῶν ἐλέησον."

ocMA.OdeW5C12.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW5C12.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C12.melody = he.h.m3_gr_GR_cog.EpiTisGis.incipit
ocMA.OdeW5C12.incipit = "Ἀπὸ παθῶν, ἀπὸ πάσης ἀνάγκης,"
ocMA.OdeW5C12.text = "Ἀπὸ παθῶν, ἀπὸ πάσης ἀνάγκης, ἀπὸ δεινῆς περιστάσεως Λόγε, ῥῦσαι τοὺς δούλους σου, ταῖς τῶν Ἀποστόλων σου, εὐπροσδέκτοις παρακλήσεσιν."

ocMA.OdeW5C13.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW5C13.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C13.melody = he.h.m3_gr_GR_cog.EpiTisGis.incipit
ocMA.OdeW5C13.incipit = "Οἴμοι ψυχὴ παναθλία! πῶς μέλλεις,"
ocMA.OdeW5C13.text = "Οἴμοι ψυχὴ παναθλία! πῶς μέλλεις, τῷ φοβερῷ δικαστῇ παραστῆναι, ἄκαρπος μένουσα; σπεῦσον μετανόησον, ἀρετῶν καρποὺς βλαστάνουσα."

ocMA.OdeW5C1T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW5C1T.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C1T.melody = he.h.m3_gr_GR_cog.EpiTisGis.incipit
ocMA.OdeW5C1T.incipit = "Ἐπὶ τῆς γῆς, τὸν ὀφθέντα,"
ocMA.OdeW5C1T.text = "Ἐπὶ τῆς γῆς, τὸν ὀφθέντα, ἀφράστως, μετὰ σαρκὸς ἐξ ἁγνῶν σου αἱμάτων, ἁγνὴ ἱκέτευε, ὡς Υἱόν σου πάντοτε, ἱλασμὸν ἡμῖν δωρήσασθαι."

ocMA.OdeW5C21.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW5C21.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C21.melody = he.h.m3_gr_GR_cog.ToFosSouToAnesperon.incipit
ocMA.OdeW5C21.incipit = "Φωτὶ τῷ καθαρῷ παρεστηκώς, καὶ"
ocMA.OdeW5C21.text = "Φωτὶ τῷ καθαρῷ παρεστηκώς, καὶ ταῖς ἐκεῖθεν ἀεὶ ἐκδιδομέναις, αὐγαῖς πυρσευόμενος, πᾶσιν αἴτησαι τὸν φωτισμόν, Πάτερ καὶ εἰρήνην."

ocMA.OdeW5C22.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW5C22.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C22.melody = he.h.m3_gr_GR_cog.ToFosSouToAnesperon.incipit
ocMA.OdeW5C22.incipit = "Ἐμὲ τὸν ὑπὲρ πάντας τοὺς"
ocMA.OdeW5C22.text = "Ἐμὲ τὸν ὑπὲρ πάντας τοὺς βροτούς, σὲ παροργίσαντα, φρενὶ ἀγνώμονι, ὑπεράγαθε κατοικτείρησον, ταῖς Νικολάου θερμαῖς πρεσβείαις."

ocMA.OdeW5C23.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW5C23.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C23.melody = he.h.m3_gr_GR_cog.ToFosSouToAnesperon.incipit
ocMA.OdeW5C23.incipit = "Ῥαθύμως δαπανῶν μου τὴν ζωήν,"
ocMA.OdeW5C23.text = "Ῥαθύμως δαπανῶν μου τὴν ζωήν, καθικετεύω σε, Πάτερ Νικόλαε, διέγειρόν μου πρὸς μετάνοιαν, τὸν λογισμὸν κατεσπιλωμένον."

ocMA.OdeW5C2T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW5C2T.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C2T.melody = he.h.m3_gr_GR_cog.ToFosSouToAnesperon.incipit
ocMA.OdeW5C2T.incipit = "Ὡς μόνη τῶν Ἀγγέλων χαρμονή,"
ocMA.OdeW5C2T.text = "Ὡς μόνη τῶν Ἀγγέλων χαρμονή, χαρᾶς μου πλήρωσον, θεοχαρίτωτε, τὴν διάνοιαν σκυθρωπάζουσαν, καὶ ἀμελείᾳ βεβυθισμένην."

ocMA.OdeW6C1H.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW6C1H.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW6C1H.incipit = he.h.m3_gr_GR_cog.AvyssosEschati.incipit
ocMA.OdeW6C1H.text = he.h.m3_gr_GR_cog.AvyssosEschati.text

ocMA.OdeW6C11.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW6C11.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C11.melody = he.h.m3_gr_GR_cog.AvyssosEschati.incipit
ocMA.OdeW6C11.incipit = "Ἄβυσσον σοφίας πνευματικῆς, ἀναπηγάσας, Ἀποστόλων"
ocMA.OdeW6C11.text = "Ἄβυσσον σοφίας πνευματικῆς, ἀναπηγάσας, Ἀποστόλων ὁ σύλλογος, κοσμικῆς ἐξήρανε, σοφίας πάντα ῥεύματα, καὶ κατήρδευσεν, εὐσεβῶν τὰ συστήματα."

ocMA.OdeW6C12.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW6C12.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C12.melody = he.h.m3_gr_GR_cog.AvyssosEschati.incipit
ocMA.OdeW6C12.incipit = "Στέναξον καὶ δάκρυσον ταπεινή, ψυχή"
ocMA.OdeW6C12.text = "Στέναξον καὶ δάκρυσον ταπεινή, ψυχή μου καὶ βόησον, καὶ πρὸς Κύριον λέγουσα· Ἥμαρτόν σοι Δέσποτα· Ἱλάσθητί μοι εὔσπλαγχνε, ταῖς δεήσεσι, τῶν σοφῶν Ἀποστόλων σου."

ocMA.OdeW6C13.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW6C13.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C13.melody = he.h.m3_gr_GR_cog.AvyssosEschati.incipit
ocMA.OdeW6C13.incipit = "Ῥεύσαντες οἱ χείμαῤῥοι τῶν παθῶν,"
ocMA.OdeW6C13.text = "Ῥεύσαντες οἱ χείμαῤῥοι τῶν παθῶν, κατέπτωσάν μου, τῆς καρδίας τὸ οἴκημα, ποταμοὶ τοῦ Πνεύματος, πεφυκότες Ἀπόστολοι, συντριβέντα με, πρὸς ζωὴν ἀναπλάσατε."

ocMA.OdeW6C1T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW6C1T.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C1T.melody = he.h.m3_gr_GR_cog.AvyssosEschati.incipit
ocMA.OdeW6C1T.incipit = "Δῆμός σε τῶν Ἀποστόλων, Χριστέ,"
ocMA.OdeW6C1T.text = "Δῆμός σε τῶν Ἀποστόλων, Χριστέ, καθικετεύει, μετὰ τῆς κυησάσης σε, ἱλασμὸν κατάπεμψον, καὶ εἰρήνην τοῖς δούλοις σου, ὡς φιλάνθρωπος, καὶ Θεὸς εὐδιάλλακτος."

ocMA.OdeW6C21.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW6C21.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C21.melody = he.h.m3_gr_GR_cog.VythosMoiTonPathon.incipit
ocMA.OdeW6C21.incipit = "Νεκρώσας ἐγκρατείᾳ τὰ μέλη, ἐκτήσω"
ocMA.OdeW6C21.text = "Νεκρώσας ἐγκρατείᾳ τὰ μέλη, ἐκτήσω τὴν ζωὴν τὴν ἀγήρω, ἧς καὶ ἡμᾶς κοινωνούς, σοῦ ταῖς εὐχαῖς ἐργάζου σοφέ, ἐκφεύξει τῆς πονηρᾶς ἐργασίας."

ocMA.OdeW6C22.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW6C22.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C22.melody = he.h.m3_gr_GR_cog.VythosMoiTonPathon.incipit
ocMA.OdeW6C22.incipit = "Ἱστίῳ ἱερῶν πρεσβειῶν σου, πελάγους"
ocMA.OdeW6C22.text = "Ἱστίῳ ἱερῶν πρεσβειῶν σου, πελάγους πειρασμῶν πολυτρόπων, καὶ ἁμαρτίας βυθοῦ ῥῦσαι ἡμᾶς, πρὸς τὸν ὅρμον τῆς ζωῆς, διαβιβάζων σοφὲ Ἱεράρχα."

ocMA.OdeW6C23.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW6C23.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C23.melody = he.h.m3_gr_GR_cog.VythosMoiTonPathon.incipit
ocMA.OdeW6C23.incipit = "Κοσμήσας τὴν τῶν Μύρων καθέδραν,"
ocMA.OdeW6C23.text = "Κοσμήσας τὴν τῶν Μύρων καθέδραν, ἐδείχθης κόσμος Ἀρχιερέων, ἀλλὰ πρεσβείαις ταῖς σαῖς, τῶν κοσμικῶν ἡμᾶς πειρασμῶν, διάσῳζε ἀβλαβεῖς Ἱεράρχα."

ocMA.OdeW6C2T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW6C2T.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C2T.melody = he.h.m3_gr_GR_cog.VythosMoiTonPathon.incipit
ocMA.OdeW6C2T.incipit = "Οἰκήσας ὁ Λόγος ἐν γαστρί"
ocMA.OdeW6C2T.text = "Οἰκήσας ὁ Λόγος ἐν γαστρί σου, τὴν πάλαι ἐν ἀνθρώποις οἰκοῦσαν, τῇ παραβάσει φθοράν, Μήτηρ ἁγνὴ ὡς μόνος Θεός, ἐξῴκισε διὰ σπλάγχνα ἐλέους."

ocMA.OdeW7C1H.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW7C1H.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW7C1H.incipit = he.h.m3_gr_GR_cog.ToPrinEikoni.incipit
ocMA.OdeW7C1H.text = he.h.m3_gr_GR_cog.ToPrinEikoni.text

ocMA.OdeW7C11.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW7C11.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C11.melody = he.h.m3_gr_GR_cog.ToPrinEikoni.incipit
ocMA.OdeW7C11.incipit = "Τὸ φῶς τοῦ κόσμου ὁ"
ocMA.OdeW7C11.text = "Τὸ φῶς τοῦ κόσμου ὁ Χριστός, φῶς ὑμᾶς ἀνέδειξε, φωτοφόροι Ἀπόστολοι, ἐκμειοῦντας τὸ σκότος τῆς ἀπάτης, καὶ φωτίζοντας πιστῶν τὰς διανοίας."

ocMA.OdeW7C12.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW7C12.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C12.melody = he.h.m3_gr_GR_cog.ToPrinEikoni.incipit
ocMA.OdeW7C12.incipit = "Τὰς καθ' ἡμῶν τοῦ δυσμενοῦς,"
ocMA.OdeW7C12.text = "Τὰς καθ' ἡμῶν τοῦ δυσμενοῦς, παγίδας συντρίψατε, φωτοφόροι Ἀπόστολοι, καὶ τὰς τρίβους ἡμῖν τῆς μετανοίας, ὁμαλίσατε ὑμῖν προσπεφευγόσι."

ocMA.OdeW7C13.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW7C13.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C13.melody = he.h.m3_gr_GR_cog.ToPrinEikoni.incipit
ocMA.OdeW7C13.incipit = "Τὴν μωρανθεῖσάν μου ψυχήν, σαρκικοῖς"
ocMA.OdeW7C13.text = "Τὴν μωρανθεῖσάν μου ψυχήν, σαρκικοῖς παθήμασιν, ἅλας θεῖον ὑπάρχοντες, θεηγόροι Ἀπόστολοι Κυρίου, ἐκκαθάρατε, πιστοὺς ζωογονοῦντες."

ocMA.OdeW7C1T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW7C1T.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C1T.melody = he.h.m3_gr_GR_cog.ToPrinEikoni.incipit
ocMA.OdeW7C1T.incipit = "Ἡ καλλονὴ τοῦ Ἰακώβ, καλῶν"
ocMA.OdeW7C1T.text = "Ἡ καλλονὴ τοῦ Ἰακώβ, καλῶν ταῖς ἰδέαις με, δέομαι καταφαίδρυνον, δυσωποῦσα νῦν σὺν τοῖς Ἀποστόλοις, τὸν τεχθέντα ἐκ τῶν σῶν ἀγνῶν αἱμάτων."

ocMA.OdeW7C21.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW7C21.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C21.melody = he.h.m3_gr_GR_cog.TreisPaidesEnKamino.incipit
ocMA.OdeW7C21.incipit = "Λυμαινόμενον πάλαι, τὸν παράφρονα Ἄρειον,"
ocMA.OdeW7C21.text = "Λυμαινόμενον πάλαι, τὸν παράφρονα Ἄρειον, τοῦ Κυρίου τὸν λαὸν ἐναπηγχόνισας, ταῖς νευραῖς σου τῶν λόγων, ὡς ποιμενάρχης ἀληθής, Πάτερ Νικόλαε."

ocMA.OdeW7C22.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW7C22.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C22.melody = he.h.m3_gr_GR_cog.TreisPaidesEnKamino.incipit
ocMA.OdeW7C22.incipit = "Ἁγίως διανύσας, τὸν πανάγιον βίον"
ocMA.OdeW7C22.text = "Ἁγίως διανύσας, τὸν πανάγιον βίον σου, σὺν Ἀποστόλοις κατοικεῖς, Πάτερ Νικόλαε, ἁγιασμὸν ἐπιπέμπων, καὶ φωτισμὸν τοῖς εὐσεβῶς σε μακαρίζουσιν."

ocMA.OdeW7C23.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW7C23.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C23.melody = he.h.m3_gr_GR_cog.TreisPaidesEnKamino.incipit
ocMA.OdeW7C23.incipit = "Ὡς ἕτοιμόν σε ῥύστην, ὡς"
ocMA.OdeW7C23.text = "Ὡς ἕτοιμόν σε ῥύστην, ὡς θερμὸν ἀντιλήπτορα, προσκαλούμεθα ἀεὶ Πάτερ Νικόλαε, τῶν παθῶν ἡμᾶς ῥῦσαι, καὶ ἀδοκήτων πειρασμῶν ἐπερχομένων ἡμῖν."

ocMA.OdeW7C2T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW7C2T.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C2T.melody = he.h.m3_gr_GR_cog.TreisPaidesEnKamino.incipit
ocMA.OdeW7C2T.incipit = "Ἐμὲ τὸν ἀμελείᾳ, τὴν ψυχὴν"
ocMA.OdeW7C2T.text = "Ἐμὲ τὸν ἀμελείᾳ, τὴν ψυχὴν σκοτιζόμενον, φωταγώγησον Ἁγνή, τὸν τῆς καρδίας μου, προσεπανάπτουσα λύχνον, ἐν προθυμίᾳ ἀγαθή, ὅπως δοξάζω σε."

ocMA.OdeW8C1H.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW8C1H.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW8C1H.incipit = he.h.m3_gr_GR_cog.AstektoPyri.incipit
ocMA.OdeW8C1H.text = he.h.m3_gr_GR_cog.AstektoPyri.text

ocMA.OdeW8C11.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW8C11.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C11.melody = he.h.m3_gr_GR_cog.AstektoPyri.incipit
ocMA.OdeW8C11.incipit = "Ἐπλήγην παθῶν τῇ ῥομφαίᾳ, καὶ"
ocMA.OdeW8C11.text = "Ἐπλήγην παθῶν τῇ ῥομφαίᾳ, καὶ ἐτραυματίσθην τὴν καρδίαν ἀπονοίᾳ, τοῦ κακίαν δημιουργοῦντος. Ἀπόστολοι ἔνδοξοι, ἰάσασθε ὅλον ἐξηπορημένον με, ψυχῶν καὶ σωμάτων, ἰατροὶ πεφυκότες."

ocMA.OdeW8C12.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW8C12.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C12.melody = he.h.m3_gr_GR_cog.AstektoPyri.incipit
ocMA.OdeW8C12.incipit = "Εἰς πᾶσαν τὴν γῆν τῶν"
ocMA.OdeW8C12.text = "Εἰς πᾶσαν τὴν γῆν τῶν κηρύκων, φθόγγος ἐξεχύθη ὁ θεῖος, ἐκδιδάσκων, παραδόξως μίαν οὐσίαν, μίαν φύσιν σέβεσθαι, τῆς Τριάδος μίαν ὄντως κυριότητα, μίαν Βασιλείαν, εἰς πάντας τοὺς αἰῶνας."

ocMA.OdeW8C13.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW8C13.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C13.melody = he.h.m3_gr_GR_cog.AstektoPyri.incipit
ocMA.OdeW8C13.incipit = "Ἐμὲ τὸν πολλὰ ἐπταικότα, καὶ"
ocMA.OdeW8C13.text = "Ἐμὲ τὸν πολλὰ ἐπταικότα, καὶ μακροθυμίαν σου τὴν θείαν δαπανῶντα, τῇ ἐπιμόνῳ πλημμελείᾳ, ἐπίστρεψον Δέσποτα, ὁ εἰδώς μου Λόγε τὴν πολλὴν ἀσθένειαν, καὶ τὴν ῥαθυμίαν, καὶ τὴν κακοφροσύνην."

ocMA.OdeW8C1T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW8C1T.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C1T.melody = he.h.m3_gr_GR_cog.AstektoPyri.incipit
ocMA.OdeW8C1T.incipit = "Ἡ θεία λαβὶς ἡ τὸν"
ocMA.OdeW8C1T.text = "Ἡ θεία λαβὶς ἡ τὸν θεῖον, ἄνθρακα ἀῤῥήτως δεξαμένη Θεοτόκε, τῶν παθῶν μου πεπυρωμένους, ἄνθρακας κατάσβεσον Παναγία, δρόσῳ σου τῶν παρακλήσεων, καὶ τῶν πανενδόξων, καὶ θείων Ἀποστόλων."

ocMA.OdeW8C21.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW8C21.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C21.melody = he.h.m3_gr_GR_cog.TonYpAngelon.incipit
ocMA.OdeW8C21.incipit = "Γῆν τῶν πρᾳέων ἐκληρώσω, πρᾷος"
ocMA.OdeW8C21.text = "Γῆν τῶν πρᾳέων ἐκληρώσω, πρᾷος Ὅσιε γενόμενος· διό σου δέομαι πιστῶς, καταπράϋνον πρεσβείαις σου, τὰς ἐπεγειρομένας διηνεκῶς, τοῦ πονηροῦ κατ' ἐμοῦ τρικυμίας."

ocMA.OdeW8C22.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW8C22.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C22.melody = he.h.m3_gr_GR_cog.TonYpAngelon.incipit
ocMA.OdeW8C22.incipit = "Ὡς ἐλυτρώσω στρατηλάτας, τοὺς ἀδίκως"
ocMA.OdeW8C22.text = "Ὡς ἐλυτρώσω στρατηλάτας, τοὺς ἀδίκως θνῄσκειν μέλλοντας, οὕτως ἀνθρώπων πονηρῶν, τῆς κακίας ἡμᾶς λύτρωσαι, καὶ πάσης τῶν δαιμόνων, ἐπιβουλῆς Νικόλαε, δυσωπῶν τὸν Σωτῆρα."

ocMA.OdeW8C23.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW8C23.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C23.melody = he.h.m3_gr_GR_cog.TonYpAngelon.incipit
ocMA.OdeW8C23.incipit = "Ὁδὸν ὑπέδειξας εὐθεῖαν, τοῖς ἀνθρώποις"
ocMA.OdeW8C23.text = "Ὁδὸν ὑπέδειξας εὐθεῖαν, τοῖς ἀνθρώποις καὶ σωτήριον, ἐν ᾗ ὁδήγησον ἡμᾶς, ἐν τῷ βίῳ παροδεύοντας, Νικόλαε τρισμάκαρ, ὅπως ὁμοῦ εἰσέλθομεν, τῆς ζωῆς πρός τὴν πόλιν."

ocMA.OdeW8C2T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW8C2T.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C2T.melody = he.h.m3_gr_GR_cog.TonYpAngelon.incipit
ocMA.OdeW8C2T.incipit = "Ἰσχὺς καὶ ὕμνησις ὑπάρχει, ὁ"
ocMA.OdeW8C2T.text = "Ἰσχὺς καὶ ὕμνησις ὑπάρχει, ὁ ἐκ σοῦ τεχθεὶς Πανάμωμε, ὃν ἐκδυσώπει ἐκτενῶς, παρειμένον με τοῖς πάθεσι, Παρθένε ἐνισχῦσαι, τοῦ ἐκτελεῖν τὰς σωτηρίους αὐτοῦ διατάξεις."

ocMA.OdeW9C1H.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW9C1H.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW9C1H.incipit = he.h.m3_gr_GR_cog.EnNomoSkia.incipit
ocMA.OdeW9C1H.text = he.h.m3_gr_GR_cog.EnNomoSkia.text

ocMA.OdeW9C11.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW9C11.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C11.melody = he.h.m3_gr_GR_cog.EnNomoSkia.incipit
ocMA.OdeW9C11.incipit = "Ὁ λόγῳ τῶν σῶν στηρίξας,"
ocMA.OdeW9C11.text = "Ὁ λόγῳ τῶν σῶν στηρίξας, Μαθητῶν τῆς γῆς τὰ πέρατα, Πατρὸς ἀνάρχου Λόγε, ταῖς αὐτῶν δεήσεσι κᾀμέ, τὸν πρὸς τὰ πάθη ἀλόγως, κατολισθήσαντα, καὶ τῶν δαιμόνων τῇ πλάνῃ, ὑπαχθέντα κατοικτείρησον."

ocMA.OdeW9C12.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW9C12.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C12.melody = he.h.m3_gr_GR_cog.EnNomoSkia.incipit
ocMA.OdeW9C12.incipit = "Ψυχή μου παθῶν δουλεύουσα, ὁρμαῖς"
ocMA.OdeW9C12.text = "Ψυχή μου παθῶν δουλεύουσα, ὁρμαῖς τῷ παθητῷ διὰ σέ, προσάγαγε δεήσεις, ὅπως ῥύσηταί σε ἐκ παθῶν, ἱκετευόντων προδήλως, τῶν ἱερῶν Μαθητῶν, μιμησαμένων τὰ τούτου, τῇ σαρκὶ αὐτῶν παθήματα."

ocMA.OdeW9C13.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW9C13.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C13.melody = he.h.m3_gr_GR_cog.EnNomoSkia.incipit
ocMA.OdeW9C13.incipit = "Χριστοῦ Μαθηταὶ ἡνίκα, σὺν αὐτῷ"
ocMA.OdeW9C13.text = "Χριστοῦ Μαθηταὶ ἡνίκα, σὺν αὐτῷ κρῖναι καθίσητε, τοὺς κρίσει ὑπευθύνους, ἀκατάκριτόν μου τὴν ψυχήν, τὴν μολυνθεῖσαν ἀτόποις ἔργοις τηρήσατε, ὡς ἀγαθοὶ μου προστάται, ὡς τοῦ κόσμου ἀντιλήπτορες."

ocMA.OdeW9C1T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW9C1T.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C1T.melody = he.h.m3_gr_GR_cog.EnNomoSkia.incipit
ocMA.OdeW9C1T.incipit = "Παρθένε ἁγνὴ Θεόνυμφε, καθαρὸν Χριστοῦ"
ocMA.OdeW9C1T.text = "Παρθένε ἁγνὴ Θεόνυμφε, καθαρὸν Χριστοῦ παλάτιον, Παρθένε Παναγία τῶν ἁγίων ἅγιον Θεόν, ὑπὲρ αἰτίαν καὶ λόγον κυοφορήσασα, σὺν Ἀποστόλοις ἁγίοις, ὑπὲρ πάντων καθικέτευε."

ocMA.OdeW9C21.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW9C21.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C21.melody = he.h.m3_gr_GR_cog.EnSinaioToOrei.incipit
ocMA.OdeW9C21.incipit = "Ὡς χαράκωμα θεῖον καὶ στήριγμα,"
ocMA.OdeW9C21.text = "Ὡς χαράκωμα θεῖον καὶ στήριγμα, καὶ καλὴν καταφυγήν, ὁ περίγειος κόσμος σε κέκτηται, μεσιτείαις σου ἀεί, ἐκλυτρούμενον πάσης πειρασμῶν στενώσεως, Πάτερ Νικόλαε· ὅθεν ἐν αἰνέσει, πιστῶς μακαρίζει σε."

ocMA.OdeW9C22.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW9C22.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C22.melody = he.h.m3_gr_GR_cog.EnSinaioToOrei.incipit
ocMA.OdeW9C22.incipit = "Στενωθεὶς ταῖς πολλαῖς περιστάσεσι, πρὸς"
ocMA.OdeW9C22.text = "Στενωθεὶς ταῖς πολλαῖς περιστάσεσι, πρὸς τὸ πλάτος τῆς θερμῆς, καταφεύγω παμμάκαρ πρεσβείας σου, μεταποίησον βοῶ, τῆς ψυχῆς μου τὸν πόνον, τὰ κύματα πράϋνον, τῆς ἀπογνώσεως, καὶ εἰρήνευσόν μου, τὸν νοῦν ταραττόμενον."

ocMA.OdeW9C23.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW9C23.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C23.melody = he.h.m3_gr_GR_cog.EnSinaioToOrei.incipit
ocMA.OdeW9C23.incipit = "Ἡ τοῦ Κτίστου ἐφέστηκεν ἔλευσις,"
ocMA.OdeW9C23.text = "Ἡ τοῦ Κτίστου ἐφέστηκεν ἔλευσις, κρῖναι ἅπασαν τὴν γῆν, καὶ ὑπάρχων ὁ τάλας ἀνέτοιμος, ὅλως φρίττω ἐννοῶν, τὴν πληθὺν τῶν κακῶν μου, Κύριε μακρόθυμε, οἴκτειρον σῶσόν με, θείαις Νικολάου, εὐχαῖς τοῦ ὁσίου σου."

ocMA.OdeW9C2T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW9C2T.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C2T.melody = he.h.m3_gr_GR_cog.EnSinaioToOrei.incipit
ocMA.OdeW9C2T.incipit = "Φωτισμός μου σωτήριος φάνηθι, ἡ"
ocMA.OdeW9C2T.text = "Φωτισμός μου σωτήριος φάνηθι, ἡ κυήσασα τὸ φῶς, τῆς ψυχῆς μου τὰ νέφη διώκουσα, τὰ πολλὰ καὶ χαλεπά, ὅπως υἱὸς ἡμέρας, εὐχαῖς σου γενήσωμαι, πράττων τὰ ὅσια, καὶ ὑμνολογίαις, ἀεὶ μακαρίζω σε."

ocMA.Aposticha1.mode = misc_gr_GR_cog.Mode3
ocMA.Aposticha1.melody = ""
ocMA.Aposticha1.incipit = oc.m3.d5_gr_GR_cog.ocVE.Aposticha1.incipit
ocMA.Aposticha1.text = oc.m3.d5_gr_GR_cog.ocVE.Aposticha1.text

ocMA.Aposticha2.mode = misc_gr_GR_cog.Mode3
ocMA.Aposticha2.melody = ""
ocMA.Aposticha2.incipit = oc.m3.d5_gr_GR_cog.ocVE.Aposticha2.incipit
ocMA.Aposticha2.text = oc.m3.d5_gr_GR_cog.ocVE.Aposticha2.text

ocMA.Aposticha3.mode = misc_gr_GR_cog.Mode3
ocMA.Aposticha3.melody = ""
ocMA.Aposticha3.incipit = "Αἱ δυνάμεις τῶν Ἁγίων Ἀγγέλων,"
ocMA.Aposticha3.text = "Αἱ δυνάμεις τῶν Ἁγίων Ἀγγέλων, ἐθαύμασαν τῶν Μαρτύρων τοὺς ἄθλους, ὅτι σῶμα θνητὸν περικείμενοι, βασάνων κατεφρόνησαν, μιμηταὶ γενόμενοι, τοῦ Πάθους τοῦ Σωτῆρος ἡμῶν, καὶ πρεσβεύουσιν ὑπὲρ τῶν ψυχῶν ὑμῶν."

