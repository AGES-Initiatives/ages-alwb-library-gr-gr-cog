ocMA.C1.acrostic.title = "Κανὼν Σταυρώσιμος,"
ocMA.C1.acrostic.notice1 = "οὗ ἡ ἀκροστιχίς·"
ocMA.C1.acrostic.text = "Σταυροῦ παγέντος, δαιμόνων πίπτει πλάνη. Ἰωσήφ."

ocMA.C2.acrostic.title = "Κανὼν τῆς Θεοτόκου,"
ocMA.C2.acrostic.notice1 = "οὗ ἡ ἀκροστιχίς·"
ocMA.C2.acrostic.text = "Τοὺς προστρέχοντας εἰς σὲ σῷζε, Παρθένε."
ocMA.C2.poet = poets_gr_GR_cog.po.IoannouEfchaiton

ocMA.CA.notice = ""

