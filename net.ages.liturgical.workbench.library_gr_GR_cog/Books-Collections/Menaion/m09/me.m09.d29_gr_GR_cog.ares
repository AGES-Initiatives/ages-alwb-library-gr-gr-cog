meVE.AposGlory.text = me.m01.d15_gr_GR_cog.meVE.AposGlory.text

meVE.AposTheotokion.mode = miscellanea_gr_GR_cog.misc.Mode6
meVE.AposTheotokion.greekmelody = he.a.m6_gr_GR_cog.heAU.TriimerosAnestis.greekname
meVE.AposTheotokion.melody = he.a.m6_gr_GR_cog.heAU.TriimerosAnestis.name
meVE.AposTheotokion.incipit = "Μεγίστων δωρημάτων ἡμῖν, ὁ τόκος…"
meVE.AposTheotokion.text = "Μεγίστων δωρημάτων ἡμῖν, ὁ τόκος ὁ ἀλόχευτος, τῆς Παρθένου, πᾶσι γέγονε σαφῶς· Θεὸς γὰρ τὴν φθαρεῖσαν, καινίζει βροτῶν φύσιν, καὶ ἀναπλάττει τὸ ἀνθρώπινον."

meVE.AposStavrotheotokion.mode = miscellanea_gr_GR_cog.misc.Mode6
meVE.AposStavrotheotokion.greekmelody = he.a.m6_gr_GR_cog.heAU.TriimerosAnestis.greekname
