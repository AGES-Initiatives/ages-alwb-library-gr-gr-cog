meMA.Ode6C1H.incipit = he.h.m8_gr_GR_cog.heHE.IlasthitiMoiSotir.incipit
meMA.Ode6C1H.text = he.h.m8_gr_GR_cog.heHE.IlasthitiMoiSotir.text

meMA.Ode6C11.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C11.greekmelody = he.h.m8_gr_GR_cog.heHE.IlasthitiMoiSotir.greekname
meMA.Ode6C11.melody = he.h.m8_gr_GR_cog.heHE.IlasthitiMoiSotir.name
meMA.Ode6C11.incipit = "Συνήθλησέ σοι χορός, σεπτῶν γυναίων…"
meMA.Ode6C11.text = "Συνήθλησέ σοι χορός, σεπτῶν γυναίων ἀοίδιμε· ῥωσθεῖσαι γὰρ τοῦ Σταυροῦ, δυνάμει συνέτριψαν, ἐχθροῦ πολυμήχανον, κεφαλὴν καὶ νίκης, ἀνεπλέξαντο τὸ διάδημα."

meMA.Ode6C12.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C12.melody = he.h.m8_gr_GR_cog.heHE.IlasthitiMoiSotir.name
meMA.Ode6C12.incipit = "Ἐῤῥώσθη τὸ ἀσθενές, Θεοῦ δυνάμει…"
meMA.Ode6C12.text = "Ἐῤῥώσθη τὸ ἀσθενές, Θεοῦ δυνάμει καὶ χάριτι· ἰδοὺ γὰρ νεανικῶς, γυναῖκες ἠρίστευσαν, τὸν ὄφιν πατήσασαι, τὸν ποτὲ τὴν Εὔαν, Παραδείσου ἐξοικίσαντα."

meMA.Ode6C13.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C13.melody = he.h.m8_gr_GR_cog.heHE.IlasthitiMoiSotir.name
meMA.Ode6C13.incipit = "Βυθίσασαι πονηρῶν, δαιμόνων ἄψυχα ξόανα…"
meMA.Ode6C13.text = "Βυθίσασαι πονηρῶν, δαιμόνων ἄψυχα ξόανα, ἐπουρανίου ναοῦ, κειμήλια ἔμψυχα, ἑαυτὰς προσήξατε, καὶ τῶν πρωτοτόκων, Ἐκκλησίαν ἐφαιδρύνατε."

meMA.Ode6C1T.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C1T.melody = he.h.m8_gr_GR_cog.heHE.IlasthitiMoiSotir.name
meMA.Ode6C1T.incipit = "Ὁ θρόνος ὁ τοῦ Θεοῦ…"
meMA.Ode6C1T.text = "Ὁ θρόνος ὁ τοῦ Θεοῦ, τῶν Χερουβὶμ ὁ ὑπέρτερος, νεφέλη ἡ τοῦ φωτός, ψυχῆς μου τὰ ὄμματα, Παναγία φώτισον, καὶ παθῶν ἀχλύος, τὴν καρδίαν μου καθάρισον."

meMA.Kontakion1.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Kontakion1.greekmelody = he.a.m2_gr_GR_cog.heAU.TaAnoZiton.greekname
meMA.Kontakion1.melody = he.a.m2_gr_GR_cog.heAU.TaAnoZiton.name
meMA.Kontakion1.incipit = "Ὁ θεῖος βλαστός, τὸ ἄνθος…"
meMA.Kontakion1.text = "Ὁ θεῖος βλαστός, τὸ ἄνθος τὸ ἀμάραντον, ἀμπέλου Χριστοῦ, τὸ κλῆμα τὸ πολύφορον, θεοφόρε Βλάσιε, τοὺς ἐν πίστει τελοῦντας τὴν μνήμην σου, εὐφροσύνης πλήρωσον τῆς σῆς, πρεσβεύων ἀπαύστως ὑπὲρ πάντων ἡμῶν."

meMA.Oikos1.text = "Ῥώμῃ Θεοῦ ἐνισχυθείς, ὦ Θεόφρον, ὡς ἱερὸς ἱεράρχης καὶ λειτουργὸς αὐτοῦ, ἐν πράξεσι θείαις ἀναδέδειξαι· τῶν Ἀθλοφόρων ἐν ταῖς χορείαις ἀθλοφόρος ὡράθης καὶ στεφανίτης· τῆς Ἀαρὼν ὑπερήρθης θυσίας, ὡς μύστης Χριστοῦ· σὺν Ἄβελ αἷμα τὸ θεῖον ἀνακράζει Θεῷ τὴν σφαγήν σου· καὶ θρόνῳ θείῳ τοῦ κτίσαντος παρεστώς, σὺν Ἀγγέλων τοῖς τάγμασι πρεσβεύεις ἀπαύστως ὑπὲρ πάντων ἡμῶν."

meMA.Ode7C1H.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C1H.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode7C1H.greekmelody = he.h.m8_gr_GR_cog.heHE.TonChaldaionIKaminos.greekname
meMA.Ode7C1H.melody = he.h.m8_gr_GR_cog.heHE.TonChaldaionIKaminos.melody
meMA.Ode7C1H.incipit = he.h.m8_gr_GR_cog.heHE.TonChaldaionIKaminos.incipit
meMA.Ode7C1H.text = he.h.m8_gr_GR_cog.heHE.TonChaldaionIKaminos.text

meMA.Ode7C11.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode7C11.greekmelody = he.h.m8_gr_GR_cog.heHE.TonChaldaionIKaminos.greekname
meMA.Ode7C11.melody = he.h.m8_gr_GR_cog.heHE.TonChaldaionIKaminos.name
meMA.Ode7C11.incipit = "Φωτισμοῦ θείου ἔμπλεων, κτησάμενος καρδίαν…"
meMA.Ode7C11.text = "Φωτισμοῦ θείου ἔμπλεων, κτησάμενος καρδίαν, ἀπροσκόπτως παρέδραμες, προσκόμματα τῆς πλάνης, ἐχθρῶν διαβήματα, ὑποσκελίσας Σοφέ, τῇ παρουσίᾳ σου."

meMA.Ode7C12.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode7C12.melody = he.h.m8_gr_GR_cog.heHE.TonChaldaionIKaminos.name
meMA.Ode7C12.incipit = "Ῥαντισμῷ τῶν αἱμάτων σου, πλάνης…"
meMA.Ode7C12.text = "Ῥαντισμῷ τῶν αἱμάτων σου, πλάνης φλὸξ ἀπεσβέσθη, τῇ ἐλλάμψει τῶν λόγων σου, ἡ κτίσις ἐφωτίσθη, κραυγάζουσα Βλάσιε· Εὐλογητὸς ὁ Θεός, ὁ τῶν Πατέρων ἡμῶν."

meMA.Ode7C13.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode7C13.melody = he.h.m8_gr_GR_cog.heHE.TonChaldaionIKaminos.name
meMA.Ode7C13.incipit = "Οὐ σκιαῖς οὐκ αἰνίγμασιν, ὃν…"
meMA.Ode7C13.text = "Οὐ σκιαῖς οὐκ αἰνίγμασιν, ὃν ἐπόθησας βλέπεις, πρὸς δὲ πρόσωπον, πρόσωπον λυθέντων τῶν ἐσόπτρων, καὶ μέλπεις γηθόμενος· Εὐλογητὸς ὁ Θεός, ὁ τῶν Πατέρων ἡμῶν."

meMA.Ode7C1T.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode7C1T.melody = he.h.m8_gr_GR_cog.heHE.TonChaldaionIKaminos.name
meMA.Ode7C1T.incipit = "Νυσταγμῷ κρατηθέντα με, Παρθένε ἀμελείας…"
meMA.Ode7C1T.text = "Νυσταγμῷ κρατηθέντα με, Παρθένε ἀμελείας, ἁμαρτίας κατέλαβεν, ὕπνος θανατηφόρος· διὸ μετανοίας με, πρὸς ζωηφόρον αὐγήν, Ἁγνὴ ἐξέγειρον καὶ σῶσόν με."

meMA.Ode8C1H.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C1H.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C1H.greekmelody = he.h.m8_gr_GR_cog.heHE.OStegazonEnYdasi.greekname
meMA.Ode8C1H.melody = he.h.m8_gr_GR_cog.heHE.OStegazonEnYdasi.melody
meMA.Ode8C1H.incipit = he.h.m8_gr_GR_cog.heHE.OStegazonEnYdasi.incipit
meMA.Ode8C1H.text = he.h.m8_gr_GR_cog.heHE.OStegazonEnYdasi.text

meMA.Ode8C11.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C11.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C11.greekmelody = he.h.m8_gr_GR_cog.heHE.OStegazonEnYdasi.greekname
meMA.Ode8C11.melody = he.h.m8_gr_GR_cog.heHE.OStegazonEnYdasi.name
meMA.Ode8C11.incipit = "Ὡς κριὸς ἐθελόθυτος, ὡς ἱερεῖον…"
meMA.Ode8C11.text = "Ὡς κριὸς ἐθελόθυτος, ὡς ἱερεῖον δεκτόν, ὡς ἀμνὸς ἄκακος, χαίρων προσήχθης τῷ τυθέντι ἀμνῷ, εἰς ἡμῶν λύτρωσιν, καὶ Μαρτύρων συνήφθης ὁμηγύρει, μέλπων σύν Ἀγγέλοις, τὴν ἱερωτάτην Παμμάκαρ ὑμνῳδίαν."

meMA.Ode8C12.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C12.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C12.melody = he.h.m8_gr_GR_cog.heHE.OStegazonEnYdasi.name
meMA.Ode8C12.incipit = "Σταλαγμοῖς τῶν αἱμάτων σου, ἀποξηράνας…"
meMA.Ode8C12.text = "Σταλαγμοῖς τῶν αἱμάτων σου, ἀποξηράνας βυθὸν ἀθεΐας Μάρτυς, ἔβλυσας κόσμῳ, ἰαμάτων κρουνούς, παύοντας καύσωνα, παθημάτων παντοίων, τῶν ἐν πίστει σοι καταφευγόντων, καὶ ὑπερυψούντων Χριστὸν εἰς τοὺς αἰῶνας."

meMA.Ode8C13.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C13.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C13.melody = he.h.m8_gr_GR_cog.heHE.OStegazonEnYdasi.name
meMA.Ode8C13.incipit = "Οὐκ ἀρνούμεθα Κύριον, τὸν ἐπὶ…"
meMA.Ode8C13.text = "Οὐκ ἀρνούμεθα Κύριον, τὸν ἐπὶ πάντων Θεόν, ἱερῶν γυναίων χορός, ἐβόα ὁ Μακάριος· ὅθεν ξεόμεναι, καὶ πυρὶ ὁμιλοῦσαι, καὶ σιδήρῳ κάρας ἐκτμηθεῖσαι, ἔχαιρον ὑμνοῦσαι, Χριστὸν εἰς τοὺς αἰῶνας."

meMA.Ode8C1T.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode8C1T.melody = he.h.m8_gr_GR_cog.heHE.OStegazonEnYdasi.name
meMA.Ode8C1T.incipit = "Ἱερὰν σε προήγγειλαν, Παρθενομῆτορ φωναί…"
meMA.Ode8C1T.text = "Ἱερὰν σε προήγγειλαν, Παρθενομῆτορ φωναί, ἐσομένην Κόρη, τοῦ Ποιητοῦ σου, ὑπὲρ φύσιν τροφόν, γέννησιν ἄῤῥητον, ὡς καὶ σύλληψιν ξένην κεκτημένην· ὅθεν σε ὑμνοῦμεν, εἰς πάντας τοὺς αἰῶνας."

meMA.Ode9C1H.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C1H.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode9C1H.greekmelody = he.h.m8_gr_GR_cog.heHE.TonProdilothenta.greekname
