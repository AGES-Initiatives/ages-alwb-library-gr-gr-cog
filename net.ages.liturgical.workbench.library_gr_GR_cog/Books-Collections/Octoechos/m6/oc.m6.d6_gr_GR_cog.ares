ocMA.Kathisma22.incipit = oc.m6.d4_gr_GR_cog.ocMA.Kathisma22.incipit
ocMA.Kathisma22.text = oc.m6.d4_gr_GR_cog.ocMA.Kathisma22.text

ocMA.Kathisma23.mode = miscellanea_gr_GR_cog.misc.Mode6
ocMA.Kathisma23.greekmelody = he.a.m6_gr_GR_cog.heAU.ElpisTouKosmouAgathi.greekname
ocMA.Kathisma23.melody = he.a.m6_gr_GR_cog.heAU.ElpisTouKosmouAgathi.name
ocMA.Kathisma23.incipit = "Παρισταμένη τῷ Σταυρῷ…"
ocMA.Kathisma23.text = "Παρισταμένη τῷ Σταυρῷ, ἡ Παρθένος καὶ Μήτηρ, τοῦ ἄνευ πάθους ἐξ αὐτῆς, ὑπὲρ νοῦν σαρκωθέντος, ἐκραύγαζε δακρυῤῥοοῦσα μητρικῶς· Οὐ φέρω σε νεκρὸν κρεμάμενον ὁρᾷν, πνοὴν τὸν παρεχόμενον, τοῖς ζῶσιν ἐπὶ γῆς, ὦ Υἱὲ καὶ Θεέ μου."

ocMA.Kathisma31.mode = miscellanea_gr_GR_cog.misc.Mode6
ocMA.Kathisma31.greekmelody = he.a.m6_gr_GR_cog.heAU.ElpisTouKosmouAgathi.greekname
ocMA.Kathisma31.melody = he.a.m6_gr_GR_cog.heAU.ElpisTouKosmouAgathi.name
ocMA.Kathisma31.incipit = "Σταυρὲ δαιμόνων ὀλετήρ…"
ocMA.Kathisma31.text = "Σταυρὲ δαιμόνων ὀλετήρ, ἰατρὲ τῶν νοσούντων, ἰσχὺς καὶ φύλαξ τῶν πιστῶν, βασιλέων τὸ νῖκος, τὸ καύχημα τῶν Ὀρθοδόξων ἀληθῶς, τὸ στήριγμα τῆς Ἐκκλησίας τοῦ Χριστοῦ, γενοῦ ἡμῖν ἀσφάλεια, καὶ τεῖχος καὶ φρουρός, ξύλον εὐλογημένον."

ocMA.Kathisma32.mode = miscellanea_gr_GR_cog.misc.Mode6
ocMA.Kathisma32.melody = ""
ocMA.Kathisma32.incipit = oc.m6.d2_gr_GR_cog.ocMA.Kathisma32.incipit
ocMA.Kathisma32.text = oc.m6.d2_gr_GR_cog.ocMA.Kathisma32.text

ocMA.Kathisma33.mode = miscellanea_gr_GR_cog.misc.Mode6
ocMA.Kathisma33.melody = ""
ocMA.Kathisma33.incipit = "Παναγία Θεοτόκε…"
ocMA.Kathisma33.text = "Παναγία Θεοτόκε, τὸν χρόνον τῆς ζωῆς μου, μὴ ἐγκαταλίπῃς με· ἀνθρωπίνῃ προστασία, μὴ καταπιστεύσῃς με, ἀλλʼ αὐτὴ ἀντιλαβοῦ, καὶ ἐλέησόν με."

ocMA.C1.acrostic.title = "Κανὼν Σταυρώσιμος,"
ocMA.C1.acrostic.notice1 = "οὗ ἡ ἀκροστιχίς, ἄνευ τῶν Σταυροθεοτοκίων τῆς αʹ ἕως καὶ τῆς Ϛʹ ᾠδῆς·"
ocMA.C1.acrostic.text = "Πάθος τὸ σεπτὸν αἰνέσω τοῦ Δεσπότου. Ἰωσήφ."

ocMA.C2.acrostic.title = "Κανὼν τῆς Θεοτόκου,"
ocMA.C2.acrostic.notice1 = "οὗ ἡ ἀκροστιχὶς κατʼ Ἀλφάβητον."

ocMA.CA.notice = rubrical_gr_GR_cog.ru.unavailable.octoechos

