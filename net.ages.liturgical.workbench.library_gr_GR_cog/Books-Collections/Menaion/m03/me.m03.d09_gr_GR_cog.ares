meMA.Ode1C1H.melody = he.h.m2_gr_GR_cog.heHE.DefteLaoi.melody
meMA.Ode1C1H.incipit = he.h.m2_gr_GR_cog.heHE.DefteLaoi.incipit
meMA.Ode1C1H.text = he.h.m2_gr_GR_cog.heHE.DefteLaoi.text

meMA.Ode1C11.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C11.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode1C11.greekmelody = he.h.m2_gr_GR_cog.heHE.DefteLaoi.greekname
meMA.Ode1C11.melody = he.h.m2_gr_GR_cog.heHE.DefteLaoi.name
meMA.Ode1C11.incipit = "Θεοστεφῆ, φάλαγγα μέλπω Μαρτύρων Χριστοῦ…"
meMA.Ode1C11.text = "Θεοστεφῆ, φάλαγγα μέλπω Μαρτύρων Χριστοῦ, ἐν θεοπνεύστοις ᾄσμασι, τῶν Τεσσαράκοντα, τὴν ἐτήσιον μνήμην, φαιδρῶς πανηγυρίζων, ὅτι δεδόξασται."

meMA.Ode1C12.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C12.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode1C12.melody = he.h.m2_gr_GR_cog.heHE.DefteLaoi.name
meMA.Ode1C12.incipit = "Ἐπὶ τῆς γῆς, ἅπασαν ἀπαρνησάμενοι…"
meMA.Ode1C12.text = "Ἐπὶ τῆς γῆς, ἅπασαν ἀπαρνησάμενοι προσηγορίαν εἵλοντο οἱ Τεσσαράκοντα, τὴν Χριστώνυμον κλῆσιν, διʼ ἧς ἐν τοῖς ὑψίστοις, νῦν πολιτεύονται."

meMA.Ode1C13.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C13.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode1C13.melody = he.h.m2_gr_GR_cog.heHE.DefteLaoi.name
meMA.Ode1C13.incipit = "Οἱ ἐν Χριστῷ, σάρκα καὶ…"
meMA.Ode1C13.text = "Οἱ ἐν Χριστῷ, σάρκα καὶ κόσμον μισήσαντες, τὸν παλαιὸν μὲν ἄνθρωπον συνεξεδύσασθε, τῇ προσκαίρῳ ἐσθῆτι, στολὴν δὲ ἀφθαρσίας περιεβάλεσθε."

meMA.Ode1C1T.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C1T.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode1C1T.melody = he.h.m2_gr_GR_cog.heHE.DefteLaoi.name
meMA.Ode1C1T.incipit = "Τίς ἐξειπεῖν, σοῦ κατʼ ἀξίαν…"
meMA.Ode1C1T.text = "Τίς ἐξειπεῖν, σοῦ κατʼ ἀξίαν δυνήσεται, τὴν ὑπὲρ λόγον σύλληψιν· Θεὸν γὰρ τέτοκας, ἐν σαρκὶ Παναγία, ἡμῖν ἐπιφανέντα Σωτῆρα πάντων ἡμῶν."

meMA.Ode3C1H.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C1H.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode3C1H.greekmelody = he.h.m2_gr_GR_cog.heHE.StereosonImasEnSoiKyrie.greekname
meMA.Ode3C1H.melody = he.h.m2_gr_GR_cog.heHE.StereosonImasEnSoiKyrie.melody
meMA.Ode3C1H.incipit = he.h.m2_gr_GR_cog.heHE.StereosonImasEnSoiKyrie.incipit
meMA.Ode3C1H.text = he.h.m2_gr_GR_cog.heHE.StereosonImasEnSoiKyrie.text

meMA.Ode3C11.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C11.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode3C11.greekmelody = he.h.m2_gr_GR_cog.heHE.StereosonImasEnSoiKyrie.greekname
meMA.Ode3C11.melody = he.h.m2_gr_GR_cog.heHE.StereosonImasEnSoiKyrie.name
meMA.Ode3C11.incipit = "Στρατείας καὶ ζωῆς καὶ ὡραιότητος…"
meMA.Ode3C11.text = "Στρατείας καὶ ζωῆς καὶ ὡραιότητος, σωμάτων καὶ ὄλβου ἠλογηκότες, εὐκλεῶς οἱ Τεσσαράκοντα, τὸν Χριστὸν ἀντὶ πάντων ἐκληρώσαντο."

meMA.Ode3C12.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C12.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode3C12.melody = he.h.m2_gr_GR_cog.heHE.StereosonImasEnSoiKyrie.name
meMA.Ode3C12.incipit = "Τοῖς λίθοις ἀφειδῶς οἱ Τεσσαράκοντα…"
meMA.Ode3C12.text = "Τοῖς λίθοις ἀφειδῶς οἱ Τεσσαράκοντα, προστάξει βαλλόμενοι τῶν τυράννων, τὰς βολὰς διὰ τοῦ Πνεύματος, κατὰ τῶν προσταττόντων ἀπεκρούσαντο."

meMA.Ode3C13.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C13.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode3C13.melody = he.h.m2_gr_GR_cog.heHE.StereosonImasEnSoiKyrie.name
meMA.Ode3C13.incipit = "Ἐλάλει κατὰ τῶν τυράννων χείλεσιν…"
meMA.Ode3C13.text = "Ἐλάλει κατὰ τῶν τυράννων χείλεσιν, ὁ ὄφις τοῦ Κτίσαντος βλασφημίας, ἀλλʼ αὐτοῦ τὸ θεομάχον στόμα, τοῖς κατὰ τῶν Μαρτύρων λίθοις θλάττεται."

meMA.Ode3C1T.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C1T.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode3C1T.melody = he.h.m2_gr_GR_cog.heHE.StereosonImasEnSoiKyrie.name
meMA.Ode3C1T.incipit = "Τοῦ ἄνθρακος χρυσοῦν θυμιατήριον, τοῦ…"
meMA.Ode3C1T.text = "Τοῦ ἄνθρακος χρυσοῦν θυμιατήριον, τοῦ θείου ἀνύμφευτε γενομένη, τὸ δυσῶδες τῆς καρδίας μου, εὐωδίασον μόνη Ἀειπάρθενε."

meMA.Ode3Kathisma1.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3Kathisma1.greekmelody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.greekname
meMA.Ode3Kathisma1.melody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.name
meMA.Ode3Kathisma1.incipit = "Τῷ Χριστῷ στρατευθέντες μαρτυρικῶς, τὸν…"
meMA.Ode3Kathisma1.text = "Τῷ Χριστῷ στρατευθέντες μαρτυρικῶς, τὸν ἐχθρὸν καθελόντες ἀθλητικῶς, ἔργοις ἐκπληρώσαντες, τοῦ Προφήτου τὰ ῥήματα· διὰ πυρὸς γὰρ καὶ ὕδατος, γενναίως διήλθετε, ἀναψυχὴν εὑράμενοι, ζωὴν τὴν αἰώνιον· ὅθεν καὶ στεφάνους οὐρανόθεν λαβόντες, χοροῖς συνευφραίνεσθε, Ἀσωμάτων δυνάμεων, Ἀθλοφόροι πανεύφημοι, πρεσβεύσατε Χριστῷ τῷ Θεῷ, τῶν πταισμάτων ἄφεσιν δωρήσασθαι, τοῖς ἑορτάζουσι πόθῳ, τὴν ἁγίαν μνήμην ὑμῶν."

meMA.Ode3KathismaT.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3KathismaT.melody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.name
meMA.Ode3KathismaT.incipit = "Ἐνθυμοῦμαι τὴν κρίσιν καὶ δειλιῶ…"
meMA.Ode3KathismaT.text = "Ἐνθυμοῦμαι τὴν κρίσιν καὶ δειλιῶ, τὴν ἐξέτασιν τρέμω τὴν φοβεράν, φρίττω τὴν ἀπόφασιν, καὶ πτοοῦμαι τὴν κόλασιν, τὴν τοῦ πυρὸς ὀδύνην, τὸ σκότος τὸν τάρταρον, τὸν βρυγμὸν τῶν ὀδόντων, τὸν ἄϋπνον σκώληκα· Οἴμοι! τί ποιήσω, ὅταν τίθωνται θρόνοι, καὶ βίβλοι ἀνοίγωνται, καὶ αἱ πράξεις ἐλέγχωνται, καὶ κρυπτὰ δημοσιεύωνται! Τότε Δέσποινα βοήθειά μοι γενοῦ, καὶ προστάτις θερμότατος· σὲ γὰρ ἔχω ἐλπίδα, ὁ ἀχρεῖος οἰκέτης σου."

meMA.Ode4C1H.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C1H.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode4C1H.greekmelody = he.h.m2_gr_GR_cog.heHE.EisakikoaKyrieTinAkoin.greekname
meMA.Ode4C1H.melody = he.h.m2_gr_GR_cog.heHE.EisakikoaKyrieTinAkoin.melody
meMA.Ode4C1H.incipit = he.h.m2_gr_GR_cog.heHE.EisakikoaKyrieTinAkoin.incipit
meMA.Ode4C1H.text = he.h.m2_gr_GR_cog.heHE.EisakikoaKyrieTinAkoin.text

meMA.Ode4C11.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C11.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode4C11.greekmelody = he.h.m2_gr_GR_cog.heHE.EisakikoaKyrieTinAkoin.greekname
meMA.Ode4C11.melody = he.h.m2_gr_GR_cog.heHE.EisakikoaKyrieTinAkoin.name
meMA.Ode4C11.incipit = "Φρενοβλαβεῖτε ἔλεγον, οἱ Ἀθληταὶ τὴν…"
meMA.Ode4C11.text = "Φρενοβλαβεῖτε ἔλεγον, οἱ Ἀθληταὶ τὴν πρόξενον ζημίας, δωρεὰν προτείνοντες, ἀθεώτατοι."

meMA.Ode4C12.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C12.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode4C12.melody = he.h.m2_gr_GR_cog.heHE.EisakikoaKyrieTinAkoin.name
meMA.Ode4C12.incipit = "Ἠκονημένα ξίφει τε, θῆρας καὶ…"
meMA.Ode4C12.text = "Ἠκονημένα ξίφει τε, θῆρας καὶ πῦρ, σταυρόν τε τοῖς Ἁγίοις, οἱ Χριστοῦ διῶκται ἐπανετείνοντο."

meMA.Ode4C13.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C13.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode4C13.melody = he.h.m2_gr_GR_cog.heHE.EisakikoaKyrieTinAkoin.name
meMA.Ode4C13.incipit = "Φοβερὸν ἡμῖν ἔλεγον, οἱ Ἀθληταὶ…"
meMA.Ode4C13.text = "Φοβερὸν ἡμῖν ἔλεγον, οἱ Ἀθληταὶ τὸ πῦρ τῆς γεέννης, τὸ δὲ νῦν, ὡς σύνδουλον, οὐ πτοούμεθα."

meMA.Ode4C14.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C14.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode4C14.melody = he.h.m2_gr_GR_cog.heHE.EisakikoaKyrieTinAkoin.name
meMA.Ode4C14.incipit = "Ἀπαστράπτοντες Ἔνδοξοι, τῇ νοερᾷ Χριστοῦ…"
meMA.Ode4C14.text = "Ἀπαστράπτοντες Ἔνδοξοι, τῇ νοερᾷ Χριστοῦ φωταγωγίᾳ, πρὸς ἀκτῖνα θείαν με ὁδηγήσατε."

meMA.Ode4C1T.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C1T.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode4C1T.melody = he.h.m2_gr_GR_cog.heHE.EisakikoaKyrieTinAkoin.name
meMA.Ode4C1T.incipit = "Δυσωποῦμέν σε Ἄχραντε, τὴν τὸν…"
meMA.Ode4C1T.text = "Δυσωποῦμέν σε Ἄχραντε, τὴν τὸν Θεὸν ἀσπόρως συλλαβοῦσαν, τοῦ ἀεὶ πρεσβευειν ὑπὲρ τῶν δούλων σου."

meMA.Ode5C1H.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C1H.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode5C1H.greekmelody = he.h.m2_gr_GR_cog.heHE.OTouFotosChorigos.greekname
meMA.Ode5C1H.melody = he.h.m2_gr_GR_cog.heHE.OTouFotosChorigos.melody
meMA.Ode5C1H.incipit = he.h.m2_gr_GR_cog.heHE.OTouFotosChorigos.incipit
meMA.Ode5C1H.text = he.h.m2_gr_GR_cog.heHE.OTouFotosChorigos.text

meMA.Ode5C11.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C11.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode5C11.greekmelody = he.h.m2_gr_GR_cog.heHE.OTouFotosChorigos.greekname
meMA.Ode5C11.melody = he.h.m2_gr_GR_cog.heHE.OTouFotosChorigos.name
meMA.Ode5C11.incipit = "Λύσσῃ παράφρονι, τῶν διωκτῶν οἱ…"
meMA.Ode5C11.text = "Λύσσῃ παράφρονι, τῶν διωκτῶν οἱ Ἀθληταὶ αἴθριοι, ἐν τῷ κρυμῷ διανυκτερεύειν καταδικασθέντες, ἀνέμελπον ὕμνον, Θεῷ χαριστήριον."

meMA.Ode5C12.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C12.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode5C12.melody = he.h.m2_gr_GR_cog.heHE.OTouFotosChorigos.name
meMA.Ode5C12.incipit = "Ἀγαλλιώμενοι, οἱ Τεσσαράκοντα Χριστοῦ Μάρτυρες…"
meMA.Ode5C12.text = "Ἀγαλλιώμενοι, οἱ Τεσσαράκοντα Χριστοῦ Μάρτυρες, τὴν ἀλγεινὴν ὑπέμειναν πῆξιν ἐν λίμνῃ ἑστῶτες, ἐλπίδι τῶν θείων στεφάνων νευρούμενοι."

meMA.Ode5C13.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C13.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode5C13.melody = he.h.m2_gr_GR_cog.heHE.OTouFotosChorigos.name
meMA.Ode5C13.incipit = "Γέλως προτίθεται, τοῖς Τεσσαράκοντα Χριστοῦ…"
meMA.Ode5C13.text = "Γέλως προτίθεται, τοῖς Τεσσαράκοντα Χριστοῦ Μάρτυσιν, ἀποπνιγεὶς ὁ πρὶν ἐμφωλεύων, τοῖς ὕδασιν ὄφις, τὴν ὀλεθροτόκον· ἰσχὺν γὰρ ἀφῄρηται."

meMA.Ode5C1T.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C1T.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode5C1T.melody = he.h.m2_gr_GR_cog.heHE.OTouFotosChorigos.name
meMA.Ode5C1T.incipit = "Σοὶ τῇ Τεκούσῃ Χριστόν, τὸν…"
meMA.Ode5C1T.text = "Σοὶ τῇ Τεκούσῃ Χριστόν, τὸν τοῦ παντὸς Δημιουργὸν κράζομεν· Χαῖρε Ἁγνή, χαῖρε ἡ τὸ φῶς ἀνατείλασα ἡμῖν, χαῖρε ἡ χωρήσασα, Θεὸν τὸν ἀχώρητον."

meMA.Ode6C1H.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C1H.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode6C1H.greekmelody = he.h.m2_gr_GR_cog.heHE.EnAvyssoPtaismaton.greekname
meMA.Ode6C1H.melody = he.h.m2_gr_GR_cog.heHE.EnAvyssoPtaismaton.melody
meMA.Ode6C1H.incipit = he.h.m2_gr_GR_cog.heHE.EnAvyssoPtaismaton.incipit
meMA.Ode6C1H.text = he.h.m2_gr_GR_cog.heHE.EnAvyssoPtaismaton.text

meMA.Ode6C11.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C11.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode6C11.greekmelody = he.h.m2_gr_GR_cog.heHE.EnAvyssoPtaismaton.greekname
meMA.Ode6C11.melody = he.h.m2_gr_GR_cog.heHE.EnAvyssoPtaismaton.name
meMA.Ode6C11.incipit = "Γεγηθὼς ὁ ἀρχέκακος ἥρπασεν, ὡς…"
meMA.Ode6C11.text = "Γεγηθὼς ὁ ἀρχέκακος ἥρπασεν, ὡς τῆς δωδεκάδος Ἰούδαν τὸν δείλαιον, καὶ τῆς Ἐδὲμ τὸν ἄνθρωπον, τῆς τεσσαρακοντάδος τὸν ἔκπτωτον."

meMA.Ode6C12.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C12.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode6C12.melody = he.h.m2_gr_GR_cog.heHE.EnAvyssoPtaismaton.name
meMA.Ode6C12.incipit = "Ἀναιδὴς ὢν δικαίως φρυάττεται, οἷα…"
meMA.Ode6C12.text = "Ἀναιδὴς ὢν δικαίως φρυάττεται, οἷα γὰρ Λῃστῇ, καὶ Ματθίᾳ τὸ πρότερον, οὕτω καὶ νῦν ὁ τύραννος, τοῦ φρουροῦντος τῇ κλήσει σπαράττεται."

meMA.Ode6C13.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C13.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode6C13.melody = he.h.m2_gr_GR_cog.heHE.EnAvyssoPtaismaton.name
meMA.Ode6C13.incipit = "Ματαιόφρων καὶ θρήνων ἐπάξιος, ὃς…"
meMA.Ode6C13.text = "Ματαιόφρων καὶ θρήνων ἐπάξιος, ὃς τις τῶν ζωῶν ἀμφοτέρων διήμαρτε· διὰ πυρὸς γὰρ λέλυται, καὶ πρὸς πῦρ ἐξεδήμησεν ἄσβεστον."

meMA.Ode6C1T.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C1T.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode6C1T.melody = he.h.m2_gr_GR_cog.heHE.EnAvyssoPtaismaton.name
meMA.Ode6C1T.incipit = "Ἀπειράνδρως Παρθένε ἐκύησας, καὶ διαιωνίζεις…"
meMA.Ode6C1T.text = "Ἀπειράνδρως Παρθένε ἐκύησας, καὶ διαιωνίζεις Παρθένος ἐμφαίνουσα, τῆς ἀληθοῦς Θεότητος, τοῦ Υἱοῦ καὶ Θεοῦ σου τὰ σύμβολα."

meMA.Kontakion1.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Kontakion1.melody = he.a.m6_gr_GR_cog.heAU.TinYperImon.name
meMA.Kontakion1.incipit = "Πᾶσαν στρατιὰν τοῦ κόσμου καταλιπόντες…"
meMA.Kontakion1.text = "Πᾶσαν στρατιὰν τοῦ κόσμου καταλιπόντες, τῷ ἐν οὐρανοῖς Δεσπότῃ προσεκολλήθητε, Ἀθλοφόροι Κυρίου Τεσσαράκοντα· διὰ πυρὸς γὰρ καὶ ὕδατος, διελθόντες μακάριοι, ἐπαξίως ἐκομίσασθε, δόξαν ἐκ τῶν οὐρανῶν, καὶ στεφάνων πληθύν."

meMA.Oikos1.text = "Τῷ ἐν θρόνῳ ἀστέκτῳ ἐποχουμένῳ, τῷ ἐκτείναντι τὸν οὐρανὸν καθάπερ δέῤῥιν, τῷ τὴν γῆν ἑδράσαντι, καὶ συνάξαντι τὰ ὕδατα εἰς τὰς συναγωγὰς αὐτῶν, τῷ τὰ πάντα ἐκ μὴ ὄντων ποιήσαντι ὑπάρχειν, καὶ πᾶσι χορηγοῦντι πνοὴν καὶ ζωήν, τῷ προσδεχομένῳ τῶν Ἀρχαγγέλων τὸν ὕμνον, καὶ ὑπʼ ἀγγέλων δοξαζομένῳ, καὶ ὑπὸ πάντων προσκυνουμένῳ, Χριστῷ τῷ παντοκράτορι, τῷ Πλάστῃ καὶ Θεῷ ἡμῶν, προσπίπτω ὁ ἀνάξιος προσάγων μου τὴν δέησιν, λόγου χάριν αἰτῶν, ἵνα ἰσχύσω εὐσεβῶς ὑμνῆσαι κἀγὼ τοὺς Ἁγίους, οὓς αὐτὸς ἔδειξας νικητάς, δωρησάμενος αὐτοῖς δόξαν ἐκ τῶν οὐρανῶν, καὶ στεφάνων πληθύν."

meMA.Ode7C1H.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C1H.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode7C1H.greekmelody = he.h.m2_gr_GR_cog.heHE.EikonosChrysis.greekname
meMA.Ode7C1H.melody = he.h.m2_gr_GR_cog.heHE.EikonosChrysis.melody
meMA.Ode7C1H.incipit = he.h.m2_gr_GR_cog.heHE.EikonosChrysis.incipit
meMA.Ode7C1H.text = he.h.m2_gr_GR_cog.heHE.EikonosChrysis.text

meMA.Ode7C11.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C11.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode7C11.greekmelody = he.h.m2_gr_GR_cog.heHE.EikonosChrysis.greekname
meMA.Ode7C11.melody = he.h.m2_gr_GR_cog.heHE.EikonosChrysis.name
meMA.Ode7C11.incipit = "Ἐξέστη ὁρῶν, τοὺς στεφάνους ὁ…"
meMA.Ode7C11.text = "Ἐξέστη ὁρῶν, τοὺς στεφάνους ὁ φρουρὸς τῶν Τεσσαράκοντα, καὶ παρωσάμενος τὸ φιλόζωον, ἀνεπτερώθη τῷ ἔρωτι, τῆς ἐπιφανείσης σου δόξης, καὶ σὺν τοῖς Μάρτυσιν ἔψαλλεν· Εὐλογητὸς εἶ ὁ Θεός, ὁ τῶν Πατέρων ἡμῶν."

meMA.Ode7C12.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C12.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode7C12.melody = he.h.m2_gr_GR_cog.heHE.EikonosChrysis.name
meMA.Ode7C12.incipit = "Λουτρῷ προσδραμών, ψυχοφθόρῳ θανατοῦται ὁ…"
meMA.Ode7C12.text = "Λουτρῷ προσδραμών, ψυχοφθόρῳ θανατοῦται ὁ φιλόζωος, ὁ δὲ φιλόχριστος ἅρπαξ ἄριστος, τῶν θεαθέντων γενόμενος, ὥσπερ ἐν λουτρῷ ἀφθαρσίας, σὺν τοῖς Μάρτυσιν ἔψαλλεν·Εὐλογητὸς εἶ ὁ Θεός, ὁ τῶν Πατέρων ἡμῶν."

meMA.Ode7C13.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C13.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode7C13.melody = he.h.m2_gr_GR_cog.heHE.EikonosChrysis.name
meMA.Ode7C13.incipit = "Πυρὸς νοητοῦ, ἀναφθέντος ἐν φρεσὶ…"
meMA.Ode7C13.text = "Πυρὸς νοητοῦ, ἀναφθέντος ἐν φρεσὶ τῶν Τεσσαράκοντα, ἡ πολυμήχανος κατεφλέγετο, τῶν ἀσεβούντων ἀπόνοια, ὥσπέρ τις κηρὸς τηκομένη, σοὶ δὲ Χριστὲ ἀνεμέλπετο· Εὐλογητὸς εἶ ὁ Θεός, ὁ τῶν Πατέρων ἡμῶν."

meMA.Ode7C14.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C14.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode7C14.melody = he.h.m2_gr_GR_cog.heHE.EikonosChrysis.name
meMA.Ode7C14.incipit = "Ὡς λίαν φαιδρά, εὐπρεπής τε…"
meMA.Ode7C14.text = "Ὡς λίαν φαιδρά, εὐπρεπής τε τοῦ Σταυροῦ Χριστὲ ἡ δύναμις, διʼ ἐναντίων στέφη πλέκουσα, τοῖς τεσσαράκοντα Μάρτυσιν· ὕδωρ γὰρ καὶ πῦρ διελθόντες, ἐν ἀφθαρσίᾳ κραυγάζουσιν· Εὐλογητὸς εἶ ὁ Θεός, ὁ τῶν Πατέρων ἡμῶν."

meMA.Ode7C1T.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C1T.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode7C1T.melody = he.h.m2_gr_GR_cog.heHE.EikonosChrysis.name
meMA.Ode7C1T.incipit = "Σὲ βάτον Μωσῆς, ἐν τῷ…"
meMA.Ode7C1T.text = "Σὲ βάτον Μωσῆς, ἐν τῷ ὄρει τῷ Σινᾷ πυρπολουμένην, Ἁγνὴ προεθεώρει τὴν ἐνέγκασαν, ἀκαταφλέκτως τὴν ἄστεκτον, αἴγλην τῆς ἀῤῥήτου οὐσίας, ἑνωθείσης παχύτητι, σαρκὸς μιᾶς τῶν ἐν αὐτῇ, Ἁγίων ὑποστάσεων."

meMA.Ode8C1H.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C1H.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode8C1H.greekmelody = he.h.m2_gr_GR_cog.heHE.TonEnKaminoTouPyros.greekname
meMA.Ode8C1H.melody = he.h.m2_gr_GR_cog.heHE.TonEnKaminoTouPyros.melody
meMA.Ode8C1H.incipit = he.h.m2_gr_GR_cog.heHE.TonEnKaminoTouPyros.incipit
meMA.Ode8C1H.text = he.h.m2_gr_GR_cog.heHE.TonEnKaminoTouPyros.text

meMA.Ode8C11.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C11.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode8C11.greekmelody = he.h.m2_gr_GR_cog.heHE.TonEnKaminoTouPyros.greekname
meMA.Ode8C11.melody = he.h.m2_gr_GR_cog.heHE.TonEnKaminoTouPyros.name
meMA.Ode8C11.incipit = "Μανιωδῶς τοῖς Ἀθληταῖς, ἐποτρύνας ὁ…"
meMA.Ode8C11.text = "Μανιωδῶς τοῖς Ἀθληταῖς, ἐποτρύνας ὁ ἐχθρὸς ἅπασαν κτίσιν, διὰ πάσης ᾐσχύνθη· οἱ Τεσσαράκοντα γὰρ ἀπαύστως· Ὑμνεῖτε κραυγάζουσι, καὶ ὑπερυψοῦτε, Χριστόν εἰς τοὺς αἰῶνας."

meMA.Ode8C12.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C12.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode8C12.melody = he.h.m2_gr_GR_cog.heHE.TonEnKaminoTouPyros.name
meMA.Ode8C12.incipit = "Ἀνηλεῶς ὑπὲρ Χριστοῦ, τὰ τοῦ…"
meMA.Ode8C12.text = "Ἀνηλεῶς ὑπὲρ Χριστοῦ, τὰ τοῦ σώματος ὑμῶν μέλη θλασθέντες, ὁλοκαύτωμα θεῖον προσενεχθέντες Θεῷ, ἀγγέλων χοροῖς συναγάλλεσθε, Μάρτυρες ὑμνοῦντες, Χριστόν εἰς τοὺς αἰῶνας."

meMA.Ode8C13.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C13.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode8C13.melody = he.h.m2_gr_GR_cog.heHE.TonEnKaminoTouPyros.name
meMA.Ode8C13.incipit = "Ῥωμαλεότητι φρενῶν, ὃν ἐκύησεν ἐπ'…"
meMA.Ode8C13.text = "Ῥωμαλεότητι φρενῶν, ὃν ἐκύησεν ἐπʼ ὤμων ἀραμένη, ἡ φιλόθεος μήτηρ, τῆς εὐσεβείας καρπόν, προσάγει σὺν Μάρτυσι Μάρτυρα, τὴν ἱερουργίαν, Ἀβραὰμ μιμουμένη."

meMA.Ode8C14.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C14.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode8C14.melody = he.h.m2_gr_GR_cog.heHE.TonEnKaminoTouPyros.name
meMA.Ode8C14.incipit = "Τὴν πρὸς τὴν ἄληκτον ζωήν…"
meMA.Ode8C14.text = "Τὴν πρὸς τὴν ἄληκτον ζωήν, εὐθυδρόμως, ὦ υἱέ, στέλλου πορείαν, ἡ φιλόχριστος μήτηρ, τῷ φιλοχρίστῳ παιδί, ἐβόα· Οὐ φέρω σε δεύτερον, τῷ ἀγωνοθέτῃ, Θεῷ ἐμφανισθῆναι."

meMA.Ode8C1Tr.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C1Tr.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode8C1Tr.melody = he.h.m2_gr_GR_cog.heHE.TonEnKaminoTouPyros.name
meMA.Ode8C1Tr.incipit = "Τὴν ὡς ἡλίοις ἐν τρισίν…"
meMA.Ode8C1Tr.text = "Τὴν ὡς ἡλίοις ἐν τρισίν, ἑνιαίαν εὐσεβῶς φύσιν νοοῦντες, ἐχομένοις ἀλλήλων, τὸ τρισυπόστατον Φῶς, ὑμνοῦμεν τὰ ἔργα ὡς Κύριον καὶ ὑπερυψοῦμεν, εἰς πάντας τους αἰῶνας."

meMA.Ode8C1T.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C1T.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode8C1T.melody = he.h.m2_gr_GR_cog.heHE.TonEnKaminoTouPyros.name
meMA.Ode8C1T.incipit = "Νεῦσον δεήσεσιν ἡμῶν, τῶν σῶν…"
meMA.Ode8C1T.text = "Νεῦσον δεήσεσιν ἡμῶν, τῶν σῶν δούλων ἀγαθὴ Θεοκυῆτορ, καὶ δεινῶν τὰς ἐφόδους, καὶ πειρασμῶν προσβολάς, ταχέως κατάπαυσον Δέσποινα, ἵνα σε τιμῶμεν, ὑπερευλογημένη."

meMA.Ode9C1H.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C1H.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode9C1H.greekmelody = he.h.m2_gr_GR_cog.heHE.TonEkTheouTheonLogon.greekname
meMA.Ode9C1H.melody = he.h.m2_gr_GR_cog.heHE.TonEkTheouTheonLogon.melody
meMA.Ode9C1H.incipit = he.h.m2_gr_GR_cog.heHE.TonEkTheouTheonLogon.incipit
meMA.Ode9C1H.text = he.h.m2_gr_GR_cog.heHE.TonEkTheouTheonLogon.text

meMA.Ode9C11.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C11.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode9C11.greekmelody = he.h.m2_gr_GR_cog.heHE.TonEkTheouTheonLogon.greekname
meMA.Ode9C11.melody = he.h.m2_gr_GR_cog.heHE.TonEkTheouTheonLogon.name
meMA.Ode9C11.incipit = "Ὑπὲρ Χριστοῦ γυμνωθέντες, καὶ τοῖς…"
meMA.Ode9C11.text = "Ὑπὲρ Χριστοῦ γυμνωθέντες, καὶ τοῖς λίθοις βληθέντες, ἀέρος ὑπεμείνατε κρυμόν, ὕδατος πῆξιν, καὶ θλᾶσιν μελῶν, καὶ πυρὶ φλογισθέντες, ἐν ποταμίῳ λάμπετε ῥοΐ, τηλαυγεῖς ὡς φωστῆρες, Μάρτυρες τεσσαράκοντα."

meMA.Ode9C12.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C12.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode9C12.melody = he.h.m2_gr_GR_cog.heHE.TonEkTheouTheonLogon.name
meMA.Ode9C12.incipit = "Ῥάβδον δυνάμεως θείας, τὸν Σταυρὸν…"
meMA.Ode9C12.text = "Ῥάβδον δυνάμεως θείας, τὸν Σταυρὸν κεκτημένοι, ἐβόων τῷ Χριστῷ οἱ Ἀθληταί, οἱ τεσσαράκοντα, Δέσποτα πανσθενεῖ τροπαιούχῳ, τῇ σῇ στεφανωθείημεν χειρί, ἵνα πάντες σε ὕμνοις, ἀπαύστως μεγαλύνωμεν."

meMA.Ode9C13.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C13.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode9C13.melody = he.h.m2_gr_GR_cog.heHE.TonEkTheouTheonLogon.name
meMA.Ode9C13.incipit = "Ὡς ἀλγεινὴ μὲν ἡ πῆξις…"
meMA.Ode9C13.text = "Ὡς ἀλγεινὴ μὲν ἡ πῆξις, ὡς δὲ λίαν ἐκτόπως, δριμὺς ὃν ὑπεμείνατε κρυμόν, ἀλλὰ γλυκὺς ὁ Παράδεισος· Ἀβραὰμ γὰρ οἱ κόλποι, τοῦ Πατριάρχου θάλπουσιν ὑμᾶς, ἐν σκηναῖς αἰωνίοις, Μάρτυρες τεσσαράκοντα."

meMA.Ode9C14.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C14.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode9C14.melody = he.h.m2_gr_GR_cog.heHE.TonEkTheouTheonLogon.name
meMA.Ode9C14.incipit = "Νενικηκότες τοῖς ἄθλοις, καὶ τῇ…"
meMA.Ode9C14.text = "Νενικηκότες τοῖς ἄθλοις, καὶ τῇ τρισυποστάτῳ Τριάδι, παρεστῶτες, Ἀθληταί, νῦν δωρηθῆναι πρεσβεύσατε, τὴν εἰρήνην τῷ κόσμῳ, νίκας τῷ φιλοχρίστῳ Βασιλεῖ, καὶ ἡμῖν σωτηρίαν, Μάρτυρες τεσσαράκοντα."

meMA.Ode9C1T.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C1T.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode9C1T.melody = he.h.m2_gr_GR_cog.heHE.TonEkTheouTheonLogon.name
meMA.Ode9C1T.incipit = "Χαῖρε πηγὴ ἀφθαρσίας, χαῖρε κούφη…"
meMA.Ode9C1T.text = "Χαῖρε πηγὴ ἀφθαρσίας, χαῖρε κούφη νεφέλη, χαῖρε τερπνὲ Παράδεισε Θεοῦ, χαῖρε πηγὴ ἰαμάτων ψυχῶν, χαῖρε ἅγιον ὄρος, ὃ εἶδεν ὁ Προφήτης Δανιήλ, χαῖρε Μήτηρ Παρθένε, χαῖρε ἡ πάντων Ἄνασσα."

meMA.Exaposteilarion1.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Exaposteilarion1.greekmelody = he.a.m2_gr_GR_cog.heAU.GynaikesAkoutisthite.greekname
meMA.Exaposteilarion1.melody = he.a.m2_gr_GR_cog.heAU.GynaikesAkoutisthite.name
meMA.Exaposteilarion1.incipit = "Χορὸς τετραδεκάριθμος, Μαρτύρων Τεσσαράκοντα, τῆς…"
meMA.Exaposteilarion1.text = "Χορὸς τετραδεκάριθμος, Μαρτύρων Τεσσαράκοντα, τῆς ὑπερθέου Τριάδος, τῶν ἀρετῶν τετρακτύϊ, πυρὸς ἀέρος ὕδατος, καὶ γῆς στοιχείων τετράδος, ᾄσμασι θείοις ὑμνείσθω, ὑπὲρ Χριστοῦ ὡς ἀθλήσας, τοῦ τῶν ἁπάντων Δεσπότου."

meMA.ExapTheotokion.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.ExapTheotokion.melody = he.a.m2_gr_GR_cog.heAU.GynaikesAkoutisthite.name
meMA.ExapTheotokion.incipit = "Χαῖρε κατάρας λύτρωσις, καὶ τοῦ…"
meMA.ExapTheotokion.text = "Χαῖρε κατάρας λύτρωσις, καὶ τοῦ Ἀδὰμ ἀνάκλησις, χαῖρε Ἁγνὴ Θεοτόκε, ἐλπὶς καὶ σκέπη τοῦ κόσμου, χαῖρε σεμνὴ μητρόθεε, χαῖρε τὸ θεῖον ὄχημα, χαῖρε ἡ κλῖμαξ καὶ πύλη, χαῖρε κούφη νεφέλη, χαῖρε τῆς Εὔας ἡ λύσις."

meMA.Lauds1.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Lauds1.greekmelody = he.a.m5_gr_GR_cog.heAU.ChairoisAskitikon.greekname
meMA.Lauds1.melody = he.a.m5_gr_GR_cog.heAU.ChairoisAskitikon.name
meMA.Lauds1.incipit = "Δεῦτε μαρτυρικὴν Ἀδελφοί, μέτʼ ἐγκωμίων…"
meMA.Lauds1.text = "Δεῦτε μαρτυρικὴν Ἀδελφοί, μέτʼ ἐγκωμίων ἀνυμνήσωμεν φάλαγγα, τῷ κρύει πυρποληθεῖσαν, καὶ τὸν τῆς πλάνης κρυμόν, διαπύρῳ ζήλῳ πυρπολήσασαν, στρατὸν γενναιότατον, ἱερώτατον σύνταγμα, συνασπισμόν τε, ἀῤῥαγῆ καὶ ἀνίκητον, τοὺς τῆς πίστεως, περιβόλους καὶ φύλακας, Μάρτυρας τεσσαράκοντα, χορείαν τὴν ἔνθεον, τῆς ἐκκλησίας τοὺς πρέσβεις, τοὺς δυνατῶς ἱκετεύοντας, Χριστὸν καταπέμψαι, ταῖς ψυχαῖς ἡμών εἰρήνην, καὶ μέγα ἔλεος."

meMA.Lauds2.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Lauds2.melody = he.a.m5_gr_GR_cog.heAU.ChairoisAskitikon.name
meMA.Lauds2.incipit = "Χαίροις συναγωγὴ κραταιά, καὶ Ἱερὰ…"
meMA.Lauds2.text = "Χαίροις συναγωγὴ κραταιά, καὶ Ἱερὰ καὶ τροπαιοῦχος παράταξις, οἱ πύργοι τῆς εὐσεβείας, οἱ στρατιῶται Χριστοῦ, οἱ στεῤῥοὶ ὁπλῖται καὶ ἀήττητοι, τὸν νοῦν εὐτονώτατοι, καὶ ψυχὴν ἀνδρειότατοι, τῷ ὄντι θεῖοι, καὶ Θεῷ ποθεινότατοι, χορὸς Ἅγιος καὶ θεόλεκτον σύστημα, Μάρτυρες τεσσαράκοντα, οἱ ἴσοι τὴν ἄθλησιν, ἴσοι τὴν γνώμην καὶ ἴσους, καὶ τοὺς στεφάνους δεξάμενοι, Χριστὸν δυσωπεῖτε, ταῖς ψυχαῖς ἡμῶν δοθῆναι, τὸ μέγα ἔλεος."

meMA.Lauds3.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Lauds3.melody = he.a.m5_gr_GR_cog.heAU.ChairoisAskitikon.name
meMA.Lauds3.incipit = "Χαίροις τροπαιοφόρος πληθύς, ἡ ἐν…"
meMA.Lauds3.text = "Χαίροις τροπαιοφόρος πληθύς, ἡ ἐν πολέμοις ἀνδρικῶς ἀριστεύσασα, ἀστέρες οἱ διελθόντες, διὰ πυρὸς καὶ κρυμοῦ, καὶ ὑδάτων πῆξιν διαλύσαντες, οἱ γῆν οὐρανώσαντες, καὶ τὰ πάντα φωτίσαντες, οἱ ἐν τοῖς κόλποις, Ἀβραὰμ νῦν θαλπόμενοι, οἱ χορεύοντες, σὺν Ἀγγέλων στρατεύμασι, Μάρτυρες τεσσαράκοντα, τὰ ἄνθη τὰ πνέοντα, τὴν εὐωδίαν τῆς ὄντως, πνευματικῆς διαδόσεως, Χριστὸν δυσωπεῖτε, ταῖς ψυχαῖς ἡμῶν δοθῆναι, τὸ μέγα ἔλεος."

meMA.Lauds4.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Lauds4.melody = miscellanea_gr_GR_cog.misc.Idiomelon
meMA.Lauds4.incipit = "Χορὸς τετραδεκαπύρσευτος, στρατὸς ὅλος θεοσύλλεκτος…"
meMA.Lauds4.text = "Χορὸς τετραδεκαπύρσευτος, στρατὸς ὅλος θεοσύλλεκτος, συνεξέλαμψε τῇ νηστείᾳ ἄθλοις σεπτοῖς, ἁγιάζων καὶ φωτίζων τὰς ψυχὰς ἡμῶν."

meMA.Lauds5.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Lauds5.melody = miscellanea_gr_GR_cog.misc.Idiomelon
meMA.Lauds5.incipit = "Τὴν τετραδεκάριθμον χορείαν τῶν Μαρτύρων…"
meMA.Lauds5.text = "Τὴν τετραδεκάριθμον χορείαν τῶν Μαρτύρων, τίς μὴ ἀνυμνήσει; τῷ γὰρ ὕδατι τῆς λίμνης εἰσῆλθον θαρσαλέως, καὶ τῷ κρύει σφιγγόμενοι, τὴν ᾠδὴν ἀνέμελπον τῷ Κυρίῳ. Μὴ ἐν ποταμοῖς ὀργισθῇς ἡμῖν Κύριε, μὴ ἐν ποταμοῖς ὀργισθῇς ἡμῖν φιλάνθρωπε, ἐλάφρυνον τὸ βάρος, καὶ τὴν πικρότητα τοῦ ἀέρος· τῷ οἰκείῳ γὰρ αἵματι, ἐβάφησαν ἡμῶν οἱ πόδες, καὶ εἰσήγαγες ἡμᾶς ὁ Θεὸς εἰς τὰς αἰωνίους σου σκηνάς, ἵνα ὁ κόλπος ἡμᾶς θάλψῃ τοῦ Πατριάρχου Ἀβραάμ."

meMA.Lauds6.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Lauds6.melody = miscellanea_gr_GR_cog.misc.Idiomelon
meMA.Lauds6.incipit = "Ἀληθείας κρατῆρα, ἐξ οἰκείων αἱμάτων…"
meMA.Lauds6.text = "Ἀληθείας κρατῆρα, ἐξ οἰκείων αἱμάτων, τῷ πυρὶ τῶν βασάνων, καὶ τῇ τοῦ ὕδατος πήξει, τοῖς πιστοῖς κατήρδευσαν· τετραρίθμῳ γὰρ ᾄδοντες, δεκάδι τῷ Σωτῆρι, εἷς μὲν ὄντες τοῖς πνεύμασιν, ἐν σώματι δὲ πλείους, προσηνέχθησαν Χριστῷ, καὶ Θεόνυμφος Μήτηρ τῷ φιλοχρίστῳ παιδί, ἐπʼ ὤμων ἀραμένη ἔλεγε· Δεῦρο Ἀθλητά, καὶ συναγωνίζου τοῖς προλοίποις."

meMA.LaudsGlory.poet = poets_gr_GR_cog.po.IoannouMonachou
meMA.LaudsGlory.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.LaudsGlory.melody = miscellanea_gr_GR_cog.misc.Idiomelon
meMA.LaudsGlory.incipit = "Ἀθλοφόροι Χριστοῦ, τὴν πάνσεπτον νηστείαν…"
meMA.LaudsGlory.text = "Ἀθλοφόροι Χριστοῦ, τὴν πάνσεπτον νηστείαν φαιδροτέραν ἀπειργάσασθε, τῇ μνήμῃ τῆς ἐνδόξου ὑμῶν ἀθλήσεως· Τεσσαράκοντα γὰρ ὄντες, τὴν τεσσαρακονθήμερον ἁγιάζετε, τὸ σωτήριον πάθος μιμησάμενοι, διὰ τῆς ὑπὲρ Χριστοῦ ὑμῶν ἀθλήσεως. Διὸ ἔχοντες παῤῥησίαν πρεσβεύσατε, ἐν εἰρήνῃ καταντῆσαι ἡμᾶς, εἰς τὴν τριήμερον Ἀνάστασιν τοῦ Θεοῦ, καὶ Σωτῆρος τῶν ψυχῶν ἡμῶν."

