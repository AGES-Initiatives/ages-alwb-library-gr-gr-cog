meMA.Ode5C1H.incipit = he.h.m6_gr_GR_cog.heHE.ToTheioFengeiSou.incipit
meMA.Ode5C1H.text = he.h.m6_gr_GR_cog.heHE.ToTheioFengeiSou.text

meMA.Ode5C11.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C11.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode5C11.greekmelody = he.h.m6_gr_GR_cog.heHE.ToTheioFengeiSou.greekname
meMA.Ode5C11.melody = he.h.m6_gr_GR_cog.heHE.ToTheioFengeiSou.name
meMA.Ode5C11.incipit = "Ἁγιασθέντα ἀσκητικῶς, καὶ τελειωθέντα σε…"
meMA.Ode5C11.text = "Ἁγιασθέντα ἀσκητικῶς, καὶ τελειωθέντα σε διʼ αἵματος, γεραίρομεν Ὅσιε· μνήμη γὰρ δικαίων μετʼ ἐγκωμίων ἀεί, πιστῶς ἐπιτελεῖται, καὶ μνημονεύεται."

meMA.Ode5C12.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C12.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode5C12.melody = he.h.m6_gr_GR_cog.heHE.ToTheioFengeiSou.name
meMA.Ode5C12.incipit = "Σοφῶς ὁ πάντα μεταποιῶν, καὶ…"
meMA.Ode5C12.text = "Σοφῶς ὁ πάντα μεταποιῶν, καὶ μετασκευάζων πρὸς τὸ κρεῖττον, ὡς βούλεται Κύριος, τῇ τῶν δυσσεβούντων μιαιφονίᾳ τῇ σῇ, ἀσκήσει μαρτυρίου κλέος προσέθετο."

meMA.Ode5C13.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C13.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode5C13.melody = he.h.m6_gr_GR_cog.heHE.ToTheioFengeiSou.name
meMA.Ode5C13.incipit = "Ἐπὶ τῶν ὤμων ἀναλαβών, τὴν…"
meMA.Ode5C13.text = "Ἐπὶ τῶν ὤμων ἀναλαβών, τὴν διὰ Σταυροῦ πανοπλίαν, σὺ τῷ Χριστῷ ἠκολούθησας, τῶν ὀπίσω Πάτερ λήθην ποιούμενος, καὶ τοῖς ἔμπροσθεν δρόμοις ἐπεκτεινόμενος."

meMA.Ode5C1T.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C1T.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode5C1T.melody = he.h.m6_gr_GR_cog.heHE.ToTheioFengeiSou.name
meMA.Ode5C1T.incipit = "Ὑμνοῦμεν Πάναγνε τὸν ἐκ σοῦ…"
meMA.Ode5C1T.text = "Ὑμνοῦμεν Πάναγνε τὸν ἐκ σοῦ, σάρκα παθητήν τε καὶ θνητήν, περιβαλλόμενον Κύριον, καὶ τεθεωμένην ἀπεργασάμενον, ἑνώσει ἀσυγχύτῳ τῇ καθʼ ὑπόστασιν."

meMA.Ode6C1H.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C1H.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode6C1H.greekmelody = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.greekname
meMA.Ode6C1H.melody = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.melody
meMA.Ode6C1H.incipit = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.incipit
meMA.Ode6C1H.text = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.text

meMA.Ode6C11.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C11.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode6C11.greekmelody = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.greekname
meMA.Ode6C11.melody = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.name
meMA.Ode6C11.incipit = "Προβαίνων ἀείμνηστε, ἀπὸ δόξης ἀληθῶς…"
meMA.Ode6C11.text = "Προβαίνων ἀείμνηστε, ἀπὸ δόξης ἀληθῶς, ἀσκητικῆς ἀνέδραμες, πρὸς Μαρτύρων ἀνδρείαν τε καὶ τιμήν, οἰκείου ἐξ αἵματος, ἐρυθρὰν πορφυρίδα περικείμενος."

meMA.Ode6C12.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C12.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode6C12.melody = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.name
meMA.Ode6C12.incipit = "Ῥεόντων τὸ ἄστατον, λογισάμενος ψυχῇ…"
meMA.Ode6C12.text = "Ῥεόντων τὸ ἄστατον, λογισάμενος ψυχῇ, καὶ λογισμῷ θεόφρονι, τοῖς ἀεὶ διαμένουσιν ἀγαθοῖς, καὶ πέρας οὐκ ἔχουσιν, εὐσεβῶς ἑπτερώθης παμμακάριστε."

meMA.Ode6C13.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C13.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode6C13.melody = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.name
meMA.Ode6C13.incipit = "Ἐγκράτειαν σύντονον, καὶ πραότητα Χριστός…"
meMA.Ode6C13.text = "Ἐγκράτειαν σύντονον, καὶ πραότητα Χριστός, σὲ καθορῶν πλουτήσαντα, φιλανθρώπως ἐπέβλεψεν ἐπὶ σέ, καὶ δόξης ἀνέδειξε, κοινωνὸν Θεοφόρε παναοίδιμε."

meMA.Ode6C1T.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C1T.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode6C1T.melody = he.h.m6_gr_GR_cog.heHE.TouViouTinThalassan.name
meMA.Ode6C1T.incipit = "Πανάμωμε Δέσποινα, τῆς ψυχῆς μου…"
meMA.Ode6C1T.text = "Πανάμωμε Δέσποινα, τῆς ψυχῆς μου τὰ δεινά, καὶ χαλεπὰ συντρίμματα, χειρουργίαις ἰάτρευσον μυστικαῖς, δραστήρια φάρμακα, τοῦ Υἱοῦ σου βαλοῦσα τὰ παθήματα."

meMA.Kontakion1.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Kontakion1.melody = he.a.m4_gr_GR_cog.heAU.EpefanisSimeron.name
meMA.Kontakion1.incipit = "Ἑωσφόρον σήμερον ἡ Ἐκκλησία, κεκτημένη…"
meMA.Kontakion1.text = "Ἑωσφόρον σήμερον ἡ Ἐκκλησία, κεκτημένη ἅπασαν, κακοδοξίας τὴν ἀχλύν, διασκεδάζει τιμῶσά σε, οὐρανομύστα Προκόπιε ἔνδοξε."

meMA.Ode7C1H.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C1H.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode7C1H.greekmelody = he.h.m6_gr_GR_cog.heHE.DrosovolonMenTinKaminon.greekname
meMA.Ode7C1H.melody = he.h.m6_gr_GR_cog.heHE.DrosovolonMenTinKaminon.melody
meMA.Ode7C1H.incipit = he.h.m6_gr_GR_cog.heHE.DrosovolonMenTinKaminon.incipit
meMA.Ode7C1H.text = he.h.m6_gr_GR_cog.heHE.DrosovolonMenTinKaminon.text

meMA.Ode7C11.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C11.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode7C11.greekmelody = he.h.m6_gr_GR_cog.heHE.DrosovolonMenTinKaminon.greekname
meMA.Ode7C11.melody = he.h.m6_gr_GR_cog.heHE.DrosovolonMenTinKaminon.name
meMA.Ode7C11.incipit = "Ὡς ὑπέρλαμπρος ὁ βίος σου…"
meMA.Ode7C11.text = "Ὡς ὑπέρλαμπρος ὁ βίος σου θεόσοφε, γέγονε διʼ ἀσκήσεως, οὕτως ὤφθη σου προφανῶς, ἡ ἄθλησις στεῤῥά. Χριστὸν γὰρ ἐδόξασας βοῶν· Εὐλογητὸς εἶ ὁ Θεός, ὁ τῶν Πατέρων ἡμῶν."

meMA.Ode7C12.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C12.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode7C12.melody = he.h.m6_gr_GR_cog.heHE.DrosovolonMenTinKaminon.name
meMA.Ode7C12.incipit = "Σωφροσύνῃ καθαρότητα…"
meMA.Ode7C12.text = "Σωφροσύνῃ καθαρότητα θεσπέσιε, Πάτερ περιζωσάμενος, ἐν ἀνδρείᾳ δὲ μαρτυρίου, στέφανον λαμπρόν, δεξάμενος χάριτι βοᾷς· Εὐλογητὸς εἶ ὁ Θεός, ὁ τῶν Πατέρων ἡμῶν."

meMA.Ode7C13.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C13.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode7C13.melody = he.h.m6_gr_GR_cog.heHE.DrosovolonMenTinKaminon.name
meMA.Ode7C13.incipit = "Μὴ τὴν ἄχραντον εἰκόνα προσκυνοῦντές…"
meMA.Ode7C13.text = "Μὴ τὴν ἄχραντον εἰκόνα προσκυνοῦντές σου, Σῶτερ οἱ ἀσεβέστατοι, τὸν οἰκέτην σου μαστιγοῦντες, ἔξεον δεινῶς, ὑμνοῦντα καὶ κράζοντα πρὸς σέ· Εὐλογητὸς εἶ ὁ Θεός, ὁ τῶν Πατέρων ἡμῶν."

meMA.Ode7C1T.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C1T.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode7C1T.melody = he.h.m6_gr_GR_cog.heHE.DrosovolonMenTinKaminon.name
meMA.Ode7C1T.incipit = "Εὐφραινόμενοι τῷ τόκῳ σου Πανάμωμε…"
meMA.Ode7C1T.text = "Εὐφραινόμενοι τῷ τόκῳ σου Πανάμωμε, πάντες σε μακαρίζομεν· διʼ αὐτοῦ γὰρ νῦν σαρκικῆς, γεννήσεως ἡμεῖς, ῥυσθέντες κραυγάζομεν αὐτῷ· Εὐλογητὸς εἶ ὁ Θεός, ὁ τῶν Πατέρων ἡμῶν."

meMA.Ode8C1H.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C1H.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode8C1H.greekmelody = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.greekname
meMA.Ode8C1H.melody = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.melody
meMA.Ode8C1H.incipit = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.incipit
meMA.Ode8C1H.text = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.text

meMA.Ode8C11.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C11.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode8C11.greekmelody = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.greekname
meMA.Ode8C11.melody = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.name
meMA.Ode8C11.incipit = "Λελυμένος τῆς ϋλης καὶ πρὸ…"
meMA.Ode8C11.text = "Λελυμένος τῆς ϋλης καὶ πρὸ θανάτου ὀφθείς, μετετέθης ἐντεῦθεν πρὸς τὴν ἀκήρατον, Ὅσιε ζωήν, τῷ θανάτῳ χρησάμενος, κλίμακι προσφόρῳ, Προκόπιε θεόφρον."

meMA.Ode8C12.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C12.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode8C12.melody = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.name
meMA.Ode8C12.incipit = "Πεπλησμένος ἐφάνης φωτὸς τῆς πίστεως…"
meMA.Ode8C12.text = "Πεπλησμένος ἐφάνης φωτὸς τῆς πίστεως, καὶ πρὸς ἄδυτον φέγγος Πάτερ ἐφοίτησας, πᾶσαν ὑλικήν, ἀποθέμενος ἔφεσιν· ὅθεν σε τιμῶμεν, Προκόπιε θεόφρον."

meMA.Ode8C13.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C13.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode8C13.melody = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.name
meMA.Ode8C13.incipit = "Ὡπλισμένος δυνάμει τοῦ θείου Πνεύματος…"
meMA.Ode8C13.text = "Ὡπλισμένος δυνάμει τοῦ θείου Πνεύματος, πονηρίας πνευμάτων ἐτρέψω φάλαγγας· ὅθεν οὐρανῶν, πύλαι σοι ἠνεῴχθησαν, μάκαρ ἀνυμνοῦντι, Χριστὸν εἰς τοὺς αἰῶνας."

meMA.Ode8C1T.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C1T.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode8C1T.melody = he.h.m6_gr_GR_cog.heHE.EkFlogosToisOsiois.name
meMA.Ode8C1T.incipit = "Μακαρίζουσι πᾶσαι γενεαὶ Δέσποινα, ὡς…"
meMA.Ode8C1T.text = "Μακαρίζουσι πᾶσαι γενεαὶ Δέσποινα, ὡς προέφης ἡ μόνη θεομακάριστος, σὲ τὴν ἀληθῆ, Θεοτόκον καὶ πάναγνον· ἣν ὑπερυψοῦμεν, εἰς πάντας τοὺς αἰῶνας."

meMA.Ode9C1H.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C1H.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode9C1H.greekmelody = he.h.m6_gr_GR_cog.heHE.TheonAnthropois.greekname
meMA.Ode9C1H.melody = he.h.m6_gr_GR_cog.heHE.TheonAnthropois.melody
meMA.Ode9C1H.incipit = he.h.m6_gr_GR_cog.heHE.TheonAnthropois.incipit
meMA.Ode9C1H.text = he.h.m6_gr_GR_cog.heHE.TheonAnthropois.text

meMA.Ode9C11.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C11.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode9C11.greekmelody = he.h.m6_gr_GR_cog.heHE.TheonAnthropois.greekname
meMA.Ode9C11.melody = he.h.m6_gr_GR_cog.heHE.TheonAnthropois.name
meMA.Ode9C11.incipit = "Ἀθλεῖν νομίμως Σοφὲ ἑλόμενος, τῶν…"
meMA.Ode9C11.text = "Ἀθλεῖν νομίμως σοφὲ ἑλόμενος, τῶν παθῶν ὁρμὰς διʼ ἐγκρατείας ἐνέκρωσας, καὶ σαρκὸς σκιρτήματα θεράπον Χριστοῦ· ὅθεν τῆς ἀπαθείας, νῦν τὴν ἀνώλεθρον, σύν ταῖς ἀσωμάτοις Στρατιαῖς εὗρες ἀπόλαυσιν."

meMA.Ode9C12.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C12.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode9C12.melody = he.h.m6_gr_GR_cog.heHE.TheonAnthropois.name
meMA.Ode9C12.incipit = "Κριτῇ δικαίῳ φαιδρῶς παρίστασαι, ὡς…"
meMA.Ode9C12.text = "Κριτῇ δικαίῳ φαιδρῶς παρίστασαι, ὡς νικητὴς στεφάνους ἀμαράντους δεξάμενος, ἐκτενῶς Προκόπιε δεόμενος, ὑπὲρ τῶν ἐκτελούντων, σοῦ τὴν πανίερον, μνήμην καὶ τὴν θείαν ἑορτὴν τῆς μεταστάσεως."

meMA.Ode9C13.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C13.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode9C13.melody = he.h.m6_gr_GR_cog.heHE.TheonAnthropois.name
meMA.Ode9C13.incipit = "Αὐγαῖς τρισὶ νῦν καταλαμπόμενος, ταῖς…"
meMA.Ode9C13.text = "Αὐγαῖς τρισὶ νῦν καταλαμπόμενος, ταῖς ἐκ μιᾶς ἀφράστως προϊούσαις Θεότητος, πανολβίου λήξεως ἠξίωσαι, καὶ τερπνῆς θυμηδίας, συναγαλλόμενος, ταῖς ὑπερκοσμίαις Στρατιαῖς, Πάτερ Προκόπιε."

meMA.Ode9C1T.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C1T.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode9C1T.melody = he.h.m6_gr_GR_cog.heHE.TheonAnthropois.name
meMA.Ode9C1T.incipit = "Ῥευστὴν ὁ Λόγος Θεοῦ ὁ…"
meMA.Ode9C1T.text = "Ῥευστὴν ὁ Λόγος Θεοῦ ὁ ἄῤῥευστος, μορφὴν λαβὼν ἀνθρώπους ἀφθαρσίαν ἐνέδυσεν, εὐδοκίᾳ Πατρικῇ σκηνώσας ἐν σοί, τῇ κεχαριτωμένῃ· ὅθεν Πανάχραντε, σὺν ταῖς οὐρανίαις Στρατιαῖς σὲ μεγαλύνομεν."

meMA.Exaposteilarion1.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Exaposteilarion1.greekmelody = he.a.m2_gr_GR_cog.heAU.GynaikesAkoutisthite.greekname
meMA.Exaposteilarion1.melody = he.a.m2_gr_GR_cog.heAU.GynaikesAkoutisthite.name
meMA.Exaposteilarion1.incipit = "Σαρκίου τοῦ τιμίου σου, ἀπολυθεὶς…"
meMA.Exaposteilarion1.text = "Σαρκίου τοῦ τιμίου σου, ἀπολυθεὶς Προκόπιε, παρέστης νῦν σὺν Ἀγγέλοις, τῇ ἀπροσίτῳ Τριάδι· πρὸς ἣν ἡμῶν μνημόνευε, τῶν ἐκτελούντων ἔνδοξε, τὴν παναγίαν μνήμην σου, καὶ σὲ τιμώντων ἐκ πόθου, Πάτερ σοφὲ Ἱεράρχα."

meMA.ExapTheotokion.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.ExapTheotokion.melody = he.a.m2_gr_GR_cog.heAU.GynaikesAkoutisthite.name
meMA.ExapTheotokion.incipit = "Τὸ μέγα καταφύγιον, καὶ σκέπη…"
meMA.ExapTheotokion.text = "Τὸ μέγα καταφύγιον, καὶ σκέπη τῶν ψυχῶν ἡμῶν, ἡ οὐρανῶν πλατυτέρα, καὶ Χερουβὶμ ὑπερτέρα, τὸν σὸν Υἱὸν δυσώπησον, ὑπὲρ οἰκείων δούλων σου, Θεοκυῆτορ πάναγνε, ὅπως ῥυσθείημεν πάντες, ἁμαρτιῶν καὶ κινδύνων."

meLI.Kontakion1.mode = meMA.Kontakion1.mode
meLI.Kontakion1.melody = meMA.Kontakion1.melody
meLI.Kontakion1.incipit = meMA.Kontakion1.incipit
meLI.Kontakion1.text = meMA.Kontakion1.text

meMegalynarion.text = ""

