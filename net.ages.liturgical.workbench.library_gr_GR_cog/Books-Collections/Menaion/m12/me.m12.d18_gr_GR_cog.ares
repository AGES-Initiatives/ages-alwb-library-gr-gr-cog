meVE.Stichera01.mode = miscellanea_gr_GR_cog.misc.Mode1
meVE.Stichera01.greekmelody = he.a.m1_gr_GR_cog.heAU.PaneffimoiMartyres.greekname
meVE.Stichera01.melody = he.a.m1_gr_GR_cog.heAU.PaneffimoiMartyres.name
meVE.Stichera01.incipit = "Αἱμάτων τιμίων σου βαφαῖς, Μάρτυς…"
meVE.Stichera01.text = "Αἱμάτων τιμίων σου βαφαῖς, Μάρτυς κατεσκεύασας, σαυτῷ πορφύραν πολύτιμον, ἣν στολισάμενος, κατοικεῖς τὰ ἄνω, ἀληθῶς βασίλεια, Θεῷ παμβασιλεῖ παριστάμενος· διὸ ἱκέτευε, δωρηθῆναι ταῖς ψυχαῖς ἡμῶν, τὴν εἰρήνην, καὶ τὸ μέγα ἔλεος."

meVE.Stichera02.mode = miscellanea_gr_GR_cog.misc.Mode1
meVE.Stichera02.melody = he.a.m1_gr_GR_cog.heAU.PaneffimoiMartyres.name
meVE.Stichera02.incipit = "Σεβαστιανός τε καὶ Ζωή, Μᾶρκος…"
meVE.Stichera02.text = "Σεβαστιανός τε καὶ Ζωή, Μᾶρκος Μαρκελλῖνός τε, σὺν Τιβουρτίῳ συνήθλησαν, καθυπομείναντες, τρικυμίας πόνων, καὶ νῦν κατεσκήνωσαν, πρὸς ἄπονον καὶ θείαν κατάπαυσιν, ἐν ᾗ πρεσβεύουσι, δωρηθῆναι ταῖς ψυχαῖς ἡμῶν, τὴν εἰρήνην, καὶ τὸ μέγα ἔλεος."

meVE.Stichera03.mode = miscellanea_gr_GR_cog.misc.Mode1
meVE.Stichera03.melody = he.a.m1_gr_GR_cog.heAU.PaneffimoiMartyres.name
meVE.Stichera03.incipit = "Ἀήττητοι Μάρτυρες τῇ γῇ, τὰ…"
meVE.Stichera03.text = "Ἀήττητοι Μάρτυρες τῇ γῇ, τὰ τῆς γῆς ἐάσαντες, πρὸς οὐρανοὺς ἀνεδράμετε, ὑμῶν τοῖς στίγμασι, καθωραϊσμένοι, καὶ σεπτοῖς παθήμασι, λαμπρῶς πεποικιλμένοι μακάριοι· διὸ πρεσβεύσατε, δωρηθῆναι ταῖς ψυχαῖς ἡμῶν, τὴν εἰρήνην, καὶ τὸ μέγα ἔλεος."

meVE.SticTheotokion.mode = miscellanea_gr_GR_cog.misc.Mode1
meVE.SticTheotokion.greekmelody = he.a.m1_gr_GR_cog.heAU.PaneffimoiMartyres.greekname
meVE.SticTheotokion.melody = he.a.m1_gr_GR_cog.heAU.PaneffimoiMartyres.name
meVE.SticTheotokion.incipit = "Παρθένε πανύμνητε Μωσῆς, τὸ ἐν…"
meVE.SticTheotokion.text = "Παρθένε πανύμνητε Μωσῆς, τὸ ἐν σοὶ Μυστήριον, προφητικοῖς εἶδεν ὄμμασι, Βάτον μὴ φλέγεσθαι, καὶ περ καιομένην· πῦρ γὰρ τῆς θεότητος, τὴν μήτραν σου Ἁγνὴ οὐ κατέφλεξε· διὸ αἰτοῦμέν σε, ὡς Μητέρα τοῦ Θεοῦ ἡμῶν, τὴν εἰρήνην, τῷ κόσμῳ σου δώρησαι." //similar to oc.m1.d3.ocMA.AposTheotokion.text

meVE.SticStavrotheotokion.mode = miscellanea_gr_GR_cog.misc.Mode1
meVE.SticStavrotheotokion.greekmelody = he.a.m1_gr_GR_cog.heAU.PaneffimoiMartyres.greekname
meVE.SticStavrotheotokion.melody = he.a.m1_gr_GR_cog.heAU.PaneffimoiMartyres.name
meVE.SticStavrotheotokion.incipit = oc.m1.d4_gr_GR_cog.ocVE.SticTheotokion.incipit
meVE.SticStavrotheotokion.text = oc.m1.d4_gr_GR_cog.ocVE.SticTheotokion.text

meVE.Apolytikion1ger.poet = poets_gr_GR_cog.po.Gerasimou
meVE.Apolytikion1ger.mode = miscellanea_gr_GR_cog.misc.Mode1
meVE.Apolytikion1ger.greekmelody = he.a.m1_gr_GR_cog.heAU.TisErimouPolitis.greekname
meVE.Apolytikion1ger.melody = he.a.m1_gr_GR_cog.heAU.TisErimouPolitis.name
meVE.Apolytikion1ger.incipit = "Συγκλήτου σφαλλομένης παριδὼν τὰ συνέδρια…"
meVE.Apolytikion1ger.text = "Συγκλήτου σφαλλομένης παριδὼν τὰ συνέδρια, Σεβαστιανὲ πονολβίαν, συναγείρεις συνέλευσιν, Μαρτύρων ἀληθῶς πανευκλεῶν, σὺν σοὶ καταβαλόντν τὸν ἐχθρόν, μεθʼ ὧν θείας συναυλίας ἀξιωθείς, φαιδρύνεις τοὺς βοῶντάς σοι· δόξα τῷ δεδωκότι σοι ἰσχύν, δόξα τῷ σὲ στεφανώσαντι, δόξα τῷ ἐνεργοῦντι διὰ σοῦ, πᾶσιν ἰάματα."

meMA.C1.acrostic.title = "Κανὼν τοῦ Μάρτυρος,"
meMA.C1.acrostic.notice1 = "οὗ ἡ Ἀκροστιχίς·"
meMA.C1.acrostic.text = "Θεῖον σέβας σοι Μάρτυς ἐμφρόνως νέμω. Ἰωσήφ."
meMA.C1.title = titles_gr_GR_cog.ti.CanonMartyrM
meMA.C1.verse = prayers_gr_GR_cog.pr.verse_SaintM.text

meMA.Ode1C11.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C11.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode1C11.greekmelody = he.h.m1_gr_GR_cog.heHE.SouITropaiouchos.greekname
meMA.Ode1C11.melody = he.h.m1_gr_GR_cog.heHE.SouITropaiouchos.name
meMA.Ode1C11.incipit = "Θείαις φαιδρυνόμενος αὐγαῖς, τῆς σεβασμίου…"
meMA.Ode1C11.text = "Θείαις φαιδρυνόμενος αὐγαῖς, τῆς σεβασμίου Τριάδος Ἀοίδιμε, πίστει τοὺς τιμῶντάς σου, τὴν φωταυγῆ καὶ ἱερὰν πανήγυριν, σοῦ ταῖς ἱκεσίαις, φωτὸς μετόχους ἀνάδειξον."

meMA.Ode1C12.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C12.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode1C12.melody = he.h.m1_gr_GR_cog.heHE.SouITropaiouchos.name
meMA.Ode1C12.incipit = "Ἔχων λογισμὸν φωτοειδῆ, καὶ δεκτικὸν…"
meMA.Ode1C12.text = "Ἔχων λογισμὸν φωτοειδῆ, καὶ δεκτικὸν καθαρῶν ἐπιλάμψεων, μόνον ἐπεπόθησας, τὸ ὄντως Ὄν, ὡς νουνεχὴς καὶ φρόνιμος, Μάρτυς ἀθλοφόρε, καὶ τούτου πίστει τετύχηκας."

meMA.Ode1C13.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C13.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode1C13.melody = he.h.m1_gr_GR_cog.heHE.SouITropaiouchos.name
meMA.Ode1C13.incipit = "Ἱδρῶσιν ἐνθέοις ἀληθῶς, τὰ τῆς…"
meMA.Ode1C13.text = "Ἱδρῶσιν ἐνθέοις ἀληθῶς, τὰ τῆς σαρκὸς ἀμαυρώσας φρονήματα, αἵμασιν ἀθλήσεως, τοὺς θολεροὺς χειμάῤῥους ἀπεξήρανας, τῆς πολυθεΐας, θεόφρον Μάρτυς πολύαθλε."

meMA.Ode1C14.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C14.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode1C14.melody = he.h.m1_gr_GR_cog.heHE.SouITropaiouchos.name
meMA.Ode1C14.incipit = "Ὅμιλον προσῆξας Ἀθλητῶν, διδασκαλίαις σου…"
meMA.Ode1C14.text = "Ὅμιλον προσῆξας Ἀθλητῶν, διδασκαλίαις σου θείαις τῷ Κτίσαντι, πᾶσαν τὴν διάνοιαν, τῶν διωκτῶν καταβαλὼν τῇ χάριτι, Πνεύματος Ἁγίου, μεθʼ ὧν πρεσβεύεις σωθῆναι ἡμᾶς."

meMA.Ode1C1T.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C1T.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode1C1T.melody = he.h.m1_gr_GR_cog.heHE.SouITropaiouchos.name
meMA.Ode1C1T.incipit = "Νέον ἀπεκύησας Υἱόν, τὸν πρὸ…"
meMA.Ode1C1T.text = "Νέον ἀπεκύησας Υἱόν, τὸν πρὸ αἰώνων Πατρὶ συννοούμενον, φύσεως καινίσασα, νόμους ἁγνή, Παρθενομῆτορ ἄχραντε, ἐν τῇ ὑπὲρ φύσιν, σεπτῇ καὶ θείᾳ γεννήσει σου."

meMA.Ode3C11.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C11.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode3C11.greekmelody = he.h.m1_gr_GR_cog.heHE.OMonosEidos.greekname
meMA.Ode3C11.melody = he.h.m1_gr_GR_cog.heHE.OMonosEidos.name
meMA.Ode3C11.incipit = "Στρατὸν ἐπαγόμενος σεπτόν, τῆς θείας…"
meMA.Ode3C11.text = "Στρατὸν ἐπαγόμενος σεπτόν, τῆς θείας ὅπλοις πίστεως, ὠχυρωμένον Μάρτυς ἀήττητε, τὰς παρατάξεις τοῦ πολεμήτορος, καὶ τὰ πανουργεύματα τῶν Τυράννων ἅπαντα, καταβάλλεις τῷ σθένει τοῦ Πνεύματος."

meMA.Ode3C12.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C12.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode3C12.melody = he.h.m1_gr_GR_cog.heHE.OMonosEidos.name
meMA.Ode3C12.incipit = "Ἐρείσας ἐν πέτρᾳ νοητή, τὰς…"
meMA.Ode3C12.text = "Ἐρείσας ἐν πέτρᾳ νοητή, τὰς βάσεις τῆς καρδίας σου, εὐσεβοφρόνως Μάρτυς πολύαθλε, οὐ παρετράπης τοῖς μηχανήμασι, τοῦ δολίου δράκοντος, ἀλλὰ πᾶσιν ἔρεισμα, εὐσεβείας ἐν Πνεύματι γέγονας."

meMA.Ode3C13.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C13.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode3C13.melody = he.h.m1_gr_GR_cog.heHE.OMonosEidos.name
meMA.Ode3C13.incipit = "Βελῶν ἐκκεντήσεις χαλεπάς, ὑπέφερες δεσμούμενος…"
meMA.Ode3C13.text = "Βελῶν ἐκκεντήσεις χαλεπάς, ὑπέφερες δεσμούμενος, καὶ ἐπὶ ξύλου Μάρτυς τεινόμενος, Θεοῦ δὲ θείᾳ δυνάμει ἔνδοξε, ἀσινὴς ὁλόκληρος, αὖθις ἀπεδείκνυσο, καταισχύνων ἐχθροῦ μηχανήματα."

meMA.Ode3C1T.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C1T.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode3C1T.melody = he.h.m1_gr_GR_cog.heHE.OMonosEidos.name
meMA.Ode3C1T.incipit = "Ἁγίασμα ὤφθης νοητόν, τοῦ πάντας…"
meMA.Ode3C1T.text = "Ἁγίασμα ὤφθης νοητόν, τοῦ πάντας ἁγιάζοντος, καὶ καθαρὸν Πανάμωμε σκήνωμα· ἐκ σοῦ γὰρ ὤφθη Θεὸς σαρκούμενος, καὶ διπλοῦς ταῖς φύσεσι, κόσμῳ γνωριζόμενος, ἐν μιᾷ ὑποστάσει Θεόνυμφε."

meMA.Ode3H.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3H.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode3H.greekmelody = he.h.m1_gr_GR_cog.heHE.OMonosEidos.greekname
meMA.Ode3H.melody = he.h.m1_gr_GR_cog.heHE.OMonosEidos.melody
meMA.Ode3H.incipit = he.h.m1_gr_GR_cog.heHE.OMonosEidos.incipit
meMA.Ode3H.text = he.h.m1_gr_GR_cog.heHE.OMonosEidos.text

meMA.Ode3Kathisma1.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode3Kathisma1.greekmelody = he.a.m1_gr_GR_cog.heAU.TonTafonSouSotir.greekname
meMA.Ode3Kathisma1.melody = he.a.m1_gr_GR_cog.heAU.TonTafonSouSotir.name
meMA.Ode3Kathisma1.incipit = "Τῷ βίῳ τῷ φαιδρῷ, συναστράπτοντα…"
meMA.Ode3Kathisma1.text = "Τῷ βίῳ τῷ φαιδρῷ, συναστράπτοντα ἔχων τὸν λόγον ἱερῶς, ἐφειλκύσω ἀγέλας, πιστῶν πρὸς εὐσέβειαν, μεθʼ ὧν χαίρων ἐνήθλησας, πᾶσαν κάκωσιν, ὑπενεγκὼν τῶν Τυράννων· ὅθεν σήμερον τὴν παναγίαν σου μνήμην, τελοῦντες ὑμνοῦμέν σε."

meMA.Ode3KathismaT.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode3KathismaT.melody = he.a.m1_gr_GR_cog.heAU.TonTafonSouSotir.name
meMA.Ode3KathismaT.incipit = "Μαρία τὸ σεπτόν, τοῦ Δεσπότου…"
meMA.Ode3KathismaT.text = "Μαρία τὸ σεπτόν, τοῦ Δεσπότου δοχεῖον, ἀνάστησον ἡμᾶς, πεπτωκότας εἰς βάθος, δεινῆς ἀπογνώσεως, καὶ πταισμάτων καὶ θλίψεων· σὺ γὰρ πέφυκας, ἁμαρτωλῶν σωτηρία, καὶ βοήθεια, καὶ κραταιὰ προστασία, καὶ σῴζεις τοὺς δούλους σου."

meMA.Ode3KathismaSt.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode3KathismaSt.melody = he.a.m1_gr_GR_cog.heAU.TonTafonSouSotir.name
meMA.Ode3KathismaSt.incipit = "Ὢ θαύματος φρικτοῦ! ὢ καινοῦ…"
meMA.Ode3KathismaSt.text = "Ὢ θαύματος φρικτοῦ! ὢ καινοῦ μυστηρίου! ἐβόα ἡ ἁγνή, καὶ πανάμωμος Κόρη, ἐν ξύλῳ ὡς ἔβλεψε, τανυσθέντα σε Κύριε, πῶς ἀδέκαστε, Κριτὰ ἁπάντων καὶ Λόγε, ὡς κατάκριτος, ὑπὸ κριτῶν παρανόμων, Σταυρῷ κατακέκρισαι;"

meMA.Ode4C11.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C11.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode4C11.greekmelody = he.h.m1_gr_GR_cog.heHE.OrosSeTiChariti.greekname
meMA.Ode4C11.melody = he.h.m1_gr_GR_cog.heHE.OrosSeTiChariti.name
meMA.Ode4C11.incipit = "Σθένοις σοι Τιβούρτιε, δωρεῖται ὁ…"
meMA.Ode4C11.text = "Σθένοις σοι Τιβούρτιε, δωρεῖται ὁ Κύριος, διʼ ἀσθενείας τῆς σαρκός, καταπαλαῖσαι τὸν ἐχθρόν· πυρὶ γὰρ φλογίζοντι, νεανικῶς, ἐπέβης Μάρτυς στεῤῥότατα, καὶ τὴν οὐράνιον δρόσον ἀντέλαβες."

meMA.Ode4C12.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C12.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode4C12.melody = he.h.m1_gr_GR_cog.heHE.OrosSeTiChariti.name
meMA.Ode4C12.incipit = "Συνήφθης τοῖς ἄνω, λειτουργοῖς ὦ…"
meMA.Ode4C12.text = "Συνήφθης τοῖς ἄνω, λειτουργοῖς ὦ Τιβούρτιε, ὡς λειτουργήσας τῷ Χριστῷ, καὶ ἀνενέγκας καθαράς, θυσίας ἐν πνεύματι, Ἱερουργέ, Μαρτύρων κλέος καὶ καύχημα, χαρμονικῶς· διὰ τοῦτο τιμῶμέν σε."

meMA.Ode4C13.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C13.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode4C13.melody = he.h.m1_gr_GR_cog.heHE.OrosSeTiChariti.name
meMA.Ode4C13.incipit = "Ὄρθιος κρεμάμενος, καὶ πλήκτροις ξεόμενος…"
meMA.Ode4C13.text = "Ὄρθιος κρεμάμενος, καὶ πλήκτροις ξεόμενος, καὶ ταῖςσφοδραῖς τῶν αἰκισμῶν, περικυκλούμενος φοραῖς, καὶ βόθρῳ χωννύμενος, καὶ ἐν αὐτῷ τὸ θεῖον τέλος δεχόμενος, ὁ γενναιόφρων ἠγάλλετο Κάστουλος."

meMA.Ode4C14.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C14.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode4C14.melody = he.h.m1_gr_GR_cog.heHE.OrosSeTiChariti.name
meMA.Ode4C14.incipit = "Ἰάσεις ἐν Πνεύματι Ἁγίῳ μακάριε…"
meMA.Ode4C14.text = "Ἰάσεις ἐν Πνεύματι Ἁγίῳ μακάριε, ἐπιτελῶν πρὸς φωτισμόν, τῶν καθορώντων εὐσεβῶς, λαοῖς τὸ σωτήριον, θεοπρεπῶς Σεβαστιανὲ κατήγγειλας, μαρτυρικῶς δὲ τὸν δρόμον ἐτέλεσας."

meMA.Ode4C1T.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C1T.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode4C1T.melody = he.h.m1_gr_GR_cog.heHE.OrosSeTiChariti.name
meMA.Ode4C1T.incipit = "Μόνην σε πασῶν, ἐκ γενεῶν…"
meMA.Ode4C1T.text = "Μόνην σε πασῶν, ἐκ γενεῶν ἐξελέξατο, ὁ ὑπερούσιος Θεός, καὶ οὐσιώθη καθʼ ἡμᾶς γενόμενος ἄνθρωπος, ὁ πλαστουργός, τῆς ἀνθρωπίνης ὑπάρξεως, Θεογεννῆτορ πανάμωμε Δέσποινα."

meMA.Ode5C11.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C11.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode5C11.greekmelody = he.h.m1_gr_GR_cog.heHE.OFotisasTiEllampsei.greekname
meMA.Ode5C11.melody = he.h.m1_gr_GR_cog.heHE.OFotisasTiEllampsei.name
meMA.Ode5C11.incipit = "Αἱ τρίβοι σου, καὶ πορεῖαι…"
meMA.Ode5C11.text = "Αἱ τρίβοι σου, καὶ πορεῖαι πρὸς μόνον φερόμεναι, τὸν Κύριον, ἀνοδίας ἐχθρῶν ὑπεξέκλιναν, καὶ πολλοῖς γεγόνασιν, ὁδὸς εὐθεῖα καὶ πραεῖα, Μάρτυς Χριστοῦ πανσεβάσμιε."

meMA.Ode5C12.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C12.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode5C12.melody = he.h.m1_gr_GR_cog.heHE.OFotisasTiEllampsei.name
meMA.Ode5C12.incipit = "Ῥητορεύει, Μαρκελλῖνος καὶ Μᾶρκος σοφώτατα…"
meMA.Ode5C12.text = "Ῥητορεύει, Μαρκελλῖνος καὶ Μᾶρκος σοφώτατα, πρὸ βημάτων, τοῦ Χριστοῦ τὴν σωτήριον ἔλευσιν, προσδεθέντες ἄμφω δέ, κέντροις τοὺς πόδας καθηλοῦνται, μαρτυρικῶς κλεϊζόμενοι."

meMA.Ode5C13.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C13.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode5C13.melody = he.h.m1_gr_GR_cog.heHE.OFotisasTiEllampsei.name
meMA.Ode5C13.incipit = "Τὸ πάθημα, τοῦ τὰ πάθη…"
meMA.Ode5C13.text = "Τὸ πάθημα, τοῦ τὰ πάθη ἡμῶν θανατώσαντος, Μαρκελλῖνος, καὶ ὁ Μᾶρκος στεῤῥῶς εἰκονίζοντες, λόγχῃ σφαγιάζονται, καὶ στεφηφόροι ταῖς χορείαις, τῶν ἀθλοφόρων συνάπτονται."

meMA.Ode5C1T.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C1T.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode5C1T.melody = he.h.m1_gr_GR_cog.heHE.OFotisasTiEllampsei.name
meMA.Ode5C1T.incipit = "Ὑπέρφωτε, τοῦ Ἡλίου Νεφέλη πανάμωμε…"
meMA.Ode5C1T.text = "Ὑπέρφωτε, τοῦ Ἡλίου Νεφέλη πανάμωμε, ἀπέλασον, τῆς ψυχῆς μου τὰ νέφη πρεσβείαις σου, καὶ τὸν νοῦν μου φώτισον, τῇ ἀμελείᾳ σκοτισθέντα, ὅπως ὑμνῶ σε Πανύμνητε."

meMA.Ode6C11.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C11.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode6C11.greekmelody = he.h.m1_gr_GR_cog.heHE.EkyklosenImas.greekname
meMA.Ode6C11.melody = he.h.m1_gr_GR_cog.heHE.EkyklosenImas.name
meMA.Ode6C11.incipit = "Στεῤῥότητι νοὸς σεβασμιώτατε, ἐδείχθης ἀταπείνωτος…"
meMA.Ode6C11.text = "Στεῤῥότητι νοὸς σεβασμιώτατε, ἐδείχθης ἀταπείνωτος, ἐν τῷ τύπτεσθαι ταῖς ῥάβδοις ἀφειδῶς, καὶ κατακεντᾶσθαι πολλοῖς τοῖς βέλεσιν· ὅθεν πρὸς φῶς, μετέβης χαίρων Μάρτυς ἀνέσπερον."

meMA.Ode6C12.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C12.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode6C12.melody = he.h.m1_gr_GR_cog.heHE.EkyklosenImas.name
meMA.Ode6C12.incipit = "Ἐνεύρου σε Χριστὸς ἀγωνιζόμενον, καὶ…"
meMA.Ode6C12.text = "Ἐνεύρου σε Χριστὸς ἀγωνιζόμενον, καὶ δαίμοσι μαχόμενον, καὶ αἰκίαις ὁμιλοῦντα χαλεπαῖς, Σεβαστιανὲ Μάρτυς ἀήττητε, καὶ πρὸς αὐτὸν ἀποσκοποῦντα καρδίας ὄμμασι."

meMA.Ode6C13.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C13.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode6C13.melody = he.h.m1_gr_GR_cog.heHE.EkyklosenImas.name
meMA.Ode6C13.incipit = "Μοχλεύσεσι στεῤῥαῖς πανσόφως λόγων σου…"
meMA.Ode6C13.text = "Μοχλεύσεσι στεῤῥαῖς πανσόφως λόγων σου, τὴν πλάνην ἀνεμόχλευσας, καὶ σεβάσματα κατέαξας ἐχθρῶν, καὶ εἰδωλικοὺς ναοὺς ἠδάφισας, καὶ σεαυτὸν ναὸν τοῦ θείου ἤγειρας Πνεύματος."

meMA.Ode6C1T.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C1T.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode6C1T.melody = he.h.m1_gr_GR_cog.heHE.EkyklosenImas.name
meMA.Ode6C1T.incipit = "Φθαρεῖσαν τὴν ἡμῶν ἐκ παραβάσεως…"
meMA.Ode6C1T.text = "Φθαρεῖσαν τὴν ἡμῶν ἐκ παραβάσεως, οὐσίαν ἀνεκαίνισας, τὸν καινίζοντα τὰ πάντα θεϊκῷ, μόνῳ τῷ βουλήματι κυήσασα, ἐν γυναιξὶν εὐλογημένη θεοχαρίτωτε."

meMA.Ode6H.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6H.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode6H.greekmelody = he.h.m1_gr_GR_cog.heHE.EkyklosenImas.greekname
meMA.Ode6H.melody = he.h.m1_gr_GR_cog.heHE.EkyklosenImas.melody
meMA.Ode6H.incipit = he.h.m1_gr_GR_cog.heHE.EkyklosenImas.incipit
meMA.Ode6H.text = he.h.m1_gr_GR_cog.heHE.EkyklosenImas.text

meMA.Ode7C11.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C11.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode7C11.greekmelody = he.h.m1_gr_GR_cog.heHE.SeNoitin.greekname
meMA.Ode7C11.melody = he.h.m1_gr_GR_cog.heHE.SeNoitin.name
meMA.Ode7C11.incipit = "Ῥώμῃ τῇ σῇ, κραταιᾷ σθενούμενοι…"
meMA.Ode7C11.text = "Ῥώμῃ τῇ σῇ, κραταιᾷ σθενούμενοι, οἱ Ἀθλοφόροι σου Χριστέ, ἐτροπώσαντο τὸν ἐχθρόν, πλάνην κατεπάτησαν, σώματα παρέδωκαν, τοῖς αἰκισμοῖς ἀναμέλποντες, ὁ αἰνετός τῶν Πατέρων, Θεὸς καὶ ὑπερένδοξος."

meMA.Ode7C12.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C12.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode7C12.melody = he.h.m1_gr_GR_cog.heHE.SeNoitin.name
meMA.Ode7C12.incipit = "Οἱ θεαυγεῖς, Ἀθλοφόροι Μάρτυρες, ἀγωνισάμενοι…"
meMA.Ode7C12.text = "Οἱ θεαυγεῖς, Ἀθλοφόροι Μάρτυρες, ἀγωνισάμενοι καλῶς, Μαρκελλῖνος καὶ ὁ κλεινός, Μᾶρκος καὶ Τιβούρτιος, Κάστουλος ὁ πάνσοφος, πρὸς οὐρανὸν μετετέθησαν, τὸν αἰνετὸν ἀνυμνοῦντες, Θεὸν τὸν ὑπερένδοξον."

meMA.Ode7C13.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C13.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode7C13.melody = he.h.m1_gr_GR_cog.heHE.SeNoitin.name
meMA.Ode7C13.incipit = "Νεανικῶς, ἀθλητὰ Τιβούρτιε, ὥσπερ οἱ…"
meMA.Ode7C13.text = "Νεανικῶς, ἀθλητὰ Τιβούρτιε, ὥσπερ οἱ Παῖδες τῇ πυρᾷ, ἐπιβέβηκας νοητῶς, δρόσον τὴν τοῦ Πνεύματος, ἄνωθεν δεχόμενος, καὶ μελῳδῶν ἐμμελέστατα· ὁ αἰνετὸς τῶν Πατέρων, Θεὸς καὶ ὑπερένδοξος."

meMA.Ode7C14.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C14.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode7C14.melody = he.h.m1_gr_GR_cog.heHE.SeNoitin.name
meMA.Ode7C14.incipit = "Ὥσπερ ἀμνάς, κρεμαμένη ἔνδοξε, ἐναπηγχόνισας…"
meMA.Ode7C14.text = "Ὥσπερ ἀμνάς, κρεμαμένη ἔνδοξε, ἐναπηγχόνισας ἐχθρόν, ταῖς νευραῖς τῶν μαρτυρικῶν, πόνων σου πανεύφημε, ζῇς δὲ μετὰ θάνατον, Μάρτυς Ζωὴ ἀναμέλπουσα· ὁ αἰνετός τῶν Πατέρων, Θεὸς καὶ ὑπερένδοξος."

meMA.Ode7C1T.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C1T.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode7C1T.melody = he.h.m1_gr_GR_cog.heHE.SeNoitin.name
meMA.Ode7C1T.incipit = "Στάμνον χρυσῆν, παναγίαν τράπεζαν, τοῦ…"
meMA.Ode7C1T.text = "Στάμνον χρυσῆν, παναγίαν τράπεζαν, τοῦ θείου Ἄρτου τῆς ζωῆς, ὀνομάζομέν σε Ἁγνή. Τόπον ἁγιάσματος, θρόνον ὑψηλότατον, ἐν ᾧ Θεὸς ἀνεπαύσατο· ὁ αἰνετὸς τῶν Πατέρων, Θεὸς καὶ ὑπερένδοξος."

meMA.Ode8C11.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C11.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode8C11.greekmelody = he.h.m1_gr_GR_cog.heHE.EnKaminoPaides.greekname
meMA.Ode8C11.melody = he.h.m1_gr_GR_cog.heHE.EnKaminoPaides.name
meMA.Ode8C11.incipit = "Νεκρωθῆναι κόσμῳ βουληθείς, καὶ τοῖς…"
meMA.Ode8C11.text = "Νεκρωθῆναι κόσμῳ βουληθείς, καὶ τοῖς ἐν κόσμῳ πᾶσι, τῆς ὄντως κατηξιώθης, Ἀεισέβαστε ζωῆς, βοῶν προθυμότατα· Εὐλογεῖτε πάντα τὰ ἔργα Κυρίου, τὸν Κύριον ὑμνεῖτε, καὶ ὑπερυψοῦτε, εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8C12.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C12.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode8C12.melody = he.h.m1_gr_GR_cog.heHE.EnKaminoPaides.name
meMA.Ode8C12.incipit = "Ἔθεντό σε ἄνομοι δεινῶς, ἐν…"
meMA.Ode8C12.text = "Ἔθεντό σε ἄνομοι δεινῶς, ἐν λάκκῳ κατωτάτῳ, χωννύντες καὶ θανατοῦντες, ἀναμέλποντα τρανῶς, ἀοίδιμε Κάστουλε· Εὐλογεῖτε πάντα τὰ ἔργα Κυρίου, τὸν Κύριον ὑμνεῖτε, καὶ ὑπερυψοῦτε, εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8C13.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C13.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode8C13.melody = he.h.m1_gr_GR_cog.heHE.EnKaminoPaides.name
meMA.Ode8C13.incipit = "Μακαρίας ἔτυχες ζωῆς, ὡς ἐπώνυμος…"
meMA.Ode8C13.text = "Μακαρίας ἔτυχες ζωῆς, ὡς ἐπώνυμος ταύτης· πνιγμῷ γὰρ βιαιοτάτῳ, τὴν ἐπίκηρον ζωήν, κατέλιπες μέλπουσα· Εὐλογεῖτε πάντα τὰ ἔργα Κυρίου, τὸν Κύριον ὑμνεῖτε, καὶ ὑπερυψοῦτε, εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8C1T.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C1T.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode8C1T.melody = he.h.m1_gr_GR_cog.heHE.EnKaminoPaides.name
meMA.Ode8C1T.incipit = "Ὡραιώθης τέξασα Θεόν, ὡραῖον καὶ…"
meMA.Ode8C1T.text = "Ὡραιώθης τέξασα Θεόν, ὡραῖον καὶ Δεσπότην, τὸν θείαις καταφαιδρύναντα, ἀκτῖσι τοὺς βροτούς· διὸ ἀναμέλπομεν· Εὐλογεῖτε πάντα τὰ ἔργα Κυρίου, τὸν Κύριον ὑμνεῖτε, καὶ ὑπερυψοῦτε, εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8H.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8H.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode8H.greekmelody = he.h.m1_gr_GR_cog.heHE.EnKaminoPaides.greekname
meMA.Ode8H.melody = he.h.m1_gr_GR_cog.heHE.EnKaminoPaides.melody
meMA.Ode8H.incipit = he.h.m1_gr_GR_cog.heHE.EnKaminoPaides.incipit
meMA.Ode8H.text = he.h.m1_gr_GR_cog.heHE.EnKaminoPaides.text

meMA.Ode9C11.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C11.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode9C11.greekmelody = he.h.m1_gr_GR_cog.heHE.TyponTisAgnis.greekname
meMA.Ode9C11.melody = he.h.m1_gr_GR_cog.heHE.TyponTisAgnis.name
meMA.Ode9C11.incipit = "Ἵνα θεϊκῆς λαμπρότητος, καὶ αἰωνίου…"
meMA.Ode9C11.text = "Ἵνα θεϊκῆς λαμπρότητος, καὶ αἰωνίου δόξης καὶ ὡραιότητος, Σεβαστιανέ, ἀξιωθῇς ἀξιάγαστε, τὰς βασάνους, τοὺς πόνους, τὸν θάνατον, ὑπέμεινας ἀνδρείως· διὸ σε πάντες μακαρίζομεν."

meMA.Ode9C12.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C12.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode9C12.melody = he.h.m1_gr_GR_cog.heHE.TyponTisAgnis.name
meMA.Ode9C12.incipit = "Ὤφθης μέσον ἄστρων ἥλιος, τῶν…"
meMA.Ode9C12.text = "Ὤφθης μέσον ἄστρων ἥλιος, τῶν συναθλούντων Μάρτυς καὶ νῦν φωτίζεται, ἄθλων ἱερῶν, ἱερωτάταις λαμπρότησιν, Ἐκκλησίας τὸ θεῖον στερέωμα, καὶ πᾶσα διαβόλου, διασκεδάζεται ἀπόνοια."

meMA.Ode9C13.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C13.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode9C13.melody = he.h.m1_gr_GR_cog.heHE.TyponTisAgnis.name
meMA.Ode9C13.incipit = "Στέργοντες Χριστὸν Τιβούρτιος, καὶ Μαρκελλῖνος…"
meMA.Ode9C13.text = "Στέργοντες Χριστὸν Τιβούρτιος, καὶ Μαρκελλῖνος, Μᾶρκος, Ζωὴ καὶ Κάστουλος, Σεβαστιανῷ, καθηγητῇ καὶ θεόφρονι, συναθλοῦσι, καὶ ἴσους κομίζονται, τῆς νίκης τοὺς στεφανους, ὑπὲρ ἡμῶν ἀεὶ πρεσβεύοντες."

meMA.Ode9C14.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C14.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode9C14.melody = he.h.m1_gr_GR_cog.heHE.TyponTisAgnis.name
meMA.Ode9C14.incipit = "Ἤρθη πρὸς μονὰς σκηνούμενος, φωτοειδὴς…"
meMA.Ode9C14.text = "Ἤρθη πρὸς μονὰς σκηνούμενος, φωτοειδὴς Μαρτύρων χορὸς ὁ ἔνθεος, καὶ τοὺς οὐρανούς, περιπολεῖ ἀγαλλόμενος, ὑπὲρ πάντων ἡμῶν τὸν φιλάνθρωπον, ἀεὶ παρακαλοῦντες, μακαριζόντων αὐτοὺς πάντοτε."

meMA.Ode9C1T.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C1T.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode9C1T.melody = he.h.m1_gr_GR_cog.heHE.TyponTisAgnis.name
meMA.Ode9C1T.incipit = "Φρίττει νοερὰ Στρατεύματα, τὸ τοῦ…"
meMA.Ode9C1T.text = "Φρίττει νοερὰ Στρατεύματα, τὸ τοῦ Πατρὸς ὁρῶντα θεῖον ἀπαύγασμα, σοῦ ἐν ταῖς χερσίν, ἀνερμηνεύτως κρατούμενον, καὶ τὸ σὸν κεκτημένον ὁμοίωμα, ἵνα βροτούς θεώσῃ, Παρθενομῆτορ ἀπειρόγαμε."

meMA.Ode9H.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9H.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode9H.greekmelody = he.h.m1_gr_GR_cog.heHE.TyponTisAgnis.greekname
meMA.Ode9H.melody = he.h.m1_gr_GR_cog.heHE.TyponTisAgnis.melody
meMA.Ode9H.incipit = he.h.m1_gr_GR_cog.heHE.TyponTisAgnis.incipit
meMA.Ode9H.text = he.h.m1_gr_GR_cog.heHE.TyponTisAgnis.text

meLI.Megalynarion.text = "Χαίροις Ἐκκλησίας ἄστρον λαμπρόν, καὶ Σιὼν τῆς θείας, ὁ κοσμήτωρ ὁ ἱερός· χαίροις ἱεράρχα, Μόδεστε θεοφόρε, ἡμῶν πρὸς τὸν Δεσπότην, πρέσβυς θερμότατος."
