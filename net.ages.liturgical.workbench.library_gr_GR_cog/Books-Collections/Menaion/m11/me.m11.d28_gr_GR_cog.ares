meMA.C1.verse = prayers_gr_GR_cog.pr.verse_DevoutMan.text

meMA.C2.acrostic.title = "Ὁ Κανὼν τοῦ Μάρτυρος,"
meMA.C2.acrostic.notice1 = "οὗ ἡ Ἀκροστιχίς·"
meMA.C2.acrostic.text = "Εἰρηνικὴν μοι προσνέμοις Μάρτυς χάριν."
meMA.C2.poet = poets_gr_GR_cog.po.Theofanous
meMA.C2.title = titles_gr_GR_cog.ti.CanonMartyrM
