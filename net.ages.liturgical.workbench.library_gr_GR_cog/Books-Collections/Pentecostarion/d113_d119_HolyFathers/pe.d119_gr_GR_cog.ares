peMA.Ode3Kathisma2.text = me.m06.d29_gr_GR_cog.meMA.Kathisma2T.text

peMA.Ode4C1H.ode = miscellanea_gr_GR_cog.misc.Ode4
peMA.Ode4C1H.mode = miscellanea_gr_GR_cog.misc.Mode6
peMA.Ode4C1H.greekmelody = he.h.m6_gr_GR_cog.heHE.ChristosMouDynamis.greekname
