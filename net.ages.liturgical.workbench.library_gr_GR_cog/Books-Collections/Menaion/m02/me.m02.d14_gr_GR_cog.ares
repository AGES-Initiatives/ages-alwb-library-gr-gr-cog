meMA.Ode1C1H.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C1H.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode1C1H.melody = he.h.m4_gr_GR_cog.heHE.ThalassisToErythraion.melody
meMA.Ode1C1H.incipit = he.h.m4_gr_GR_cog.heHE.ThalassisToErythraion.incipit
meMA.Ode1C1H.text = he.h.m4_gr_GR_cog.heHE.ThalassisToErythraion.text

meMA.Ode1C11.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode1C11.melody = he.h.m4_gr_GR_cog.heHE.ThalassisToErythraion.name
meMA.Ode1C11.incipit = "Αὐξήσας, τὴν πρὸς Θεὸν ἀγάπησιν…"
meMA.Ode1C11.text = "Αὐξήσας, τὴν πρὸς Θεὸν ἀγάπησιν, Πάτερ Αὐξέντιε, καὶ κοσμικὴν στοργὴν καταλιπών, θεοφόρε κειμήλιον, χωρητικὸν τοῦ Πνεύματος, τῶν χαρισμάτων ἀναδέδειξαι."

meMA.Ode1C12.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode1C12.melody = he.h.m4_gr_GR_cog.heHE.ThalassisToErythraion.name
meMA.Ode1C12.incipit = "Ὑπῆλθες, ἐθελουσίοις νεύμασι, Πάτερ Αὐξέντιε…"
meMA.Ode1C12.text = "Ὑπῆλθες, ἐθελουσίοις νεύμασι, Πάτερ Αὐξέντιε, σὺ τὸν ζυγὸν Κυρίου τὸν χρηστόν, καὶ τὴν γῆν ἐκαινούργησας, τῶν ἀρετῶν τοῖς δάκρυσι, καταπιαίνων ταύτην ἔνδοξε."

meMA.Ode1C13.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode1C13.melody = he.h.m4_gr_GR_cog.heHE.ThalassisToErythraion.name
meMA.Ode1C13.incipit = "Ξενώσας, βιοτικῆς συγχύσεως τὴν σὴν…"
meMA.Ode1C13.text = "Ξενώσας, βιοτικῆς συγχύσεως τὴν σὴν ψυχὴν καὶ τὸν νοῦν, καὶ πρὸς Θεὸν συνάψας ἐμμελῶς, ἐν συντόνοις ἀσκήσεσιν, ἐπὶ τῆς γῆς ὡς ἄσαρκος, ἐπολιτεύσω Παμμακάριστε."

meMA.Ode1C1T.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode1C1T.melody = he.h.m4_gr_GR_cog.heHE.ThalassisToErythraion.name
meMA.Ode1C1T.incipit = "Ἐν σοὶ νῦν, ἐθεωρήθη Δέσποινα…"
meMA.Ode1C1T.text = "Ἐν σοὶ νῦν, ἐθεωρήθη Δέσποινα, ἡ ἀκατάληπτος, θεανδρικὴ πορεία τοῦ Χριστοῦ, ὃς Θεός τε καὶ ἄνθρωπος, ἐκ σοῦ Ἁγνὴ γεγέννηται, ἀνακαινίζων τὴν οὐσίαν μου."

meMA.Ode3C1H.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C1H.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C1H.melody = he.h.m4_gr_GR_cog.heHE.OukEnSofia.melody
meMA.Ode3C1H.incipit = he.h.m4_gr_GR_cog.heHE.OukEnSofia.incipit
meMA.Ode3C1H.text = he.h.m4_gr_GR_cog.heHE.OukEnSofia.text

meMA.Ode3C11.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C11.melody = he.h.m4_gr_GR_cog.heHE.OukEnSofia.name
meMA.Ode3C11.incipit = "Νενευρωμένος, τοῦ Σταυροῦ τῇ δυνάμει…"
meMA.Ode3C11.text = "Νενευρωμένος, τοῦ Σταυροῦ τῇ δυνάμει κατήργησας, τῶν δαιμόνων τὰς ὁρμάς, καὶ μεθοδείας διέλυσας, ἀσκήσει τρεπόμενος, τὴν τούτων ἔφοδον."

meMA.Ode3C12.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C12.melody = he.h.m4_gr_GR_cog.heHE.OukEnSofia.name
meMA.Ode3C12.incipit = "Τὴν τῶν Ἀγγέλων, ἐπὶ γῆς…"
meMA.Ode3C12.text = "Τὴν τῶν Ἀγγέλων, ἐπὶ γῆς πολιτείαν μιμούμενος, τὸ καθαρὸν ἐν προσευχαῖς, ἐν ἀγρυπνίαις τὸ εὔτονον, προθύμως διήνυσας, Πάτερ Αὐξέντιε."

meMA.Ode3C13.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C13.melody = he.h.m4_gr_GR_cog.heHE.OukEnSofia.name
meMA.Ode3C13.incipit = "Ἰὸν θανάτου, ταῖς ψυχαῖς προξενοῦσαν…"
meMA.Ode3C13.text = "Ἰὸν θανάτου, ταῖς ψυχαῖς προξενοῦσαν Αὐξέντιε, ἐπιγνοὺς τὴν ἡδονήν, διʼ ἐγκρατείας ἐνέκρωσας, αὐτῆς τὰ κινήματα, θεόφρον Ὅσιε."

meMA.Ode3C1T.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3C1T.melody = he.h.m4_gr_GR_cog.heHE.OukEnSofia.name
meMA.Ode3C1T.incipit = "Ὁ διαπλάσας, κατʼ ἀρχάς ἐκ…"
meMA.Ode3C1T.text = "Ὁ διαπλάσας, κατʼ ἀρχάς ἐκ χοὸς με τὸν ἄνθρωπον, πλαστουργεῖται διʼ ἐμέ, ἐν τῇ γαστρί σου Πανάμωμε, τὴν πάλαι κατάπτωσιν, ἐπανορθούμενος."

meMA.Ode3Kathisma1.mode = miscellanea_gr_GR_cog.misc.Mode1
meMA.Ode3Kathisma1.melody = he.a.m1_gr_GR_cog.heAU.TonTafonSouSotir.name
meMA.Ode3Kathisma1.incipit = "Εἰς ὄρος ἀνελθών, θεωρίας Παμμάκαρ…"
meMA.Ode3Kathisma1.text = "Εἰς ὄρος ἀνελθών, θεωρίας Παμμάκαρ, καὶ πράξεως σαφῶς, ἀστραπαῖς τῶν θαυμάτων, ὡς ἥλιος ἔλαμψας, καταυγάζων τὰ πέρατα· ὅθεν σήμερον, τὴν παναγίαν σου μνήμην, ἑορτάζομεν, ὑμνολογοῦντές σε πίστει, καὶ πόθῳ γεραίρομεν."

meMA.Ode3Kathisma2.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3Kathisma2.melody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.name
meMA.Ode3Kathisma2.incipit = "Ἀνελθὼν ἐν τῷ ὄρει τῶν…"
meMA.Ode3Kathisma2.text = "Ἀνελθὼν ἐν τῷ ὄρει τῶν ἀρετῶν, βραχυτάτῳ κελλίῳ μάκαρ σαυτόν, συγκλείσας στενούμενος, ἐπὶ γῆς διʼ ἀσκήσεως, καὶ πρὸς πλάτος θεῖον, πανσόφως πτερούμενος, λογισμῷ τελείῳ ἀνῆλθες μακάριε· ὅθεν τοὺς εἰς βάθη πειρασμῶν ἐκπεσόντας, ἀνείλκυσας Ὅσιε, ἐνεργῶν τὰ τεράστια, θεοφόρε Αὐξέντιε· πρέσβευε Χριστῷ τῷ Θεῷ, τῶν πταισμάτων ἄφεσιν δωρήσασθαι, τοῖς ἑορτάζουσι πόθῳ, τὴν ἁγίαν μνήμην σου."

meMA.Ode3KathismaT.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3KathismaT.melody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.name
meMA.Ode3KathismaT.incipit = me.m02.d07_gr_GR_cog.meMA.Ode3KathismaT.incipit
meMA.Ode3KathismaT.text = me.m02.d07_gr_GR_cog.meMA.Ode3KathismaT.text

meMA.Ode3KathismaSt.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3KathismaSt.melody = he.a.m8_gr_GR_cog.heAU.TinSofianKaiLogon.name
meMA.Ode3KathismaSt.incipit = me.m01.d24_gr_GR_cog.meMA.Ode3KathismaSt.incipit
meMA.Ode3KathismaSt.text = me.m01.d24_gr_GR_cog.meMA.Ode3KathismaSt.text

meMA.Ode4C1H.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C1H.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C1H.melody = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.melody
meMA.Ode4C1H.incipit = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.incipit
meMA.Ode4C1H.text = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.text

meMA.Ode4C11.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C11.melody = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.name
meMA.Ode4C11.incipit = "Νικητὴς κατὰ πνευμάτων τῆς πονηρίας…"
meMA.Ode4C11.text = "Νικητὴς κατὰ πνευμάτων τῆς πονηρίας, τῇ τοῦ Ἁγίου Πνεύματος, Πάτερ συνεργείᾳ, ὤφθης καὶ τῇ χάριτι, κραυγάζων Αὐξέντιε· Δόξα τῇ δυνάμει σου Κύριε."

meMA.Ode4C12.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C12.melody = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.name
meMA.Ode4C12.incipit = "Τῶν χειμάῤῥων θεοφόρε τῆς ἀνομίας…"
meMA.Ode4C12.text = "Τῶν χειμάῤῥων θεοφόρε τῆς ἀνομίας, τῇ προσευχῇ παμμάκαρ, σεαυτὸν ὀχυρώσας, ἔφυγες τὸν τάραχον· τρυφῆς τὸν χειμάῤῥουν δέ, πέπωκας ἀξίως πανόλβιε."

meMA.Ode4C13.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C13.melody = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.name
meMA.Ode4C13.incipit = "Ὁ θεοφόρος τὴν πίστιν καὶ…"
meMA.Ode4C13.text = "Ὁ θεοφόρος τὴν πίστιν καὶ τὴν ἀγάπην, τὴν πρὸς Θεὸν αὐξήσας, πρὸς μετάρσιον ὕψος, θείας οἰκειώσεως, ὑψώθη κραυγάζων σοι· Δόξα τῇ δυνάμει σου Κύριε."

meMA.Ode4C14.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C14.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C14.melody = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.name
meMA.Ode4C14.incipit = "Νουνεχῶς Πάτερ προκρίνας, τῆς τῶν…"
meMA.Ode4C14.text = "Νουνεχῶς Πάτερ προκρίνας, τῆς τῶν προσκαίρων χαμαιπετοῦς κυβείας, τὴν μὴ σαλευομένην, θείαν ὡραιότητα, προθύμως, ἐκραύγαζες· Δόξα τῇ δυνάμει σου Κύριε."

meMA.Ode4C1T.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode4C1T.melody = he.h.m4_gr_GR_cog.heHE.EparthentaSeIdousa.name
meMA.Ode4C1T.incipit = "Θεὸν Λόγον σε τεκοῦσαν σαφῶς…"
meMA.Ode4C1T.text = "Θεὸν Λόγον σε τεκοῦσαν σαφῶς εἰδότες σωματικῶς κυρίως, οἱ πιστοὶ Θεοτόκον πάντες ὀνομάζομεν, τὴν κλῆσιν κατάλληλον, φύσει τῶν πραγμάτων προσάγοντες."

meMA.Ode5C1H.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C1H.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C1H.melody = he.h.m4_gr_GR_cog.heHE.SyKyrieMouFos.melody
meMA.Ode5C1H.incipit = he.h.m4_gr_GR_cog.heHE.SyKyrieMouFos.incipit
meMA.Ode5C1H.text = he.h.m4_gr_GR_cog.heHE.SyKyrieMouFos.text

meMA.Ode5C11.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C11.melody = he.h.m4_gr_GR_cog.heHE.SyKyrieMouFos.name
meMA.Ode5C11.incipit = "Ἐχόμενα τῆς σῆς, ὁδοῦ Πάτερ…"
meMA.Ode5C11.text = "Ἐχόμενα τῆς σῆς, ὁδοῦ Πάτερ τὰ σκάνδαλα, ὀξύτητι διανοίας, ἀλωβήτως διῆλθες, καὶ ψυχῆς καθαρότητι."

meMA.Ode5C12.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C12.melody = he.h.m4_gr_GR_cog.heHE.SyKyrieMouFos.name
meMA.Ode5C12.incipit = "Ἰσότητι φρενῶν, τὴν ζωὴν διελήλυθας…"
meMA.Ode5C12.text = "Ἰσότητι φρενῶν, τὴν ζωὴν διελήλυθας, τὰ πρόσκαιρα παρατρέχων, αἰωνίοις δὲ Πάτερ, εἰς ἀεὶ προστιθέμενος."

meMA.Ode5C13.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C13.melody = he.h.m4_gr_GR_cog.heHE.SyKyrieMouFos.name
meMA.Ode5C13.incipit = "Ὁ βίος σου λαμπρός, καὶ…"
meMA.Ode5C13.text = "Ὁ βίος σου λαμπρός, καὶ ἡ πίστις ὀρθόδοξος, ἡ ἄσκησις θαυμασία, ἠρτυμένος ὁ λόγος, τῇ χάριτι Αὐξέντιε."

meMA.Ode5C1T.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode5C1T.melody = he.h.m4_gr_GR_cog.heHE.SyKyrieMouFos.name
meMA.Ode5C1T.incipit = "Νοῦν ἔχοντες ὀρθόν, διδαγμάτων ἀνάπλεων…"
meMA.Ode5C1T.text = "Νοῦν ἔχοντες ὀρθόν, διδαγμάτων ἀνάπλεων, σὲ πάναγνε Θεοτόκον, ὀνομάζομεν πάντες, καὶ πόθῳ μακαρίζομεν."

meMA.Ode6C1H.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C1H.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C1H.melody = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.melody
meMA.Ode6C1H.incipit = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.incipit
meMA.Ode6C1H.text = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.text

meMA.Ode6C11.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C11.melody = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.name
meMA.Ode6C11.incipit = "Υἱὸν σε, καὶ φωτὸς καὶ…"
meMA.Ode6C11.text = "Υἱὸν σε, καὶ φωτὸς καὶ ἡμέρας ἀνέδειξεν, ἡ πολιτεία σου μάκαρ, εὐσχημόνως Πάτερ περιπατοῦντα, καὶ φωστῆρα, ζωῆς ἐν κόσμῳ λόγον ἐπέχοντα."

meMA.Ode6C12.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C12.melody = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.name
meMA.Ode6C12.incipit = "Μελέτην, τοῦ θανάτου τὸν βίον…"
meMA.Ode6C12.text = "Μελέτην, τοῦ θανάτου τὸν βίον σου θέμενος, διαφερόντως τὸν ὅρον, τῆς σοφίας Πάτερ κατανοήσας, μετετέθης, πρὸς τὴν Σοφίαν τὴν ἐνυπόστατον."

meMA.Ode6C1T.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode6C1T.melody = he.h.m4_gr_GR_cog.heHE.ThysoSoiMetaFonis.name
meMA.Ode6C1T.incipit = "Ναὸν σε, τοῦ Θεοῦ καὶ…"
meMA.Ode6C1T.text = "Ναὸν σε, τοῦ Θεοῦ καὶ παστάδα γινώσκομεν, στάμνον λυχνίαν καὶ πλάκα, κεκτημένην ἔνδον ἐγγεγραμμένον, τὸν διʼ οἶκτον, σωματωθέντα Λόγον πανάμωμε."

meMA.Kontakion1.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Kontakion1.melody = he.a.m6_gr_GR_cog.heAU.TinEnPresveiais.name
meMA.Kontakion1.incipit = "Κατατρυφήσας θεόφρον τῆς ἐγκρατείας, καὶ…"
meMA.Kontakion1.text = "Κατατρυφήσας θεόφρον τῆς ἐγκρατείας, καὶ τὰς ὀρέξεις τῆς σαρκὸς χαλινώσας, ὤφθης τῇ πίστει σου αὐξανόμενος, καὶ ὡς φυτὸν ἐν μέσῳ τοῦ Παραδείσου ἐξήνθησας, Αὐξεντιε Πάτερ ἱερώτατε."

meMA.Oikos1.text = "Τίς τοὺς ἀγῶνάς σου νῦν ἐπαξίως ἐξείπῃ, ἢ τοὺς πόνους σου Πάτερ, οὓς ὑπέστης ἐν γῇ, διὰ τὴν θείαν ἀπόλαυσιν; ἀπὸ βρέφους γὰρ νόμοις Κυρίου ἀκολουθήσας, καὶ προστάγμασι τούτου ὑπηρετήσας, νέος ἡμῖν ἀνεδείχθης Ἰὼβ τοῖς παλαίσμασι· τοῦ κόσμου πάροικος ὤφθης, καὶ τῆς γῆς ἀπάσης ἀλλότριος· νηστείαν πίστει ἐξήσκησας· ἀγρυπνίαν, ἁγνείαν ἠγάπησας, Αὐξέντιε Πάτερ ἱερώτατε."

meMA.Ode7C1H.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C1H.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C1H.melody = he.h.m4_gr_GR_cog.heHE.EnTiKaminoAvramiaioi.melody
meMA.Ode7C1H.incipit = he.h.m4_gr_GR_cog.heHE.EnTiKaminoAvramiaioi.incipit
meMA.Ode7C1H.text = he.h.m4_gr_GR_cog.heHE.EnTiKaminoAvramiaioi.text

meMA.Ode7C11.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C11.melody = he.h.m4_gr_GR_cog.heHE.EnTiKaminoAvramiaioi.name
meMA.Ode7C11.incipit = "Ὡς τετρωμένος, τῇ τοῦ Δεσπότου…"
meMA.Ode7C11.text = "Ὡς τετρωμένος, τῇ τοῦ Δεσπότου Πάτερ θείᾳ στοργῇ, πᾶσαν πρὸς αὐτὸν ἀνένδοτον τὴν ῥοπήν, τῆς καρδίας σου ἐκέκτησο· Εὐλογημένος εἶ, ὁ Θεός μου κραυγάζων καὶ Κύριος."

meMA.Ode7C12.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C12.melody = he.h.m4_gr_GR_cog.heHE.EnTiKaminoAvramiaioi.name
meMA.Ode7C12.incipit = "Πεφωτισμένος, τῇ τοῦ Σωτῆρος αἴγλῃ…"
meMA.Ode7C12.text = "Πεφωτισμένος, τῇ τοῦ Σωτῆρος αἴγλῃ Πάνσοφε, σκότους τὸν προστάτην ἤλεγξας κραταιῶς, κοσμοκράτορας καθεῖλες δέ· Εὐλογημένος εἶ, ὁ Θεός μου κραυγάζων καὶ Κύριος."

meMA.Ode7C1T.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode7C1T.melody = he.h.m4_gr_GR_cog.heHE.EnTiKaminoAvramiaioi.name
meMA.Ode7C1T.incipit = "Ῥεύσασαν πάλαι, τῆς πρὸς τὸ…"
meMA.Ode7C1T.text = "Ῥεύσασαν πάλαι, τῆς πρὸς τὸ κρεῖττον ἀνανεύσεως, φύσιν τῶν ἀνθρώπων πάναγνε βουληθείς, ἀναπλάσαι κατεσκήνωσεν, ὁ πλαστουργὸς Θεός, ἐν τῇ γαστρί σου μόνη Πανύμνητε."

meMA.Ode8C1H.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C1H.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C1H.melody = he.h.m4_gr_GR_cog.heHE.LytrotaTouPantos.melody
meMA.Ode8C1H.incipit = he.h.m4_gr_GR_cog.heHE.LytrotaTouPantos.incipit
meMA.Ode8C1H.text = he.h.m4_gr_GR_cog.heHE.LytrotaTouPantos.text

meMA.Ode8C11.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C11.melody = he.h.m4_gr_GR_cog.heHE.LytrotaTouPantos.name
meMA.Ode8C11.incipit = "Ὁλικῶς πρὸς Θεὸν μεταθέμενος, ἀπεσείσω…"
meMA.Ode8C11.text = "Ὁλικῶς πρὸς Θεὸν μεταθέμενος, ἀπεσείσω σαρκὸς τὴν εὐπάθειαν, καὶ τῶν θαυμάτων εἴληφας, τὴν ἐνέργειαν μέλπων· Πάντα τὰ ἔργα, εὐλογεῖτε ὑμνεῖτε τὸν Κύριον."

meMA.Ode8C12.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C12.melody = he.h.m4_gr_GR_cog.heHE.LytrotaTouPantos.name
meMA.Ode8C12.incipit = "Φοβερὸς ἀπεφάνθης τοῖς δαίμοσι, τὸν…"
meMA.Ode8C12.text = "Φοβερὸς ἀπεφάνθης τοῖς δαίμοσι, τὸν Χριστὸν κεκτημένος συλλήπτορα, οὗ τὸν Σταυρὸν ἀράμενος, ἠκολούθησας μέλπων· Πάντα τὰ ἔργα, εὐλογεῖτε ὑμνεῖτε τὸν Κύριον."

meMA.Ode8C1T.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode8C1T.melody = he.h.m4_gr_GR_cog.heHE.LytrotaTouPantos.name
meMA.Ode8C1T.incipit = "Ῥητορεύει φανεὶς ὁ Ἀρχάγγελος, καὶ…"
meMA.Ode8C1T.text = "Ῥητορεύει φανεὶς ὁ Ἀρχάγγελος, καὶ μηνύει τοῦ Λόγου σοι Πάναγνε, τὴν ὑπὲρ λόγον σύλληψιν, καὶ σωτήριον κόσμῳ· Πάντα τὰ ἔργα, εὐλογεῖτε κραυγάζων τὸν Κύριον."

meMA.Ode9C1H.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C1H.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode9C1H.melody = he.h.m4_gr_GR_cog.heHE.LithosAcheirotmitos.melody
meMA.Ode9C1H.incipit = he.h.m4_gr_GR_cog.heHE.LithosAcheirotmitos.incipit
meMA.Ode9C1H.text = he.h.m4_gr_GR_cog.heHE.LithosAcheirotmitos.text

meMA.Ode9C11.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C11.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode9C11.melody = he.h.m4_gr_GR_cog.heHE.LithosAcheirotmitos.name
meMA.Ode9C11.incipit = "Ὅλῃ τῇ Θεότητι Πάτερ, τῷ…"
meMA.Ode9C11.text = "Ὅλῃ τῇ Θεότητι Πάτερ, τῷ βαπτισμῷ σου συνετάξω, καὶ διατηρήσας ἀκραιφνῶς, τὸ τῆς εἰκόνος Μάκαρ ἀξίωμα, τρισσοφεγγεῖ λαμπρότητι, μετʼ εὐφροσύνης προσεχώρησας."

meMA.Ode9C12.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C12.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode9C12.melody = he.h.m4_gr_GR_cog.heHE.LithosAcheirotmitos.name
meMA.Ode9C12.incipit = "Νῦν οὐκ ἐν αἰνίγματι Πάτερ…"
meMA.Ode9C12.text = "Νῦν οὐκ ἐν αἰνίγματι Πάτερ, οὐδʼ ἐν ἐσόπτρῳ κατοπτεύεις, τὸν τῆς θεαρχίας φωτισμόν, πρὸς πρόσωπον δὲ πάνσοφε πρόσωπον, τῆς ὑπὲρ νοῦν λαμπρότητος, καὶ θεοπτίας ἀξιούμενος."

meMA.Ode9C13.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C13.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode9C13.melody = he.h.m4_gr_GR_cog.heHE.LithosAcheirotmitos.name
meMA.Ode9C13.incipit = "Ὡς ἔχων πολλὴν παῤῥησίαν, πρὸς…"
meMA.Ode9C13.text = "Ὡς ἔχων πολλὴν παῤῥησίαν, πρὸς τὸν ἁπάντων Βασιλέα, Πάτερ λυτρωθῆναι δυσώπει, τοὺς τὴν σὴν μνήμην ἐπιτελοῦντας πιστῶς, ἐκ πάσης περιστάσεως, ἵνα σε πάντες μακαρίζωμεν."

meMA.Ode9C1T.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C1T.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode9C1T.melody = he.h.m4_gr_GR_cog.heHE.LithosAcheirotmitos.name
meMA.Ode9C1T.incipit = "Σειρὰς τῶν ἐμῶν νῦν πταισμάτων…"
meMA.Ode9C1T.text = "Σειρὰς τῶν ἐμῶν νῦν πταισμάτων, λῦσον Παρθένε Θεοτόκε, ἡ τῆς εὐσπλαγχνίας τὴν πηγήν, τεκοῦσα μόνη Θεομακάριστε, καὶ θυμηδίας ἔμπλησον, ὅπως ἀξίως μεγαλύνω σε."

meMA.Exaposteilarion1.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Exaposteilarion1.melody = he.a.m2_gr_GR_cog.heAU.GynaikesAkoutisthite.name
meMA.Exaposteilarion1.incipit = "Αὐξήσει τῶν ἀγώνων σου, ἐπηύξησας…"
meMA.Exaposteilarion1.text = "Αὐξήσει τῶν ἀγώνων σου, ἐπηύξησας τὰ τάλαντα, ἃ ἐπιστεύθης καὶ ταῦτα, τῷ σῷ προσῆξας Δεσπότῃ, καὶ τὸ Εὖ δοῦλε ἤκουσας, καὶ τὰ τούτου ἀκόλουθα, πρὸς ὃν ἀεὶ μνημόνευε, τῶν σὲ τιμώντων ἐκ πόθου, Αὐξέντιε θεοφόρε."

meMA.ExapTheotokion.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.ExapTheotokion.melody = he.a.m2_gr_GR_cog.heAU.GynaikesAkoutisthite.name
meMA.ExapTheotokion.incipit = "Καὶ σχετικῶς ἀσπάζομαι, καὶ πόθῳ…"
meMA.ExapTheotokion.text = "Καὶ σχετικῶς ἀσπάζομαι, καὶ πόθῳ τὴν προσκύνησιν, προσνέμω πίστει καὶ πόθῳ, τῇ παναχράντῳ καὶ θείᾳ, εἰκόνι σου Πανάμωμε, ἀφʼ ἧς ψυχῶν ἐκβλύζουσι, καὶ τῶν σωμάτων Δέσποινα, ἰάματα τοῖς ὑμνοῦσι, σὲ Θεοτόκον κυρίως."

meLI.Kontakion1.mode = meMA.Kontakion1.mode
meLI.Kontakion1.melody = meMA.Kontakion1.melody
meLI.Kontakion1.incipit = meMA.Kontakion1.incipit
meLI.Kontakion1.text = meMA.Kontakion1.text

meMegalynarion.text = ""

