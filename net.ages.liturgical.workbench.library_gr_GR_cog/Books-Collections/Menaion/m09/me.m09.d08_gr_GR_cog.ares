meMA.Ode4C11.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C11.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode4C11.greekmelody = he.h.m2_gr_GR_cog.heHE.EisakikoaKyrieTinAkoin.greekname
meMA.Ode4C11.melody = he.h.m2_gr_GR_cog.heHE.EisakikoaKyrieTinAkoin.name
meMA.Ode4C11.incipit = "Ἀνυμνοῦμέν σε Κύριε, τὸν τοῖς…"
meMA.Ode4C11.text = "Ἀνυμνοῦμέν σε Κύριε, τὸν τοῖς πιστοῖς σωτήριον λιμένα, παρασχόντα πᾶσι τὴν σὲ κυήσασαν."

meMA.Ode4C12.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C12.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode4C12.melody = he.h.m2_gr_GR_cog.heHE.EisakikoaKyrieTinAkoin.name
meMA.Ode4C12.incipit = "Σὲ Θεόνυμφε καύχημα, πᾶσι Χριστὸς…"
meMA.Ode4C12.text = "Σὲ Θεόνυμφε καύχημα, πᾶσι Χριστὸς ἀνέδειξε καὶ κράτος, τοῖς ὑμνοῦσι πίστει σοῦ τὸ μυστήριον."

meMA.Ode4C13.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C13.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode4C13.melody = he.h.m2_gr_GR_cog.heHE.EisakikoaKyrieTinAkoin.name
meMA.Ode4C13.incipit = "Ἀπειρόγαμε Δέσποινα, ταῖς σαῖς λιταῖς…"
meMA.Ode4C13.text = "Ἀπειρόγαμε Δέσποινα, ταῖς σαῖς λιταῖς λυτρούμενοι πταισμάτων, εὐγνωμόνως πάντες σὲ μακαρίζομεν."

meMA.Ode4C2H.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C2H.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C2H.greekmelody = he.h.m8_gr_GR_cog.heHE.AkikoaKyrieTinAkoin.greekname
meMA.Ode4C2H.melody = he.h.m8_gr_GR_cog.heHE.AkikoaKyrieTinAkoin.melody
meMA.Ode4C2H.incipit = he.h.m8_gr_GR_cog.heHE.AkikoaKyrieTinAkoin.incipit
meMA.Ode4C2H.text = he.h.m8_gr_GR_cog.heHE.AkikoaKyrieTinAkoin.text

meMA.Ode4C21.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C21.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C21.greekmelody = he.h.m8_gr_GR_cog.heHE.AkikoaKyrieTinAkoin.greekname
meMA.Ode4C21.melody = he.h.m8_gr_GR_cog.heHE.AkikoaKyrieTinAkoin.name
meMA.Ode4C21.incipit = "Τὴν πάνσεπτον γέννησιν τῆς Θεοτόκου…"
meMA.Ode4C21.text = "Τὴν πάνσεπτον γέννησιν τῆς Θεοτόκου, ψαλμοῖς καὶ ὕμνοις, δοξολογοῦντες πιστοί, τὸν ἄψευστον, τὸν τῷ Δαυῒδ προομόσαντα καρπόν, ἐκ τῆς ὀσφύος παρασχεῖν, πίστει προσκυνήσωμεν."

meMA.Ode4C22.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C22.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C22.melody = he.h.m8_gr_GR_cog.heHE.AkikoaKyrieTinAkoin.name
meMA.Ode4C22.incipit = "Διήνοιξας Κύριε τὴν μήτραν Σάῤῥας…"
meMA.Ode4C22.text = "Διήνοιξας Κύριε τὴν μήτραν Σάῤῥας, καρπὸν ἐν γήρᾳ τὸν Ἰσαὰκ παρασχών· ὁ αὐτὸς καὶ σήμερον, τῇ εὐσεβεῖ Ἄννῃ δέδωκας Σωτήρ, ἐκ μήτρας γόνιμον καρπόν, ἄσπιλον Μητέρα τὴν σήν."

meMA.Ode4C23.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C23.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C23.melody = he.h.m8_gr_GR_cog.heHE.AkikoaKyrieTinAkoin.name
meMA.Ode4C23.incipit = "Ἐπήκουσας Κύριε τῆς προσευχῆς μου…"
meMA.Ode4C23.text = "Ἐπήκουσας Κύριε τῆς προσευχῆς μου, λεγέτω Ἄννα, ἐπαγγελίας καρπόν, παρασχών μοι σήμερον, τὴν ἐκ πασῶν γενεῶν καὶ γυναικῶν προορισθεῖσαν, εἰς ἁγνὴν ἄχραντον Μητέρα σου."

meMA.Ode4C24.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C24.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C24.melody = he.h.m8_gr_GR_cog.heHE.AkikoaKyrieTinAkoin.name
meMA.Ode4C24.incipit = "Συγχαίρει σοι σήμερον θεόφρον Ἄννα…"
meMA.Ode4C24.text = "Συγχαίρει σοι σήμερον θεόφρον Ἄννα, ἡ οἰκουμένη· τοῦ λυτρωτοῦ γὰρ αὐτῆς, τὴν Μητέρα ἤνθησας, τὴν ἐκ ῥίζης βλαστήσασαν Δαυΐδ, δυνάμεως ῥάβδον ἡμῖν, φέρουσαν τὸ ἄνθος Χριστόν."

meMA.Ode4C2Tr.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C2Tr.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C2Tr.melody = he.h.m8_gr_GR_cog.heHE.AkikoaKyrieTinAkoin.name
meMA.Ode4C2Tr.incipit = "Δοξάζω σε ἄναρχε Τριάς, ἀμέριστε…"
meMA.Ode4C2Tr.text = "Δοξάζω σε ἄναρχε Τριάς, ἀμέριστε τῇ οὐσίᾳ, χερουβικῶς ἐκβοῶν, τῇ πηλίνῃ γλώσσῃ μου· Ἅγιος Ἅγιος Ἅγιος, ὁ ὤν καὶ διαμένων εἷς ἀεί, εἷς Θεὸς ἀΐδιος."

meMA.Ode4C2T.ode = miscellanea_gr_GR_cog.misc.Ode4
meMA.Ode4C2T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode4C2T.melody = he.h.m8_gr_GR_cog.heHE.AkikoaKyrieTinAkoin.name
meMA.Ode4C2T.incipit = "Πεπλήρωνται Ἄχραντε, τῶν θεηγόρων αἱ…"
meMA.Ode4C2T.text = "Πεπλήρωνται Ἄχραντε, τῶν θεηγόρων αἱ προφητεῖαι, ἐν τῇ γεννήσει τῇ σῇ, τῶν πιστῶς καλούντων σε, σκηνὴν καὶ πύλην καὶ ὄρος νοητόν, βάτον καὶ ῥάβδον Ἀαρών, φυεῖσαν ἐκ ῥίζης Δαυΐδ."

meMA.Ode5C1H.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C1H.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode5C1H.greekmelody = he.h.m2_gr_GR_cog.heHE.OSkiografonAchlyn.greekname
meMA.Ode5C1H.melody = he.h.m2_gr_GR_cog.heHE.OSkiografonAchlyn.melody
meMA.Ode5C1H.incipit = he.h.m2_gr_GR_cog.heHE.OSkiografonAchlyn.incipit
meMA.Ode5C1H.text = he.h.m2_gr_GR_cog.heHE.OSkiografonAchlyn.text

meMA.Ode5C11.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C11.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode5C11.greekmelody = he.h.m2_gr_GR_cog.heHE.OSkiografonAchlyn.greekname
meMA.Ode5C11.melody = he.h.m2_gr_GR_cog.heHE.OSkiografonAchlyn.name
meMA.Ode5C11.incipit = "Ἀνυμνήσωμεν λαοί, τὴν τῶν πάντων…"
meMA.Ode5C11.text = "Ἀνυμνήσωμεν λαοί, τὴν τῶν πάντων αἰτίαν· τοῦ καθʼ ἡμᾶς γενέσθαι τὸν αἴτιον· ἧς τὸν τύπον ἔχαιρον, ἀξιούμενοι Προφῆται, ἐναργῆ σωτηρίαν ταύτης καρπούμενοι."

meMA.Ode5C12.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C12.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode5C12.melody = he.h.m2_gr_GR_cog.heHE.OSkiografonAchlyn.name
meMA.Ode5C12.incipit = "Τῆς ἀνίκμου ὁ βλαστὸς ῥάβδου…"
meMA.Ode5C12.text = "Τῆς ἀνίκμου ὁ βλαστὸς ῥάβδου τοῦ ἱερέως, τῷ Ἰσραὴλ ἐδήλωσε πρόκρισιν· καὶ νῦν τὴν λαμπρότητα, τῶν φυσάντων παραδόξως, δᾳδουχεῖ τὸ ἐκ στείρας πανένδοξον κύημα."

meMA.Ode5C2H.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C2H.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C2H.greekmelody = he.h.m8_gr_GR_cog.heHE.KyrieOTheosImon.greekname
meMA.Ode5C2H.melody = he.h.m8_gr_GR_cog.heHE.KyrieOTheosImon.melody
meMA.Ode5C2H.incipit = he.h.m8_gr_GR_cog.heHE.KyrieOTheosImon.incipit
meMA.Ode5C2H.text = he.h.m8_gr_GR_cog.heHE.KyrieOTheosImon.text

meMA.Ode5C21.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C21.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C21.greekmelody = he.h.m8_gr_GR_cog.heHE.KyrieOTheosImon.greekname
meMA.Ode5C21.melody = he.h.m8_gr_GR_cog.heHE.KyrieOTheosImon.name
meMA.Ode5C21.incipit = "Ἄχραντός σου ἡ γέννησις, Παρθένε…"
meMA.Ode5C21.text = "Ἄχραντός σου ἡ γέννησις, Παρθένε ἄχραντε· ἄφραστος καὶ ἡ σύλληψις, καὶ ἡ ὠδίς· ἄῤῥητος ὁ τόκος σου, νύμφη ἀνύμφευτε· Θεὸς γὰρ ἦν, ὅλον φορέσας ἐμέ."

meMA.Ode5C22.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C22.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C22.melody = he.h.m8_gr_GR_cog.heHE.KyrieOTheosImon.name
meMA.Ode5C22.incipit = "Σήμερον εὐφραινέσθωσαν, Ἀγγέλων τάγματα· ᾄσμασι…"
meMA.Ode5C22.text = "Σήμερον εὐφραινέσθωσαν, Ἀγγέλων τάγματα· ᾄσμασι χορευέτωσαν οἱ ἐξ Ἀδάμ· ἐτέχθη γὰρ ῥάβδος, τὸ ἄνθος βλαστάνουσα, Χριστὸν τὸν μόνον λυτρωτὴν τοῦ Ἀδάμ."

meMA.Ode5C23.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C23.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C23.melody = he.h.m8_gr_GR_cog.heHE.KyrieOTheosImon.name
meMA.Ode5C23.incipit = "Σήμερον Εὔα λέλυται τῆς καταδίκης…"
meMA.Ode5C23.text = "Σήμερον Εὔα λέλυται τῆς καταδίκης, λέλυται καὶ ὁ Ἀδάμ, τῆς ἀρχαίας ἀρᾶς, ἐπὶ τῇ γεννήσει τῇ σῇ βοῶν ἄχραντε· Ἐν σοὶ τῆς φθορᾶς ἐλυτρώθημεν."

meMA.Ode5C24.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C24.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C24.melody = he.h.m8_gr_GR_cog.heHE.KyrieOTheosImon.name
meMA.Ode5C24.incipit = "Δόξα σοι τῷ δοξάσαντι, τὴν…"
meMA.Ode5C24.text = "Δόξα σοι τῷ δοξάσαντι, τὴν στεῖραν σήμερον· ἔτεκε γὰρ τὴν ῥάβδον τὴν ἀειθαλῆ, ἐξ ἐπαγγελίας, ἐξ ἧς ἀνεβλάστησε Χριστός, τὸ ἄνθος τῆς ζωῆς ἡμῶν."

meMA.Ode5C2Tr.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C2Tr.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C2Tr.melody = he.h.m8_gr_GR_cog.heHE.KyrieOTheosImon.name
meMA.Ode5C2Tr.incipit = "Ἄναρχον προσκυνοῦμέν σε Τριὰς ἀμέριστε…"
meMA.Ode5C2Tr.text = "Ἄναρχον προσκυνοῦμέν σε Τριὰς ἀμέριστε, ἄκτιστον συναΐδιον καὶ συμφυῆ, ἐν μιᾷ οὐσίᾳ, τρισὶν ὑποστάσεσιν, ὑπερφυῶς κηρυττομένην ἀεί."

meMA.Ode5C2T.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C2T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode5C2T.melody = he.h.m8_gr_GR_cog.heHE.KyrieOTheosImon.name
meMA.Ode5C2T.incipit = "Γέγονεν ἡ κοιλία σου Ἁγία…"
meMA.Ode5C2T.text = "Γέγονεν ἡ κοιλία σου Ἁγία Τράπεζα, ἔμεινεν ἡ ἁγνεία σου ὥσπερ τὸ πρίν, ἀσινὴς Παρθένε· Χριστὸς γὰρ ὁ ἥλιος, ὡς ἐκ παστοῦ νυμφίος ὤφθη ἐκ σοῦ."

meMA.Ode6C1H.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C1H.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode6C1H.greekmelody = he.h.m2_gr_GR_cog.heHE.ProsKyrionEkKitous.greekname
meMA.Ode6C1H.melody = he.h.m2_gr_GR_cog.heHE.ProsKyrionEkKitous.melody
meMA.Ode6C1H.incipit = he.h.m2_gr_GR_cog.heHE.ProsKyrionEkKitous.incipit
meMA.Ode6C1H.text = he.h.m2_gr_GR_cog.heHE.ProsKyrionEkKitous.text

meMA.Ode6C11.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C11.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode6C11.greekmelody = he.h.m2_gr_GR_cog.heHE.ProsKyrionEkKitous.greekname
meMA.Ode6C11.melody = he.h.m2_gr_GR_cog.heHE.ProsKyrionEkKitous.name
meMA.Ode6C11.incipit = "Πρὸς Κύριον ἐν θλίψει στειρώσεως…"
meMA.Ode6C11.text = "Πρὸς Κύριον ἐν θλίψει στειρώσεως ἐβόησαν, τῆς Θεομήτορος, οἱ θεόφρονες γεννήτορες, καὶ ταύτην γενεαῖς γενεῶν ἐκύησαν, εἰς κοινὴν σωτηρίαν καὶ καύχημα."

meMA.Ode6C12.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C12.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode6C12.melody = he.h.m2_gr_GR_cog.heHE.ProsKyrionEkKitous.name
meMA.Ode6C12.incipit = "Ἐδέξαντο οὐράνιον δῶρον ἀξιόθεον, τῆς…"
meMA.Ode6C12.text = "Ἐδέξαντο οὐράνιον δῶρον ἀξιόθεον, τῆς Θεομήτορος, οἱ θεόφρονες γεννήτορες, αὐτῶν τῶν Χερουβὶμ ὑπερφέρον ὄχημα, τὴν τοῦ Λόγου καὶ Κτίστου λοχεύτριαν."

meMA.Ode6C2H.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C2H.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C2H.greekmelody = he.h.m8_gr_GR_cog.heHE.OsYdataThalassis.greekname
meMA.Ode6C2H.melody = he.h.m8_gr_GR_cog.heHE.OsYdataThalassis.melody
meMA.Ode6C2H.incipit = he.h.m8_gr_GR_cog.heHE.OsYdataThalassis.incipit
meMA.Ode6C2H.text = he.h.m8_gr_GR_cog.heHE.OsYdataThalassis.text

meMA.Ode6C21.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C21.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C21.greekmelody = he.h.m8_gr_GR_cog.heHE.OsYdataThalassis.greekname
meMA.Ode6C21.melody = he.h.m8_gr_GR_cog.heHE.OsYdataThalassis.name
meMA.Ode6C21.incipit = "Ὑμνοῦμεν τὴν ἁγίαν σου γέννησιν…"
meMA.Ode6C21.text = "Ὑμνοῦμεν τὴν ἁγίαν σου γέννησιν, τιμῶμεν καὶ τὴν ἄσπορον σύλληψιν, σοῦ νύμφη Θεόνυμφε καὶ Παρθένε· σκιρτῶσι δὲ σὺν ἡμῖν Ἀγγέλων τάξεις, καὶ τῶν Ἁγίων ψυχαί."

meMA.Ode6C22.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C22.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C22.melody = he.h.m8_gr_GR_cog.heHE.OsYdataThalassis.name
meMA.Ode6C22.incipit = "Ἁγίαν τῶν Ἁγίων ὑπάρχουσαν, οἱ…"
meMA.Ode6C22.text = "Ἁγίαν τῶν Ἁγίων ὑπάρχουσαν, οἱ σώφρονες πατέρες σου Ἄχραντε, ἀνέθεντό σε ἐν οἴκῳ Κυρίου, ἀνατραφῆναι σεμνῶς, καὶ εἰς Μητέρα ἑτοιμασθῆναι αὐτῷ."

meMA.Ode6C23.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C23.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C23.melody = he.h.m8_gr_GR_cog.heHE.OsYdataThalassis.name
meMA.Ode6C23.incipit = "Αἱ στεῖραι καὶ μητέρες χορεύσατε…"
meMA.Ode6C23.text = "Αἱ στεῖραι καὶ μητέρες χορεύσατε· θαρσεῖτε καὶ σκιρτήσατε ἄγονοι· ἡ ἄτεκνος γὰρ στεῖρα, τὴν Θεοτόκον βλαστάνει, ἥτις λύσει τῶν ὠδίνων τὴν Εὔαν, καὶ τῆς ἀρᾶς τὸν Ἀδάμ."

meMA.Ode6C24.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C24.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C24.melody = he.h.m8_gr_GR_cog.heHE.OsYdataThalassis.name
meMA.Ode6C24.incipit = "Ἀκούω τοῦ Δαυῒδ μελῳδοῦντός σοι…"
meMA.Ode6C24.text = "Ἀκούω τοῦ Δαυῒδ μελῳδοῦντός σοι· Ἀχθήσονται παρθένοι ὀπίσω σου, ἀχθήσονται εἰς ναὸν Βασιλέως· καὶ σὺν αὐτῷ σε κἀγώ, θυγατέρα τοῦ Βασιλέως ὑμνῶ."

meMA.Ode6C2Tr.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C2Tr.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C2Tr.melody = he.h.m8_gr_GR_cog.heHE.OsYdataThalassis.name
meMA.Ode6C2Tr.incipit = "Ἐν σοὶ τὸ τῆς Τριάδος…"
meMA.Ode6C2Tr.text = "Ἐν σοὶ τὸ τῆς Τριάδος μυστήριον, ὑμνεῖται καὶ δοξάζεται ἄχραντε· Πατὴρ γὰρ ηὐδόκησε, καὶ ὁ Λόγος ἐσκήνωσεν ἐν ἡμῖν, καὶ θεῖον Πνεῦμα σοὶ ἐπεσκίασε."

meMA.Ode6C2T.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C2T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode6C2T.melody = he.h.m8_gr_GR_cog.heHE.OsYdataThalassis.name
meMA.Ode6C2T.incipit = "Χρυσοῦν θυμιατήριον γέγονας· τὸ πῦρ…"
meMA.Ode6C2T.text = "Χρυσοῦν θυμιατήριον γέγονας· τὸ πῦρ γὰρ ἐν γαστρί σου ἐσκήνωσεν, ὁ Λόγος ἐκ Πνεύματος τοῦ Ἁγίου, καὶ ἐν ἀνθρώπου μορφῇ καθωράθη, Θεογεννῆτορ ἁγνή."

meMA.Kontakion1.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Kontakion1.melody = ""
meMA.Kontakion1.incipit = "Ἰωακεὶμ καὶ Ἄννα…"
meMA.Kontakion1.text = "Ἰωακεὶμ καὶ Ἄννα ὀνειδισμοῦ ἀτεκνίας, καὶ Ἀδὰμ καὶ Εὔα, ἐκ τῆς φθορᾶς τοῦ θανάτου, ἠλευθερώθησαν, Ἄχραντε, ἐν τῇ ἁγίᾳ γεννήσει σου· αὐτὴν ἑορτάζει καὶ ὁ λαός σου, ἐνοχῆς τῶν πταισμάτων, λυτρωθεὶς ἐν τῷ κράζειν σοι· Ἡ στεῖρα τίκτει τὴν Θεοτόκον, καὶ τροφὸν τῆς ζωῆς ἡμῶν."

meMA.Oikos1.text = "Ἡ προσευχὴ ὁμοῦ καὶ στεναγμός, τῆς στειρώσεως καὶ ἀτεκνώσεως Ἰωακείμ τε καὶ Ἄννης, εὐπρόσδεκτος, καὶ εἰς τὰ ὦτα Κυρίου ἐλήλυθε, καὶ ἐβλάστησαν καρπὸν ζωηφόρον τῷ κόσμῳ· ὁ μὲν γὰρ προσευχὴν ἐν τῷ ὄρει ἐτέλει, ἡ δὲ ἐν παραδείσῳ ὄνειδος φέρει· ἀλλὰ μετὰ χαράς, ἡ στεῖρα τίκτει τὴν Θεοτόκον, καὶ τροφὸν τῆς ζωῆς ἡμῶν."

meMA.Ode7C1H.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C1H.mode = miscellanea_gr_GR_cog.misc.Mode2
