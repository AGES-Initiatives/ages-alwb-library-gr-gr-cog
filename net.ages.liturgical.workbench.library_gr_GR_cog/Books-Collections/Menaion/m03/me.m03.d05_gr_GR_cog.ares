meMA.Ode5C1H.incipit = he.h.m5_gr_GR_cog.heHE.OrthrizontesVoomenSoi.incipit
meMA.Ode5C1H.text = he.h.m5_gr_GR_cog.heHE.OrthrizontesVoomenSoi.text

meMA.Ode5C11.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C11.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode5C11.greekmelody = he.h.m5_gr_GR_cog.heHE.OrthrizontesVoomenSoi.greekname
meMA.Ode5C11.melody = he.h.m5_gr_GR_cog.heHE.OrthrizontesVoomenSoi.name
meMA.Ode5C11.incipit = "Ἄχράντου σε δοχεῖον ἡ χάρις…"
meMA.Ode5C11.text = "Ἄχράντου σε δοχεῖον ἡ χάρις, καὶ ἀκηράτου φωτὸς ἀπειργάσατο, Μαρτύρων κλέος καὶ καύχημα."

meMA.Ode5C12.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C12.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode5C12.melody = he.h.m5_gr_GR_cog.heHE.OrthrizontesVoomenSoi.name
meMA.Ode5C12.incipit = "Ῥημάτων ἐπλουτίσθης ἀῤῥήτων, τὴν θείαν…"
meMA.Ode5C12.text = "Ῥημάτων ἐπλουτίσθης ἀῤῥήτων, τὴν θείαν γνῶσιν, καὶ ταύτην ἐμύησας, λαοὺς ἀπείρους ὑπέρτιμε."

meMA.Ode5C13.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C13.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode5C13.melody = he.h.m5_gr_GR_cog.heHE.OrthrizontesVoomenSoi.name
meMA.Ode5C13.incipit = "Τὴν χάριν σου ὁρῶντες οἱ…"
meMA.Ode5C13.text = "Τὴν χάριν σου ὁρῶντες οἱ ἄνομοι, τῶν θαυμάτων ἐβόων, ὁ Κόνωνος Θεός, τὴν πλάνην ἐνίκησε."

meMA.Ode5C1T.ode = miscellanea_gr_GR_cog.misc.Ode5
meMA.Ode5C1T.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode5C1T.melody = he.h.m5_gr_GR_cog.heHE.OrthrizontesVoomenSoi.name
meMA.Ode5C1T.incipit = "Παρθένον μετὰ τόκον ὑμνοῦμέν σε…"
meMA.Ode5C1T.text = "Παρθένον μετὰ τόκον ὑμνοῦμέν σε, Θεοτόκε· σὺ γὰρ τὸν Θεὸν Λόγον, σαρκὶ τῷ κόσμῳ ἐκύησας."

meMA.Ode6C1H.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C1H.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode6C1H.greekmelody = he.h.m5_gr_GR_cog.heHE.EkykloseMeAvyssos.greekname
meMA.Ode6C1H.melody = he.h.m5_gr_GR_cog.heHE.EkykloseMeAvyssos.melody
meMA.Ode6C1H.incipit = he.h.m5_gr_GR_cog.heHE.EkykloseMeAvyssos.incipit
meMA.Ode6C1H.text = he.h.m5_gr_GR_cog.heHE.EkykloseMeAvyssos.text

meMA.Ode6C11.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C11.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode6C11.greekmelody = he.h.m5_gr_GR_cog.heHE.EkykloseMeAvyssos.greekname
meMA.Ode6C11.melody = he.h.m5_gr_GR_cog.heHE.EkykloseMeAvyssos.name
meMA.Ode6C11.incipit = "Ὑπῆρξε τοῖς πιστεύσασιν, ἡ χάρις…"
meMA.Ode6C11.text = "Ὑπῆρξε τοῖς πιστεύσασιν, ἡ χάρις τοῦ πλούτου ἐπαύξησις, τοῖς δὲ ἀπειθήσασιν, ἡ ἔνστασις ὄλεθρος, ταῖς ἱκεσίαις, σοῦ Κόνων παμμακάριστε."

meMA.Ode6C12.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C12.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode6C12.melody = he.h.m5_gr_GR_cog.heHE.EkykloseMeAvyssos.name
meMA.Ode6C12.incipit = "Σαφῶς σοι ὑποτέτακται, δαιμόνων τὰ…"
meMA.Ode6C12.text = "Σαφῶς σοι ὑποτέτακται, δαιμόνων τὰ στίφη μακάριε, σαυτὸν ὑποτάξαντι, Θεῷ τῷ ποιήσαντι, αὐτεξουσίως, ὦ Μάρτυς Κόνων ἔνδοξε."

meMA.Ode6C13.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C13.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode6C13.melody = he.h.m5_gr_GR_cog.heHE.EkykloseMeAvyssos.name
meMA.Ode6C13.incipit = "Ὑπὸ σοῦ ἐπαιδεύετο, τὰ πλήθη…"
meMA.Ode6C13.text = "Ὑπὸ σοῦ ἐπαιδεύετο, τὰ πλήθη αἱρεῖσθαι τὸν ἔλεον, καὶ τρόπον βδελύττεσθαι, ἐνθέως φιλάργυρον, Παμμάκαρ ἔργῳ, καὶ λόγῳ διδασκόμενα."

meMA.Ode6C1T.ode = miscellanea_gr_GR_cog.misc.Ode6
meMA.Ode6C1T.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode6C1T.melody = he.h.m5_gr_GR_cog.heHE.EkykloseMeAvyssos.name
meMA.Ode6C1T.incipit = "Βάτον σε ἀκατάφλεκτον, καὶ ὄρος…"
meMA.Ode6C1T.text = "Βάτον σε ἀκατάφλεκτον, καὶ ὄρος καὶ κλίμακα ἔμψυχον, καὶ πύλην οὐράνιον, ἀξίως δοξάζομεν, Μαρία ἔνδοξε, Ὀρθοδόξων καύχημα."

meMA.Kontakion1ger.poet = poets_gr_GR_cog.po.Gerasimou
meMA.Kontakion1ger.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Kontakion1ger.melody = he.a.m4_gr_GR_cog.heAU.OYpsotheisEnToStavro.name
meMA.Kontakion1ger.incipit = "Ἀγγελικῆς ἀξιωθεὶς ὀπτασίας, τὴν ἐν…"
meMA.Kontakion1ger.text = "Ἀγγελικῆς ἀξιωθεὶς ὀπτασίας, τὴν ἐν Χριστῷ μεμυσταγώγησαι πίστιν, καὶ τῶν θαυμάτων εἴληφας τὴν δύναμιν σοφέ· ὅθεν καθυπέταξας, τὴν ὀφρὺν τῶν δαιμόνων, καὶ τῆς πλάνης ἔσβεσας, ἐναθλήσας τὴν φλόγα. Ὁσιομάρτυς Κόνων Ἀθλητά, ἐξευμενίζου ἡμῖν τὸν Φιλάνθρωπον."

meMA.Ode7C1H.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C1H.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode7C1H.greekmelody = he.h.m5_gr_GR_cog.heHE.OEnKaminoPyros.greekname
meMA.Ode7C1H.melody = he.h.m5_gr_GR_cog.heHE.OEnKaminoPyros.melody
meMA.Ode7C1H.incipit = he.h.m5_gr_GR_cog.heHE.OEnKaminoPyros.incipit
meMA.Ode7C1H.text = he.h.m5_gr_GR_cog.heHE.OEnKaminoPyros.text

meMA.Ode7C11.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C11.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode7C11.greekmelody = he.h.m5_gr_GR_cog.heHE.OEnKaminoPyros.greekname
meMA.Ode7C11.melody = he.h.m5_gr_GR_cog.heHE.OEnKaminoPyros.name
meMA.Ode7C11.incipit = "Μαρτυρικῶς Ἀθλητά, ἠγωνισμένος ἀνεβόας· Εὐλογητὸς…"
meMA.Ode7C11.text = "Μαρτυρικῶς Ἀθλητά, ἠγωνισμένος ἀνεβόας· Εὐλογητὸς ὁ Θεός, ὁ τῶν Πατέρων ἡμῶν."

meMA.Ode7C12.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C12.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode7C12.melody = he.h.m5_gr_GR_cog.heHE.OEnKaminoPyros.name
meMA.Ode7C12.incipit = "Νενικηκώς τὸν ἐχθρόν, σὺ ἀνεδείχθης…"
meMA.Ode7C12.text = "Νενικηκώς τὸν ἐχθρόν, σὺ ἀνεδείχθης Ἀθλοφόρος· εὐλογητὸς ὁ Θεός, ὁ τῶν Πατέρων βοῶν."

meMA.Ode7C13.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C13.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode7C13.melody = he.h.m5_gr_GR_cog.heHE.OEnKaminoPyros.name
meMA.Ode7C13.incipit = "Ἐπὶ τῆς γῆς νικητής, ἐν…"
meMA.Ode7C13.text = "Ἐπὶ τῆς γῆς νικητής, ἐν οὐρανοῖς δὲ στεφανίτης, ἀνηγορεύθης τοῦ Χριστοῦ, Μάρτυς ἀήττητε."

meMA.Ode7C14.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C14.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode7C14.melody = he.h.m5_gr_GR_cog.heHE.OEnKaminoPyros.name
meMA.Ode7C14.incipit = "Ἱερουργήσας Θεῷ, τὴν τῶν ἐν…"
meMA.Ode7C14.text = "Ἱερουργήσας Θεῷ, τὴν τῶν ἐν πλάνῃ σωτηρίαν· Εὐλογητὸς ὁ Θεός, ὁ τῶν Πατέρων βοᾷς."

meMA.Ode7C1T.ode = miscellanea_gr_GR_cog.misc.Ode7
meMA.Ode7C1T.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode7C1T.melody = he.h.m5_gr_GR_cog.heHE.OEnKaminoPyros.name
meMA.Ode7C1T.incipit = "Ὁ ἐκ Παρθένου τεχθείς, ἣν…"
meMA.Ode7C1T.text = "Ὁ ἐκ Παρθένου τεχθείς, ἣν Θεοτόκον ἀπειργάσω· εὐλογητὸς ὁ Θεός, ὁ τῶν Πατέρων ἡμῶν."

meMA.Ode8C1H.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C1H.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode8C1H.greekmelody = he.h.m5_gr_GR_cog.heHE.TonEkPatrosProAionon.greekname
meMA.Ode8C1H.melody = he.h.m5_gr_GR_cog.heHE.TonEkPatrosProAionon.melody
meMA.Ode8C1H.incipit = he.h.m5_gr_GR_cog.heHE.TonEkPatrosProAionon.incipit
meMA.Ode8C1H.text = he.h.m5_gr_GR_cog.heHE.TonEkPatrosProAionon.text

meMA.Ode8C11.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C11.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode8C11.greekmelody = he.h.m5_gr_GR_cog.heHE.TonEkPatrosProAionon.greekname
meMA.Ode8C11.melody = he.h.m5_gr_GR_cog.heHE.TonEkPatrosProAionon.name
meMA.Ode8C11.incipit = "Σεσυλημένον ὁρῶντες, τὸν τοῦ σκότους…"
meMA.Ode8C11.text = "Σεσυλημένον ὁρῶντες, τὸν τοῦ σκότους κοσμοκράτορα, τὴν δεισιδαίμονα πλάνην, τοῦ Ἁγίου ταῖς ἐντεύξεσιν· Ἱερεῖς ὑμνεῖτε, λαὸς ὑπερυψοῦτε, Χριστὸν εἰς τοὺς αἰῶνας."

meMA.Ode8C12.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C12.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode8C12.melody = he.h.m5_gr_GR_cog.heHE.TonEkPatrosProAionon.name
meMA.Ode8C12.incipit = "Θεοπρεπῶς σοι Παμμάκαρ, τῶν κακούργων…"
meMA.Ode8C12.text = "Θεοπρεπῶς σοι Παμμάκαρ, τῶν κακούργων τὰ συστήματα, διʼ ὀλεθρίων πνευμάτων, τιμωρούμενα ἐκραύγαζον· Ἱερεῖς ὑμνεῖτε, λαὸς ὑπερυψοῦτε, Χριστὸν εἰς τοὺς αἰῶνας."

meMA.Ode8C13.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C13.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode8C13.melody = he.h.m5_gr_GR_cog.heHE.TonEkPatrosProAionon.name
meMA.Ode8C13.incipit = "Ὡς μητρικαῖς ἀγκάλαις, ἀσινὲς τὸ…"
meMA.Ode8C13.text = "Ὡς μητρικαῖς ἀγκάλαις, ἀσινὲς τὸ βρέφος ἔσωσας, τῇ ἐνεργεῖ σου προστάξει, ἐν ταῖς μύλαις τοῦ ἀγρίου θηρός· Ἱερεῖς ὑμνεῖτε, λαὸς ὑπερυψοῦτε, Χριστὸν βοῶν ἀπαύστως."

meMA.Ode8C14.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C14.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode8C14.melody = he.h.m5_gr_GR_cog.heHE.TonEkPatrosProAionon.name
meMA.Ode8C14.incipit = "Κατελαμπρύνθης ἀφθόνως, Ἀθλητὰ τῷ τρισηλίῳ…"
meMA.Ode8C14.text = "Κατελαμπρύνθης ἀφθόνως, Ἀθλητὰ τῷ τρισηλίῳ φωτί, τὴν τρισυπόστατον φύσιν· ἀναμέλπων γὰρ ἐκραύγαζες· Ἱερεῖς ὑμνεῖτε, λαὸς ὑπερυψοῦτε, εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8C1T.ode = miscellanea_gr_GR_cog.misc.Ode8
meMA.Ode8C1T.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode8C1T.melody = he.h.m5_gr_GR_cog.heHE.TonEkPatrosProAionon.name
meMA.Ode8C1T.incipit = "Τῶν Χερουβὶμ ὑπερτέρα, ἀνεδείχθης ὑπερένδοξε…"
meMA.Ode8C1T.text = "Τῶν Χερουβὶμ ὑπερτέρα, ἀνεδείχθης ὑπερένδοξε, Θεογεννῆτορ Μαρία· ἐν γαστρί σου γὰρ ἐσκήνωσεν, ὃν σὺν Ἀσωμάτοις, βροτοὶ δοξολογοῦμεν, εἰς πάντας τοὺς αἰῶνας."

meMA.Ode9C1H.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C1H.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode9C1H.greekmelody = he.h.m5_gr_GR_cog.heHE.SeTinYperNoun.greekname
meMA.Ode9C1H.melody = he.h.m5_gr_GR_cog.heHE.SeTinYperNoun.melody
meMA.Ode9C1H.incipit = he.h.m5_gr_GR_cog.heHE.SeTinYperNoun.incipit
meMA.Ode9C1H.text = he.h.m5_gr_GR_cog.heHE.SeTinYperNoun.text

meMA.Ode9C11.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C11.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode9C11.greekmelody = he.h.m5_gr_GR_cog.heHE.SeTinYperNoun.greekname
meMA.Ode9C11.melody = he.h.m5_gr_GR_cog.heHE.SeTinYperNoun.name
meMA.Ode9C11.incipit = "Ὅλος τοῦ ἀΰλου πόθου γενόμενος…"
meMA.Ode9C11.text = "Ὅλος τοῦ ἀΰλου πόθου γενόμενος, τοῦ Χριστοῦ ἐπεπόθησας, πιεῖν τὸ ποτήριον· διὸ πάντες σε Μάκαρ μακαρίζομεν."

meMA.Ode9C12.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C12.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode9C12.melody = he.h.m5_gr_GR_cog.heHE.SeTinYperNoun.name
meMA.Ode9C12.incipit = "Νόμῳ θείῳ θεαρέστως ἑπόμενος, οὐ…"
meMA.Ode9C12.text = "Νόμῳ θείῳ θεαρέστως ἑπόμενος, οὐ θωπείαις Μακάριε, τοῦ Χριστοῦ τὸ ποτήριον, ὁλοψύχως ὤφθης ἀρυσάμενος."

meMA.Ode9C13.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C13.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode9C13.melody = he.h.m5_gr_GR_cog.heHE.SeTinYperNoun.name
meMA.Ode9C13.incipit = "Ὤφθης τῶν Μαρτύρων ἔνδοξε καλλονή…"
meMA.Ode9C13.text = "Ὤφθης τῶν Μαρτύρων ἔνδοξε καλλονή, τοῖς Χριστοῦ καλλυνόμενος, σεπτοτάτοις στίγμασι· διὰ τοῦτό σε, ἅπαντες γεραίρομεν."

meMA.Ode9C14.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C14.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode9C14.melody = he.h.m5_gr_GR_cog.heHE.SeTinYperNoun.name
meMA.Ode9C14.incipit = "Νῦν ἐν οὐρανίῳ λήξει γενόμενος…"
meMA.Ode9C14.text = "Νῦν ἐν οὐρανίῳ λήξει γενόμενος, τῶν τοῦ βίου με λύτρωσαι, δεινῶν περιστάσεων, Ἀθλητὰ Κυρίου Κόνων ἔνδοξε."

meMA.Ode9C1T.ode = miscellanea_gr_GR_cog.misc.Ode9
meMA.Ode9C1T.mode = miscellanea_gr_GR_cog.misc.Mode5
meMA.Ode9C1T.melody = he.h.m5_gr_GR_cog.heHE.SeTinYperNoun.name
meMA.Ode9C1T.incipit = "Χαῖρε Θεοτόκε Μῆτερ Χριστοῦ τοῦ…"
meMA.Ode9C1T.text = "Χαῖρε Θεοτόκε Μῆτερ Χριστοῦ τοῦ Θεοῦ, χαῖρε ὄρος τὸ ἅγιον, χαῖρε τὸ κεφάλαιον, τῆς σεπτῆς παρθενίας, χαῖρε Δέσποινα."

