meMA.Ode1C11.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode1C11.melody = he.h.m4_gr_GR_cog.ThalassisToErythraion.name
meMA.Ode1C11.incipit = "Τὴν δόσιν, τὴν ἀγαθὴν Ἰάκωβε…"
meMA.Ode1C11.text = "Τὴν δόσιν, τὴν ἀγαθὴν Ἰάκωβε, καὶ δῶρον τέλειον, ἐκ τοῦ Πατρὸς τῶν φώτων τοῖς βροτοῖς, χορηγεῖσθαι διδάσκεις σαφῶς, ἧς μετασχεῖν Ἀπόστολε, τοὺς σὲ ὑμνοῦντας καθικέτευε."

meMA.Ode1C12.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode1C12.melody = he.h.m4_gr_GR_cog.ThalassisToErythraion.name
meMA.Ode1C12.incipit = "Τῇ αἴγλῃ, σὺ φωτισθεὶς Ἰάκωβε…"
meMA.Ode1C12.text = "Τῇ αἴγλῃ, σὺ φωτισθεὶς Ἰάκωβε, τοῦ θείου Πνεύματος, καὶ σὺν αὐτῷ τοὺς νόμους ἐκτιθείς, Ἐκκλησίας ῥυθμίζεις ἐθνῶν, ὡς τοῦ Χριστοῦ Ἀπόστολος, ἱεροφάντορ παμμακάριστε."

meMA.Ode1C13.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode1C13.melody = he.h.m4_gr_GR_cog.ThalassisToErythraion.name
meMA.Ode1C13.incipit = "Χορείαν, τῶν Ἀποστόλων πάνσοφε, σαφῶς…"
meMA.Ode1C13.text = "Χορείαν, τῶν Ἀποστόλων πάνσοφε, σαφῶς ἐκόσμησας, Ἀρχιερεὺς ὡς πρῶτος γεγονώς, αὐτουργίᾳ τοῦ Λόγου χρισθείς, ὡς μαθητὴς θεάδελφος, ἱεροκήρυξ μυστικώτατος."

meMA.Ode1C1T.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode1C1T.melody = he.h.m4_gr_GR_cog.ThalassisToErythraion.name
meMA.Ode1C1T.incipit = "Πηγήν σε, τῶν ἰαμάτων ἔχοντες…"
meMA.Ode1C1T.text = "Πηγήν σε, τῶν ἰαμάτων ἔχοντες, ἀπαρυόμεθα, ὡς τὴν πηγὴν τεκοῦσαν τῆς ζωῆς, Θεοτόκε πανύμνητε, καὶ τῶν ψυχῶν τὴν ἴασιν, καὶ τῶν σωμάτων τὴν ἀνάκτησιν."

meMA.Ode3C11.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode3C11.melody = he.h.m4_gr_GR_cog.EffrainetaiEpiSoi.name
meMA.Ode3C11.incipit = "Εὐφραίνεται ἡ φυλή, ἡ τοῦ…"
meMA.Ode3C11.text = "Εὐφραίνεται ἡ φυλή, ἡ τοῦ Ἰούδα ἐπὶ σοὶ σήμερον, ταῖς τοῦ Χριστοῦ βλέπουσα, σὲ μαρμαρυγαῖς ἀπαστράπτοντα."

meMA.Ode3C12.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode3C12.melody = he.h.m4_gr_GR_cog.EffrainetaiEpiSoi.name
meMA.Ode3C12.incipit = "Ἀστέρα θεολαμπῆ, σὲ ἡ Ἐκκλησία…"
meMA.Ode3C12.text = "Ἀστέρα θεολαμπῆ, σὲ ἡ Ἐκκλησία τῶν ἐθνῶν ἔχουσα, τὸν εὐκλεῆ κήρυκα, θείαις δᾳδουχίαις λαμπρύνεται."

meMA.Ode3C13.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode3C13.melody = he.h.m4_gr_GR_cog.EffrainetaiEpiSoi.name
meMA.Ode3C13.incipit = "Τὸν νόμον τὸν τῆς ζωῆς…"
meMA.Ode3C13.text = "Τὸν νόμον τὸν τῆς ζωῆς, τῇ Ἐκκλησίᾳ τοῦ Χριστοῦ τέθεικας, ζωοποιῷ Πνεύματι, σὺ νομοθετῶν καὶ φθεγγόμενος."

meMA.Ode3C1T.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode3C1T.melody = he.h.m4_gr_GR_cog.EffrainetaiEpiSoi.name
meMA.Ode3C1T.incipit = "Ὃν ἔτεκες ἐν σαρκί, Παρθενομῆτορ…"
meMA.Ode3C1T.text = "Ὃν ἔτεκες ἐν σαρκί, Παρθενομῆτορ ἐκ Πατρὸς λάμψαντα, τοῦτον Ἁγνή, Ἰάκωβος, ὡς Θεὸν τῶν ὅλων ἐκήρυττε."

meMA.Ode3H.ode = misc_gr_GR_cog.Ode3
meMA.Ode3H.mode = misc_gr_GR_cog.Mode4
meMA.Ode3H.melody = he.h.m4_gr_GR_cog.EffrainetaiEpiSoi.melody
meMA.Ode3H.incipit = he.h.m4_gr_GR_cog.EffrainetaiEpiSoi.incipit
meMA.Ode3H.text = he.h.m4_gr_GR_cog.EffrainetaiEpiSoi.text

meMA.Ode3Kathisma1.mode = misc_gr_GR_cog.Mode1
meMA.Ode3Kathisma1.melody = he.a.m1_gr_GR_cog.TonTafonSouSotir.name
meMA.Ode3Kathisma1.incipit = "Ἀπόστολε Χριστοῦ, καὶ αὐτόπτα καὶ…"
meMA.Ode3Kathisma1.text = "Ἀπόστολε Χριστοῦ, καὶ αὐτόπτα καὶ Μάρτυς, Ἰάκωβε σοφέ, Ἀδελφόθεε μάκαρ, τοὺς πίστει ἑορτάζοντας, τὸ σεπτόν σου μνημόσυνον, ἐλευθέρωσον, τῶν συνεχόντων πταισμάτων, ταῖς πρεσβείαις σου, τοὺς οὐρανοὺς ἐμβατεύων, δυνάμει τοῦ Πνεύματος."

meMA.Ode3Kathisma2.mode = misc_gr_GR_cog.Mode1
meMA.Ode3Kathisma2.melody = he.a.m8_gr_GR_cog.TinSofianKaiLogon.name
meMA.Ode3Kathisma2.incipit = "Συγγενὴς κατὰ σάρκα τοῦ Ἰησοῦ…"
meMA.Ode3Kathisma2.text = "Συγγενὴς κατὰ σάρκα τοῦ Ἰησοῦ, χρηματίσας πρὸς τούτου χάριν πολλήν, Ἀπόστολε εἴληφας, καὶ τοῖς πᾶσι μετέδωκας, φωτισμοῦ καὶ γνώσεως, πανσόφου, Ἰάκωβε, καὶ εἰδώλων πλάνην, ῥιζόθεν ἀνέσπασας· ὅθεν οἱ τοῦ σκότους, προϊστάμενοι πλάνοι, ἀδίκως σε κτείνουσι, τὸν Σωτῆρα κηρύττοντα· διὰ τοῦτο βοῶμέν σοι· Πρέσβευε Χριστῷ τῷ Θεῷ, τῶν πταισμάτων ἄφεσιν δωρήσασθαι, τοῖς ἑορτάζουσι πόθῳ, τὴν ἁγίαν μνήμην σου."

meMA.Ode3KathismaT.mode = misc_gr_GR_cog.Mode1
meMA.Ode3KathismaT.melody = he.a.m8_gr_GR_cog.TinSofianKaiLogon.name
meMA.Ode3KathismaT.incipit = me.m01.d17_gr_GR_cog.meMA.Ode3KathismaT.incipit
meMA.Ode3KathismaT.text = me.m01.d17_gr_GR_cog.meMA.Ode3KathismaT.text

meMA.Ode4C11.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode4C11.melody = he.h.m4_gr_GR_cog.DiAgapisinOiktirmon.name
meMA.Ode4C11.incipit = "Ἐπεβίβασας εἰς θάλασσαν σοῦ τοὺς…"
meMA.Ode4C11.text = "Ἐπεβίβασας εἰς θάλασσαν σοῦ τοὺς ἵππους, τοὺς Ἀποστόλους Σῶτερ, καὶ ἐφώτισαν τὰ ἔθνη, πρὸς τὴν σὴν ἐπίγνωσιν, ἑλκύσαντες Κύριε."

meMA.Ode4C12.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode4C12.melody = he.h.m4_gr_GR_cog.DiAgapisinOiktirmon.name
meMA.Ode4C12.incipit = "Ἀκηλίδωτον ὡς ἔσοπτρον δεδειγμένος, καὶ…"
meMA.Ode4C12.text = "Ἀκηλίδωτον ὡς ἔσοπτρον δεδειγμένος, καὶ πρωτουργοὺς ἀκτῖνας, τῆς ἀκτίστου Τριάδος, ἀκριβῶς δεξάμενος, τοῖς πᾶσιν ἐξέλαμψας."

meMA.Ode4C13.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode4C13.melody = he.h.m4_gr_GR_cog.DiAgapisinOiktirmon.name
meMA.Ode4C13.incipit = "Κυλιόμενόν σε λίθον ἡ προφητεία…"
meMA.Ode4C13.text = "Κυλιόμενόν σε λίθον ἡ προφητεία, ἡγιασμένον βλέπει, καὶ συντρίβοντα πλάνης, σαθρὰν ματαιότητα, ἰσχὺῒ τοῦ Πνεύματος."

meMA.Ode4C1T.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode4C1T.melody = he.h.m4_gr_GR_cog.DiAgapisinOiktirmon.name
meMA.Ode4C1T.incipit = "Ὑποδὺς Παρθενομῆτορ τὴν σὴν γαστέρα…"
meMA.Ode4C1T.text = "Ὑποδὺς Παρθενομῆτορ τὴν σὴν γαστέρα, ὁ πρὸ αἰώνων Λόγος, γεννηθεὶς ἀποῤῥήτως, ἐκ Πατρός, Ἰάκωβον, Ἀπόστολον δείκνυσι."

meMA.Ode5C11.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode5C11.melody = he.h.m4_gr_GR_cog.SyKyrieMouFos.name
meMA.Ode5C11.incipit = "Σὺ Κύριε φωτός, ἀστραπὴν τὸν…"
meMA.Ode5C11.text = "Σὺ Κύριε φωτός, ἀστραπὴν τὸν Ἰάκωβον, ἀνέδειξας φρυκτωροῦσαν, καὶ βροτοὺς τῆς ἀγνοίας, καὶ πλάνης ἀπαλλάττουσαν."

meMA.Ode5C12.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode5C12.melody = he.h.m4_gr_GR_cog.SyKyrieMouFos.name
meMA.Ode5C12.incipit = "Σὲ Κύριε φωνή, ἐλευθέρα ἐκήρυξεν…"
meMA.Ode5C12.text = "Σὲ Κύριε φωνή, ἐλευθέρα ἐκήρυξεν, ὁ δίκαιος ἀδελφός σου, μιμητής τε τοῦ πάθους, ἐγένετο κτεινόμενος."

meMA.Ode5C13.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode5C13.melody = he.h.m4_gr_GR_cog.SyKyrieMouFos.name
meMA.Ode5C13.incipit = "Τίς βρότειός σοι νοῦς, ἐγκωμίων…"
meMA.Ode5C13.text = "Τίς βρότειός σοι νοῦς, ἐγκωμίων ἐφύμνιον, δυνήσεται προσκομίζειν, ἀρετῆς λαμπρυνθέντι, ταῖς χάρισιν Ἰάκωβε;"

meMA.Ode5C1T.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode5C1T.melody = he.h.m4_gr_GR_cog.SyKyrieMouFos.name
meMA.Ode5C1T.incipit = "Σὲ θεῖον ἀληθῶς, εὑρηκὼς ἐνδιαίτημα…"
meMA.Ode5C1T.text = "Σὲ θεῖον ἀληθῶς, εὑρηκὼς ἐνδιαίτημα, κατῴκησεν ὦ Παρθένε, τὴν ἡμῶν σωτηρίαν, ὁ Λόγος προμηθούμενος."

meMA.Ode6C11.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode6C11.melody = he.h.m4_gr_GR_cog.ThysoSoiMetaFonis.name
meMA.Ode6C11.incipit = "Στολὴν σε, τῆς νομικῆς…"
meMA.Ode6C11.text = "Στολὴν σε, τῆς νομικῆς ἐνέδυσε κρείττονα, καὶ Ἱεράρχην δεικνύει, Ἀαρὼν τοῦ πάλαι τιμιωτέραν, ὁ διʼ οἶκτον, εἰς ἀδελφότητά σε δεξάμενος."

meMA.Ode6C12.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode6C12.melody = he.h.m4_gr_GR_cog.ThysoSoiMetaFonis.name
meMA.Ode6C12.incipit = "Ὄργανον, ἐνεργείαις…"
meMA.Ode6C12.text = "Ὄργανον, ἐνεργείαις κρουόμενον Πνεύματος, πᾶσαν διάνοιαν θέλγον, καὶ Θεῷ προσάγον τοὺς σῳζομένους, ἐκ δουλείας, χαλεπωτάτης γέγονας ἄριστον."

meMA.Ode6C13.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode6C13.melody = he.h.m4_gr_GR_cog.ThysoSoiMetaFonis.name
meMA.Ode6C13.incipit = "Διψήσας, τοῦ τῆς ζωῆς…"
meMA.Ode6C13.text = "Διψήσας, τοῦ τῆς ζωῆς ἐξέπιες νάματος, καὶ ποταμοὺς ἀναβλύζεις, θεοῤῥημοσύνης τῆς σῆς κοιλίας, ἁλλομένους, τῇ οἰκουμένῃ τῷ Θείῳ Πνεύματι."

meMA.Ode6C1T.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode6C1T.melody = he.h.m4_gr_GR_cog.ThysoSoiMetaFonis.name
meMA.Ode6C1T.incipit = "Εὔα μέν, παρακοῇ…"
meMA.Ode6C1T.text = "Εὔα μέν, παρακοῇ τὸν θάνατον ἔτεκεν· ἡ Θεοτόκος ζωὴν δέ, συλλαβοῦσα τέτοκε τὴν ἀγήρω, καὶ τὸ πάλαι, ὑπὲρ πάντων ἀπέτισεν ὄφλημα."

meMA.Ode6H.ode = misc_gr_GR_cog.Ode6
meMA.Ode6H.mode = misc_gr_GR_cog.Mode4
meMA.Ode6H.melody = he.h.m4_gr_GR_cog.ThysoSoiMetaFonis.melody
meMA.Ode6H.incipit = he.h.m4_gr_GR_cog.ThysoSoiMetaFonis.incipit
meMA.Ode6H.text = he.h.m4_gr_GR_cog.ThysoSoiMetaFonis.text

meMA.Kontakion1.mode = misc_gr_GR_cog.Mode4
meMA.Kontakion1.melody = he.a.m4_gr_GR_cog.OYpsotheisEnToStavro.name
meMA.Kontakion1.incipit = "Ὁ του Πατρὸς μονογενὴς Θεὸς…"
meMA.Kontakion1.text = "Ὁ του Πατρὸς μονογενὴς Θεὸς Λόγος, ἐπιδημήσας πρὸς ἡμᾶς ἐπʼ ἐσχάτων, τῶν ἡμερῶν Ἰάκωβε θεσπέσιε, πρῶτόν σε ἀνέδειξε τῶν Ἱεροσολύμων, Ποιμένα καὶ Διδάσκαλον, καὶ πιστὸν οἰκονόμον, τῶν Μυστηρίων τῶν πνευματικῶν· ὄθεν σε πάντες τιμῶμεν Ἀπόστολε."

meMA.Oikos1.text = "Τὸν γόνον σε τοῦ Ἰωσήφ, καὶ Ἱεροσολύμων τὸν πρῶτον ἱεράρχην, Ἰάκωβε θεόπτα, καὶ τοῦ Κυρίου ἀδελφὸν ὕμνοις ἐγκωμίων ἀνυμνοῦμεν εὐσεβῶς, καὶ πίστει ἀνακράζομεν· Δώρησαι ἡμῖν δώρημα τέλειον ἐκ τοῦ Πατρὸς τῶν φώτων, καὶ ἀπέλασον τὴν θλῖψιν τὴν ἐπερχομένην καὶ ἐνεστῶσαν ἐκ πλήθους πταισμάτων· ἐπῆραν γὰρ οἱ ἐχθροὶ καθʼ ἡμῶν τὴν πτέρναν, καὶ ἐκύκλωσαν ἡμᾶς Ἰσμαηλῖται· ὧν θραῦσον ταχὺ τὰ τόξα ἱεροφάντορ, ἵνα σε πάντες τιμῶμεν Ἀπόστολε."

meMA.Ode7C11.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode7C11.melody = he.h.m4_gr_GR_cog.EnTiKaminoAvramiaioi.name
meMA.Ode7C11.incipit = "Ὡς ἐν καμίνῳ τῷ ὑπερώῳ…"
meMA.Ode7C11.text = "Ὡς ἐν καμίνῳ τῷ ὑπερώῳ πυρσευόμενος, Θείου παρουσίᾳ Πνεύματος μυστικῶς, ὁ Ἰάκωβος ἐβόησεν· Εὐλογημένος εἶ, ἐν τῷ ναῷ τῆς δόξης σου Κύριε."

meMA.Ode7C12.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode7C12.melody = he.h.m4_gr_GR_cog.EnTiKaminoAvramiaioi.name
meMA.Ode7C12.incipit = "Ἡ ἐκ τοῦ ὕψους, πνοὴ…"
meMA.Ode7C12.text = "Ἡ ἐκ τοῦ ὕψους, πνοὴ βιαία τὸν Ἰάκωβον, γλώσσῃ πυριμόρφῳ φθέγγεσθαι τοῦ Θεοῦ, μεγαλεῖα παρεσκεύασεν· Εὐλογημένος εἶ, ἐν τῷ ναῷ τῆς δόξης σου Κύριε."

meMA.Ode7C13.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode7C13.melody = he.h.m4_gr_GR_cog.EnTiKaminoAvramiaioi.name
meMA.Ode7C13.incipit = "Τῆς θεωρίας, τῆς πρακτικῆς σὺ…"
meMA.Ode7C13.text = "Τῆς θεωρίας, τῆς πρακτικῆς σὺ ἐκτιθέμενος, δέλτον ἐκ πυξίδος ὥσπερ πνευματικῆς, τοὺς ἀνθρώπους ἐβελτίωσας· Εὐλογημένος εἶ, ὁ Θεός μου κραυγάζων καὶ Κύριος."

meMA.Ode7C1T.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode7C1T.melody = he.h.m4_gr_GR_cog.EnTiKaminoAvramiaioi.name
meMA.Ode7C1T.incipit = "Ὁ ἀναπλάσας, κόσμον ἐν μήτρᾳ…"
meMA.Ode7C1T.text = "Ὁ ἀναπλάσας, κόσμον ἐν μήτρᾳ σου Θεόνυμφε, θείας λειτουργίας δείκνυσι τελεστήν, τὸν Ἰάκωβον κραυγάζοντα· Εὐλογημένη σύ, ἐν γυναιξὶν ὑπάρχεις πανάμωμε."

meMA.Ode8C11.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode8C11.melody = he.h.m4_gr_GR_cog.CheirasEkpetasasDaniil.name
meMA.Ode8C11.incipit = "Χεῖρας ἐκπετάσας κραταιάς, τοὺς Ἀποστόλους…"
meMA.Ode8C11.text = "Χεῖρας ἐκπετάσας κραταιάς, τοὺς Ἀποστόλους σου Σῶτερ εὐλόγησας, ἐπανερχόμενος Δέσποτα, πρὸς τὸν ἄναρχον Πατέρα σου, καὶ παρεσκεύασας αὐτοὺς μέτʼ εὐφροσύνης βοᾶν· Εὐλογεῖτε, πάντα τὰ ἔργα Κυρίου τὸν Κύριον."

meMA.Ode8C12.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode8C12.melody = he.h.m4_gr_GR_cog.CheirasEkpetasasDaniil.name
meMA.Ode8C12.incipit = "Ὥσπερ ἐπηγγείλω ἀψευδῶς, τοῖς, Ἀποστόλοις…"
meMA.Ode8C12.text = "Ὥσπερ ἐπηγγείλω ἀψευδῶς, τοῖς, Ἀποστόλοις σου Πνεῦμα ἀπέστειλας, τούτου, Ἰάκωβος Δέσποτα, τῇ λαμπρότητι λαμπόμενος, τὴν οἰκουμένην ἀστραπαῖς, καταγλαΐζει φαιδρῶς, καὶ κραυγάζει· Πάντα τὰ ἔργα ὑμνεῖτε τὸν Κύριον."

meMA.Ode8C13.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode8C13.melody = he.h.m4_gr_GR_cog.CheirasEkpetasasDaniil.name
meMA.Ode8C13.incipit = "Σοφίαν Ἰάκωβε σοφέ, τὴν ἐνυπόστατον…"
meMA.Ode8C13.text = "Σοφίαν Ἰάκωβε σοφέ, τὴν ἐνυπόστατον ἔσχες διδάσκαλον, μυσταγωγοῦσάν σε ἄῤῥητα, καὶ ἀνέκφραστα μυστήρια, καὶ πρὸς εὐσέβειαν λαούς, μέλπων διήγειρας· Εὐλογεῖτε, πάντα τὰ ἔργα Κυρίου τὸν Κύριον."

meMA.Ode8C1T.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode8C1T.melody = he.h.m4_gr_GR_cog.CheirasEkpetasasDaniil.name
meMA.Ode8C1T.incipit = "Ὄρος ἐκφανθεῖσαν νοητόν, Παρθένε ἄχραντε…"
meMA.Ode8C1T.text = "Ὄρος ἐκφανθεῖσαν νοητόν, Παρθένε ἄχραντε, Προφήτης βλέπει σε· ἐκ σοῦ ὑπέρτιμος λίθος γάρ, ἐκτμηθεὶς περιεσώσατο, τὴν οἰκουμένην ἀληθῶς, ἐνθέως ψάλλουσαν· Εὐλογεῖτε, πάντα τὰ ἔργα Κυρίου τὸν Κύριον."

meMA.Ode8H.ode = misc_gr_GR_cog.Ode8
meMA.Ode8H.mode = misc_gr_GR_cog.Mode4
