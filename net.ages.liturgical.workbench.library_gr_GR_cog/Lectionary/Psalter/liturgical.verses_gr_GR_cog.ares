psHO.ecloge.annunciation.info = ""
psHO.ecloge.annunciation.intro.text = "Τῷ υἱῷ τοῦ βασιλέως."
psHO.ecloge.annunciation.v01.info = "71:1"
psHO.ecloge.annunciation.v01.text = "Ὁ Θεός, τὸ κρίμα σου τῷ βασιλεῖ δὸς καὶ τὴν δικαιοσύνην σου τῷ υἱῷ τοῦ βασιλέως."
psHO.ecloge.annunciation.v02.info = "71:2"
psHO.ecloge.annunciation.v02.text = "Κρίνειν τὸν λαόν σου ἐν δικαιοσύνῃ καὶ τοὺς πτωχούς σου ἐν κρίσει."
psHO.ecloge.annunciation.v03.info = "71:3"
psHO.ecloge.annunciation.v03.text = "Ἀναλαβέτω τὰ ὄρη εἰρήνην τῷ λαῷ σου καὶ οἱ βουνοὶ δικαιοσύνην."
psHO.ecloge.annunciation.v04.info = "95:2"
psHO.ecloge.annunciation.v04.text = "Εὐαγγελίζεσθε ἡμέραν ἐξ ἡμέρας τὸ σωτήριον τοῦ Θεοῦ ἡμῶν."
psHO.ecloge.annunciation.v05.info = "67:12"
psHO.ecloge.annunciation.v05.text = "Κύριος δώσει ρῆμα τοῖς εὐαγγελιζομένοις δυνάμει πολλῇ."
psHO.ecloge.annunciation.v06.info = "67:13"
psHO.ecloge.annunciation.v06.text = "Ὁ βασιλεὺς τῶν δυνάμεων τοῦ ἀγαπητοῦ."
psHO.ecloge.annunciation.v07.info = "131:11"
psHO.ecloge.annunciation.v07.text = "Ὤμοσε Κύριος τῷ Δαυῒδ ἀλήθειαν καὶ οὐ μὴ ἀθετήσει αὐτήν."
psHO.ecloge.annunciation.v08.info = "131:11"
psHO.ecloge.annunciation.v08.text = "Ἐκ καρποῦ τῆς κοιλίας σου θήσομαι ἐπὶ τοῦ θρόνου σου."
psHO.ecloge.annunciation.v09.info = "71:6"
psHO.ecloge.annunciation.v09.text = "Καταβήσεται ὡς ὑετὸς ἐπὶ πόκον καὶ ὡσεὶ σταγὼν ἡ στάζουσα ἐπὶ τὴν γῆν."
psHO.ecloge.annunciation.v10.info = "143:5"
psHO.ecloge.annunciation.v10.text = "Κύριε, κλῖνον οὐρανοὺς καὶ κατάβηθι."
psHO.ecloge.annunciation.v11.info = "17:10"
psHO.ecloge.annunciation.v11.text = "Καὶ ἔκλινεν οὐρανοὺς καὶ κατέβη."
psHO.ecloge.annunciation.v12.info = "131:13"
psHO.ecloge.annunciation.v12.text = "Ὅτι ἐξελέξατο Κύριος τὴν Σιών, ᾑρετίσατο αὐτὴν εἰς κατοικίαν ἑαυτῷ."
psHO.ecloge.annunciation.v13.info = "45:5"
psHO.ecloge.annunciation.v13.text = "Ἡγίασε τὸ σκήνωμα αὐτοῦ ὁ Ὕψιστος."
psHO.ecloge.annunciation.v14.info = "47:9"
psHO.ecloge.annunciation.v14.text = "Ὁ Θεὸς ἐθεμελίωσεν αὐτὴν εἰς τὸν αἰῶνα."
psHO.ecloge.annunciation.v15.info = "49:3"
psHO.ecloge.annunciation.v15.text = "Ὁ Θεὸς ἐμφανῶς ἥξει, ὁ Θεὸς ἡμῶν, καὶ οὐ παρασιωπήσεται."
psHO.ecloge.annunciation.v16.info = "71:7"
psHO.ecloge.annunciation.v16.text = "Ἀνατελεῖ ἐν ταῖς ἡμέραις αὐτοῦ δικαιοσύνη καὶ πλῆθος εἰρήνης, ἕως οὗ ἀνταναιρεθῇ ἡ σελήνη."
psHO.ecloge.annunciation.v17.info = "71:11"
psHO.ecloge.annunciation.v17.text = "Καὶ προσκυνήσουσιν αὐτῷ πάντες οἱ βασιλεῖς τῆς γῆς."
psHO.ecloge.annunciation.v18.info = "71:11"
psHO.ecloge.annunciation.v18.text = "Πάντα τὰ ἔθνη δουλεύσουσιν αὐτῷ."
psHO.ecloge.annunciation.v19.info = "71:12"
psHO.ecloge.annunciation.v19.text = "Ὅτι ἐρρύσατο πτωχὸν ἐκ δυνάστου καὶ πένητα, ᾧ οὐχ ὑπῆρχε βοηθός."
psHO.ecloge.annunciation.v20.info = "71:17"
psHO.ecloge.annunciation.v20.text = "Ἔσται τὸ ὄνομα αὐτοῦ εὐλογημένον εἰς τοὺς αἰῶνας."
psHO.ecloge.annunciation.v21.info = "71:17"
psHO.ecloge.annunciation.v21.text = "Πάντα τὰ ἔθνη μακαριοῦσιν αὐτόν."
psHO.ecloge.annunciation.v22.info = "71:18"
psHO.ecloge.annunciation.v22.text = "Εὐλογητὸς Κύριος, ὁ Θεὸς τοῦ Ἰσραήλ, ὁ ποιῶν θαυμάσια μόνος,"
psHO.ecloge.annunciation.v23.info = "71:19"
psHO.ecloge.annunciation.v23.text = "Καὶ εὐλογητὸν τὸ ὄνομα τῆς δόξης αὐτοῦ εἰς τὸν αἰῶνα καὶ εἰς τὸν αἰῶνα τοῦ αἰῶνος."
psHO.ecloge.annunciation.v24.info = "71:19"
psHO.ecloge.annunciation.v24.text = "Καὶ πληρωθήσεται τῆς δόξης αὐτοῦ πᾶσα ἡ γῆ. Γένοιτο, γένοιτο."

