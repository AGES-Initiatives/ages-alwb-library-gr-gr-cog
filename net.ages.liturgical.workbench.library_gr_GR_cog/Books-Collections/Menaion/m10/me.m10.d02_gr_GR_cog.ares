meVE.Stichera01.mode = misc_gr_GR_cog.Mode4
meVE.Stichera01.melody = he.a.m4_gr_GR_cog.OsGennaionEnMartysi.name
meVE.Stichera01.incipit = "Ἱερέων ἐν χρίσματι, καὶ Μαρτύρων…"
meVE.Stichera01.text = "Ἱερέων ἐν χρίσματι, καὶ Μαρτύρων ἐν αἵματι τῷ Θεῷ προσήγγισας, τελεώτατα, Κυπριανὲ παναοίδιμε, τὸ ἄνθος τῆς φύσεως, ἡ τῶν λόγων καλλονή· τῆς σοφίας ἀκρόπολις, τῆς ὀρθότητος, τῶν δογμάτων ἡ στάθμη, τῶν κανόνων, παναρμόνιος εὐθύτης· Ἐκκλησιῶν ἡ εὐπρέπεια."

meVE.Stichera02.mode = misc_gr_GR_cog.Mode4
meVE.Stichera02.melody = he.a.m4_gr_GR_cog.OsGennaionEnMartysi.name
meVE.Stichera02.incipit = "Τῶν ἀθλούντων τὴν εὔκλειαν, τῶν…"
meVE.Stichera02.text = "Τῶν ἀθλούντων τὴν εὔκλειαν, τῶν Μαρτύρων τὸν στέφανον, ὑπογράφων ἔπεισας τοὺς θεόφρονας, κατατολμᾶν γενναιότατα, ποικίλων κολάσεων, καὶ δεσμῶν καὶ φυλακῆς, καὶ σωμάτων γυμνώσεως, καὶ στρεβλώσεων, καὶ κρυμοῦ δριμυτάτου καὶ μαστίγων, καὶ θανάτου τελευταῖον, Κυπριανὲ πανσεβάσμιε."

meVE.Stichera03.mode = misc_gr_GR_cog.Mode4
meVE.Stichera03.melody = he.a.m4_gr_GR_cog.OsGennaionEnMartysi.name
meVE.Stichera03.incipit = "Ἐπῳδαῖς ταῖς τοῦ δαίμονος, τὰς…"
meVE.Stichera03.text = "Ἐπῳδαῖς ταῖς τοῦ δαίμονος, τὰς ᾠδὰς τὰς τοῦ Πνεύματος, καὶ Σταυροῦ τὸ τρόπαιον ἀντιτάξασα, τὴν παρθενίαν ἐτήρησας· καὶ Μάρτυς ἀήττητος, προσηνέχθης τῷ Χριστῷ, ἱερώτατον σφάγιον· ὅθεν ἔτυχες, τῶν στεφάνων τῆς νίκης, Ἰουστίνα, τῶν Παρθένων καὶ Μαρτύρων, κεκοσμημένη φαιδρότησι."

meVE.MM.Stichera1123as78910.text = properties_gr_GR_cog.media.key

meVE.SticGlory.mode = misc_gr_GR_cog.Mode2
meVE.SticGlory.melody = ""
meVE.SticGlory.incipit = "Διδασκαλίαν τῆς πλάνης, καὶ γνῶσιν…"
meVE.SticGlory.text = "Διδασκαλίαν τῆς πλάνης, καὶ γνῶσιν τῶν εἰδώλων, ἀπεδύσω ἀληθῶς, καὶ Σταυρὸν ἠμφιάσω, ὅπλον εἰς σωτηρίαν, Κυπριανὲ παμμακάριστε. Ἔῤῥιψας τὰς βίβλους τὰς μαγικάς, τὸν ἄνθρωπον ἀπέθου τὸν παλαιόν, καὶ ᾤκησεν ἐν σοὶ θεογνωσίᾳ, ἡ Τριὰς ἡ ὁμοούσιος· Αὐτὴν ἱκέτευε Ἱεράρχα, σὺν Ἀγγέλοις τοῦ σωθῆναι ἡμᾶς."

meVE.SticTheotokion.mode = misc_gr_GR_cog.Mode2
meVE.SticTheotokion.melody = he.a.m2_gr_GR_cog.OteEkTouXylou.name
meVE.SticTheotokion.incipit = "Πάντων, θλιβομένων ἡ χαρά, καὶ…"
meVE.SticTheotokion.text = "Πάντων, θλιβομένων ἡ χαρά, καὶ ἀδικουμένων προστάτις, καὶ πενομένων τροφή, ξένων τε παράκλησις, χειμαζομένων λιμήν, ἀσθενούντων ἐπίσκεψις, καταπονουμένων, σκέπη καὶ ἀντίληψις, καὶ βακτηρία τυφλῶν, Μήτηρ τοῦ Θεοῦ τοῦ ὑψίστου, σὺ ὑπάρχεις. Ἄχραντε σπεῦσον, δυσωποῦμεν ῥύσασθαι τοὺς δούλους σου." //similar to oc.m2.d1_gr_GR_cog.ocSV.Aposticha1.text

meVE.SticStavrotheotokion.mode = misc_gr_GR_cog.Mode2
meVE.SticStavrotheotokion.melody = he.a.m2_gr_GR_cog.OteEkTouXylou.name
meVE.SticStavrotheotokion.incipit = "Ξύλῳ, τοῦ Σταυροῦ σε Ἰησοῦ…"
meVE.SticStavrotheotokion.text = "Ξύλῳ, τοῦ Σταυροῦ σε Ἰησοῦ, προσαναρτηθέντα ὁρῶσα ἡ Ἀπειρόγαμος, ἔκλαιε καὶ ἔλεγε· Τέκνον γλυκύτατον, ἵνα τί ἐγκατέλιπες, ἐμὲ τὴν τεκοῦσαν, μόνην φῶς ἀπρόσιτον, τοῦ προανάρχου Πατρός; σπεῦσον, καὶ δοξάσθητι ὅπως, δόξης ἐπιτύχωσι θείας, οἱ τὰ θεῖα πάθη σου δοξάζοντες."

meVE.AposGlory.mode = misc_gr_GR_cog.Mode2
meVE.AposGlory.melody = ""
meVE.AposGlory.incipit = "Τὸν φωστῆρα τὸν θεολαμπῆ, τὸν…"
meVE.AposGlory.text = "Τὸν φωστῆρα τὸν θεολαμπῆ, τὸν τοῦ Παύλου συνόμιλον, καὶ ἔργοις συμμέτοχον, Κυπριανὸν εὐφημήσωμεν· μέτʼ Ἀγγέλων γὰρ ἀγάλλεται, λαβὼν τὸ στέφος τῆς ἀφθαρσίας, παρὰ τοῦ μόνου Θεοῦ, καὶ πρεσβεύει, σωθῆναι τὰς ψυχὰς ἡμῶν."

meVE.AposTheotokion.mode = misc_gr_GR_cog.Mode2
meVE.AposTheotokion.melody = he.a.m2_gr_GR_cog.OteEkTouXylou.name
meVE.AposTheotokion.incipit = "Ἄλλην, κραταιὰν καταφυγήν, καὶ ἰσχύος…"
meVE.AposTheotokion.text = "Ἄλλην, κραταιὰν καταφυγήν, καὶ ἰσχύος πύργον καὶ τεῖχος, ἀκαταμάχητον, ὄντως οὐ κεκτήμεθα, εἰμὴ σὲ Πάναγνε, καὶ πρὸς σὲ καταφεύγομεν, καὶ σοὶ ἐκβοῶμεν· Δέσποινα βοήθησον, μὴ ἀπολώμεθα, δεῖξον, εἰς ἡμᾶς τὴν σὴν χάριν, καὶ τῆς δυναστείας τὴν δόξαν, καὶ τῆς εὐσπλαγχνίας σου τὸ μέγεθος."

meVE.AposStavrotheotokion.mode = misc_gr_GR_cog.Mode2
meVE.AposStavrotheotokion.melody = he.a.m2_gr_GR_cog.OteEkTouXylou.name
meVE.AposStavrotheotokion.incipit = me.m01.d15_gr_GR_cog.meVE.SticStavrotheotokion.incipit
meVE.AposStavrotheotokion.text = me.m01.d15_gr_GR_cog.meVE.SticStavrotheotokion.text

meVE.GeneralApolytikion1.mode = misc_gr_GR_cog.Mode4
meVE.GeneralApolytikion1.melody = he.ga_gr_GR_cog.KaiTroponMetochos.name
meVE.MM.GeneralApolytikion1.text = properties_gr_GR_cog.media.key
meVE.GeneralApolytikion1.name = "Κυπριανέ,"

meVE.Apolytikion2.poet = poets_gr_GR_cog.Gerasimou
meVE.Apolytikion2.mode = misc_gr_GR_cog.Mode3
meVE.Apolytikion2.melody = he.a.m3_gr_GR_cog.TheiasPisteos.name
meVE.Apolytikion2.incipit = "Θείας πίστεως, τῇ φωταυγείᾳ, σκότος…"
meVE.Apolytikion2.text = "Θείας πίστεως, τῇ φωταυγείᾳ, σκότος ἔλιπες, τῆς ἀσεβείας, καὶ φωστὴρ τῆς ἀληθείας γεγένησαι· ποιμαντικῶς γὰρ φαιδρύνας τὸν βίον σου, Κυπριανέ τῇ ἀθλήσει δεδόξασαι. Πάτερ Ὅσιε, τὸν Κτίστην ἡμῖν ἱλέωσαι, ὁμοῦ σὺν Ἰουστίνῃ τῇ θεόφρονι."

meMA.C1.acrostic.title = "Κανὼν τῶν Ἁγίων,"
meMA.C1.acrostic.notice1 = "οὗ ἡ Ἀκροστιχίς·"
meMA.C1.acrostic.text = "Μέλπω, μάκαρ, σοῦ τὴν κυπρίζουσαν χάριν."
meMA.C1.title = titles_gr_GR_cog.ti.CanonSaints
meMA.C1.verse = prayers_gr_GR_cog.pr.verse_SaintsM.text

meMA.Ode1C11.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode1C11.melody = he.h.m4_gr_GR_cog.ThalassisToErythraion.name
meMA.Ode1C11.incipit = "Μετὰ τῆς ὑπερκοσμίου…"
meMA.Ode1C11.text = "Μετὰ τῆς ὑπερκοσμίου τάξεως, νῦν αὐλιζόμενος, ὦ ἱερὰ καὶ θεία κεφαλή, οὐρανόθεν ἐπίβλεψον, καὶ τοὺς πιστῶς ὑμνοῦντάς σε, ταῖς σαῖς πρεσβείαις περιφρούρησον."

meMA.Ode1C12.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode1C12.melody = he.h.m4_gr_GR_cog.ThalassisToErythraion.name
meMA.Ode1C12.incipit = "Ἐν ζόφῳ τῆς ἀγνωσίας…"
meMA.Ode1C12.text = "Ἐν ζόφῳ τῆς ἀγνωσίας πρότερον, δεινῶς κρατούμενος, καὶ ψυχολέθροις πάθεσι σαρκός, ἀκρασίᾳ πυρούμενος, μεταβολὴν παράδοξον, εὗρες ἀθρόως παμμακάριστε."

meMA.Ode1C13.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode1C13.melody = he.h.m4_gr_GR_cog.ThalassisToErythraion.name
meMA.Ode1C13.incipit = "Λοιμῶν μὲν ἐπὶ καθέδραν…"
meMA.Ode1C13.text = "Λοιμῶν μὲν ἐπὶ καθέδραν πρότερον, Πάτερ ἐκάθισας, καὶ ἐν καθέδρᾳ πάλιν τὸν Χριστόν, πρεσβυτέρων ἐδόξασας, ἀλλοιωθεὶς σαφέστατα, τῇ τοῦ Ὑψίστου θείᾳ χάριτι."

meMA.Ode1C1T.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode1C1T.melody = he.h.m4_gr_GR_cog.ThalassisToErythraion.name
meMA.Ode1C1T.incipit = "Παρθένος μετὰ τὸν τόκον…"
meMA.Ode1C1T.text = "Παρθένος μετὰ τὸν τόκον ἔμεινας· αὐτὸν γὰρ τέτοκας, τὸν Ποιητὴν τῶν ὅλων καὶ Θεόν, ἀσυνήθως καὶ ξένως ἡμῖν, ἐπιφανέντα σώματι, Θεογεννῆτορ Μητροπάρθενε."

meMA.Ode3C11.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode3C11.melody = he.h.m4_gr_GR_cog.EffrainetaiEpiSoi.name
meMA.Ode3C11.incipit = "Ὡς Παῦλος πρὸς τὸν Χριστόν…"
meMA.Ode3C11.text = "Ὡς Παῦλος πρὸς τὸν Χριστόν, τὴν τῆς ψυχῆς μεταβαλὼν ἔφεσιν, Κυπριανὲ πάνσοφε, τούτου μαθητὴς ἐχρημάτισας."

meMA.Ode3C12.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode3C12.melody = he.h.m4_gr_GR_cog.EffrainetaiEpiSoi.name
meMA.Ode3C12.incipit = "Μνηστεύεταί σε Χριστός…"
meMA.Ode3C12.text = "Μνηστεύεταί σε Χριστός, ἀντὶ χιτῶνος τῶν παθῶν δόξης σοι, καταστολὴν δωρούμενος, καὶ ἀναγεννήσεως ἔνδυμα."

meMA.Ode3C13.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode3C13.melody = he.h.m4_gr_GR_cog.EffrainetaiEpiSoi.name
meMA.Ode3C13.incipit = "Ἀλείπτης ἀσκητικός…"
meMA.Ode3C13.text = "Ἀλείπτης ἀσκητικός, προγεγονὼς καρτερικὸς ὕστερον, Κυπριανὲ πανεύφημε, Μάρτυς ἀληθείας γεγένησαι."

meMA.Ode3C1T.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode3C1T.melody = he.h.m4_gr_GR_cog.EffrainetaiEpiSoi.name
meMA.Ode3C1T.incipit = "Κυρίως καὶ ἀληθῶς…"
meMA.Ode3C1T.text = "Κυρίως καὶ ἀληθῶς, σὲ Θεοτόκον οἱ πιστοὶ σέβομεν· σὺ γὰρ Θεὸν τέτοκας, σάρκα γεγονότα πανάμωμε."

meMA.Ode3H.ode = misc_gr_GR_cog.Ode4
meMA.Ode3H.mode = misc_gr_GR_cog.Mode3
meMA.Ode3H.melody = he.h.m4_gr_GR_cog.EffrainetaiEpiSoi.melody
meMA.Ode3H.incipit = he.h.m4_gr_GR_cog.EffrainetaiEpiSoi.incipit
meMA.Ode3H.text = he.h.m4_gr_GR_cog.EffrainetaiEpiSoi.text

meMA.Ode3Kathisma1.mode = misc_gr_GR_cog.Mode8
meMA.Ode3Kathisma1.melody = he.a.m8_gr_GR_cog.TinSofianKaiLogon.name
meMA.Ode3Kathisma1.incipit = "Παιδευθεὶς ἐν τῇ πλάνῃ…"
meMA.Ode3Kathisma1.text = "Παιδευθεὶς ἐν τῇ πλάνῃ ἐπιμελῶς, ὡς ὁ Παῦλος ἐκλήθης ἐξ οὐρανοῦ, σταυρῷ ὁδηγούμενος, πρὸς τὸ φῶς τὸ τῆς γνώσεως· τῆς γὰρ σεμνῆς Παρθένου, τῷ πόθῳ φλεγόμενος, διʼ αὐτῆς ἡρμόσθης, τῷ Πλάστη τῆς κτίσεως· ὄθεν θριαμβεύσας, τοῦ ἐχθροῦ τὸ ἀνίσχυρον, σὺν αὐτῇ κατηξίωσαι, τοῦ χοροῦ τῶν Μαρτύρων, Κυπριανὲ ἱερώτατε, πρέσβευε Χριστῷ τῷ Θεῷ, τῶν πταισμάτων ἄφεσιν δωρήσασθαι, τοῖς ἑορτάζουσι πόθῳ, τὴν ἁγίαν μνήμην σου."

meMA.Ode3KathismaT.mode = misc_gr_GR_cog.Mode8
meMA.Ode3KathismaT.melody = he.a.m8_gr_GR_cog.TinSofianKaiLogon.name
meMA.Ode3KathismaT.incipit = "Λογισμοῖς ὀλισθαίνων…"
meMA.Ode3KathismaT.text = "Λογισμοῖς ὀλισθαίνων τοῖς πονηροῖς, εἰς βυθὸν κατηνέχθην ἁμαρτιῶν· καὶ στένων κραυγάζω σοι, ἐκ καρδίας Πανάχραντε· Ἐν ἐμοὶ θαυμάστωσον, τὸ πλούσιον ἔλεος, καὶ τῆς σῆς εὐσπλαγχνίας, τὸ ἄπειρον πέλαγος, καὶ τῶν οἰκτιρμῶν σου, τὸν ἀμέτρητον πλοῦτον· καὶ δός μοι μετάνοιαν, καὶ τοῦ βίου διόρθωσιν, ἵνα πόθῳ κραυγάζω σοι· Πρέσβευε τῷ σῷ Υἱῷ καὶ Θεῷ, τῶν πταισμάτων ἄφεσιν δοθῆναί μοι· σὲ γὰρ ἔχω ἐλπίδα, ὁ ἀνάξιος δοῦλός σου."

meMA.Ode3KathismaSt.mode = misc_gr_GR_cog.Mode8
meMA.Ode3KathismaSt.melody = he.a.m8_gr_GR_cog.TinSofianKaiLogon.name
meMA.Ode3KathismaSt.incipit = oc.m8.d4_gr_GR_cog.ocMA.Kathisma13.incipit
meMA.Ode3KathismaSt.text = oc.m8.d4_gr_GR_cog.ocMA.Kathisma13.text

meMA.Ode4C11.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode4C11.melody = he.h.m4_gr_GR_cog.EparthentaSeIdousa.name
meMA.Ode4C11.incipit = "Ἀνανήψας θεοφάντορ…"
meMA.Ode4C11.text = "Ἀνανήψας θεοφάντορ τῆς πρὶν μανίας, δαιμονικὴν ἀπάτην, καὶ ψυχόλεθρον πλάνην, πᾶσαν ἐθριάμβευσας, καὶ χαίρων ἐκραύγαζες· Δόξα τῇ δυνάμει σου Κύριε."

meMA.Ode4C12.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode4C12.melody = he.h.m4_gr_GR_cog.EparthentaSeIdousa.name
meMA.Ode4C12.incipit = "Ῥωμαλέως Ἰουστίνα…"
meMA.Ode4C12.text = "Ῥωμαλέως Ἰουστίνα κραταιουμένη, καὶ τὴν ἁγνὴν Παρθένον καὶ κεχαριτωμένην, σφόδρα ἱκετεύουσα, τοὺς βρόχους διέφυγες, καὶ τὰ τῶν ἐχθρῶν μηχανήματα."

meMA.Ode4C13.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode4C13.melody = he.h.m4_gr_GR_cog.EparthentaSeIdousa.name
meMA.Ode4C13.incipit = "Στηριζομένη τῇ πίστει…"
meMA.Ode4C13.text = "Στηριζομένη τῇ πίστει τοῦ σοῦ Νυμφίου, καὶ τοῦ Σταυροῦ τὴν δύναμιν, περιβεβλημένη, δαίμοσιν ἀνάλωτος, βοῶσα διέμεινας· Δόξα τῇ δυνάμει σου Κύριε."

meMA.Ode4C1T.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode4C1T.melody = he.h.m4_gr_GR_cog.EparthentaSeIdousa.name
meMA.Ode4C1T.incipit = "Ὁ κατὰ φύσιν…"
meMA.Ode4C1T.text = "Ὁ κατὰ φύσιν ἐλεύθερος τὴν ἰδίαν, δούλου μορφὴν ἑπτώχευσε, χρηστότητος πλούτῳ, Μῆτερ ἀειπάρθενε, ἐκ σοῦ καθʼ ὑπόστασιν, ὅλον προσλαβὼν τὸ ἀνθρώπινον."

meMA.Ode5C11.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode5C11.melody = he.h.m4_gr_GR_cog.SyKyrieMouFos.name
meMA.Ode5C11.incipit = "Ὑπέρμαχον σαφῶς…"
meMA.Ode5C11.text = "Ὑπέρμαχον σαφῶς, τὴν ἁγνὴν θεομήτορα, πλουτίσασα Ἰουστίνα, τὴν σεπτὴν παρθενίαν, ἐτήρησας ἀμώμητον."

meMA.Ode5C12.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode5C12.melody = he.h.m4_gr_GR_cog.SyKyrieMouFos.name
meMA.Ode5C12.incipit = "Τὸ ἔμψυχον Θεοῦ…"
meMA.Ode5C12.text = "Τὸ ἔμψυχον Θεοῦ, καὶ τερπνότατον ἄγαλμα, τιμήσωμεν Ἰουστίναν, τὸ ἀπόθετον κάλλος, τὸ ἄσυλον ἀνάθημα."

meMA.Ode5C13.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode5C13.melody = he.h.m4_gr_GR_cog.SyKyrieMouFos.name
meMA.Ode5C13.incipit = "Ἡ νύμφη τοῦ Χριστοῦ…"
meMA.Ode5C13.text = "Ἡ νύμφη τοῦ Χριστοῦ, ἡ γνησία καὶ ἄσπιλος, τὴν ἄθλησιν τῇ ἀσκήσει, προσλαβοῦσα δικαίως, διπλοῦν στέφος κομίζεται."

meMA.Ode5C1T.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode5C1T.melody = he.h.m4_gr_GR_cog.SyKyrieMouFos.name
meMA.Ode5C1T.incipit = "Νοῦς οὐκ ἀγγελικός…"
meMA.Ode5C1T.text = "Νοῦς οὐκ ἀγγελικός, οὐκ ἀνθρώπινος δύναται, τὸ ἄφραστον ἑρμηνεῦσαι, καὶ παράδοξον θαῦμα, τοῦ τόκου σου πανάχραντε."

meMA.Ode6C11.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode6C11.melody = he.h.m4_gr_GR_cog.ThysoSoiMetaFonis.name
meMA.Ode6C11.incipit = "Κακίας τὸν πυθμένα…"
meMA.Ode6C11.text = "Κακίας, τὸν πυθμένα τὸν κάτω κατείληφας· τῆς ἀκροτάτης δὲ πάλιν, ἀρετῆς ἀνῆλθες Πάτερ εἰς ὕψος, παραδόξως, ἀλλοιωθεὶς τῷ θείῳ Βαπτίσματι."

meMA.Ode6C12.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode6C12.melody = he.h.m4_gr_GR_cog.ThysoSoiMetaFonis.name
meMA.Ode6C12.incipit = "Ὑπῆρξας ὑπηρέτης…"
meMA.Ode6C12.text = "Ὑπῆρξας, ὑπηρέτης δαιμόνων τὸ πρότερον· ἀλλὰ Χριστοῦ θεοφάντορ, μαθητὴς ἐσύστερον ἀνεδείχθης, διαθέσει, τῶν ὀρεκτῶν ποθήσας τὸ ἔσχατον."

meMA.Ode6C13.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode6C13.melody = he.h.m4_gr_GR_cog.ThysoSoiMetaFonis.name
meMA.Ode6C13.incipit = "Πολλοὺς σύ, τῷ Δεσπότῃ…"
meMA.Ode6C13.text = "Πολλοὺς σύ, τῷ Δεσπότῃ προσήγαγες Μάρτυρας, πραγματειῶν τὴν μεγίστην, ὑποφαίνων αἵματι βραχυτάτῳ, βασιλείαν, τῶν οὐρανῶν θεόφρον ὠνήσασθαι."

meMA.Ode6C1T.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode6C1T.melody = he.h.m4_gr_GR_cog.ThysoSoiMetaFonis.name
meMA.Ode6C1T.incipit = "Ῥῆξόν μου τὰς σειρὰς…"
meMA.Ode6C1T.text = "Ῥῆξόν μου, τὰς σειρὰς τῶν πταισμάτων Θεόνυμφε, τῆς ἁμαρτίας τοῦ νόμου, τῆς ζωῆς τῷ νόμῳ ἐλευθερίαν, δωρουμένη, ἡ τὸν δοτῆρα τοῦ νόμου κυήσασα."

meMA.Ode6H.ode = misc_gr_GR_cog.Ode6
meMA.Ode6H.mode = misc_gr_GR_cog.Mode4
meMA.Ode6H.melody = he.h.m4_gr_GR_cog.ThysoSoiMetaFonis.melody
meMA.Ode6H.incipit = he.h.m4_gr_GR_cog.ThysoSoiMetaFonis.incipit
meMA.Ode6H.text = he.h.m4_gr_GR_cog.ThysoSoiMetaFonis.text

meMA.Kontakion1.mode = misc_gr_GR_cog.Mode8
meMA.Kontakion1.melody = he.a.m8_gr_GR_cog.OsAparchas.name
meMA.Kontakion1.incipit = "Ὡς ἱεράρχην τίμιον…"
meMA.Kontakion1.text = "Ὡς ἱεράρχην τίμιον, καὶ ἀθλητὴν στεῤῥότατον, ἡ οἰκουμένη ἀξίως γεραίρει σε, Κυπριανὲ ἀοίδιμε, καὶ τοῖς ὕμνοις δοξάζει, τὴν ἁγίαν σου μνήμην, αἰτοῦσα πάντοτε, πταισμάτων ἄφεσιν, διὰ σοῦ δωρηθῆναι τοῖς μέλπουσιν· Ἀλληλούϊα."

meMA.Oikos1.text = "Τὸν σοφὸν Ἱεράρχην τιμήσωμεν, ὡς ποιμένα σοφὸν καὶ διδάσκαλον· ὅτι ἐξ ἀκανθῶν πλάνης ἤνθησεν, ὥσπερ ῥόδον τερπνότατον, καὶ ἡμᾶς τοὺς πιστοὺς κατεμύρισεν, ἰαμάτων ὀδμαῖς καὶ βολαῖς θαυμάτων· ὥστε ψάλλειν ἡμᾶς τοῦ Δαυῒδ τὴν ᾠδήν· Ἀλληλούϊα."

meMA.Ode7C11.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode7C11.melody = he.h.m4_gr_GR_cog.ODiasosasEnPyri.name
meMA.Ode7C11.incipit = "Ἱεραρχίας ἐπιβάς…"
meMA.Ode7C11.text = "Ἱεραρχίας ἐπιβάς, ἱεραρχικώτατον τύπον, τοῖς ἱεράρχαις σεαυτόν, καὶ εἰκόνα κραυγάζων ἀνέδειξας· Ὑπερύμνητε Κύριε, ὁ Θεὸς ὁ τῶν Πατέρων εὐλογητὸς εἶ."

meMA.Ode7C12.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode7C12.melody = he.h.m4_gr_GR_cog.ODiasosasEnPyri.name
meMA.Ode7C12.incipit = "Ζωαρχικῆς τῆς δεξιᾶς…"
meMA.Ode7C12.text = "Ζωαρχικῆς τῆς δεξιᾶς, ὄντως σὴ ἀλλοίωσις Πάτερ, Κυπριανέ· ἣ καὶ σοφόν, θεηγόρον εἰργάσατο ψάλλοντα· Ὑπερύμνητε Κύριε, ὁ Θεὸς ὁ τῶν Πατέρων εὐλογητὸς εἶ."

meMA.Ode7C13.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode7C13.melody = he.h.m4_gr_GR_cog.ODiasosasEnPyri.name
meMA.Ode7C13.incipit = "Ὅπλον ἀήττητον ἡμῖν…"
meMA.Ode7C13.text = "Ὅπλον ἀήττητον ἡμῖν, κατὰ τοῦ ἀλάστορος μάκαρ, σὺ ἀνεδείχθης τὰς αὐτοῦ, στηλιτεύων ἀπάτας τοῖς μέλπουσιν· Ὑπερύμνητε Κύριε, ὁ Θεὸς ὁ τῶν Πατέρων εὐλογητός εἶ."

meMA.Ode7C1T.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode7C1T.melody = he.h.m4_gr_GR_cog.ODiasosasEnPyri.name
meMA.Ode7C1T.incipit = "Ὑπερουράνιοι χοροί…"
meMA.Ode7C1T.text = "Ὑπερουράνιοι χοροί, καὶ οἱ ἐπουράνιοι δῆμοι, ὑμνολογοῦσι τὸν ἐκ σοῦ, Θεοτόκε τεχθέντα κραυγάζοντες· Ὑπερύμνητε Κύριε, ὁ Θεὸς ὁ τῶν Πατέρων εὐλογητὸς εἶ."

meMA.Ode8C11.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode8C11.melody = he.h.m4_gr_GR_cog.EffranthitiIerousalim.name
meMA.Ode8C11.incipit = "Σοφώτατα τῆς ἑλληνικῆς…"
meMA.Ode8C11.text = "Σοφώτατα τῆς ἑλληνικῆς, καταπεφρόνηκας σοφίας ἔνδοξε· τῶν Ἀποστόλων δὲ τὸν θεῖον φθόγγον, Πνεύματι τὸν κρουόμενον Πάτερ, καὶ πυρίνῃ γλώσσῃ τρανῶς ἐκφωνούμενον, ἀνεβόας· Εὐλογεῖτε τὰ ἔργα τὸν Κύριον."

meMA.Ode8C12.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode8C12.melody = he.h.m4_gr_GR_cog.EffranthitiIerousalim.name
meMA.Ode8C12.incipit = "Ἀνέδραμες πρὸς τὰ ὑψηλά…"
meMA.Ode8C12.text = "Ἀνέδραμες πρὸς τὰ ὑψηλά, καὶ ὑπερκόσμια φαιδρῶς σκηνώματα, Χριστῷ διʼ αἵματος ὡς ἱερεῖον, ἄμωμον προσαγόμενος Πάτερ, καὶ θυσία ζῶσα δεκτὴ καὶ εὐάρεστος, ἀναμέλπων· Εὐλογεῖτε τὰ ἔργα τὸν Κύριον."

meMA.Ode8C13.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode8C13.melody = he.h.m4_gr_GR_cog.EffranthitiIerousalim.name
meMA.Ode8C13.incipit = "Νικώμεναι ὑπὸ τῆς ἐν σοί…"
meMA.Ode8C13.text = "Νικώμεναι ὑπὸ τῆς ἐν σοί, σκηνούσης χάριτος δαιμόνων φάλαγγες, ἀποδιώκονται, καὶ δραπετεύει, πάθη Κυπριανὲ τῶν νοσούντων· οἱ πιστοὶ δὲ θείου φωτὸς ἐμφορούμενοι, βοῶμεν· Εὐλογεῖτε τὰ ἔργα τὸν Κύριον."

meMA.Ode8C1T.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode8C1T.melody = he.h.m4_gr_GR_cog.EffranthitiIerousalim.name
meMA.Ode8C1T.incipit = "Χορεύουσι σὺν ἀγγελικαῖς…"
meMA.Ode8C1T.text = "Χορεύουσι σὺν ἀγγελικαῖς, Ἁγνὴ Δυνάμεσιν ἐπὶ τῷ τόκῳ σου, οἱ Θεομήτορα καὶ Θεοτόκον, πίστει σε καταγγέλλοντες, σὺ γὰρ τὸν Δεσπότην καὶ λυτρωτὴν ἡμῶν τέτοκας, ᾧ βοῶμεν· Εὐλογεῖτε τὰ ἔργα τὸν Κύριον."

meMA.Ode8H.ode = misc_gr_GR_cog.Ode8
meMA.Ode8H.mode = misc_gr_GR_cog.Mode4
meMA.Ode8H.melody = he.h.m4_gr_GR_cog.EffranthitiIerousalim.melody
meMA.Ode8H.incipit = he.h.m4_gr_GR_cog.EffranthitiIerousalim.incipit
meMA.Ode8H.text = he.h.m4_gr_GR_cog.EffranthitiIerousalim.text

meMA.Ode9C11.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode9C11.melody = he.h.m4_gr_GR_cog.EvaMenToTisParakois.name
meMA.Ode9C11.incipit = " Ἄνωθεν ἡμᾶς…"
meMA.Ode9C11.text = "Ἄνωθεν ἡμᾶς τοὺς εὐσεβῶς ὑμνοῦντάς σε, ἐποπτεύων θείᾳ χάριτι, Κυπριανὲ σαῖς ἱκεσίαις, ἐν ὅπλῳ εὐδοκίας στεφάνωσον, εἰρήνην καὶ σωτηρίαν βραβεύων ἡμῖν, ὡς ἱεράρχης ἐνθεώτατος."

meMA.Ode9C12.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode9C12.melody = he.h.m4_gr_GR_cog.EvaMenToTisParakois.name
meMA.Ode9C12.incipit = "Ῥεῖθρον ἰαμάτων…"
meMA.Ode9C12.text = "Ῥεῖθρον ἰαμάτων δαψιλῶς ἡ κόνις σου, τοῖς ποθοῦσιν ἀναδέδεικται· σὲ γὰρ γυναῖκες θεοφόροι, ὡς θεῖον θησαυρὸν ὑποδέχονται, καὶ πρόθυμοι φανεροῦσι κρυπτόμενον, ἵνα σοῦ πάντες ἀπολαύσωμεν."

meMA.Ode9C13.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode9C13.melody = he.h.m4_gr_GR_cog.EvaMenToTisParakois.name
meMA.Ode9C13.incipit = "Ἴθυνον ἡμῶν πρὸς τὸν Χριστὸν…"
meMA.Ode9C13.text = "Ἴθυνον ἡμῶν πρὸς τὸν Χριστὸν πανόλβιε, τὴν πορείαν διὰ πράξεως, καὶ θεαρέστου πολιτείας, καὶ εἰλικρινεστάτης καθάρσεως, τὴν ὄντως θείαν γαλήνην αἰτούμενος, ὡς ἱεράρχης συμπαθέστατος."

meMA.Ode9C1T.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode9C1T.melody = he.h.m4_gr_GR_cog.EvaMenToTisParakois.name
meMA.Ode9C1T.incipit = "Νεύρωσον ἡμῶν τὴν τῆς ψυχῆς…"
meMA.Ode9C1T.text = "Νεύρωσον ἡμῶν τὴν τῆς ψυχῆς ἀσθένειαν, Θεομῆτορ τῇ δυνάμει σου· λῦσον τὸ κράτος Παναγία, τῶν ἐπιτιθεμένων τοῖς δούλοις σου, ἡ κόσμῳ δικαιοσύνης τὸν Ἥλιον, ἀνερμηνεύτως ἀνατείλασα."

meMA.Ode9H.ode = misc_gr_GR_cog.Ode9
meMA.Ode9H.mode = misc_gr_GR_cog.Mode4
meMA.Ode9H.melody = he.h.m4_gr_GR_cog.EvaMenToTisParakois.melody
meMA.Ode9H.incipit = he.h.m4_gr_GR_cog.EvaMenToTisParakois.incipit
meMA.Ode9H.text = he.h.m4_gr_GR_cog.EvaMenToTisParakois.text

meMA.Exaposteilarion1.mode = misc_gr_GR_cog.Mode2
meMA.Exaposteilarion1.melody = he.a.m2_gr_GR_cog.GynaikesAkoutisthite.name
meMA.Exaposteilarion1.incipit = "Τὸν πρότερον ὑπάρχοντα…"
meMA.Exaposteilarion1.text = "Τὸν πρότερον ὑπάρχοντα, κακίας μύστην ὕστερον, διὰ τιμίας Παρθένου, Ἀρχιερέα φανέντα, Κυπριανὸν τὸν μέγιστον, συμφώνως μακαρίσωμεν, καὶ πρὸς αὐτὸν βοήσωμεν· Ταῖς σαῖς λιταῖς εὐμενίζου, ἡμῖν τὸν πάντων Δεσπότην."

meMA.ExapTheotokion.mode = misc_gr_GR_cog.Mode2
meMA.ExapTheotokion.melody = he.a.m2_gr_GR_cog.GynaikesAkoutisthite.name
meMA.ExapTheotokion.incipit = "Χαρὰν τὴν ἀνεκλάλητον…"
meMA.ExapTheotokion.text = "Χαρὰν τὴν ἀνεκλάλητον, πάναγνε ἡ κυήσασα, χαρᾶς ἡμᾶς αἰωνίου, μετόχους ποίησον πάντας, Παρθένε θεονύμφευτε, ἡ σκέπη τὸ κραταίωμα, τῶν εὐσεβῶς βοώντων σοι· Μὴ ἐπιλάθῃ σῶν δούλων, Θεοκυῆτορ Μαρία."

meMA.MM.Ve1123as5678.text = properties_gr_GR_cog.media.key

meMA.AposGlory.mode = misc_gr_GR_cog.Mode8
meMA.AposGlory.melody = ""
meMA.AposGlory.incipit = "Ὁ τῆς κακίας πρότερον…"
meMA.AposGlory.text = "Ὁ τῆς κακίας πρότερον, ἀκριβὴς ὑπέρμαχος, τῆς Ἐκκλησίας ὕστερον, ἀληθὴς διδάσκαλος· ὁ λειτουργὸς τῆς πλάνης τῶν εἰδώλων, ἀρχιερεὺς τῆς πίστεως ἐφάνη· καὶ ὁ ἀκόλαστος, σώφρων διὰ τιμίας Παρθένου. Αμφοτέρων ταῖς εὐχαῖς, σῶσον ἡμᾶς Κύριε."

meMA.AposTheotokion.mode = misc_gr_GR_cog.Mode8
meMA.AposTheotokion.melody = he.a.m8_gr_GR_cog.OTouParadoxou.name
meMA.AposTheotokion.incipit = "Πῶς σου τὴν χάριν…"
meMA.AposTheotokion.text = "Πῶς σου τὴν χάριν ὑμνήσαιμι, καὶ τὴν πολλὴν πρὸς ἐμέ, τὸν ἀνάξιον δοῦλόν σου, καθʼ ἑκάστην πρόνοιαν, ἣν σαφῶς ἐπιδείκνυσαι; πῶς δέ σου φράσω τὴν ἀγαθότητα, καὶ τὴν ποικίλην ὄντως κυβέρνησιν; σὺ οὖν καὶ ἔτι νῦν, εἰς ἀεί μου πρόστηθι, παντὸς κακοῦ, ζῶντα καὶ θανόντα με, ἐκλυτρουμένη Σεμνή."

meMA.AposStavrotheotokion.mode = misc_gr_GR_cog.Mode8
meMA.AposStavrotheotokion.melody = he.a.m8_gr_GR_cog.OTouParadoxou.name
