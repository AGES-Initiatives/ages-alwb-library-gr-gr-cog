meVE.Stichera01.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.Stichera01.greekmelody = he.a.m4_gr_GR_cog.heAU.OsGennaionEnMartysi.greekname
meVE.Stichera01.melody = he.a.m4_gr_GR_cog.heAU.OsGennaionEnMartysi.name
meVE.Stichera01.incipit = "Τὴν ὑπέρτιμον ἄθλησιν, τὰ γενναῖα…"
meVE.Stichera01.text = "Τὴν ὑπέρτιμον ἄθλησιν, τὰ γενναῖα παλαίσματα, τῶν μαρτύρων ἔνδοξε θεασάμενος, αὐτομολεῖς πρὸς τὰ σκάμματα, ἀνδρείῳ φρονήματι, ἀφειδήσας τῆς σαρκός, διʼ ἀγάπην τὴν ἔνθεον· ὅθεν ἤνυσας, τοὺς γενναίους ἀγῶνας, ταπεινώσας, τὴν ὀφρὺν τοῦ ἀντιπάλου, Ἀδριανὲ καρτερώτατε."

meVE.Stichera02.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.Stichera02.melody = he.a.m4_gr_GR_cog.heAU.OsGennaionEnMartysi.name
meVE.Stichera02.incipit = "Ταῖς εἱρκταῖς συγκλειόμενος, τοῖς βουνεύροις…"
meVE.Stichera02.text = "Ταῖς εἱρκταῖς συγκλειόμενος, τοῖς βουνεύροις τυπτόμενος, τοῖς κλοιοῖς ἀοίδιμε βαρυνόμενος, καὶ τοῖς μοχλοῖς συνθλιβόμενος, σὺν πλείοσι μάρτυσι, σὺν αὐτοῖς τῶν ἀγαθῶν, οὐρανίων ἐπέτυχες, ἐπαλείφουσαν, τὴν ὁμόζυγον ἔχων Ναταλίαν, τὴν Θεῷ πεποθημένην, Ἀδριανὲ γενναιότατε."

meVE.Stichera03.mode = miscellanea_gr_GR_cog.misc.Mode4
meVE.Stichera03.melody = he.a.m4_gr_GR_cog.heAU.OsGennaionEnMartysi.name
meVE.Stichera03.incipit = "Τὸν Ἀδὰμ ἡ ὁμόζυγος, Παραδείσου…"
meVE.Stichera03.text = "Τὸν Ἀδὰμ ἡ ὁμόζυγος, Παραδείσου ἐξώρισε, συμβουλίᾳ ὄφεως, Ναταλία δέ, Ἀδριανὸν πρὸς Παράδεισον, πανσόφως εἰσήγαγεν, ἱεραῖς προσλαλιαῖς, νουθετοῦσα διδάσκουσα, τὰ ἐπίπονα, τῆς ἀθλήσεως φέρειν οὐρανίους, ἀντιδόσεις προξενοῦντα, καὶ αἰωνίζουσαν εὔκλειαν."

meVE.SticGlory.poet = poets_gr_GR_cog.po.EfraimKarias
meVE.SticGlory.mode = miscellanea_gr_GR_cog.misc.Mode1
meVE.SticGlory.melody = ""
meVE.SticGlory.incipit = "Ζῆλος ἀνδρὸς εὐσεβοῦς, εἵλκυσε γυναῖκα…"
meVE.SticGlory.text = "Ζῆλος ἀνδρὸς εὐσεβοῦς, εἵλκυσε γυναῖκα θεοφιλῆ, πρὸς παραίνεσιν φαιδράν· Ἀδριανὸς γὰρ ὁ πανάριστος, Ναταλίας τῶν ῥημάτων ὑπαχθείς, ἀθλήσεως τὸν δρόμον ἐκτετέλεκεν· Ὢ γυναικὸς θεοφιλοῦς! οὐχ ὡς γὰρ Εὔα τῷ, Ἀδὰμ ἤνεγκε φθοράν, ἀλλὰ ζωὴν ἄληκτον τῷ συζύγῳ προεξένησε. Ταύτην σὺν τῷ ἀνδρὶ ἐπαινοῦντες, βοήσωμεν Χριστῷ· Δὸς ἡμῖν βοήθειαν, ταῖς πρεσβείαις τῶν Ἁγίων σου."

meVE.SticTheotokion.mode = miscellanea_gr_GR_cog.misc.Mode1
meVE.SticTheotokion.greekmelody = he.a.m1_gr_GR_cog.heAU.TonOuranionTagmaton.greekname
meVE.SticTheotokion.melody = he.a.m1_gr_GR_cog.heAU.TonOuranionTagmaton.name
meVE.SticTheotokion.incipit = "Ἐκ τῆς ἁγίας εἰκόνος τῆς…"
meVE.SticTheotokion.text = "Ἐκ τῆς ἁγίας εἰκόνος τῆς σῆς Πανάχραντε, ἰάσεις ἰαμάτων, χορηγοῦνται ἀφθόνως, τοῖς πίστει προσιοῦσιν· ὅθεν κἀμοῦ, τὰς ἀσθενείας ἐπίσκεψαι, καὶ τὴν ψυχήν μου ἐλέησον ἀγαθή, καὶ τὸ σῶμά μου θεράπευσον."

meVE.SticStavrotheotokion.mode = miscellanea_gr_GR_cog.misc.Mode1
meVE.SticStavrotheotokion.greekmelody = he.a.m1_gr_GR_cog.heAU.TonOuranionTagmaton.greekname
meVE.SticStavrotheotokion.melody = he.a.m1_gr_GR_cog.heAU.TonOuranionTagmaton.name
meVE.SticStavrotheotokion.incipit = me.m06.d07_gr_GR_cog.meVE.SticStavrotheotokion.incipit
meVE.SticStavrotheotokion.text = me.m06.d07_gr_GR_cog.meVE.SticStavrotheotokion.text

meVE.Aposticha1.mode = miscellanea_gr_GR_cog.misc.Mode1
meVE.Aposticha1.greekmelody = he.a.m1_gr_GR_cog.heAU.PaneffimoiMartyres.greekname
meVE.Aposticha1.melody = he.a.m1_gr_GR_cog.heAU.PaneffimoiMartyres.name
meVE.Aposticha1.incipit = "Πορφύραν ἐξ αἵματος τοῦ σοῦ…"
meVE.Aposticha1.text = "Πορφύραν ἐξ αἵματος τοῦ σοῦ, Μάρτυς κατεφοίνιξας, στολὴν μεθʼ ἧς τῷ Δεσπότῃ σου, ὡς ἡ ὑπόσχεσις, νῦν συμβασιλεύεις, ἄθλων ταῖς λαμπρότησι, καὶ θείαις καλλοναῖς σεμνυνόμενος. Χριστὸν ἱκέτευε, δωρηθῆναι ταῖς ψυχαῖς ἡμῶν, τὴν εἰρήνην, καὶ τὸ μέγα ἔλεος."

meVE.Aposticha2.mode = miscellanea_gr_GR_cog.misc.Mode1
meVE.Aposticha2.melody = he.a.m1_gr_GR_cog.heAU.PaneffimoiMartyres.name
meVE.Aposticha2.incipit = "Ἀδριανὲ Μάρτυς τὰς ὁδούς, τῶν…"
meVE.Aposticha2.text = "Ἀδριανὲ Μάρτυς τὰς ὁδούς, τῶν παθῶν ἐξέκλινας, τῷ σωτηρίῳ ἀγόμενος, δρόμῳ πανεύφημε· καὶ πρὸς τὰς ἀφθάρτους, μονὰς κατεσκήνωσας, σὺν πᾶσι τοῖς Χριστῷ πειθαρχήσασι· μεθʼ ὧν ἱκέτευε, δωρηθῆναι ταῖς ψυχαῖς ἡμῶν, τὴν εἰρήνην, καὶ τὸ μέγα ἔλεος."

meVE.Aposticha3.mode = miscellanea_gr_GR_cog.misc.Mode1
meVE.Aposticha3.melody = he.a.m1_gr_GR_cog.heAU.PaneffimoiMartyres.name
meVE.Aposticha3.incipit = "Ὢ θαῦμα καινὸν καὶ ἀληθῶς…"
meVE.Aposticha3.text = "Ὢ θαῦμα καινὸν καὶ ἀληθῶς, μέγα καὶ παράδοξον! πῶς Ναταλία ἡ πάνσοφος, τὸν ταύτης σύζυγον, πείθει ὑπομεῖναι, ξίφη κατατέμνοντα; τίς εἶδέ τι τοιοῦτον; τίς ἤκουσεν; ὅν περ ἐκέκτητο, ὀφθαλμὸν βίου πρὸς θάνατον, ἐπεπόθει, προδοῦναι σωτήριον."

meVE.AposGlory.mode = miscellanea_gr_GR_cog.misc.Mode6
meVE.AposGlory.melody = ""
meVE.AposGlory.incipit = "Ὢ ζεῦγος ἄμωμον, καὶ ἐκλεκτὸν…"
meVE.AposGlory.text = "Ὢ ζεῦγος ἄμωμον, καὶ ἐκλεκτὸν τῷ Κυρίῳ! ὢ πεποθημένη δυάς, καὶ πεφιλημένη Χριστῷ, ὢ συζυγία ἀρίστη καὶ μακαρία! Τίς οὐκ ἐκπλαγῇ ἐν τούτῳ ἀκουτισθείς, τὰς τούτων ὑπὲρ ἄνθρωπον πράξεις! πῶς τὸ θῆλυ ἠνδρίσατο κατὰ τοῦ πικροῦ τυράννου, καὶ τὸν ταύτης σύνευνον ἐνεύρωσε, μὴ ὑπενδοῦναι τοῖς δεινοῖς, ἀλλʼ ὑπὲρ τῆς πίστεως ἑλέσθαι, τὸ θανεῖν ὑπὲρ τὸ ζῆν; Ὢ θεοπλόκων ῥημάτων Ναταλίας τῆς σοφῆς! ὢ παραινέσεων θείων, τοὺς οὐρανοὺς διασχουσῶν, καὶ πρὸς αὐτὸν τὸν θρόνον τοῦ μεγάλου βασιλέως, Ἀδριανὸν τὸν ἔνδοξον γνώριμον καταστησασῶν! Ἀλλʼ ὦ ξυνωρὶς ἁγία, ὑπὲρ ἡμῶν τῷ Θεῷ ἱκετεύσατε, τῶν ἐκ πόθου τελούντων τὴν μνήμην ὑμῶν, πειρασμῶν ῥυσθῆναι καὶ πάσης θλίψεως."

