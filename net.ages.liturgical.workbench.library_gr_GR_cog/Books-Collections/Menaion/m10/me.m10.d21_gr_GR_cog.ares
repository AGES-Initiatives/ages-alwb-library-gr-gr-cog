meVE.Apolytikion1ger.poet = poets_gr_GR_cog.po.Gerasimou
meVE.Apolytikion1ger.mode = miscellanea_gr_GR_cog.misc.Mode1
meVE.Apolytikion1ger.greekmelody = he.a.m1_gr_GR_cog.heAU.TisErimouPolitis.greekname
meVE.Apolytikion1ger.melody = he.a.m1_gr_GR_cog.heAU.TisErimouPolitis.name
meVE.Apolytikion1ger.incipit = "Ἐγκρατείας τῇ αἴγλῃ λαμπρυνθεὶς τὴν…"
meVE.Apolytikion1ger.text = "Ἐγκρατείας τῇ αἴγλῃ λαμπρυνθεὶς τὴν διάνοιαν, ἤστραψας θαυμάτων ἀκτρίνας Ἱλαρίων Πατὴρ ἡμῶν, καὶ γέγονας φωστὴρ περιφανής, καὶ στῦλος εὐσεβείας θεαυγής, καταυγάζων τῇ ἐνθέῳ σου βιοτῇ, τοὺς πίστει προσιόντας σοι. Δόξα τῷ δεδωκότι σοι ἰσχύν, δόξα τῷ σὲ στεφανώσαντι, δόξα τῷ ἐνεργοῦντι διὰ σοῦ, πᾶσιν ἰάματα."

meVE.GeneralApolytikion1.name = "Ἱλαρίων Πατὴρ ἡμῶν Ὅσιε," // name of saint for General Apolytikia when not in Menaion
meVE.MM.GeneralApolytikion1.text = properties_gr_GR_cog.media.key

meVE.Apolytikion2ger.poet = poets_gr_GR_cog.po.Gerasimou
meVE.Apolytikion2ger.mode = miscellanea_gr_GR_cog.misc.Mode3
meVE.Apolytikion2ger.greekmelody = he.a.m3_gr_GR_cog.heAU.TheiasPisteos.greekname
meVE.Apolytikion2ger.melody = he.a.m3_gr_GR_cog.heAU.TheiasPisteos.name
meVE.Apolytikion2ger.incipit = "Θεῖον γόνον σε, Μονεμβασία, ἀνεβλάστησε…"
meVE.Apolytikion2ger.text = "Θεῖον γόνον σε, Μονεμβασία, ἀνεβλάστησε, καρποφοροῦντα, Ἰωάννη τὰς τῆς πίστεως χάριτας· τῶν γὰρ πατρῴων θεσμῶν ἀντεχόμενος, τοὺς ἐκ τῆς Ἀγαρ ἀθλήσας κατῄσχυνας. Μάρτυς ἔνδοξε, Χριστὸν τὸν Θεὸν ἱκέτευε, δωρήσασθαι ἡμῖν τὸ μέγα ἔλεος."

meMA.C1.acrostic.title = "Κανὼν τοῦ Αγίου,"
meMA.C1.acrostic.notice1 = "οὗ ἡ Ἀκροστιχίς·"
meMA.C1.acrostic.text = "Τὸ φαιδρὸν ὑμνῶ, παμμάκαρ, σῶν θαυμάτων."
