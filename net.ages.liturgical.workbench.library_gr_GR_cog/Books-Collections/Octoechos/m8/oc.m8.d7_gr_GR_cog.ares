ocLI.Beatitudes1.incipit = oc.m8.d1_gr_GR_cog.ocLI.Beatitudes1.incipit
ocLI.Beatitudes1.text = oc.m8.d1_gr_GR_cog.ocLI.Beatitudes1.text

ocLI.Beatitudes2.mode = miscellanea_gr_GR_cog.misc.Mode8
ocLI.Beatitudes2.melody = ""
ocLI.Beatitudes2.incipit = "Πᾶσιν ἀλγεινοῖς, στεῤῥῶς προσομιλοῦντες, τραύμασιν…"
ocLI.Beatitudes2.text = "Πᾶσιν ἀλγεινοῖς, στεῤῥῶς προσομιλοῦντες, τραύμασιν ὑμῶν, ἁγίοις Ἀθλοφόροι, ἐτραυματίσατε πάσας τὰς μυριάδας, τὰς τῶν δαιμόνων θείᾳ χάριτι."

ocLI.Beatitudes3.mode = miscellanea_gr_GR_cog.misc.Mode8
ocLI.Beatitudes3.melody = ""
ocLI.Beatitudes3.incipit = "Ὅσιοι Χριστοῦ, καὶ θεῖοι Ἱεράρχαι…"
ocLI.Beatitudes3.text = "Ὅσιοι Χριστοῦ, καὶ θεῖοι Ἱεράρχαι, στῖφος Προφητῶν, καὶ πάντων τῶν Ἁγίων, τῆς οὐρανίου ἐπέτυχον κληρουχίας, οὓς ἐπαξίως μακαρίσωμεν."

ocLI.Beatitudes4.mode = miscellanea_gr_GR_cog.misc.Mode8
ocLI.Beatitudes4.melody = ""
ocLI.Beatitudes4.incipit = "Πάντας τοὺς πιστῶς, τοῦ βίου…"
ocLI.Beatitudes4.text = "Πάντας τοὺς πιστῶς, τοῦ βίου μεταστάντας, τάξον ὁ Θεός, ἐν χώρᾳ τῶν Δικαίων, καὶ Παραδείσου ἀνάδειξον κληρονόμους, θεοπρεπῶς ὑμνολογοῦντάς σε."

ocLI.BeatitudesTr.mode = miscellanea_gr_GR_cog.misc.Mode8
ocLI.BeatitudesTr.melody = ""
ocLI.BeatitudesTr.incipit = "Πάτερ καὶ Υἱέ, καὶ Πνεῦμα…"
ocLI.BeatitudesTr.text = "Πάτερ καὶ Υἱέ, καὶ Πνεῦμα μία φύσις, σὺ εἶ ὁ Θεός, ὁ κτίσας πάντα λόγῳ, σὲ προσκυνοῦσιν οἱ ἄνθρωποι σὺν Ἀγγέλοις, ἀκαταπαύστως σὲ δοξάζοντες."

ocLI.BeatitudesT.mode = miscellanea_gr_GR_cog.misc.Mode8
ocLI.BeatitudesT.melody = ""
ocLI.BeatitudesT.incipit = "Πάντων τῶν καλῶν, τὸ πλήρωμα…"
ocLI.BeatitudesT.text = "Πάντων τῶν καλῶν, τὸ πλήρωμα τεκοῦσα, πλήρωσον ἡμῶν, Παρθένε τὰς ἐντεύξεις, ἁμαρτιῶν ἡμῶν λύτρωσιν αἰτουμένη, καὶ φωτισμὸν καὶ μέγα ἔλεος."

ocLI.MM.Beatitudes1234TrTas78910GB.text = properties_gr_GR_cog.media.key

ocBK.end.title = "ΤΕΛΟΣ ΤΟΥ ΠΛΑΓΙΟΥ Δʹ ΗΧΟΥ"
ocBK.end.verse1.text = "Ἤχων σφραγίς, Τέταρτε σὺ τῶν πλαγίων,"
ocBK.end.verse2.text = "Ὡς ἐν σεαυτῷ πᾶν καλὸν μέλος φέρων,"
ocBK.end.verse3.text = "Ἄκραν σε φωνῆς δίς σε καλῶ, εἰς τέλος."
ocBK.end.verse4.text = "Ἤχων κορωνίς, ὡς ὑπάρχων καὶ τέλος."
ocBK.end.verse5.text = "Ὡς ἄκρον ἐν φθόγγοις τε, καὶ φωνῶν στάσει,"
ocBK.end.verse6.text = "Ἀνευρύνεις, σὺ τοὺς κρότους τῶν ᾀσμάτων."

//Octoechos in Triodion

