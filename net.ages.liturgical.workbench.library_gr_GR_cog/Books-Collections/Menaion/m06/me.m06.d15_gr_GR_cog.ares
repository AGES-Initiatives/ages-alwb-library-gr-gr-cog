meVE.Stichera01.mode = misc_gr_GR_cog.Mode2
meVE.Stichera01.melody = he.a.m2_gr_GR_cog.OteEkTouXylou.name
meVE.Stichera01.incipit = "Εὗρεν, ἡ τοῦ Πνεύματος αὐγή…"
meVE.Stichera01.text = "Εὗρεν, ἡ τοῦ Πνεύματος αὐγή, τῆς σῆς διανοίας Προφῆτα, τὴν καθαρότητα, στίλβουσαν ὡς ἔσοπτρον, διαφανέστατον· καὶ τὸν κόσμον κατηύγασε, τῆς θεογνωσίας, φέγγος ἀπαστράψασα, καὶ προτυπώσασα, θείων μυστηρίων εἰκόνας, χάριν τε τὴν πᾶσιν ἀνθρώποις, μέλλουσαν δοθήσεσθαι θεσπέσιε."

meVE.Stichera02.mode = misc_gr_GR_cog.Mode2
meVE.Stichera02.melody = he.a.m2_gr_GR_cog.OteEkTouXylou.name
meVE.Stichera02.incipit = "Στόμα, χρηματίσας τοῦ Θεοῦ, τοὺς…"
meVE.Stichera02.text = "Στόμα, χρηματίσας τοῦ Θεοῦ, τοὺς τῆς ἀσεβείας ἐργάτας, σαφῶς διήλεγξας, δίκην ἀναπόδραστον, αὐτοῖς καὶ ἄφυκτον, ἀπαρτήσας πανόλβιε, τῆς δικαιοσύνης, δόγμασιν ἑπόμενος, καὶ θείοις κρίμασιν· ὅθεν τῶν σοφῶν σου ῥημάτων, βλέποντες τὴν ἔκβασιν μάκαρ, ὕμνοις ἐπαξίοις εὐφημοῦμέν σε."

meVE.Stichera03.mode = misc_gr_GR_cog.Mode2
meVE.Stichera03.melody = he.a.m2_gr_GR_cog.OteEkTouXylou.name
meVE.Stichera03.incipit = "Μύστης, τῶν ἀῤῥήτων τοῦ Θεοῦ…"
meVE.Stichera03.text = "Μύστης, τῶν ἀῤῥήτων τοῦ Θεοῦ, γέγονας κριμάτων Προφῆτα, θεομακάριστε, ἔθνη γὰρ ἐφώτισας, καὶ κατελάμπρυνας, καὶ Τριάδα ἐκήρυξας, Ἀμώς θεηγόρε· ὅθεν σου τὴν ἔνδοξον μνήμην γεραίρομεν. Ῥῦσαι ἀπὸ πάσης ἀνάγκης, πάντας τοὺς πιστῶς εὐφημοῦντας, καὶ πανηγυρίζοντάς σε ἔνδοξε."

meVE.SticTheotokion.mode = misc_gr_GR_cog.Mode2
meVE.SticTheotokion.melody = he.a.m2_gr_GR_cog.OteEkTouXylou.name
meVE.SticTheotokion.incipit = "Ὕδωρ, ὡς τεκοῦσα τῆς ζωῆς…"
meVE.SticTheotokion.text = "Ὕδωρ, ὡς τεκοῦσα τῆς ζωῆς, τῆς ἀθανασίας τὸ νέκταρ, Χριστὸν τὸν Κύριον, πότισόν με Πάναγνε, ὕδωρ ἀφέσεως, καὶ τὸν νοῦν μου χαρίτωσον, νοήμασι θείοις, ὅπως τὰ σωτήρια, πράττω προστάγματα· ἵνα σωτηρίας πρὸς ὅρμον, διὰ τῆς αὐτῶν ἐργασίας, φθάσω καὶ δοξάζω σε τὴν ἄχραντον."

meVE.SticStavrotheotokion.mode = misc_gr_GR_cog.Mode2
meVE.SticStavrotheotokion.melody = he.a.m2_gr_GR_cog.OteEkTouXylou.name
meVE.SticStavrotheotokion.incipit = me.m06.d01_gr_GR_cog.meVE.SticStavrotheotokion.incipit
meVE.SticStavrotheotokion.text = me.m06.d01_gr_GR_cog.meVE.SticStavrotheotokion.text

meVE.Apolytikion1.poet = poets_gr_GR_cog.Gerasimou
meVE.Apolytikion1.mode = misc_gr_GR_cog.Mode1
meVE.Apolytikion1.melody = he.a.m1_gr_GR_cog.TonTafonSouSotir.name
meVE.Apolytikion1.incipit = "Προφήτην σε πιστόν, καὶ τῶν…"
meVE.Apolytikion1.text = "Προφήτην σε πιστόν, καὶ τῶν ἄνω ἐπόπτην, ἀνέδειξεν Ἀμώς, ἐκ ποιμνίου ὁ Λόγος, τοῦ βίου σου δεξάμενος, εὐμενῶς τὴν χρηστότητα· ὅθεν ἤλεγξας, τοὺς ἀσεβοῦντας ἀνδρείως, καὶ τὸν θάνατον, μαρτυρικῶς δεδεγμένος, ζωῆς θείας ἔτυχες."

meMA.C1.acrostic.title = "Ὁ Κανὼν τοῦ Προφήτου,"
meMA.C1.acrostic.notice1 = "οὗ ἡ Ἀκροστιχίς·"
meMA.C1.acrostic.text = "Ἀμώς σε μέλπω τὸν Προφήτην τὸν μέγαν."
meMA.C1.title = titles_gr_GR_cog.ti.CanonProphet
meMA.C1.verse = prayers_gr_GR_cog.verse_Prophet.text

meMA.Ode1C11.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C11.mode = misc_gr_GR_cog.Mode7
meMA.Ode1C11.melody = he.h.m7_gr_GR_cog.ToEktinaxantiTheo.name
meMA.Ode1C11.incipit = "Ἀνακαθάρας μου τὸν νοῦν, τοῦ…"
meMA.Ode1C11.text = "Ἀνακαθάρας μου τὸν νοῦν, τοῦ τῶν παθῶν ἀχλυώδους λογισμοῦ, τὸν Προφήτην σου Ἀμώς, Δέσποτα μέλπειν καταύγασον."

meMA.Ode1C12.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C12.mode = misc_gr_GR_cog.Mode7
meMA.Ode1C12.melody = he.h.m7_gr_GR_cog.ToEktinaxantiTheo.name
meMA.Ode1C12.incipit = "Μυσταγωγὸς ἀναδειχθείς, τῶν ὑπὲρ νοῦν…"
meMA.Ode1C12.text = "Μυσταγωγὸς ἀναδειχθείς, τῶν ὑπὲρ νοῦν μυστηρίων τοῦ Θεοῦ, ἠξιώθης προορᾶν, Ἔνδοξε τὰ γενησόμενα."

meMA.Ode1C13.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C13.mode = misc_gr_GR_cog.Mode7
meMA.Ode1C13.melody = he.h.m7_gr_GR_cog.ToEktinaxantiTheo.name
meMA.Ode1C13.incipit = "Ὡς ἀκηλίδωτον Θεοῦ, καὶ καθαρὸν…"
meMA.Ode1C13.text = "Ὡς ἀκηλίδωτον Θεοῦ, καὶ καθαρὸν οἰκητήριον φανείς, τὴν τοῦ Πνεύματος Ἀμώς, εἴληφας θείαν ἐνέργειαν."

meMA.Ode1C1T.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C1T.mode = misc_gr_GR_cog.Mode7
meMA.Ode1C1T.melody = he.h.m7_gr_GR_cog.ToEktinaxantiTheo.name
meMA.Ode1C1T.incipit = "Σὺ τὴν πεσοῦσαν τοῦ Ἀδάμ…"
meMA.Ode1C1T.text = "Σὺ τὴν πεσοῦσαν τοῦ Ἀδάμ, προφητικῶς ἐξανέστησας σκηνήν, τὸν Σωτῆρα καὶ Θεόν, Ἄχραντε κυοφορήσασα."

meMA.Ode3C11.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C11.mode = misc_gr_GR_cog.Mode7
meMA.Ode3C11.melody = he.h.m7_gr_GR_cog.EstereothiTiPistei.name
meMA.Ode3C11.incipit = "Σὺ παρανόμους ἐλέγχων, Ἀμὼς τὴν…"
meMA.Ode3C11.text = "Σὺ παρανόμους ἐλέγχων, Ἀμὼς τὴν τοῦ Κυρίου, δικαιοσύνην ἐδείκνυς, βοῶν θεοφάντορ· Ἅγιος εἶ Κύριε, ὁ σῴζων τὰς ψυχὰς ἡμῶν."

meMA.Ode3C12.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C12.mode = misc_gr_GR_cog.Mode7
meMA.Ode3C12.melody = he.h.m7_gr_GR_cog.EstereothiTiPistei.name
meMA.Ode3C12.incipit = "Ἐτέθης στῦλος Προφῆτα, τῆς Καινῆς…"
meMA.Ode3C12.text = "Ἐτέθης στῦλος Προφῆτα, τῆς Καινῆς Διαθήκης, ἀνέχων ταύτης τὴν στέγην, καὶ πίστει κραυγάζων· Ἅγιος εἶ Κύριε, καὶ σὲ ὑμνεῖ τὸ πνεῦμά μου."

meMA.Ode3C13.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C13.mode = misc_gr_GR_cog.Mode7
meMA.Ode3C13.melody = he.h.m7_gr_GR_cog.EstereothiTiPistei.name
meMA.Ode3C13.incipit = "Μεμυημένος τὰ θεῖα, Προφῆτα θεηγόρε…"
meMA.Ode3C13.text = "Μεμυημένος τὰ θεῖα, Προφῆτα θεηγόρε, Ἀμὼς ἐνθέως φωτίζων, λαοὺς ἐμελῴδεις· Ἅγιος εἶ Κύριε, ὁ σῴζων τὰς ψυχὰς ἡμῶν."

meMA.Ode3C1T.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C1T.mode = misc_gr_GR_cog.Mode7
meMA.Ode3C1T.melody = he.h.m7_gr_GR_cog.EstereothiTiPistei.name
meMA.Ode3C1T.incipit = "Ἐστερεώθην τὸν θεῖον, τόκον ἐπεγνωκώς…"
meMA.Ode3C1T.text = "Ἐστερεώθην τὸν θεῖον, τόκον ἐπεγνωκώς σου, καὶ προστασίαν Παρθένε, πλουτῷ σε κραυγάζων· Ἅγιος εἶ Κύριε, ὁ σῴζων τὰς ψυχὰς ἡμῶν."

meMA.Ode3H.ode = misc_gr_GR_cog.Ode3
meMA.Ode3H.mode = misc_gr_GR_cog.Mode7
meMA.Ode3H.melody = he.h.m7_gr_GR_cog.EstereothiTiPistei.melody
meMA.Ode3H.incipit = he.h.m7_gr_GR_cog.EstereothiTiPistei.incipit
meMA.Ode3H.text = he.h.m7_gr_GR_cog.EstereothiTiPistei.text

meMA.Ode3Kathisma1.mode = misc_gr_GR_cog.Mode3
meMA.Ode3Kathisma1.melody = he.a.m3_gr_GR_cog.TheiasPisteos.name
meMA.Ode3Kathisma1.incipit = "Θεῖον ὄργανον τοῦ Παρακλήτου, ἐχρημάτισας…"
meMA.Ode3Kathisma1.text = "Θεῖον ὄργανον τοῦ Παρακλήτου, ἐχρημάτισας ταῖς τούτου μάκαρ, διηνεκέσι πνοαῖς ὑπηχούμενον· ἀναφωνεῖς γὰρ ἀδήλων τὴν δήλωσιν, καὶ καταυγάζεις τοὺς πίστει προστρέχοντας, Ἀμὼς ἔνδοξε, Χριστὸν τὸν Θεὸν αἰτούμενος, δωρήσασθαι ἡμῖν τὸ μέγα ἔλεος."

meMA.Ode3KathismaT.mode = misc_gr_GR_cog.Mode3
meMA.Ode3KathismaT.melody = he.a.m3_gr_GR_cog.TheiasPisteos.name
meMA.Ode3KathismaT.incipit = "Θείας φύσεως, οὐκ ἐχωρίσθη, σάρξ…"
meMA.Ode3KathismaT.text = "Θείας φύσεως, οὐκ ἐχωρίσθη, σάρξ γενόμενος, ἐν τῇ γαστρί σου, ἀλλὰ Θεὸς ἐνανθρωπήσας μεμένηκεν, ὁ μετὰ τόκον Παρθένον Μητέρα σε, ὡς πρὸ τοῦ τόκου φυλάξας Πανάμωμε, μόνος Κύριος· Αὐτὸν ἐκτενῶς ἱκέτευε, δωρήσασθαι ἡμῖν τὸ μέγα ἔλεος."

meMA.Ode3KathismaSt.mode = misc_gr_GR_cog.Mode3
meMA.Ode3KathismaSt.melody = he.a.m3_gr_GR_cog.TheiasPisteos.name
meMA.Ode3KathismaSt.incipit = me.m01.d29_gr_GR_cog.meMA.Ode3KathismaSt.incipit
meMA.Ode3KathismaSt.text = me.m01.d29_gr_GR_cog.meMA.Ode3KathismaSt.text

meMA.Ode4C11.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C11.mode = misc_gr_GR_cog.Mode7
meMA.Ode4C11.melody = he.h.m7_gr_GR_cog.AkikoaTinAkoinSou.name
meMA.Ode4C11.incipit = "Λατρεύοντα κωφοῖς εἰδώλοις, τὸν λαὸν…"
meMA.Ode4C11.text = "Λατρεύοντα κωφοῖς εἰδώλοις, τὸν λαὸν τοῦ Ἰσραὴλ ἤλεγξας· ὅθεν αἰχμάλωτον γενέσθαι, τοῦτον ἔφης Ἀξιάγαστε."

meMA.Ode4C12.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C12.mode = misc_gr_GR_cog.Mode7
meMA.Ode4C12.melody = he.h.m7_gr_GR_cog.AkikoaTinAkoinSou.name
meMA.Ode4C12.incipit = "Προφήτην σὲ τὸν πρὶν ποιμένα…"
meMA.Ode4C12.text = "Προφήτην σὲ τὸν πρὶν ποιμένα, ἔδειξεν ὁ Θεὸς ἀναλαβών, Ἱεροφάντορ Ἀμώς· διὸ σε πάντες πίστει μακαρίζομεν."

meMA.Ode4C13.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C13.mode = misc_gr_GR_cog.Mode7
meMA.Ode4C13.melody = he.h.m7_gr_GR_cog.AkikoaTinAkoinSou.name
meMA.Ode4C13.incipit = "Ὡς ἔσοπτρον τὰς τοῦ Ἁγίου…"
meMA.Ode4C13.text = "Ὡς ἔσοπτρον τὰς τοῦ Ἁγίου Πνεύματος, δεδεγμένος ἀστραπάς, τὰς φωτοβόλους Ἀμώς, πᾶσι προφαίνεις, χαίρων τὴν εὐσέβειαν."

meMA.Ode4C14.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C14.mode = misc_gr_GR_cog.Mode7
meMA.Ode4C14.melody = he.h.m7_gr_GR_cog.AkikoaTinAkoinSou.name
meMA.Ode4C14.incipit = "Τὴν ἄμεμπτόν σου πολιτείαν Ἔνδοξε…"
meMA.Ode4C14.text = "Τὴν ἄμεμπτόν σου πολιτείαν Ἔνδοξε, θεασάμενος Θεός, κήρυκα δόξης αὐτοῦ προβάλλεταί σε, θείας τε σαρκώσεως."

meMA.Ode4C1T.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C1T.mode = misc_gr_GR_cog.Mode7
meMA.Ode4C1T.melody = he.h.m7_gr_GR_cog.AkikoaTinAkoinSou.name
meMA.Ode4C1T.incipit = "Ὁ λόγος σοι κατασκηνώσας, Ἄχραντε…"
meMA.Ode4C1T.text = "Ὁ λόγος σοι κατασκηνώσας, Ἄχραντε, τὴν οὐσίαν τὴν ἐμήν, ἀνεμορφώσατο διαπεσοῦσαν, ταῖς πρὶν παραβάσεσιν."

meMA.Ode5C11.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C11.mode = misc_gr_GR_cog.Mode7
meMA.Ode5C11.melody = he.h.m7_gr_GR_cog.ProsSeOrthrizo.name
meMA.Ode5C11.incipit = "Νενεκρωμένους, τρυφῆς βολίσι Μάκαρ ὠνείδισας…"
meMA.Ode5C11.text = "Νενεκρωμένους, τρυφῆς βολίσι Μάκαρ ὠνείδισας, ὡς λογιζομένους, πεπηγέναι τὰ ῥευστά, τὰ τρεπτὴν κεκτημένα, καὶ φθειρομένην τὴν ἀπόλαυσιν."

meMA.Ode5C12.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C12.mode = misc_gr_GR_cog.Mode7
meMA.Ode5C12.melody = he.h.m7_gr_GR_cog.ProsSeOrthrizo.name
meMA.Ode5C12.incipit = "Πλουσιωτάταις, φωτοχυσίαις περιχεόμενος, ὤφθης θεοῤῥῆμον…"
meMA.Ode5C12.text = "Πλουσιωτάταις, φωτοχυσίαις περιχεόμενος, ὤφθης θεοῤῥῆμον, καὶ συνόμιλος Θεοῦ, διαγγέλλων τοῖς πᾶσι, τῆς ἀσεβείας τὰ ἐπίχειρα."

meMA.Ode5C13.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C13.mode = misc_gr_GR_cog.Mode7
meMA.Ode5C13.melody = he.h.m7_gr_GR_cog.ProsSeOrthrizo.name
meMA.Ode5C13.incipit = "Ῥυσθῆναι πάντας, τοὺς σὲ ὑμνοῦντας…"
meMA.Ode5C13.text = "Ῥυσθῆναι πάντας, τοὺς σὲ ὑμνοῦντας Μάκαρ δυσώπησον, τῶν δελεασμάτων, καὶ τῶν βρόχων τοῦ ἐχθροῦ, καὶ φωτὶ λαμπρυνθῆναι, τῆς οὐρανίου προφητείας σου."

meMA.Ode5C1T.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C1T.mode = misc_gr_GR_cog.Mode7
meMA.Ode5C1T.melody = he.h.m7_gr_GR_cog.ProsSeOrthrizo.name
meMA.Ode5C1T.incipit = "Οἱ Θεοτόκον, ὁμολογοῦντές σε Μητροπάρθενε…"
meMA.Ode5C1T.text = "Οἱ Θεοτόκον, ὁμολογοῦντές σε Μητροπάρθενε, τῆς ἀδιαδόχου, βασιλείας καὶ τρυφῆς, διὰ σοῦ Θεομῆτορ, ἐπιτυχεῖν ἀξιωθείημεν."

meMA.Ode6C11.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C11.mode = misc_gr_GR_cog.Mode7
meMA.Ode6C11.melody = he.h.m7_gr_GR_cog.OIonasEkKoilias.name
meMA.Ode6C11.incipit = "Φωτιστικαῖς, ἀληθείας ἀκτῖσιν ἐκλάμπων, ἐῤῥάπισας…"
meMA.Ode6C11.text = "Φωτιστικαῖς, ἀληθείας ἀκτῖσιν ἐκλάμπων, ἐῤῥάπισας τὸν τοῦ ψεύδους προφήτην, εὐτόνως διελέγχων, καὶ προλέγων τὴν αὐτοῦ ἐξολόθρευσιν."

meMA.Ode6C12.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C12.mode = misc_gr_GR_cog.Mode7
meMA.Ode6C12.melody = he.h.m7_gr_GR_cog.OIonasEkKoilias.name
meMA.Ode6C12.incipit = "Ἠμαύρωσας, τῆς ἀπάτης τὴν φλόγα…"
meMA.Ode6C12.text = "Ἠμαύρωσας, τῆς ἀπάτης τὴν φλόγα Προφῆτα, ὡς ἥλιος εὐσεβείας τὴν αἴγλην, τοῖς πᾶσιν ἀνατέλλων, ὡς τοῦ Πνεύματος αὐγὴν εἰσδεξάμενος."

meMA.Ode6C1T.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C1T.mode = misc_gr_GR_cog.Mode7
meMA.Ode6C1T.melody = he.h.m7_gr_GR_cog.OIonasEkKoilias.name
meMA.Ode6C1T.incipit = "Τὴν ἄχραντον, παρθενίαν ὑμνῶ σου…"
meMA.Ode6C1T.text = "Τὴν ἄχραντον, παρθενίαν ὑμνῶ σου Παρθένε, τὸν ἄφθαρτον καὶ σεβάσμιον τόκον, τιμῶ σου Παναγία, διʼ οὗ σέσωσμαι φθορᾶς καὶ νεκρώσεως."

meMA.Ode6H.ode = misc_gr_GR_cog.Ode6
meMA.Ode6H.mode = misc_gr_GR_cog.Mode7
meMA.Ode6H.melody = he.h.m7_gr_GR_cog.OIonasEkKoilias.melody
meMA.Ode6H.incipit = he.h.m7_gr_GR_cog.OIonasEkKoilias.incipit
meMA.Ode6H.text = he.h.m7_gr_GR_cog.OIonasEkKoilias.text

meMA.Ode7C11.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C11.mode = misc_gr_GR_cog.Mode7
meMA.Ode7C11.melody = he.h.m7_gr_GR_cog.OiEnKaminoTouPyros.name
meMA.Ode7C11.incipit = "Ἣν προηγόρευσας θείαν σωτηρίαν, ἐφανερώθη…"
meMA.Ode7C11.text = "Ἣν προηγόρευσας θείαν σωτηρίαν, ἐφανερώθη, Ἀμὼς προφῆτα θαυμάσιε, φωτισμοῖς εὐσεβείας λάμπουσα κόσμῳ· Εὐλογητὸς εἶ κράζοντι, ὁ Θεὸς ὁ τῶν Πατέρων ἡμῶν."

meMA.Ode7C12.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C12.mode = misc_gr_GR_cog.Mode7
meMA.Ode7C12.melody = he.h.m7_gr_GR_cog.OiEnKaminoTouPyros.name
meMA.Ode7C12.incipit = "Νενευρωμένος τῇ θείᾳ δυναστείᾳ τοῖς…"
meMA.Ode7C12.text = "Νενευρωμένος τῇ θείᾳ δυναστείᾳ τοῖς ἀντιθέοις, ἐχθροῖς ἀνάλωτος γέγονας, καρτερῶς ὡς ἀδάμας μείνας κραυγάζων· Εὐλογητὸς εἶ Κύριε, ὁ Θεὸς ὁ τῶν Πατέρων ἡμῶν."

meMA.Ode7C1T.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C1T.mode = misc_gr_GR_cog.Mode7
meMA.Ode7C1T.melody = he.h.m7_gr_GR_cog.OiEnKaminoTouPyros.name
meMA.Ode7C1T.incipit = "Τὸν ἀκατάληπτον τόκον τῆς Παρθένου…"
meMA.Ode7C1T.text = "Τὸν ἀκατάληπτον τόκον τῆς Παρθένου, δοξολογοῦμεν, διʼ οὗ θανάτου ἐῤῥύσθημεν, διʼ αὐτοῦ γεννηθέντες εἰς ἀφθαρσίαν· Εὐλογητὸς εἶ κράζοντες· ὁ Θεὸς ὁ τῶν Πατέρων ἡμῶν."

meMA.Ode8C11.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C11.mode = misc_gr_GR_cog.Mode7
meMA.Ode8C11.melody = he.h.m7_gr_GR_cog.TonMononAnarchon.name
meMA.Ode8C11.incipit = "Ὅλος ἀνέδραμες εἰς αἰθέριον ὕψος…"
meMA.Ode8C11.text = "Ὅλος ἀνέδραμες εἰς αἰθέριον ὕψος, καὶ ἐμυήθης τῶν ἀῤῥήτων τὴν γνῶσιν, καὶ γέγονας μηνυτὴς τῆς τοῦ Λόγου σαρκώσεως Ἀμώς· διὸ σε ἀνυμνοῦμεν, εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8C12.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C12.mode = misc_gr_GR_cog.Mode7
meMA.Ode8C12.melody = he.h.m7_gr_GR_cog.TonMononAnarchon.name
meMA.Ode8C12.incipit = "Νοὸς ὀξύτητι τὸν τῶν ὅλων…"
meMA.Ode8C12.text = "Νοὸς ὀξύτητι τὸν τῶν ὅλων Δεσπότην, κατανοεῖν ὡς ἐφικτὸν ἠξιώθης, μυοῦντά σε, τῶν ἀῤῥήτων τὴν γνῶσιν κραυγάζων εὐσεβῶς· Λαὸς ὑπερυψοῦτε, αὐτὸν εἰς τοὺς αἰῶνας."

meMA.Ode8C13.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C13.mode = misc_gr_GR_cog.Mode7
meMA.Ode8C13.melody = he.h.m7_gr_GR_cog.TonMononAnarchon.name
meMA.Ode8C13.incipit = "Μακαριότητος καὶ χαρᾶς ἀνεκφράστου, καὶ…"
meMA.Ode8C13.text = "Μακαριότητος καὶ χαρᾶς ἀνεκφράστου, καὶ βασιλείας οὐρανῶν ὡς προφήτης, ἐπέτυχες οὐρανίων ἐπόπτα, κραυγάζων εὐσεβῶς· Λαὸς ὑπερυψοῦτε, Χριστὸν εἰς τοὺς αἰῶνας."

meMA.Ode8C1T.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C1T.mode = misc_gr_GR_cog.Mode7
meMA.Ode8C1T.melody = he.h.m7_gr_GR_cog.TonMononAnarchon.name
meMA.Ode8C1T.incipit = "Ἐν δύο φύσεσι τῆς Τριάδος…"
meMA.Ode8C1T.text = "Ἐν δύο φύσεσι τῆς Τριάδος, τὸν ἕνα θεοπρεπῶς ἄνευ σπορᾶς Θεομῆτορ, ἐγέννησας τὸν Σωτῆρα τοῦ κόσμου· διὸ σε οἱ πιστοί, πόθῳ ὑπερυψοῦμεν, εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8H.ode = misc_gr_GR_cog.Ode8
meMA.Ode8H.mode = misc_gr_GR_cog.Mode7
