alt1.syMA.commemoration01.couplet1.verse2 = syMA.commemoration02.couplet1.verse2

syMA.lastline = "Ταῖς αὐτῶν ἁγίαις πρεσβείαις, ὁ Θεὸς ἐλέησον ἡμᾶς. Ἀμήν."

