meMA.C2.acrostic.title = "Κανὼν τοῦ Ἁγίου Μερκουρίου,"
meMA.C2.acrostic.notice1 = "ἔχων Ἀκροστιχίδα·"
meMA.C2.acrostic.text = "Μερκουρίου με τὸ σθένος φυλαττέτω. Ἰωσήφ."
meMA.C2.title = titles_gr_GR_cog.KanonTouAgiou
