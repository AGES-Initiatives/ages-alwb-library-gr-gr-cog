meCO.Ode9C1H.text = he.h.m2_gr_GR_cog.EmegalynasChriste.text

meCO.Ode9C11.ode = misc_gr_GR_cog.Ode9
meCO.Ode9C11.mode = misc_gr_GR_cog.Mode2
meCO.Ode9C11.melody = he.h.m2_gr_GR_cog.EmegalynasChriste.name
meCO.Ode9C11.incipit = "Ῥύπον πάντα ἐμπαθῆ, ἀπωσάμενοι ἐπαξίως…"
meCO.Ode9C11.text = "Ῥύπον πάντα ἐμπαθῆ, ἀπωσάμενοι ἐπαξίως, Βαπτίσματος τοῦ θείου, γνώμην ἀναλάβωμεν ἔμφρονα· ἥκει γὰρ ὁ Κτίστης, κάθαρσιν δωρήσασθαι ξένην, ῥείθροις βαπτιζόμενος, πᾶσί τε καινὴν δεῖξαι ἀλλοίωσιν."

meCO.Ode9C12.ode = misc_gr_GR_cog.Ode9
meCO.Ode9C12.mode = misc_gr_GR_cog.Mode2
meCO.Ode9C12.melody = he.h.m2_gr_GR_cog.EmegalynasChriste.name
meCO.Ode9C12.incipit = "Ἀφορῶντες εἰς Χριστόν, ταπεινούμενον ἑκουσίως…"
meCO.Ode9C12.text = "Ἀφορῶντες εἰς Χριστόν, ταπεινούμενον ἑκουσίως, μέχρι καὶ δούλου μορφῆς, ὑπὸ τὴν αὐτοῦ χεῖρα τὴν κραταιάν, νῦν ταπεινωθῶμεν, πνεύματι συνεπαρθέντες, ὅπως βαπτιζόμενον, τοῦτον καθαροῖς ἔργοις τιμήσωμεν."

meCO.Ode9C21.ode = misc_gr_GR_cog.Ode9
meCO.Ode9C21.mode = misc_gr_GR_cog.Mode2
meCO.Ode9C21.melody = he.h.m2_gr_GR_cog.ITonAchoritonTheon.name
meCO.Ode9C21.incipit = "Τῆς ἁμαρτίας τὸν βαρύν, ῥύπον…"
meCO.Ode9C21.text = "Τῆς ἁμαρτίας τὸν βαρύν, ῥύπον ἀποῤῥίψαντες, καθαροὶ τῷ πάντας καθαίροντι, ῥείθροις θείοις, ἐποφθῶμεν Δεσπότῃ."

meCO.Ode9C22.ode = misc_gr_GR_cog.Ode9
meCO.Ode9C22.mode = misc_gr_GR_cog.Mode2
meCO.Ode9C22.melody = he.h.m2_gr_GR_cog.ITonAchoritonTheon.name
meCO.Ode9C22.incipit = "Ἐν Ἰορδάνῃ ποταμῷ, βαπτισθῆναι ἥκοντι…"
meCO.Ode9C22.text = "Ἐν Ἰορδάνῃ ποταμῷ, βαπτισθῆναι ἥκοντι, τῷ Χριστῷ δεῦτε προσεγγίσωμεν, τῶν δακρύων, καθαρθέντες τοῖς ῥείθροις."

meCO.Ode9C23.ode = misc_gr_GR_cog.Ode9
meCO.Ode9C23.mode = misc_gr_GR_cog.Mode2
meCO.Ode9C23.melody = he.h.m2_gr_GR_cog.ITonAchoritonTheon.name
meCO.Ode9C23.incipit = "Ἐν τῇ Γεννήσει σου Σωτήρ…"
meCO.Ode9C23.text = "Ἐν τῇ Γεννήσει σου Σωτήρ, ἡμᾶς ἑορτάσαντας, μετὰ Μάγων καὶ Ποιμένων, νῦν πρόσδεξαι, σὲ τιμῶντας, βαπτισθῆναι μολοῦντα."

meCO.Ode9C2H.ode = misc_gr_GR_cog.Ode9
meCO.Ode9C2H.mode = misc_gr_GR_cog.Mode2
meCO.Ode9C2H.melody = misc_gr_GR_cog.Heirmos
meCO.Ode9C2H.incipit = he.h.m2_gr_GR_cog.ITonAchoritonTheon.incipit
meCO.Ode9C2H.text = he.h.m2_gr_GR_cog.ITonAchoritonTheon.text

meMA.Kathisma11.mode = misc_gr_GR_cog.Mode1
meMA.Kathisma11.melody = he.a.m1_gr_GR_cog.TonTafonSouSotir.name
meMA.Kathisma11.incipit = "Φορέσας τὴν ἐμήν, Ἰησοῦ μου…"
meMA.Kathisma11.text = "Φορέσας τὴν ἐμήν, Ἰησοῦ μου πτωχείαν, προέρχῃ σεαυτόν, τοῖς οἰκέταις μιγνύων, καὶ Βάπτισμα αἰτούμενος, ὑπὸ δούλου φιλάνθρωπε· ὅθεν βλέπων σε, ὁ Ἰωάννης ἐβόα. Πῶς βαπτίσω σε, ῥύπον μὴ ἔχοντα ὅλως, Θεὲ ὑπεραγαθέ;"

meMA.Kathisma12.mode = misc_gr_GR_cog.Mode1
meMA.Kathisma12.melody = he.a.m1_gr_GR_cog.TonTafonSouSotir.name
meMA.Kathisma12.incipit = "Προέρχεσθαι Χριστός, ἀγαθότητι μέλλει, πρὸς…"
meMA.Kathisma12.text = "Προέρχεσθαι Χριστός, ἀγαθότητι μέλλει, πρὸς ῥεῖθρα ποταμοῦ, Ἰορδάνου ἐν τούτοις, ἡμῶν τὴν ἀνάπλασιν, ἀπεργάσασθαι χάριτι, θεῖε Πρόδρομε, τοῦτον ὑπόδεξαι χαίρων, καὶ τοῖς ὕδασι, τοῦτον θελήσαντα λοῦσον, τὸν μόνον φιλάνθρωπον."

meMA.Kathisma21.mode = misc_gr_GR_cog.Mode4
meMA.Kathisma21.melody = he.a.m4_gr_GR_cog.TachyProkatalave.name
meMA.Kathisma21.incipit = "Πτωχεύσας ὁ πλούσιος, διʼ εὐσπλαγχνίαν…"
meMA.Kathisma21.text = "Πτωχεύσας ὁ πλούσιος, διʼ εὐσπλαγχνίαν πολλήν, ἡμᾶς τοὺς πτωχεύσαντας, διʼ ἀκρασίαν ποτέ, πλουτίσαι βουλόμενος, ἔρχεται ἐπιβῆναι, Ἰορδάνου τοῖς ῥείθροις, καὶ ὑπὸ Ἰωάννου, προφανῶς βαπτισθῆναι. Ὑμνήσωμεν πιστοί, τὴν αὐτοῦ ἄκραν ταπείνωσιν."

meMA.Kathisma22.mode = misc_gr_GR_cog.Mode4
meMA.Kathisma22.melody = he.a.m4_gr_GR_cog.TachyProkatalave.name
meMA.Kathisma22.incipit = "Πεπλήρωται σήμερον, τοῦ Ἠσαΐου φωνή…"
meMA.Kathisma22.text = "Πεπλήρωται σήμερον, τοῦ Ἠσαΐου φωνή, βοῶντος. Ἑτοιμάσατε, τὴν τοῦ Κυρίου ὁδόν, καὶ τρίβους ποιήσατε· ἤχησε γὰρ ὡς ἔφη, ἡ φιλέρημος σάλπιγξ, ἅπαντας συγκαλοῦσα, Ἰωάννης ὁ πάνυ· διὸ συνδράμωμεν ἅμα, καὶ παράδοξα ἴδωμεν."

meMA.C1.acrostic.title = "Ὁ Προεόρτιος Κανών,"
meMA.C1.acrostic.notice1 = "οὗ ἡ Ἀκροστιχὶς κατʼ Ἀλφάβητον, ἄνευ τῶν Εἱρμῶν."
meMA.C1.poet = poets_gr_GR_cog.Iosif
meMA.C1.title = titles_gr_GR_cog.KanonProeortios
meMA.C1.verse = prayers_gr_GR_cog.verse_God.text

meMA.C2.acrostic.title = "Ἕτερος Κανὼν τοῦ Ἁγίου,"
meMA.C2.acrostic.notice1 = "οὗ ἡ Ἀκροστιχίς·"
meMA.C2.acrostic.text = "Ὑμνεῖν σε πίστει τὴν χάριν δίδου μάκαρ. Ἰωσήφ."
meMA.C2.title = titles_gr_GR_cog.KanonTouAgiou
meMA.C2.verse = prayers_gr_GR_cog.verse_SaintM.text

meMA.Ode1C1H.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C1H.mode = misc_gr_GR_cog.Mode2
meMA.Ode1C1H.melody = he.h.m2_gr_GR_cog.ToTinAvaton.melody
meMA.Ode1C1H.incipit = he.h.m2_gr_GR_cog.ToTinAvaton.incipit
meMA.Ode1C1H.text = he.h.m2_gr_GR_cog.ToTinAvaton.text

meMA.Ode1C11.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C11.mode = misc_gr_GR_cog.Mode2
meMA.Ode1C11.melody = he.h.m2_gr_GR_cog.ToTinAvaton.name
meMA.Ode1C11.incipit = "Αἰσθανόμενος, τὴν τοῦ Κυρίου ἔλευσιν…"
meMA.Ode1C11.text = "Αἰσθανόμενος, τὴν τοῦ Κυρίου ἔλευσιν, ἐκ τῆς ἐρήμου πρόεισιν, ὁ θεῖος Πρόδρομος, ἐκβοῶν περιφανῶς· Ἤγγικεν, ἐπέφανεν, ἡ ἀπολύτρωσις, μετανοήσατε, καὶ ὕδατι καθάρθητε."

meMA.Ode1C12.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C12.mode = misc_gr_GR_cog.Mode2
meMA.Ode1C12.melody = he.h.m2_gr_GR_cog.ToTinAvaton.name
meMA.Ode1C12.incipit = "Βαπτιζόμενον, ἁμαρτιῶν με κλύδωνι, Λόγε…"
meMA.Ode1C12.text = "Βαπτιζόμενον, ἁμαρτιῶν με κλύδωνι, Λόγε Πατρὶ συνάναρχε, ὅλον καθάρισον, μετανοίας ῥαντισμῷ, ὁ τῷ βαπτισμῷ σου πιστοῖς, τὴν ἀπολύτρωσιν ἀεὶ δωρούμενος, διʼ εὐσπλαγχνίαν ἄμετρον."

meMA.Ode1C13.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C13.mode = misc_gr_GR_cog.Mode2
meMA.Ode1C13.melody = he.h.m2_gr_GR_cog.ToTinAvaton.name
meMA.Ode1C13.incipit = "Γῆ ὡς γέγραπται, τοῦ Ζαβουλὼν…"
meMA.Ode1C13.text = "Γῆ ὡς γέγραπται, τοῦ Ζαβουλὼν τὸ ἄδυτον, βλέπουσα φῶς αὐγάσθητι· Ἰδοὺ γὰρ πάρεστιν, Ἰορδάνου ταῖς ῥοαῖς, καὶ τὸν ἐπὶ πάντων Θεόν, ὕμνει κραυγάζουσα· Κυρίῳ ᾄσωμεν· ἐνδόξως γὰρ δεδόξασται."

meMA.Ode1C14.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C14.mode = misc_gr_GR_cog.Mode2
meMA.Ode1C14.melody = he.h.m2_gr_GR_cog.ToTinAvaton.name
meMA.Ode1C14.incipit = "Δῆμον ἅπαντα, ὁ θεηγόρος Πρόδρομος…"
meMA.Ode1C14.text = "Δῆμον ἅπαντα, ὁ θεηγόρος Πρόδρομος, βλέπων παραγενόμενον, τούτοις ἐφθέγγετο. Τίς ὑπέδειξεν ὑμῖν, ἀπὸ τῆς μελλούσης ὀργῆς φυγεῖν, ποιήσατε τῆς μετανοίας καρπούς, ἐν χάριτι σῳζόμενοι."

meMA.Ode1C21.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C21.mode = misc_gr_GR_cog.Mode8
meMA.Ode1C21.melody = he.h.m8_gr_GR_cog.IKekommeni.name
meMA.Ode1C21.incipit = "Υἱοθετήθης ἐν χάριτι Σίλβεστρε, Θεῷ…"
meMA.Ode1C21.text = "Υἱοθετήθης ἐν χάριτι Σίλβεστρε, Θεῷ Πατρὶ καθαρόν, βίον ἀσπασάμενος, καὶ θείαις ἀστραπαῖς, ψυχὴν φωτοειδέστατος, γενόμενος μετῆλθες πρὸς τὸ φῶς, κραυγάζων τὸ ἀνέσπερον· Τῷ Κυρίῳ ᾄσωμεν· ἐνδόξως γὰρ δεδόξασται."

meMA.Ode1C22.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C22.mode = misc_gr_GR_cog.Mode8
meMA.Ode1C22.melody = he.h.m8_gr_GR_cog.IKekommeni.name
meMA.Ode1C22.incipit = "Μεγαλοφρόνως παθῶν κατεκράτησας, καὶ σάρκα…"
meMA.Ode1C22.text = "Μεγαλοφρόνως παθῶν κατεκράτησας, καὶ σάρκα ἀσκητικαῖς, ἀγωγαῖς ὑπέταξας, τῷ Πνεύματι σοφέ, θεῖον καταγώγιον, Τριάδος γεγονὼς περιφανῶς, τῆς πονηρίας τὰ πνεύματα, ψάλλων ἐταπείνωσας· Ἐνδόξως γὰρ δεδόξασται."

meMA.Ode1C23.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C23.mode = misc_gr_GR_cog.Mode8
meMA.Ode1C23.melody = he.h.m8_gr_GR_cog.IKekommeni.name
meMA.Ode1C23.incipit = "Νόμῳ τῷ θείῳ ἐνθέως πειθόμενος…"
meMA.Ode1C23.text = "Νόμῳ τῷ θείῳ ἐνθέως πειθόμενος, καὶ θεοπνεύστου Γραφῆς, γνώσει καλλυνόμενος, Ἑλλήνων τοὺς σοφούς, ἀλήθειαν ἐπαίδευσας, Χριστὸν ὁμολογεῖν σὺν τῷ Πατρί, βοῶντας καὶ τῷ Πνεύματι· Τῷ Κυρίῳ ᾄσωμεν· ἐνδόξως γὰρ δεδόξασται."

meMA.Ode1C24.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C24.mode = misc_gr_GR_cog.Mode8
meMA.Ode1C24.melody = he.h.m8_gr_GR_cog.IKekommeni.name
meMA.Ode1C24.incipit = "Ἐν ὑποστάσει μιᾷ γνωριζόμενον, τὸν…"
meMA.Ode1C24.text = "Ἐν ὑποστάσει μιᾷ γνωριζόμενον, τὸν σαρκωθέντα Χριστόν, μάκαρ ἀνεκήρυξας, θελήσεσι διτταῖς, καὶ ἐνεργείαις Σίλβεστρε, νοούμενον, καὶ πίστει τοὺς αὐτῷ, θεώσαντα κραυγάζοντας· Τῷ Κυρίῳ ᾄσωμεν·ἐνδόξως γὰρ δεδόξασται."

meMA.Ode1C2T.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C2T.mode = misc_gr_GR_cog.Mode8
meMA.Ode1C2T.melody = he.h.m8_gr_GR_cog.IKekommeni.name
meMA.Ode1C2T.incipit = "Ἵνα τοὺς κάτω τοῖς ἄνω…"
meMA.Ode1C2T.text = "Ἵνα τοὺς κάτω τοῖς ἄνω συνάψειεν, ὁ μόνος πάντων Θεός, μήτραν ἀπειρόγαμον, ὑπέδυ καὶ σαρκός, φανεὶς ἐν ὁμοιώματι, τῆς ἔχθρας τὸ μεσότοιχον ἑλών, εἰρήνην ἐμεσίτευσε, καὶ ζωὴν ἐβράβευσε, καὶ θείαν ἀπολύτρωσιν."

meMA.Ode3C1H.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C1H.mode = misc_gr_GR_cog.Mode2
meMA.Ode3C1H.melody = he.h.m2_gr_GR_cog.StereosonImasEnSoiKyrie.melody
meMA.Ode3C1H.incipit = he.h.m2_gr_GR_cog.StereosonImasEnSoiKyrie.incipit
meMA.Ode3C1H.text = he.h.m2_gr_GR_cog.StereosonImasEnSoiKyrie.text

meMA.Ode3C11.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C11.mode = misc_gr_GR_cog.Mode2
meMA.Ode3C11.melody = he.h.m2_gr_GR_cog.StereosonImasEnSoiKyrie.name
meMA.Ode3C11.incipit = "Ἐπέφανε Χριστὸς βοᾷ ὁ Πρόδρομος…"
meMA.Ode3C11.text = "Ἐπέφανε Χριστὸς βοᾷ ὁ Πρόδρομος, καὶ ῥείθροις ἐπιβαίνει τοῦ Ἰορδάνου, ὑπαντῆσαι τούτῳ σπεύσωμεν, καθαραῖς διανοίαις φωτιζόμενοι."

meMA.Ode3C12.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C12.mode = misc_gr_GR_cog.Mode2
meMA.Ode3C12.melody = he.h.m2_gr_GR_cog.StereosonImasEnSoiKyrie.name
meMA.Ode3C12.incipit = "Ζητῶν ἀνοδίαις τὸν πλανώμενον, Θεέ…"
meMA.Ode3C12.text = "Ζητῶν ἀνοδίαις τὸν πλανώμενον, Θεέ μου πτωχείαν πλουσιωτάτην, ἐνδυσάμενος ἐφίστασαι, βαπτισθῆναι ἡ ἁπάντων ἀπολύτρωσις."

meMA.Ode3C13.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C13.mode = misc_gr_GR_cog.Mode2
meMA.Ode3C13.melody = he.h.m2_gr_GR_cog.StereosonImasEnSoiKyrie.name
meMA.Ode3C13.incipit = "Ἡ γῆ καὶ οὐρανὸς νῦν…"
meMA.Ode3C13.text = "Ἡ γῆ καὶ οὐρανὸς νῦν χορευέτωσαν, ὁ πάντων βαπτίζεται εὐεργέτης, ἐν τοῖς ὕδασι βυθίζων ἡμῶν, τὴν πληθὺν τῶν ἀμέτρων παραπτώσεων."

meMA.Ode3C21.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C21.mode = misc_gr_GR_cog.Mode8
meMA.Ode3C21.melody = he.h.m8_gr_GR_cog.TonFovonSouKyrie.name
meMA.Ode3C21.incipit = "Νοὸς καθαρότητα, κτησάμενος Ἱεράρχης γεγένησαι…"
meMA.Ode3C21.text = "Νοὸς καθαρότητα, κτησάμενος Ἱεράρχης γεγένησαι, χρῖσμα ἅγιον δεξάμενος· διὸ σε θεοφόρε Πάτερ εὐφημοῦμεν."

meMA.Ode3C22.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C22.mode = misc_gr_GR_cog.Mode8
meMA.Ode3C22.melody = he.h.m8_gr_GR_cog.TonFovonSouKyrie.name
meMA.Ode3C22.incipit = "Σταυρῷ τὸν τανύσαντα, παλάμας σαρκὶ…"
meMA.Ode3C22.text = "Σταυρῷ τὸν τανύσαντα, παλάμας σαρκὶ Θεὸν ἐτράνωσας, λογικαῖς σου ἀποδείξεσι, καὶ Ἰουδαίων θράσος, ὤλεσας παμμάκαρ."

meMA.Ode3C23.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C23.mode = misc_gr_GR_cog.Mode8
meMA.Ode3C23.melody = he.h.m8_gr_GR_cog.TonFovonSouKyrie.name
meMA.Ode3C23.incipit = "Ἐν θαύμασι μέγιστος, γενόμενος φυγαδεύεις…"
meMA.Ode3C23.text = "Ἐν θαύμασι μέγιστος, γενόμενος φυγαδεύεις τὰ πταίσματα, καὶ καθαίρεις τὰ νοσήματα, τῶν πίστει αἰτουμένων, Σίλβεστρε θέοφρον."

meMA.Ode3C2T.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C2T.mode = misc_gr_GR_cog.Mode8
meMA.Ode3C2T.melody = he.h.m8_gr_GR_cog.TonFovonSouKyrie.name
meMA.Ode3C2T.incipit = "Παρθένος Ἁγνή, μετὰ τὴν κύησιν…"
meMA.Ode3C2T.text = "Παρθένος Ἁγνή, μετὰ τὴν κύησιν Παναγία διέμεινας· Θεὸν Λόγον γὰρ ἐγέννησας, ἡμῖν ὁμοιωθέντα, χωρὶς ἁμαρτίας."

meMA.Ode3H.ode = misc_gr_GR_cog.Ode3
meMA.Ode3H.mode = misc_gr_GR_cog.Mode8
meMA.Ode3H.melody = he.h.m8_gr_GR_cog.TonFovonSouKyrie.melody
meMA.Ode3H.incipit = he.h.m8_gr_GR_cog.TonFovonSouKyrie.incipit
meMA.Ode3H.text = he.h.m8_gr_GR_cog.TonFovonSouKyrie.text

meMA.Ode3Kathisma1.mode = misc_gr_GR_cog.Mode8
meMA.Ode3Kathisma1.melody = he.a.m8_gr_GR_cog.TinSofianKaiLogon.name
meMA.Ode3Kathisma1.incipit = "Ποταμὸς εὐσεβείας ἀναδειχθείς, τῶν δογμάτων…"
meMA.Ode3Kathisma1.text = "Ποταμὸς εὐσεβείας ἀναδειχθείς, τῶν δογμάτων σου ῥείθροις πᾶσαν τὴν γῆν, κατήρδευσας Σίλβεστρε, Ἱεράρχης ὡς ἔνθεος, καὶ θαυμάτων ὄμβροις, παθῶν ῥύπον ἔπλυνας, καὶ τρυφῆς χειμάῤῥουν, ἀξίως κεκλήρωσαι· ὅθεν συνελθόντες, εὐσεβῶς σε τιμῶμεν, ἐν ᾄσμασι σήμερον, καὶ πιστῶς ἐκβοῶμέν σοι· Ἱεράρχα θεσπέσιε, πρέσβευε Χριστῷ τῷ Θεῷ, τῶν πταισμάτων ἄφεσιν δωρήσασθαι, τοῖς ἑορτάζουσι πόθῳ, τὴν Ἁγίαν μνήμην σου."

meMA.Ode3Kathisma2.mode = misc_gr_GR_cog.Mode8
meMA.Ode3Kathisma2.melody = he.a.m8_gr_GR_cog.AnestisEkNekron.name
meMA.Ode3Kathisma2.incipit = "Ἀόρατε Θεέ, ἀκατάληπτε Λόγε…"
meMA.Ode3Kathisma2.text = "Ἀόρατε Θεέ, ἀκατάληπτε Λόγε, ἐτέχθης ἐκ Πατρός, ὑπὲρ φύσιν καὶ λόγον· πάλιν δὲ ἐπʼ ἐσχάτων, ἐκ τῆς Παρθένου οὐκ ἀλλοιώσας ὃ ἦς, ἔρχῃ καὶ ἐν τοῖς ῥείθροις, τοῦ Ἰορδάνου νῦν βαπτισθῆναι σαρκί, ὁ εὐδοκήσας σῶσαι ὡς Θεός, τὸ γένος τῶν ἀνθρώπων."

meMA.Ode4C1H.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C1H.mode = misc_gr_GR_cog.Mode2
meMA.Ode4C1H.melody = he.h.m2_gr_GR_cog.ElilythasEkParthenou.melody
meMA.Ode4C1H.incipit = he.h.m2_gr_GR_cog.ElilythasEkParthenou.incipit
meMA.Ode4C1H.text = he.h.m2_gr_GR_cog.ElilythasEkParthenou.text

meMA.Ode4C11.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C11.mode = misc_gr_GR_cog.Mode2
meMA.Ode4C11.melody = he.h.m2_gr_GR_cog.ElilythasEkParthenou.name
meMA.Ode4C11.incipit = "Θαυμάζει σου, Ἰησοῦ εὐεργέτα τὴν…"
meMA.Ode4C11.text = "Θαυμάζει σου, Ἰησοῦ εὐεργέτα τὴν ἄφιξιν, ὁ μέγιστος Πρόδρομος, καὶ συστελλόμενος κράζει σοι· Πῦρ ὑπάρχων Δέσποτα, μὴ καταφλέξῃς με, χόρτον χρηματίζοντα Κύριε"

meMA.Ode4C12.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C12.mode = misc_gr_GR_cog.Mode2
meMA.Ode4C12.melody = he.h.m2_gr_GR_cog.ElilythasEkParthenou.name
meMA.Ode4C12.incipit = "Ἱστάμενον, ἐν τοῖς ὕδασι τούτοις…"
meMA.Ode4C12.text = "Ἱστάμενον, ἐν τοῖς ὕδασι τούτοις θελήματι, ὁρῶν με νῦν πρόσελθε, ὦ Ἰωάννη καὶ βάπτισον, ὅπως τὴν διʼ ὕδατος, χάριν ἀνθρώπων, τῷ γένει ἀπεργάσωμαι."

meMA.Ode4C13.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C13.mode = misc_gr_GR_cog.Mode2
meMA.Ode4C13.melody = he.h.m2_gr_GR_cog.ElilythasEkParthenou.name
meMA.Ode4C13.incipit = "Κρυπτόμενον, ὡς νεφέλη σε ἔγνωκα…"
meMA.Ode4C13.text = "Κρυπτόμενον, ὡς νεφέλη σε ἔγνωκα Ἥλιον, Θεέ μου τῷ σώματι· πῶς οὖν γυμνοῦσαι τὰ ὕδατα, νῦν περιβαλλόμενος; ὁ Ἰωάννης ἐβόα ἐκπληττόμενος."

meMA.Ode4C14.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C14.mode = misc_gr_GR_cog.Mode2
meMA.Ode4C14.melody = he.h.m2_gr_GR_cog.ElilythasEkParthenou.name
meMA.Ode4C14.incipit = "Λαμπρύνεται, τῶν ἀνθρώπων ἡ φύσις…"
meMA.Ode4C14.text = "Λαμπρύνεται, τῶν ἀνθρώπων ἡ φύσις καὶ λύτρωσιν, τοῦ σκότους εἰσδέχεται, τῆς ἁμαρτίας καὶ ἄνωθεν, θεῖον περιβάλλεται, τῆς ἀφθαρσίας χιτῶνα γυμνουμένου μου."

meMA.Ode4C21.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C21.mode = misc_gr_GR_cog.Mode8
meMA.Ode4C21.melody = he.h.m8_gr_GR_cog.EisakikoaKyrie.name
meMA.Ode4C21.incipit = "Ἱεράρχης θεόληπτος, χρίων Ἱερέας θείῳ…"
meMA.Ode4C21.text = "Ἱεράρχης θεόληπτος, χρίων Ἱερέας θείῳ ἐν Πνεύματι, ἀνεδείχθης Πάτερ Σίλβεστρε, καὶ λαοὺς φωτίζων ἱερώτατε."

meMA.Ode4C22.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C22.mode = misc_gr_GR_cog.Mode8
meMA.Ode4C22.melody = he.h.m8_gr_GR_cog.EisakikoaKyrie.name
meMA.Ode4C22.incipit = "Συνεργείᾳ τοῦ Πνεύματος, νόσους ἀπελαύνων…"
meMA.Ode4C22.text = "Συνεργείᾳ τοῦ Πνεύματος, νόσους ἀπελαύνων πάθη ἀνίατα, ἐθεράπευσας πανόλβιε, ἐπιστρέφων πλάνης τοὺς παράφρονας."

meMA.Ode4C23.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C23.mode = misc_gr_GR_cog.Mode8
meMA.Ode4C23.melody = he.h.m8_gr_GR_cog.EisakikoaKyrie.name
meMA.Ode4C23.incipit = "Τῶν αἱρέσεων Σίλβεστρε, πλάνην ἐκδιώξας…"
meMA.Ode4C23.text = "Τῶν αἱρέσεων Σίλβεστρε, πλάνην ἐκδιώξας, ποίμνην ἐποίμανας, εὐσεβείας βρύων ὕδατα, ἐπὶ χλόην θείας ἐπιγνώσεως."

meMA.Ode4C24.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C24.mode = misc_gr_GR_cog.Mode8
meMA.Ode4C24.melody = he.h.m8_gr_GR_cog.EisakikoaKyrie.name
meMA.Ode4C24.incipit = "Ἐξεχύθη σου χείλεσι, χάρις θείου…"
meMA.Ode4C24.text = "Ἐξεχύθη σου χείλεσι, χάρις θείου Πνεύματος· διὰ τοῦτο σε, ἱερέα τοῦ λαοῦ αὐτοῦ, ὁ Θεὸς παμμάκαρ Πάτερ ἔχρισεν."

meMA.Ode4C2T.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C2T.mode = misc_gr_GR_cog.Mode8
meMA.Ode4C2T.melody = he.h.m8_gr_GR_cog.EisakikoaKyrie.name
meMA.Ode4C2T.incipit = "Ἴασαί μου τὰ τραύματα, Κόρη…"
meMA.Ode4C2T.text = "Ἴασαί μου τὰ τραύματα, Κόρη τῆς καρδίας, καὶ κατευόδωσον, τῆς ψυχῆς μου τὰ κινήματα, εἰς Θεοῦ Παρθένε τὰ θελήματα."

meMA.Ode5C1H.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C1H.mode = misc_gr_GR_cog.Mode2
meMA.Ode5C1H.melody = he.h.m2_gr_GR_cog.OTouFotosChorigos.melody
meMA.Ode5C1H.incipit = he.h.m2_gr_GR_cog.OTouFotosChorigos.incipit
meMA.Ode5C1H.text = he.h.m2_gr_GR_cog.OTouFotosChorigos.text

meMA.Ode5C11.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C11.mode = misc_gr_GR_cog.Mode2
meMA.Ode5C11.melody = he.h.m2_gr_GR_cog.OTouFotosChorigos.name
meMA.Ode5C11.incipit = "Μετανοεῖτε βοῶν, ἡ Βασιλεία τοῦ…"
meMA.Ode5C11.text = "Μετανοεῖτε βοῶν, ἡ Βασιλεία τοῦ Θεοῦ ἤγγικεν, ὁ Ἱερὸς Πρόδρομος ἐφάνη, ἐκ τῆς ἐρημίας, τοὺς ἐρημωθέντας ἡμᾶς ἀνακτώμενος."

meMA.Ode5C12.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C12.mode = misc_gr_GR_cog.Mode2
meMA.Ode5C12.melody = he.h.m2_gr_GR_cog.OTouFotosChorigos.name
meMA.Ode5C12.incipit = "Νόμος ὁ πάλαι φησί, καὶ…"
meMA.Ode5C12.text = "Νόμος ὁ πάλαι φησί, καὶ προφητῶν ὁ ἱερὸς σύλλογος· Ἦλθε Χριστὸς ἡμᾶς ἀναπλάττων, τῷ θείῳ βαπτισμῷ, τοὺς παλαιωθέντας, πολλοῖς παραπτώμασι."

meMA.Ode5C13.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C13.mode = misc_gr_GR_cog.Mode2
meMA.Ode5C13.melody = he.h.m2_gr_GR_cog.OTouFotosChorigos.name
meMA.Ode5C13.incipit = "Ξένα φησὶ καὶ φρικτά, ὁ…"
meMA.Ode5C13.text = "Ξένα φησὶ καὶ φρικτά, ὁ Ἰωάννης καθορῶ σήμερον! Νῦν ἡ πηγή, τῆς ἀθανασίας, προῆλθεν ἐκ μικρᾶς, ῥανίδος τὸ θεῖον, αἰτήσασθαι βάπτισμα."

meMA.Ode5C21.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C21.mode = misc_gr_GR_cog.Mode8
meMA.Ode5C21.melody = he.h.m8_gr_GR_cog.InaTiMeAposo.name
meMA.Ode5C21.incipit = "Ταῖς πλοκαῖς τῶν σῶν λόγων…"
meMA.Ode5C21.text = "Ταῖς πλοκαῖς τῶν σῶν λόγων, ἅπαντα διέλυσας μάταιον σύνδεσμον, καὶ πρὸς θείαν Πίστιν, τοὺς τῇ πλάνῃ δεθέντας συνέδησας, διανοίγων Πάτερ, τούτων τὸν νοῦν τῇ ἀναπτύξει, τῶν Γραφῶν Ἱεράρχα μακάριε."

meMA.Ode5C22.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C22.mode = misc_gr_GR_cog.Mode8
meMA.Ode5C22.melody = he.h.m8_gr_GR_cog.InaTiMeAposo.name
meMA.Ode5C22.incipit = "Ἡ καρδία σου Πάτερ, ἄμωμος…"
meMA.Ode5C22.text = "Ἡ καρδία σου Πάτερ, ἄμωμος γεγένηται τοῖς δικαιώμασι, τοῦ κοινοῦ Δεσπότου, ὂν ποθήσας σαρκὸς κατεκράτησας, καὶ θαυμάτων χάριν, πεπλουτηκὼς νόσους ἴασαι, καὶ ψυχῶν χαλεπώτατα τραύματα."

meMA.Ode5C23.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C23.mode = misc_gr_GR_cog.Mode8
meMA.Ode5C23.melody = he.h.m8_gr_GR_cog.InaTiMeAposo.name
meMA.Ode5C23.incipit = "Νεμομένης καρδίας, νόσον ψυχολέτειραν δεινῆς…"
meMA.Ode5C23.text = "Νεμομένης καρδίας, νόσον ψυχολέτειραν δεινῆς αἱρέσεως, τοῦ πικρῶς μανέντος, δραστηρίοις φαρμάκοις τῶν λόγων σου, θεοφόρε στήσας, ὡς ἰατρὸς χάριτι θείᾳ τοῦ σεπτοῦ ἐφυγάδευσας Πνεύματος."

meMA.Ode5C2T.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C2T.mode = misc_gr_GR_cog.Mode8
meMA.Ode5C2T.melody = he.h.m8_gr_GR_cog.InaTiMeAposo.name
meMA.Ode5C2T.incipit = "Χαῖρε μόνη τεκοῦσα, τὸν Ἀπειροδύναμον…"
meMA.Ode5C2T.text = "Χαῖρε μόνη τεκοῦσα, τὸν Ἀπειροδύναμον σάρκα πτωχεύσαντα. Χαῖρε τοῦ πεσόντος, ἐπανόρθωσις κόσμου Πανάμωμε. Χαῖρε λύπης λύσις, χαῖρε πιστῶν ἡ σωτηρία, χαῖρε θρόνε Θεοῦ ὑψηλότατε."

meMA.Ode6C1H.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C1H.mode = misc_gr_GR_cog.Mode2
meMA.Ode6C1H.melody = he.h.m2_gr_GR_cog.AvyssosAmartimaton.melody
meMA.Ode6C1H.incipit = he.h.m2_gr_GR_cog.AvyssosAmartimaton.incipit
meMA.Ode6C1H.text = he.h.m2_gr_GR_cog.AvyssosAmartimaton.text

meMA.Ode6C11.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C11.mode = misc_gr_GR_cog.Mode2
meMA.Ode6C11.melody = he.h.m2_gr_GR_cog.AvyssosAmartimaton.name
meMA.Ode6C11.incipit = "Ὄμβρησον ἁμαρτημάτων, ἡμῖν φιλάνθρωπε λύσιν…"
meMA.Ode6C11.text = "Ὄμβρησον ἁμαρτημάτων, ἡμῖν φιλάνθρωπε λύσιν, ὁ τοῦ Ἰορδάνου ταῖς ῥοαῖς, βυθίσας τὰ πταίσματα, τῶν γηγενῶν, διὰ ἔλεος μέγα."

meMA.Ode6C12.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C12.mode = misc_gr_GR_cog.Mode2
meMA.Ode6C12.melody = he.h.m2_gr_GR_cog.AvyssosAmartimaton.name
meMA.Ode6C12.incipit = "Πέλαγος Χριστὲ ὑπάρχων, δικαιοσύνης προέρχῃ…"
meMA.Ode6C12.text = "Πέλαγος Χριστὲ ὑπάρχων, δικαιοσύνης προέρχῃ, νῦν ἐν Ἰορδάνῃ ποταμῷ, βυθίσαι τὸν δράκοντα, καὶ τοῦ Ἀδάμ, τὴν παράβασιν πλῦναι."

meMA.Ode6C13.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C13.mode = misc_gr_GR_cog.Mode2
meMA.Ode6C13.melody = he.h.m2_gr_GR_cog.AvyssosAmartimaton.name
meMA.Ode6C13.incipit = "Ῥεῖθρον σε τῆς ἀφθαρσίας, Χριστὲ…"
meMA.Ode6C13.text = "Ῥεῖθρον σε τῆς ἀφθαρσίας, Χριστὲ ὑπάρχοντα ὄντως, ῥεῖθρα πῶς χωρήσει ποταμοῦ; ἐβόα ὁ Πρόδρομος, δοξάζων σου, τὴν πολλὴν εὐσπλαγχνίαν."

meMA.Ode6C21.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C21.mode = misc_gr_GR_cog.Mode8
meMA.Ode6C21.melody = he.h.m8_gr_GR_cog.TinDeisinEkcheo.name
meMA.Ode6C21.incipit = "Ἀπρόϊτον καὶ φρουρᾷ αἰωνίᾳ, συγκλειόμενον…"
meMA.Ode6C21.text = "Ἀπρόϊτον καὶ φρουρᾷ αἰωνίᾳ, συγκλειόμενον εἰργάσω εὐχαῖς σου, τὸν πονηρόν, λυμαινόμενον φθόνῳ, τοὺς προσιόντας μακάριε δράκοντα, ὡς θύρας τε, καὶ ὡς μοχλούς, τοῦ Σταυροῦ τὴν σφραγῖδα ἐνθέμενος."

meMA.Ode6C22.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C22.mode = misc_gr_GR_cog.Mode8
meMA.Ode6C22.melody = he.h.m8_gr_GR_cog.TinDeisinEkcheo.name
meMA.Ode6C22.incipit = "Ῥημάτων σου μελιῤῥύτων ὁ φθόγγος…"
meMA.Ode6C22.text = "Ῥημάτων σου μελιῤῥύτων ὁ φθόγγος, εἰς τὴν σύμπασαν ἐξῆλθε Παμμάκαρ, καὶ διδαχῶν, εἰς τὰ πέρατα πάντα, τῆς οἰκουμένης τὸ φέγγος ἐξέλαμψε, θαυμάτων τε τὸ διαυγές, καὶ τῶν ἔργων ἡ θεία ἐπίδειξις."

meMA.Ode6C23.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C23.mode = misc_gr_GR_cog.Mode8
meMA.Ode6C23.melody = he.h.m8_gr_GR_cog.TinDeisinEkcheo.name
meMA.Ode6C23.incipit = "Ἰάματα καὶ χαρίσματα θεῖα, ἐπομβρίαις…"
meMA.Ode6C23.text = "Ἰάματα καὶ χαρίσματα θεῖα, ἐπομβρίαις ἀναβλύζων πλουσίως, τοῦ παντουργοῦ, Ἱεράρχα παμμάκαρ, καὶ Παναγίου ἑκάστοτε Πνεύματος, τοὺς πίστει σοι εἰλικρινεῖ, προσιόντας ἴασαι ἐν χάριτι."

meMA.Ode6C2T.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C2T.mode = misc_gr_GR_cog.Mode8
meMA.Ode6C2T.melody = he.h.m8_gr_GR_cog.TinDeisinEkcheo.name
meMA.Ode6C2T.incipit = "Νοήσαντες θεηγόροι Προφῆται, Μυστηρίου σου…"
meMA.Ode6C2T.text = "Νοήσαντες θεηγόροι Προφῆται, Μυστηρίου σου Παρθένε τὸ βάθος, προφητικῶς, προκατήγγειλαν τοῦτο, φωταγωγούμενοι θείῳ ἐν Πνεύματι, καὶ νῦν ἡμεῖς περιχαρῶς, τὰς ἐμφάσεις ὁρῶντες πιστεύομεν."

meMA.Ode6H.ode = misc_gr_GR_cog.Ode6
meMA.Ode6H.mode = misc_gr_GR_cog.Mode8
meMA.Ode6H.melody = he.h.m8_gr_GR_cog.TinDeisinEkcheo.melody
meMA.Ode6H.incipit = he.h.m8_gr_GR_cog.TinDeisinEkcheo.incipit
meMA.Ode6H.text = he.h.m8_gr_GR_cog.TinDeisinEkcheo.text

meMA.Kontakion1.mode = misc_gr_GR_cog.Mode4
meMA.Kontakion1.melody = he.a.m4_gr_GR_cog.EpefanisSimeron.name
meMA.Kontakion1.incipit = "Ἐν τοῖς ῥείθροις σήμερον τοῦ…"
meMA.Kontakion1.text = "Ἐν τοῖς ῥείθροις σήμερον τοῦ Ἰορδάνου, γεγονὼς ὁ Κύριος, τῷ Ἰωάννῃ ἐκβοᾷ· Μὴ δειλιάσῃς βαπτίσαι με· σῶσαι γὰρ ἥκω, Ἀδὰμ τὸν πρωτόπλαστον."

meMA.Oikos1.text = "Οὐκ ἀπαιτῷ σε, Βαπτιστά, τοὺς ὅρους ὑπερβῆναι, οὐ λέγω σοι· Εἰπέ μοι ἃ λέγεις τοῖς ἀνόμοις, καὶ παραινεῖς ἁμαρτωλοῖς, μόνον βάπτισόν με σιωπῶν, καὶ προσδοκῶν τὰ ὑπὸ τοῦ Βαπτίσματος· ἕξεις γὰρ διὰ τοῦτο ἀξίωμα, ὅπερ οὐχ ὑπῆρξε τοῖς Ἀγγέλοις· καὶ γὰρ πάντων τῶν Προφητῶν μείζονὰ σε ποιήσω· ἐκείνων μὲν οὐδεὶς σαφῶς με κατεῖδεν, ἀλλʼ ἐν τύποις καὶ σκιαῖς καὶ ἐνυπνίοις, σὺ δέ, ἐπὶ σοῦ ἱστάμενον κατὰ γνώμην· σῶσαι γὰρ ἥκω, Ἀδὰμ τὸν πρωτόπλαστον."

meMA.Kontakion2.mode = misc_gr_GR_cog.Mode4
meMA.Kontakion2.melody = he.a.m2_gr_GR_cog.TaAnoZiton.name
meMA.Kontakion2.incipit = "Σοφίας Θεοῦ, ἐμπλήσας τὸ στόμα…"
meMA.Kontakion2.text = "Σοφίας Θεοῦ, ἐμπλήσας τὸ στόμα σου, Τριάδος ἡμῖν, τὴν γνῶσιν ἐτράνωσας, καὶ τὴν ἀθεότητα τῶν τυράννων, κατέβαλες Σίλβεστρε, τῇ σφεδόνι τῶν λόγων σου· διὸ ὑπὲρ ἡμῶν δυσώπει τὸν Κύριον."

meMA.Oikos2.text = "Τοῦ Κορυφαίου τῶν Ἀποστόλων τὴν καθέδραν πλουτήσας, τοῦ Θεοῦ Λειτουργὸς ἐδείχθης θαυμασιώτατος, ὡραΐζων, στηρίζων, καὶ μεγαλύνων δόγμασι θείοις τὴν Ἐκκλησίαν, ὡς ἀστὴρ δὲ φωτοβόλος, φωτίζων φωτὶ ἀρετῶν, Τριάδα σέβειν διδάσκεις, ὡς ἀμέριστον μίαν Θεότητα· καὶ τὰς αἱρέσεις τῶν δυσμενῶν ἀπεδίωξας, Σίλβεστρε πάνσοφε· διὸ ὑπὲρ ἡμῶν δυσώπει τὸν Κύριον."

meMA.Ode7C1H.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C1H.mode = misc_gr_GR_cog.Mode2
meMA.Ode7C1H.melody = he.h.m2_gr_GR_cog.EikonosChrysis.melody
meMA.Ode7C1H.incipit = he.h.m2_gr_GR_cog.EikonosChrysis.incipit
meMA.Ode7C1H.text = he.h.m2_gr_GR_cog.EikonosChrysis.text

meMA.Ode7C11.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C11.mode = misc_gr_GR_cog.Mode2
meMA.Ode7C11.melody = he.h.m2_gr_GR_cog.EikonosChrysis.name
meMA.Ode7C11.incipit = "Στολὴν ἱεράν, ἐπενδύων με Χριστὲ…"
meMA.Ode7C11.text = "Στολὴν ἱεράν, ἐπενδύων με Χριστὲ τὸν γυμνωθέντα, τῇ συμβουλίᾳ τοῦ ἀλάστορος, γυμνὸς προβῆναι τοῖς ὕδασιν, εἵλου ἀγαθότητος πλούτῳ· ἀνυμνῶ σου τὸ εὔσπλαγχνον, καὶ προσκυνῶ τὴν θαυμαστὴν οἰκονομίαν σου."

meMA.Ode7C12.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C12.mode = misc_gr_GR_cog.Mode2
meMA.Ode7C12.melody = he.h.m2_gr_GR_cog.EikonosChrysis.name
meMA.Ode7C12.incipit = "Τίς αὕτη Χριστέ, ἡ πολλὴ…"
meMA.Ode7C12.text = "Τίς αὕτη Χριστέ, ἡ πολλὴ σου πρὸς ἡμᾶς οἰκονομία; τίς ἡ πτωχεία; τίς ἡ ἄφατος καὶ ὑπὲρ νοῦν συγκατάβασις; ἔφη ὁπηνίκα σε εἶδεν, ὁ Ἰωάννης γυμνούμενον, καὶ βαπτισθῆναι ὑπʼ αὐτοῦ ἐγκελευόμενον."

meMA.Ode7C13.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C13.mode = misc_gr_GR_cog.Mode2
meMA.Ode7C13.melody = he.h.m2_gr_GR_cog.EikonosChrysis.name
meMA.Ode7C13.incipit = "Ὑφῆψας Χριστέ, ὥς περ λύχνον…"
meMA.Ode7C13.text = "Ὑφῆψας Χριστέ, ὥς περ λύχνον τὴν σεπτὴν σάρκα σου μέσον, τοῦ Ἰορδάνου καὶ ἐζήτησας, τὴν συγχωσθεῖσαν τοῖς πάθεσι, καὶ τῇ ἁμαρτίᾳ εἰκόνα, καὶ εὑρὼν κατεκάλλυνας, τῷ Βαπτισμῷ σου ἀγαθέ· ὅθεν ὑμνοῦμεν σε."

meMA.Ode7C21.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C21.mode = misc_gr_GR_cog.Mode8
meMA.Ode7C21.melody = he.h.m8_gr_GR_cog.OiEkTisIoudaias.name
meMA.Ode7C21.incipit = "Δίκαιος ὥσ περ φοῖνιξ, ἐν…"
meMA.Ode7C21.text = "Δίκαιος ὥσ περ φοῖνιξ, ἐν τῷ οἴκῳ Κυρίου Πάτερ ἐξήνθησας, καρποὺς δικαιοσύνης, ἐξ ὧν οἱ σὲ τιμῶντες, διατρέφονται ψάλλοντες· Ὁ τῶν Πατέρων ἡμῶν, Θεὸς εὐλογητὸς εἶ."

meMA.Ode7C22.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C22.mode = misc_gr_GR_cog.Mode8
meMA.Ode7C22.melody = he.h.m8_gr_GR_cog.OiEkTisIoudaias.name
meMA.Ode7C22.incipit = "Ἰσχυραῖς τῶν σῶν λόγων, ἀποδείξεσιν…"
meMA.Ode7C22.text = "Ἰσχυραῖς τῶν σῶν λόγων, ἀποδείξεσιν εἷλες τοὺς ματαιόφρονας, νοσοῦντας ἀπιστίαν, καὶ χαίρων ἐμελῴδεις, παναοίδιμε Σίλβεστρε· ὁ τῶν Πατέρων ἡμῶν, Θεὸς εὐλογητὸς εἶ."

meMA.Ode7C23.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C23.mode = misc_gr_GR_cog.Mode8
meMA.Ode7C23.melody = he.h.m8_gr_GR_cog.OiEkTisIoudaias.name
meMA.Ode7C23.incipit = "Διαμένει ἡ δόξα, εἰς αἰῶνάς…"
meMA.Ode7C23.text = "Διαμένει ἡ δόξα, εἰς αἰῶνάς σου Πάτερ καὶ τὸ μνημόσυνον, σὺν πᾶσι τοῖς Δικαίοις, μεθʼ ὧν ἡμῶν τῶν πίστει, μελῳδούντων μνημόνευε· Ὁ τῶν Πατέρων ἡμῶν, Θεὸς εὐλογητὸς εἶ."

meMA.Ode7C24.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C24.mode = misc_gr_GR_cog.Mode8
meMA.Ode7C24.melody = he.h.m8_gr_GR_cog.OiEkTisIoudaias.name
meMA.Ode7C24.incipit = "Ὁ κλεινὸς Ἱεράρχης, τῇ τοῦ…"
meMA.Ode7C24.text = "Ὁ κλεινὸς Ἱεράρχης, τῇ τοῦ Πνεύματος αἴγλῃ καταλαμπόμενος, τὸ σκότος τῆς κακίας, διέλυσε καὶ πάντας, τοὺς βοῶντας ἐφώτισεν· Ὁ τῶν Πατέρων ἡμῶν, Θεὸς εὐλογητὸς εἶ."

meMA.Ode7C2T.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C2T.mode = misc_gr_GR_cog.Mode8
meMA.Ode7C2T.melody = he.h.m8_gr_GR_cog.OiEkTisIoudaias.name
meMA.Ode7C2T.incipit = "Ὑπερθαύμαστε Κόρη, τὸν πρὸ πάντων…"
meMA.Ode7C2T.text = "Ὑπερθαύμαστε Κόρη, τὸν πρὸ πάντων αἰώνων ἐκυοφόρησας, ἡμῖν ὁμοιωθέντα, διʼ ἄκραν εὐσπλαγχνίαν, ἵνα σώσῃ τοὺς ψάλλοντας· ὁ τῶν Πατέρων ἡμῶν, Θεὸς εὐλογητὸς εἶ."

meMA.Ode8C1H.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C1H.mode = misc_gr_GR_cog.Mode2
meMA.Ode8C1H.melody = he.h.m2_gr_GR_cog.TonEnKaminoTouPyros.melody
meMA.Ode8C1H.incipit = he.h.m2_gr_GR_cog.TonEnKaminoTouPyros.incipit
meMA.Ode8C1H.text = he.h.m2_gr_GR_cog.TonEnKaminoTouPyros.text

meMA.Ode8C11.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C11.mode = misc_gr_GR_cog.Mode2
meMA.Ode8C11.melody = he.h.m2_gr_GR_cog.TonEnKaminoTouPyros.name
meMA.Ode8C11.incipit = "Φωνὴ βοῶντος ἐμφανῶς, ἐν ἐρήμῳ…"
meMA.Ode8C11.text = "Φωνὴ βοῶντος ἐμφανῶς, ἐν ἐρήμῳ ἐπιστὰς ὁ Ἰωάννης, ἐπιστρέφων καρδίας, μετανοούσας θερμῶς δοξάζειν τὸν ἐπιφαινόμενον, ἐν τῷ Ἰορδάνῃ, Σωτῆρα καὶ Δεσπότην."

meMA.Ode8C12.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C12.mode = misc_gr_GR_cog.Mode2
meMA.Ode8C12.melody = he.h.m2_gr_GR_cog.TonEnKaminoTouPyros.name
meMA.Ode8C12.incipit = "Χειμάῤῥους πέφυκας τρυφῆς, ὁ θαλάσσας…"
meMA.Ode8C12.text = "Χειμάῤῥους πέφυκας τρυφῆς, ὁ θαλάσσας καὶ πηγὰς δημιουργήσας, καὶ πῶς ὕδασι βαίνεις; τί ἀποπλῦναι ζητῶν; ἡ πάντων ἔκπλυσις καὶ κάθαρσις, τῶν ὑμνολογούντων, Χριστὲ σε εἰς αἰῶνας."

meMA.Ode8C13.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C13.mode = misc_gr_GR_cog.Mode2
meMA.Ode8C13.melody = he.h.m2_gr_GR_cog.TonEnKaminoTouPyros.name
meMA.Ode8C13.incipit = "Ψῦξαι τὰ ῥεύματα ζητῶν, τῆς…"
meMA.Ode8C13.text = "Ψῦξαι τὰ ῥεύματα ζητῶν, τῆς κακίας τοῦ ἐχθροῦ καὶ τὰς θαλάσσας τῶν παθῶν ἀναστεῖλαι, καὶ τοῖς πιστοῖς ἱλασμόν, πηγάσαι καὶ ἄφεσιν Δέσποτα, ἔρχῃ βαπτισθῆναι, ἐν ῥείθροις, Ἰορδάνου."

meMA.Ode8C14.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C14.mode = misc_gr_GR_cog.Mode2
meMA.Ode8C14.melody = he.h.m2_gr_GR_cog.TonEnKaminoTouPyros.name
meMA.Ode8C14.incipit = "Ὡρῶν καὶ χρόνων Ποιητά, ὑπὸ…"
meMA.Ode8C14.text = "Ὡρῶν καὶ χρόνων Ποιητά, ὑπὸ χρόνον γεγονὼς διʼ εὐσπλαγχνίαν, ὁ ἀχρόνως ἐκλάμψας, ἐκ προανάρχου Πατρός, ἐπέστης τὰ χρόνια πταίσματα πάντων ἀποπλῦναι, ἐν ῥείθροις Ἰορδάνου."

meMA.Ode8C21.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C21.mode = misc_gr_GR_cog.Mode8
meMA.Ode8C21.melody = he.h.m8_gr_GR_cog.NikitaiTyrannou.name
meMA.Ode8C21.incipit = "Μίαν φύσιν Πάτερ, τῆς μιᾶς…"
meMA.Ode8C21.text = "Μίαν φύσιν Πάτερ, τῆς μιᾶς θεότητος θεολογήσας, ἐν τρισὶ προσώποις, νοουμένης ἔμελπες ἀκαταπαύστως· Εὐλογεῖτε πάντα τὰ ἔργα τὸν Κύριον, καὶ ὑπερυψοῦτε, εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8C22.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C22.mode = misc_gr_GR_cog.Mode8
meMA.Ode8C22.melody = he.h.m8_gr_GR_cog.NikitaiTyrannou.name
meMA.Ode8C22.incipit = "Ἀληθείας Πάτερ, ἀληθὴς διδάσκαλος ἀνηγορεύθης…"
meMA.Ode8C22.text = "Ἀληθείας Πάτερ, ἀληθὴς διδάσκαλος ἀνηγορεύθης, ψευδωνύμου πλάνης ὅθεν ἐστηλίτευσας ἀπάτην μέλπων· Εὐλογεῖτε πάντα τὰ ἔργα τὸν Κύριον, καὶ ὑπερυψοῦτε, εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8C23.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C23.mode = misc_gr_GR_cog.Mode8
meMA.Ode8C23.melody = he.h.m8_gr_GR_cog.NikitaiTyrannou.name
meMA.Ode8C23.incipit = "Κορυφαῖος ὤφθης, ἱεροῦ συστήματος ἱερομύστα…"
meMA.Ode8C23.text = "Κορυφαῖος ὤφθης, ἱεροῦ συστήματος ἱερομύστα, καὶ τοῦ Κορυφαίου, Μαθητῶν ἐκόσμησας βοῶν τὸν θρόνον· Εὐλογεῖτε πάντα τὰ ἔργα τὸν Κύριον, καὶ ὑπερυψοῦτε, εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8C24.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C24.mode = misc_gr_GR_cog.Mode8
meMA.Ode8C24.melody = he.h.m8_gr_GR_cog.NikitaiTyrannou.name
meMA.Ode8C24.incipit = "Ἀρεταῖς ἐνθέοις, Πάτερ τὴν καρδίαν…"
meMA.Ode8C24.text = "Ἀρεταῖς ἐνθέοις, Πάτερ τὴν καρδίαν σου κατακαλλύνας, ὅλος ὡραιώθης, καὶ ταῖς ἄνω τάξεσι βοῶν συνήφθης· Εὐλογεῖτε πάντα τὰ ἔργα τὸν Κύριον, καὶ ὑπερυψοῦτε, εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8C2T.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C2T.mode = misc_gr_GR_cog.Mode8
meMA.Ode8C2T.melody = he.h.m8_gr_GR_cog.NikitaiTyrannou.name
meMA.Ode8C2T.incipit = "Ῥήμασί σου θείοις, γενεαὶ ἑπόμεναι…"
meMA.Ode8C2T.text = "Ῥήμασί σου θείοις, γενεαὶ ἑπόμεναι αἱ τῶν ἀνθρώπων, μακαρίζομέν σε τὴν ἀειμακάριστον καὶ μελῳδοῦμεν· Εὐλογεῖτε πάντα τὰ ἔργα τὸν Κύριον, καὶ ὑπερυψοῦτε, εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8H.ode = misc_gr_GR_cog.Ode8
meMA.Ode8H.mode = misc_gr_GR_cog.Mode8
meMA.Ode8H.melody = he.h.m8_gr_GR_cog.NikitaiTyrannou.melody
meMA.Ode8H.incipit = he.h.m8_gr_GR_cog.NikitaiTyrannou.incipit
meMA.Ode8H.text = he.h.m8_gr_GR_cog.NikitaiTyrannou.text

meMA.Ode9C1H.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C1H.mode = misc_gr_GR_cog.Mode2
meMA.Ode9C1H.melody = he.h.m2_gr_GR_cog.TonEkTheouTheonLogon.melody
meMA.Ode9C1H.incipit = he.h.m2_gr_GR_cog.TonEkTheouTheonLogon.incipit
meMA.Ode9C1H.text = he.h.m2_gr_GR_cog.TonEkTheouTheonLogon.text

meMA.Ode9C11.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C11.mode = misc_gr_GR_cog.Mode2
meMA.Ode9C11.melody = he.h.m2_gr_GR_cog.TonEkTheouTheonLogon.name
meMA.Ode9C11.incipit = "Ἰδών σε ὁ Ἰωάννης, πρὸς…"
meMA.Ode9C11.text = "Ἰδών σε ὁ Ἰωάννης, πρὸς αὐτὸν Εὐεργέτα, ἐλθόντα καὶ αἰτοῦντα παρʼ αὐτοῦ, ἐν ταπεινώσει τὸ Βάπτισμα, ἐκπλαγεὶς συνεστάλη, μηδόλως χειραπτῆσαί σε τολμῶν, τὸν ἁπτόμενον πάντα, ὀρέων καὶ καπνίζονται."

meMA.Ode9C12.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C12.mode = misc_gr_GR_cog.Mode2
meMA.Ode9C12.melody = he.h.m2_gr_GR_cog.TonEkTheouTheonLogon.name
meMA.Ode9C12.incipit = "Ὡς χελιδὼν σεμνοτάτη, προμηνύουσα ἔαρ…"
meMA.Ode9C12.text = "Ὡς χελιδὼν σεμνοτάτη, προμηνύουσα ἔαρ, ἐπέστη νοητὸν ὁ Βαπτιστής, τοῖς ἐν χειμῶνι ὑπάρχουσι, τῶν ἀμέτρων πταισμάτων, καὶ ζάλην ὑπομένουσιν ἀεί, ἐναντίων πνευμάτων, πονηροτάτοις πάθεσι."

meMA.Ode9C13.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C13.mode = misc_gr_GR_cog.Mode2
meMA.Ode9C13.melody = he.h.m2_gr_GR_cog.TonEkTheouTheonLogon.name
meMA.Ode9C13.incipit = "Σὲ τὸ ἀπαύγασμα δόξης, τοῦ…"
meMA.Ode9C13.text = "Σὲ τὸ ἀπαύγασμα δόξης, τοῦ Πατρὸς ἐπιλάμψαν, τοῖς σκότει καθημένοις τῶν δεινῶν, ἄδυτε Ἥλιε Κύριε, καὶ ἐρχόμενον πάντας, φωτίσαι ἱερῷ σοῦ Βαπτισμῷ ὡς Θεὸν καὶ Σωτῆρα, ἐν ὕμνοις μεγαλύνομεν."

meMA.Ode9C14.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C14.mode = misc_gr_GR_cog.Mode2
meMA.Ode9C14.melody = he.h.m2_gr_GR_cog.TonEkTheouTheonLogon.name
meMA.Ode9C14.incipit = "Ἡ τοῦ θανάτου ἀξίνη, ἐν…"
meMA.Ode9C14.text = "Ἡ τοῦ θανάτου ἀξίνη, ἐν τῇ ῥίζῃ κειμένῃ, ὡς δένδρῳ ἀπειλεῖ τὴν ἐκτομήν, τὴν ἀκαρπίαν σου βλέπουσα, μὴ οὖν ἄκαρπος μείνῃς, ψυχή μου, ἀλλὰ βόησον Θεῷ, ἐν θερμῇ μετανοίᾳ· Ἡμάρτηκά σοι, σῶσόν με."

meMA.Ode9C15.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C15.mode = misc_gr_GR_cog.Mode2
meMA.Ode9C15.melody = he.h.m2_gr_GR_cog.TonEkTheouTheonLogon.name
meMA.Ode9C15.incipit = "Φωνὴ βοῶντος ἠκούσθη, ἐν ἐρήμοις…"
meMA.Ode9C15.text = "Φωνὴ βοῶντος ἠκούσθη, ἐν ἐρήμοις καρδίαις· Εὐφράνθητε, ἐπέστη ὁ Χριστός, ἄφεσιν πᾶσι δωρούμενος, ἁγιάσθητε πᾶσα, ἡ θάλασσα, πηγαὶ καὶ ποταμοί, καὶ κοιλάδες καὶ νάπαι, καὶ πᾶσα ἡ ὑφήλιος."

meMA.Ode9C21.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C21.mode = misc_gr_GR_cog.Mode8
meMA.Ode9C21.melody = he.h.m8_gr_GR_cog.KyriosTheotokon.name
meMA.Ode9C21.incipit = "Ἱέρευσας προθύμως, σεαυτὸν Παμμάκαρ, τῶν…"
meMA.Ode9C21.text = "Ἱέρευσας προθύμως, σεαυτὸν Παμμάκαρ, τῶν ἡδονῶν τῇ νεκρώσει καὶ γέγονας, τοῦ διʼ ἡμᾶς νεκρωθέντος, θῦμα εὐπρόσδεκτον."

meMA.Ode9C22.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C22.mode = misc_gr_GR_cog.Mode8
meMA.Ode9C22.melody = he.h.m8_gr_GR_cog.KyriosTheotokon.name
meMA.Ode9C22.incipit = "Ὡς θεῖος Κορυφαῖος, ἱερῶν Πατέρων…"
meMA.Ode9C22.text = "Ὡς θεῖος Κορυφαῖος, ἱερῶν Πατέρων, ἱερώτατον δόγμα ἐκράτυνας, αἱρετικῶν ἀποφράξας, ἄθεα στόματα."

meMA.Ode9C23.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C23.mode = misc_gr_GR_cog.Mode8
meMA.Ode9C23.melody = he.h.m8_gr_GR_cog.KyriosTheotokon.name
meMA.Ode9C23.incipit = "Συνήφθης ταῖς χορείαις, ταῖς τῶν…"
meMA.Ode9C23.text = "Συνήφθης ταῖς χορείαις, ταῖς τῶν Ἀσωμάτων, τοὺς ἀσωμάτους ἐχθροὺς τροπωσάμενος, περιφανῶς Ἱεράρχα, θέοφρον Σίλβεστρε."

meMA.Ode9C24.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C24.mode = misc_gr_GR_cog.Mode8
meMA.Ode9C24.melody = he.h.m8_gr_GR_cog.KyriosTheotokon.name
meMA.Ode9C24.incipit = "Ἡ μνήμη σου τῷ κόσμῳ…"
meMA.Ode9C24.text = "Ἡ μνήμη σου τῷ κόσμῳ, Πάτερ Ἱεράρχα, ὡς φωταυγὴς ἑξανέτειλεν ἥλιος, φωταγωγοῦσα τοὺς πίστει, ἀνευφημοῦντάς σε."

meMA.Ode9C2T.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C2T.mode = misc_gr_GR_cog.Mode8
meMA.Ode9C2T.melody = he.h.m8_gr_GR_cog.KyriosTheotokon.name
meMA.Ode9C2T.incipit = "Φιλάγαθε Παρθένε, τὴν κεκακωμένην, τῇ…"
meMA.Ode9C2T.text = "Φιλάγαθε Παρθένε, τὴν κεκακωμένην, τῇ ἁμαρτίᾳ ψυχήν μου ἀγάθυνον, τὸν ἀγαθὸν ἡ τεκοῦσα, Θεὸν καὶ Κύριον."

meMA.Ode9H.ode = misc_gr_GR_cog.Ode9
meMA.Ode9H.mode = misc_gr_GR_cog.Mode8
meMA.Ode9H.melody = he.h.m8_gr_GR_cog.KyriosTheotokon.melody
meMA.Ode9H.incipit = he.h.m8_gr_GR_cog.KyriosTheotokon.incipit
meMA.Ode9H.text = he.h.m8_gr_GR_cog.KyriosTheotokon.text

meMA.Exaposteilarion1.mode = misc_gr_GR_cog.Mode2
meMA.Exaposteilarion1.melody = he.a.m2_gr_GR_cog.GynaikesAkoutisthite.name
meMA.Exaposteilarion1.incipit = "Φωνὴ βοῶντος ἤχησε, καὶ οἱ…"
meMA.Exaposteilarion1.text = "Φωνὴ βοῶντος ἤχησε, καὶ οἱ βουνοὶ σκιρτήσατε, χόρευσον γένος ἀνθρώπων· ὁ γὰρ προάναρχος Λόγος, σάρκα λαβὼν ὡς ἄνθρωπος, ἥκει βαπτισθησόμενος, ὑπὸ χειρῶν ὧν ἐπλασεν, ἐν Ἰορδάνῃ καὶ λύσων, τοῦ κόσμου τὴν ἁμαρτίαν."

meMA.Exaposteilarion2.mode = misc_gr_GR_cog.Mode2
meMA.Exaposteilarion2.melody = he.a.m2_gr_GR_cog.GynaikesAkoutisthite.name
meMA.Exaposteilarion2.incipit = "Πῶς ῥεῖθρά σε ποτάμια, εἰσδέξονται…"
meMA.Exaposteilarion2.text = "Πῶς ῥεῖθρά σε ποτάμια, εἰσδέξονται Φιλάνθρωπε, τὸν ποταμοὺς καὶ θαλάσσας, ποιήσαντα ἐκ μὴ ὄντων; πῶς κορυφὴν τὴν ἄχραντον, τὴν σὴν τολμήσει Πρόδρομος, χειροθετῆσαι Δέσποτα; ὑμνοῦμεν φρίττοντες Λόγε, τῆς σῆς πτωχείας τὸ ὕψος."

meMA.Lauds1.poet = poets_gr_GR_cog.Theofanous
meMA.Lauds1.acrostic = "ᾎσμα Θεοφάνους εἰς τὰ Φῶτα"
meMA.Lauds1.mode = misc_gr_GR_cog.Mode6
meMA.Lauds1.melody = he.a.m6_gr_GR_cog.AiAngelikai.name
meMA.Lauds1.incipit = "Αἱ Ἀγγελικαί, προπορεύεσθε Δυνάμεις, ἐκ…"
meMA.Lauds1.text = "Αἱ Ἀγγελικαί, προπορεύεσθε Δυνάμεις, ἐκ τῆς Βηθλεέμ, πρὸς τὰ ῥεῖθρα Ἰορδάνου. Προέρχου Ἰωάννη, καταλείψας τὴν ἔρημον. Χαῖρε ποταμὲ καὶ εὐτρεπίζου. Πᾶσα δὲ γῆ ἀγαλλιάσθω. Χριστὸς ἔρχεται, τὴν ἁμαρτίαν τοῦ Ἀδάμ, καθᾶραι ὡς εὔσπλαγχνος."

meMA.Lauds2.mode = misc_gr_GR_cog.Mode6
meMA.Lauds2.melody = he.a.m6_gr_GR_cog.AiAngelikai.name
meMA.Lauds2.incipit = "Στόμασιν ἁγνοῖς, καὶ ψυχαῖς κεκαθαρμέναις…"
meMA.Lauds2.text = "Στόμασιν ἁγνοῖς, καὶ ψυχαῖς κεκαθαρμέναις, δεῦτε μυστικῶς, Βηθλεὲμ μεταχωροῦντες, καὶ πρὸς τὸν Ἰορδάνην, σὺν Χριστῷ ἐπειγόμενοι, ᾄσωμεν αὐτῷ μετʼ εὐφροσύνης, αἱ πατριαὶ τῆς γῆς νῦν πᾶσαι, πιστῶς λέγουσαι· Εὐλογημένος ὁ ἐλθών, Θεὸς ἡμῶν δόξα σοι."

meMA.Lauds3.mode = misc_gr_GR_cog.Mode6
meMA.Lauds3.melody = he.a.m6_gr_GR_cog.AiAngelikai.name
meMA.Lauds3.incipit = "Μέγα καὶ φρικτόν, τὸ μυστήριον…"
meMA.Lauds3.text = "Μέγα καὶ φρικτόν, τὸ μυστήριον ὑπάρχει! ὅτι ὁ Θεός, τοῖς ἀνθρώποις ὡμοιώθη, καὶ ἁμαρτίαν ὅλως, μὴ εἰδὼς ὁ ἀνεύθυνος, ὑπὸ Ἰωάννου βαπτισθῆναι, ἐν ποταμῷ τῷ Ἰορδάνῃ, αἰτεῖ σήμερον· Εὐλογημένος ὁ φανείς, Θεὸς ἡμῶν δόξα σοι."

meMA.Lauds4.mode = misc_gr_GR_cog.Mode6
meMA.Lauds4.melody = he.a.m6_gr_GR_cog.AiAngelikai.name
meMA.Lauds4.incipit = "Ἄνθος ἐκ Δαυΐδ, ἀνατείλας τῆς…"
meMA.Lauds4.text = "Ἄνθος ἐκ Δαυΐδ, ἀνατείλας τῆς Παρθένου, ἦλθεν ὁ Χριστός, πρὸς τὰ ῥεῖθρα Ἰορδάνου, τὰς ἁμαρτίας πλῦναι, τοῦ Προπάτορος ὕδασι. Χόρευσον Ἀδάμ, εὐφραίνου Εὔα, ὁ οὐρανὸς ἀγαλλιάσθω, λαοὶ εἴπωμεν· Εὐλογημένος ὁ ἐλθών, Θεὸς ἡμῶν δόξα σοι."

meMA.LaudsGlory.mode = misc_gr_GR_cog.Mode6
meMA.LaudsGlory.melody = he.a.m6_gr_GR_cog.AiAngelikai.name
meMA.LaudsGlory.incipit = "Τρέμει τῇ χειρί, δειλιῶν χειροθετῆσαι…"
meMA.LaudsGlory.text = "Τρέμει τῇ χειρί, δειλιῶν χειροθετῆσαι, χαίρει τῇ ψυχῇ, ἐννοῶν ὁ Ἰωάννης, ὅτι τὴν κορυφήν σου, ὑποκλῖναι ηὐδόκησας, δούλου τὴν μορφὴν ἐνδεδυμένος, ἵνα βροτοὺς δουλείας ῥύσῃ, ἐχθροῦ κράζοντας· Εὐλογημένος ὁ ἐλθών, Θεὸς ἡμῶν δόξα σοι."

meMA.LaudsBoth.mode = misc_gr_GR_cog.Mode6
meMA.LaudsBoth.melody = he.a.m6_gr_GR_cog.AiAngelikai.name
meMA.LaudsBoth.incipit = "Ἅπασα ἡ γῆ, μυστικῶς ἀγαλλιάσθω…"
meMA.LaudsBoth.text = "Ἅπασα ἡ γῆ, μυστικῶς ἀγαλλιάσθω, νῦν προφητικῶς, καὶ τὰ ὂρη σκιρτησάτω· ἀνάστρεψον τὸ ῥεῖθρον, Ἰορδάνη ὡς γέγραπται, φρίξον ἐκ προσώπου τοῦ Δεσπότου· ὅτι Θεὸς ἐνανθρωπήσας, βροτοὺς ἅπαντας, τῆς ἁμαρτίας τοῦ Ἀδάμ, καθᾶραι ἐλήλυθεν."

