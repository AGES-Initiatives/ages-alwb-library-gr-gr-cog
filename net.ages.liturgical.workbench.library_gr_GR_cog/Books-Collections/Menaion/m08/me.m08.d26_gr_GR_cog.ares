meMA.Ode1C11.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C11.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode1C11.greekmelody = he.h.m6_gr_GR_cog.heHE.OsEnIpeiroPezefsas.greekname
meMA.Ode1C11.melody = he.h.m6_gr_GR_cog.heHE.OsEnIpeiroPezefsas.name
meMA.Ode1C11.incipit = "Ἐν τῇ νυκτὶ με τοῦ…"
meMA.Ode1C11.text = "Ἐν τῇ νυκτὶ με τοῦ βίου ὕπνῳ δεινῷ συσχεθέντα πράξεσι, μετανοίας πρὸς τὸ φῶς, ὁ Θεὸς ἐξέγειρον εὐχαῖς, τῶν μαρτύρων σου Χριστέ, ἵνα δοξάζω σε."

meMA.Ode1C12.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C12.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode1C12.melody = he.h.m6_gr_GR_cog.heHE.OsEnIpeiroPezefsas.name
meMA.Ode1C12.incipit = "Ἀπὸ χειλέων σταζόντων τὸν γλυκασμὸν…"
meMA.Ode1C12.text = "Ἀπὸ χειλέων σταζόντων τὸν γλυκασμὸν τῆς ἐνθέου γνώσεως, γλυκανθεὶς Ἀδριανέ, τὸν ἰὸν ἐξήμεσας τῆς πρὶν ἀγνωσίας ἀληθῶς θεομακάριστε."

meMA.Ode1C13.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C13.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode1C13.melody = he.h.m6_gr_GR_cog.heHE.OsEnIpeiroPezefsas.name
meMA.Ode1C13.incipit = "Ἀνατολῆς λαμπροτέραν ἡλιακῆς, τὴν ψυχὴν…"
meMA.Ode1C13.text = "Ἀνατολῆς λαμπροτέραν ἡλιακῆς, τὴν ψυχὴν ὡς ἔχουσα, τῆς ἀθλήσεως πρὸς φῶς, συνωθεῖς ὁμόζυγον τὸν σόν, Ναταλία, γυναικῶν τὸ ἐγκαλλώπισμα."

meMA.Ode1C1T.ode = miscellanea_gr_GR_cog.misc.Ode1
meMA.Ode1C1T.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode1C1T.melody = he.h.m6_gr_GR_cog.heHE.OsEnIpeiroPezefsas.name
meMA.Ode1C1T.incipit = "Τὸν σεαυτῆς δεξαμένη Δημιουργόν, ὡς…"
meMA.Ode1C1T.text = "Τὸν σεαυτῆς δεξαμένη Δημιουργόν, ὡς αὐτὸς ἠθέλησεν, ἐξ ἀσπόρου σου γαστρός, ὑπὲρ νοῦν σαρκούμενον Ἁγνή, τῶν κτισμάτων, ἀληθῶς ἐδείχθης Δέσποινα."

meMA.Ode3C11.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C11.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode3C11.greekmelody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.greekname
meMA.Ode3C11.melody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.name
meMA.Ode3C11.incipit = "Δεσμὰ φυγὼν εἰδωλικῆς, ματαιότητος μάρτυς…"
meMA.Ode3C11.text = "Δεσμὰ φυγὼν εἰδωλικῆς, ματαιότητος μάρτυς, συνεδέθης τῷ πόθῳ, τῆς ἀγάπης τοῦ Χριστοῦ, καὶ τοῖς δεσμοῖς τῶν αὐτοῦ, ἀθλοφόρων, χαίρων ἐκοινώνησας."

meMA.Ode3C12.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C12.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode3C12.melody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.name
meMA.Ode3C12.incipit = "Μοχλῷ τὰ σκέλη σιδηρῷ, συνθλαττόμενοι…"
meMA.Ode3C12.text = "Μοχλῷ τὰ σκέλη σιδηρῷ, συνθλαττόμενοι πάντες, ἀθλοφόροι Κυρίου, ἀσεβείας τὰ ὀστᾶ, ὑπομονῆς τῷ μοχλῷ, καὶ ἀνδρείας, ὄντως συνετρίψατε."

meMA.Ode3C13.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C13.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode3C13.melody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.name
meMA.Ode3C13.incipit = "Πυρὶ ἀγάπης θεϊκῆς, τὴν ψυχὴν…"
meMA.Ode3C13.text = "Πυρὶ ἀγάπης θεϊκῆς, τὴν ψυχὴν φλεγομένη, τοῦ συζύγου ἐξῆψας, τὸν ἔρωτα εἰς Χριστόν, τὸν πόθον τὸν τῆς σαρκὸς Ναταλία, τέλεον μιμήσασα."

meMA.Ode3C1T.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C1T.mode = miscellanea_gr_GR_cog.misc.Mode6
meMA.Ode3C1T.melody = he.h.m6_gr_GR_cog.heHE.OukEstinAgios.name
meMA.Ode3C1T.incipit = "Τοῦ θείου τόκου σου Ἁγνὴ…"
meMA.Ode3C1T.text = "Τοῦ θείου τόκου σου Ἁγνὴ πᾶσαν φύσεως τάξιν, ὑπερβαίνει τὸ θαῦμα· Θεὸν γὰρ ὑπερφυῶς, συνέλαβες ἐν γαστρί, καὶ τεκοῦσα, μένεις ἀειπάρθενος."

