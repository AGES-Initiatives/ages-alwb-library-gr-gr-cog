heAU.title = "Αὐτόμελα τοῦ πλ. Αʹ Ἦχου"

heAU.ChairoisAskitikon.greekname = " "
heAU.ChairoisAskitikon.melody = miscellanea_gr_GR_cog.misc.Automelon
heAU.ChairoisAskitikon.mode = miscellanea_gr_GR_cog.misc.Mode5
heAU.ChairoisAskitikon.name = "Χαίροις ἀσκητικῶν."
heAU.ChairoisAskitikon.incipit = "Χαίροις ἀσκητικῶν…"
heAU.ChairoisAskitikon.text = "Χαίροις ἀσκητικῶν ἀληθῶς, ἀγωνισμάτων τὸ εὐῶδες κειμήλιον· σταυρὸν γὰρ ἐπʼ ὤμων ἄρας, καὶ τῷ Δεσπότῃ Χριστῷ, σεαυτὸν Παμμάκαρ ἀναθέμενος, σαρκὸς κατεπάτησας, τὸ χαμαίζηλον φρόνημα, ταῖς ἀρεταῖς δέ, τὴν ψυχὴν κατελάμπρυνας, καὶ πρὸς ἔνθεον, ἀνεπτέρωσας ἔρωτα. Ὅθεν τὴν παναγίαν σου, κυκλοῦντες πανεύφημε, λάρνακα Σάββα τῆς θείας, φιλανθρωπίας αἰτούμεθα, τυχεῖν σαῖς πρεσβείαις, καὶ τῷ κόσμῳ δωρηθῆναι τὸ μέγα ἔλεος."

heAU.LampeiSimeron.greekname = " "
heAU.LampeiSimeron.melody = miscellanea_gr_GR_cog.misc.Automelon
heAU.LampeiSimeron.mode = miscellanea_gr_GR_cog.misc.Mode5
heAU.LampeiSimeron.name = "Λάμπει σήμερον."
heAU.LampeiSimeron.incipit = "Λάμπει σήμερον…"
heAU.LampeiSimeron.text = "Λάμπει σήμερον ἡ μνήμη τῶν Ἀθλοφόρων· ἔχει γὰρ καὶ οὐρανόθεν ἀπαύγασμα, ὁ χορὸς τῶν Ἀγγέλων πανηγυρίζει, καὶ τῶν ἀνθρώπων τὸ γένος συνεορτάζει· διὸ πρεσβεύουσι τῷ Κυρίῳ, ἐλεηθῆναι τὰς ψυχὰς ἡμῶν."

heAU.OsiePater.greekname = " "
heAU.OsiePater.melody = miscellanea_gr_GR_cog.misc.Automelon
heAU.OsiePater.mode = miscellanea_gr_GR_cog.misc.Mode5
heAU.OsiePater.name = "Ὅσιε Πάτερ."
heAU.OsiePater.incipit = "Ὅσιε Πάτερ…"
heAU.OsiePater.text = "Ὅσιε Πάτερ, θεοφόρε Θεοδόσιε, μεγάλως ἠγωνίσω ἐν τῇ προσκαίρῳ ζωῇ, ἐν ὕμνοις καὶ νηστείαις καὶ ἀγρυπνίαις, τύπος γενόμενος τῶν σῶν φοιτητῶν, νῦν δὲ συγχορεύεις μετὰ τῶν Ἀσωμάτων, Χριστὸν ἀπαύστως δοξολογῶν, τὸν ἐκ Θεοῦ Θεὸν Λόγον καὶ Λυτρωτήν, τὸν ὑποκλίναντα τὴν κάραν τῷ Προδρόμῳ, καὶ ἁγιάσαντα τὴν φύσιν τῶν ὑδάτων. Αὐτὸν ἱκέτευε, αὐτὸν δυσώπει Ὅσιε, δωρηθῆναι τῇ Ἐκκλησίᾳ, ὁμόνοιαν, εἰρήνην, καὶ μέγα ἔλεος."

heAU.TonSynanarchonLogon.greekname = " "
heAU.TonSynanarchonLogon.melody = miscellanea_gr_GR_cog.misc.Automelon
heAU.TonSynanarchonLogon.mode = miscellanea_gr_GR_cog.misc.Mode5
heAU.TonSynanarchonLogon.name = "Τὸν συνάναρχον Λόγον."
heAU.TonSynanarchonLogon.incipit = "Τὸν συνάναρχον Λόγον…"
heAU.TonSynanarchonLogon.text = "Τὸν συνάναρχον Λόγον Πατρὶ καὶ Πνεύματι, τὸν ἐκ Παρθένου τεχθέντα εἰς σωτηρίαν ἡμῶν, ἀνυμνήσωμεν πιστοὶ καὶ προσκυνήσωμεν· ὅτι ηὐδόκησε σαρκί, ἀνελθεῖν ἐν τῷ Σταυρῷ, καὶ θάνατον ὑπομεῖναι, καὶ ἐγεῖραι τοὺς τεθνεῶτας, ἐν τῇ ἐνδόξῳ ἀναστάσει αὐτοῦ."
