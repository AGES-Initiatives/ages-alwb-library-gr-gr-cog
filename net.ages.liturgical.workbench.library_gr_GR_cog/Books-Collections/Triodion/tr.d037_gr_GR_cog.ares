trMA.Ode1C11.ode = miscellanea_gr_GR_cog.misc.Ode1
trMA.Ode1C11.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode1C11.melody = he.h.m8_gr_GR_cog.AsmaAnapempsomen.name
trMA.Ode1C11.incipit = "Ἔχων ὡς Θεὸς τὸ συμπαθές…"
trMA.Ode1C11.text = "Ἔχων ὡς Θεὸς τὸ συμπαθές, μετανοοῦντα δέξαι με, τὸν δαπανήσαντα, τὸν βίον μου ἀσώτως, βοῶ σοι τό· Ἥμαρτον, κατηγόρων οὐ χρῄζω, τὴν αἰσχύνην τῶν ἔργων προσφέρων."

trMA.Ode1C12.ode = miscellanea_gr_GR_cog.misc.Ode1
trMA.Ode1C12.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode1C12.melody = he.h.m8_gr_GR_cog.AsmaAnapempsomen.name
trMA.Ode1C12.incipit = "Βρῶσιν λελοιπὼς Ἀγγελικήν, παρωμοιώθην κτήνεσιν…"
trMA.Ode1C12.text = "Βρῶσιν λελοιπὼς Ἀγγελικήν, παρωμοιώθην κτήνεσιν, ἐν τῷ σιτίζεσθαι τὴν μοχθηρὰν κακίαν, ἀλλʼ οὖν ἐπιστρέφοντα, δέξαι με ὥσπερ ἕνα τῶν μισθίων, οὐράνιε Πάτερ."

trMA.Ode1C13.ode = miscellanea_gr_GR_cog.misc.Ode1
trMA.Ode1C13.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode1C13.melody = he.h.m8_gr_GR_cog.AsmaAnapempsomen.name
trMA.Ode1C13.incipit = "Νύκτα ἡδονῶν τὴν ἐκ τρυφῆς…"
trMA.Ode1C13.text = "Νύκτα ἡδονῶν τὴν ἐκ τρυφῆς, προσγινομένην ἅπαντες, πιστοὶ ἀφέμενοι, φωτὶ τῆς ἀληθείας, σπουδαίως προσέλθωμεν, ἵνα τῆς φωτοφόρου εὐωχίας, καταξιωθῶμεν."

trMA.Ode1C1T.ode = miscellanea_gr_GR_cog.misc.Ode1
trMA.Ode1C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode1C1T.melody = he.h.m8_gr_GR_cog.AsmaAnapempsomen.name
trMA.Ode1C1T.incipit = "Χαίροις ὁ πανάγιος ναός, ὁ…"
trMA.Ode1C1T.text = "Χαίροις ὁ πανάγιος ναός, ὁ πόκος ὁ θεόδροσος, ἐσφραγισμένη πηγὴ τοῦ ἀθανάτου ῥείθρου, τὴν ποίμνην σου, Δέσποινα, φύλαττε ἐκ παντοίων πολεμίων, ἀπολιορκήτως."

trMA.C2.title = ""
trMA.C2.verse = ""

trMA.Ode1C21.ode = miscellanea_gr_GR_cog.misc.Ode1
trMA.Ode1C21.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode1C21.melody = he.h.m8_gr_GR_cog.AsomenOdinToTheo.name
trMA.Ode1C21.incipit = "Νῦν ἐν ἑβδομάσι τρισίν, ὡς…"
trMA.Ode1C21.text = "Νῦν ἐν ἑβδομάσι τρισίν, ὡς πάλαι τριήμερος ὁ Ἰσραήλ, ἁγνισθέντες ἀδελφοί, ὄρει εὐχῶν φθάσωμεν, καὶ ἐκεῖθεν θείων φωνῶν ἀκούσαντες, Χριστὸν ὑμνήσωμεν."

trMA.Ode1C22.ode = miscellanea_gr_GR_cog.misc.Ode1
trMA.Ode1C22.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode1C22.melody = he.h.m8_gr_GR_cog.AsomenOdinToTheo.name
trMA.Ode1C22.incipit = "Ὥσπερ Σαμψὼν νηστείᾳ, δυναμωθέντες δεῦτε…"
trMA.Ode1C22.text = "Ὥσπερ Σαμψὼν νηστείᾳ, δυναμωθέντες δεῦτε, ὡς ἄλλον θῆρα πατάξωμεν λαοί, τὸν τῆς γαστρὸς δαίμονα, καὶ οὐ μὴ γελάσῃ ἡ Δαλιδὰ τῶν παθῶν, ἐξαπατῶσα ἡμᾶς."

trMA.Ode1C2Tr.ode = miscellanea_gr_GR_cog.misc.Ode1
trMA.Ode1C2Tr.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode1C2Tr.melody = he.h.m8_gr_GR_cog.AsomenOdinToTheo.name
trMA.Ode1C2Tr.incipit = "Τρία μέλπω χερουβικῶς, ἁγία σε…"
trMA.Ode1C2Tr.text = "Τρία μέλπω χερουβικῶς, ἁγία σε Θεότης, φῶτα καὶ φῶς, καὶ ζωήν τε καὶ ζωάς, Θεὸν τὸν γεννήσαντα, Θεὸν τὸν γεννηθέντα, Θεὸν καὶ τὸ ἐκ Πατρὸς ἐκπορευθέν, Πνεῦμα τὸ ζῶν."

trMA.Ode1C2T.ode = miscellanea_gr_GR_cog.misc.Ode1
trMA.Ode1C2T.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode1C2T.melody = he.h.m8_gr_GR_cog.AsomenOdinToTheo.name
trMA.Ode1C2T.incipit = "Χαῖρε ἡ τῆς Εὔας χαρά…"
trMA.Ode1C2T.text = "Χαῖρε ἡ τῆς Εὔας χαρά· ἡ γὰρ ἐκείνης λύπη, διὰ τοῦ τόκου σου, πέπαυται Ἁγνή, χαῖρε ἀδύτου φωτός, φωτεινὴ νεφέλη, ἐξ ἧς ἀνέτειλε Χριστὸς ὁ Θεός."

trMA.Ode1C2A1.ode = miscellanea_gr_GR_cog.misc.Ode1
trMA.Ode1C2A1.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode1C2A1.melody = he.h.m8_gr_GR_cog.AsomenOdinToTheo.name
trMA.Ode1C2A1.incipit = "Πίστει ὡς Δαυῒδ καὶ ἡμεῖς…"
trMA.Ode1C2A1.text = "Πίστει ὡς Δαυῒδ καὶ ἡμεῖς, τῇ ταπεινοφροσύνῃ, καθοπλισθέντες, ὡς ἄλλον Γολιάθ, τὸν ὑπερήφανον νοῦν, καταβεβληκότες, συνδιακόψωμεν καὶ μυριάδας παθῶν."

trMA.Ode1C2H.ode = miscellanea_gr_GR_cog.misc.Ode1
trMA.Ode1C2H.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode1C2H.melody = he.h.m8_gr_GR_cog.AsomenOdinToTheo.melody
trMA.Ode1C2H.incipit = he.h.m8_gr_GR_cog.AsomenOdinToTheo.incipit
trMA.Ode1C2H.text = he.h.m8_gr_GR_cog.AsomenOdinToTheo.text

trMA.Ode8C11.ode = miscellanea_gr_GR_cog.misc.Ode8
trMA.Ode8C11.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode8C11.melody = he.h.m8_gr_GR_cog.OiTheorrimonesPaides.name
trMA.Ode8C11.incipit = "Ἐκδαπανήσας οὐσίαν τὴν πατρῴαν, μετὰ…"
trMA.Ode8C11.text = "Ἐκδαπανήσας οὐσίαν τὴν πατρῴαν, μετὰ πορνῶν ἐξεβλήθην, τῶν προαυλίων τῆς χάριτος, ἀλλὰ δέξαι με Πάτερ, διʼ ἄμετρον ἔλεος."

trMA.Ode8C12.ode = miscellanea_gr_GR_cog.misc.Ode8
trMA.Ode8C12.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode8C12.melody = he.h.m8_gr_GR_cog.OiTheorrimonesPaides.name
trMA.Ode8C12.incipit = "Ἐν φαιδροτάτῃ νηστείας φωταυγείᾳ, τῶν…"
trMA.Ode8C12.text = "Ἐν φαιδροτάτῃ νηστείας φωταυγείᾳ, τῶν προσευχῶν λαμπρυνθέντες χρηματίσωμεν, ὅπως τῆς ἁμαρτίας, τὸ σκότος ἐκφύγωμεν."

trMA.Ode8C13.ode = miscellanea_gr_GR_cog.misc.Ode8
trMA.Ode8C13.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode8C13.melody = he.h.m8_gr_GR_cog.OiTheorrimonesPaides.name
trMA.Ode8C13.incipit = "Ἐκδυσωπεῖ σε χορὸς τῶν Ἀσωμάτων…"
trMA.Ode8C13.text = "Ἐκδυσωπεῖ σε χορὸς τῶν Ἀσωμάτων, τὰ Χερουβὶμ ἱκετεύει, καὶ τῶν Ἁγίων τὰ τάγματα, τὰς ψυχὰς ἡμῶν σῶσον, Χριστὲ πολυέλεε."

trMA.Ode8C1T.ode = miscellanea_gr_GR_cog.misc.Ode8
trMA.Ode8C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode8C1T.melody = he.h.m8_gr_GR_cog.OiTheorrimonesPaides.name
trMA.Ode8C1T.incipit = "Τὸν ἀγεώργητον βότρυν συλλαβοῦσα, ἐσκοτισμένον…"
trMA.Ode8C1T.text = "Τὸν ἀγεώργητον βότρυν συλλαβοῦσα, ἐσκοτισμένον με μέθῃ, τῆς ἁμαρτίας σωφρόνισον, Θεοτόκε Παρθένε, ἐλπὶς τῶν ψυχῶν ἡμῶν."

trMA.Ode8C21.ode = miscellanea_gr_GR_cog.misc.Ode8
trMA.Ode8C21.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode8C21.melody = he.h.m8_gr_GR_cog.OStegazonEnYdasi.name
trMA.Ode8C21.incipit = "Τὸ πατρῷον κειμήλιον, τὸ χρῆμα…"
trMA.Ode8C21.text = "Τὸ πατρῷον κειμήλιον, τὸ χρῆμα τὸ θαυμαστόν, τὴν μητέρα πάντων τῶν λατρευσάντων τῷ Δεσπότῃ Χριστῷ, δεῦτε τρυφήσωμεν, ἀδελφοὶ τὴν Νηστείαν· καὶ γὰρ αὕτη ῥώννυσι τὸ σῶμα, καὶ καταφωτίζει τὸν νοῦν, καὶ τὴν καρδίαν."

trMA.Ode8C22.ode = miscellanea_gr_GR_cog.misc.Ode8
trMA.Ode8C22.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode8C22.melody = he.h.m8_gr_GR_cog.OStegazonEnYdasi.name
trMA.Ode8C22.incipit = "Γεδεὼν τὸν θαυμάσιον, ἐκμιμουμένη ψυχή…"
trMA.Ode8C22.text = "Γεδεὼν τὸν θαυμάσιον, ἐκμιμουμένη ψυχή, ἀρετὰς φέρουσα, πίστιν ἐλπίδα, καὶ ἀγάπην Χριστοῦ, ἔξελθε κτείνουσα, τὰ ἀλλόφυλα πάθη, ὡς ἐκεῖνος, σὺν τριακοσίοις, τὸ Μαδινιαῖον ἔθνος ἀποκαθαίρων."

trMA.Ode8C2Tr.ode = miscellanea_gr_GR_cog.misc.Ode8
trMA.Ode8C2Tr.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode8C2Tr.melody = he.h.m8_gr_GR_cog.OStegazonEnYdasi.name
trMA.Ode8C2Tr.incipit = "Καὶ Τριάδα δοξάζω σε, καὶ…"
trMA.Ode8C2Tr.text = "Καὶ Τριάδα δοξάζω σε, καὶ ὡς Μονάδα ὑμνῶ, σὲ Θεότης μία, Πάτερ παντοκράτορ, καὶ μονάρχα Υἱέ, Πνεῦμα τὸ ἅγιον, καὶ πανδέσποτον κράτος, μία φύσις, μία βασιλεία, ἐν δὲ χαρακτῆρσι, τρισὶ προσκυνουμένη."

trMA.Ode8C2T.ode = miscellanea_gr_GR_cog.misc.Ode8
trMA.Ode8C2T.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode8C2T.melody = he.h.m8_gr_GR_cog.OStegazonEnYdasi.name
trMA.Ode8C2T.incipit = "'Ἂς ἐν τόκῳ διέφυγεν, ὠδῖνας…"
trMA.Ode8C2T.text = "'Ἂς ἐν τόκῳ διέφυγεν, ὠδῖνας ὑπερφυῶς ἐν τῷ σῷ τιμίῳ Πάθει, ἐπέγνω ἡ τεκοῦσά σε· ἤλγει γὰρ βλέπουσα, ἐν σταυρῷ σε παγέντα ἑκουσίως, ὑπὸ Ἰουδαίων, τὸν ἐπὶ ὑδάτων, τὴν γῆν θεμελιοῦντα."

trMA.Ode8C2A1.ode = miscellanea_gr_GR_cog.misc.Ode8
trMA.Ode8C2A1.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode8C2A1.melody = he.h.m8_gr_GR_cog.OStegazonEnYdasi.name
trMA.Ode8C2A1.incipit = "Ὅταν ἔλθῃς ἐν τῇ δόξῃ…"
trMA.Ode8C2A1.text = "Ὅταν ἔλθῃς ἐν τῇ δόξῃ σου, τοῦ κρῖναι πᾶσαν τὴν γῆν, σὺν ταῖς χιλιάσι, καὶ μυριάσι τῶν Δυνάμεων, τότε μου Κύριε φεῖσαι ἄνες, καὶ ῥῦσαι καταδίκης, μή με κατασχύνῃς, μή με κατακρίνῃς, πυρὶ τῷ αἰωνίῳ."

trMA.Ode8C2H.ode = miscellanea_gr_GR_cog.misc.Ode8
trMA.Ode8C2H.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode8C2H.melody = he.h.m8_gr_GR_cog.OStegazonEnYdasi.melody
trMA.Ode8C2H.incipit = he.h.m8_gr_GR_cog.OStegazonEnYdasi.incipit
trMA.Ode8C2H.text = he.h.m8_gr_GR_cog.OStegazonEnYdasi.text

trMA.Ode9C11.ode = miscellanea_gr_GR_cog.misc.Ode9
trMA.Ode9C11.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode9C11.melody = he.h.m8_gr_GR_cog.TonProdilothenta.name
trMA.Ode9C11.incipit = "Τὸν ἀπὸ τῆς σῆς μακρυνθέντα…"
trMA.Ode9C11.text = "Τὸν ἀπὸ τῆς σῆς μακρυνθέντα, Πάτερ ἀγάπης, καὶ ἐθελουσίοις ὁρμαῖς, ἡδονῶν δουλωθέντα, ἐπιστρέφοντα ὡς τὸν Ἄσωτον δέξαι· μόνος γὰρ ὑπάρχεις πολυέλεος."

trMA.Ode9C12.ode = miscellanea_gr_GR_cog.misc.Ode9
trMA.Ode9C12.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode9C12.melody = he.h.m8_gr_GR_cog.TonProdilothenta.name
trMA.Ode9C12.incipit = "Ἄκαρπος ἐδείχθην συκῇ, μόνην τὴν…"
trMA.Ode9C12.text = "Ἄκαρπος ἐδείχθην συκῇ, μόνην τὴν κακίαν γεωργῶν ὁ ἄθλιος, τὸ πῦρ μοι προξενοῦσαν. Σὺ οὖν Κύριε καρποφόρον με δεῖξον, πράξεις ἀγαθὰς καρποφοροῦντά σοι."

trMA.Ode9C13.ode = miscellanea_gr_GR_cog.misc.Ode9
trMA.Ode9C13.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode9C13.melody = he.h.m8_gr_GR_cog.TonProdilothenta.name
trMA.Ode9C13.incipit = "Νήστευσον ψυχή μου, κακίας καὶ…"
trMA.Ode9C13.text = "Νήστευσον ψυχή μου, κακίας καὶ πονηρίας, κράτησον ὀργῆς, καὶ θυμοῦ καὶ πάσης ἁμαρτίας. Ἰησοῦς γὰρ τοιαύτην θέλει νηστείαν, ὁ φιλανθρωπότατος Θεὸς ἡμῶν."

trMA.Ode9C1T.ode = miscellanea_gr_GR_cog.misc.Ode9
trMA.Ode9C1T.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode9C1T.melody = he.h.m8_gr_GR_cog.TonProdilothenta.name
trMA.Ode9C1T.incipit = "Σὺ εἶ Θεοτόκε, τὰ ὅπλα…"
trMA.Ode9C1T.text = "Σὺ εἶ Θεοτόκε, τὰ ὅπλα ἡμῶν καὶ τεῖχος, σὺ εἶ ἡ ἀντίληψις, τῶν εἰς σὲ προστρεχόντων, σὲ καὶ νῦν εἰς πρεσβείαν κινοῦμεν, ἵνα λυτρωθῶμεν τῶν ἐχθρῶν ἡμῶν."

trMA.Ode9C21.ode = miscellanea_gr_GR_cog.misc.Ode9
trMA.Ode9C21.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode9C21.melody = he.h.m8_gr_GR_cog.MegalynomenSeTinPanamomon.name
trMA.Ode9C21.incipit = "Καθαρὰν νηστείαν, τῶν κακῶν τὴν…"
trMA.Ode9C21.text = "Καθαρὰν νηστείαν, τῶν κακῶν τὴν ἀποχήν, προσενέγκωμεν, ὡς δῶρα εὐπρόσδεκτα, Χριστῷ τῷ Θεῷ ἡμῶν."

trMA.Ode9C22.ode = miscellanea_gr_GR_cog.misc.Ode9
trMA.Ode9C22.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode9C22.melody = he.h.m8_gr_GR_cog.MegalynomenSeTinPanamomon.name
trMA.Ode9C22.incipit = "Ἐλισσαῖος πάλαι, πῶς διέτρεφε Προφήτας…"
trMA.Ode9C22.text = "Ἐλισσαῖος πάλαι, πῶς διέτρεφε Προφήτας, μαθοῦσα ψυχή, σχεδίως ἐκτράφηθι, ἀπευχαριστοῦσα Χριστῷ."

trMA.Ode9C2Tr.ode = miscellanea_gr_GR_cog.misc.Ode9
trMA.Ode9C2Tr.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode9C2Tr.melody = he.h.m8_gr_GR_cog.MegalynomenSeTinPanamomon.name
trMA.Ode9C2Tr.incipit = "Τρισσουμένην μίαν, σὲ Θεότης ἀνυμνῶ…"
trMA.Ode9C2Tr.text = "Τρισσουμένην μίαν, σὲ Θεότης ἀνυμνῶ, τοῖς προσώποις μορφήν, Πατέρα τὸν ἄναρχον, Υἱόν, καὶ Πνεῦμα τὸ ζῶν."

trMA.Ode9C2T.ode = miscellanea_gr_GR_cog.misc.Ode9
trMA.Ode9C2T.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode9C2T.melody = he.h.m8_gr_GR_cog.MegalynomenSeTinPanamomon.name
trMA.Ode9C2T.incipit = "Θεομῆτορ Κόρη, ἡ παστὰς τοῦ…"
trMA.Ode9C2T.text = "Θεομῆτορ Κόρη, ἡ παστὰς τοῦ οὐρανίου, Βασιλέως Χριστοῦ, σῷζε τῇ πρεσβείᾳ σου, τοὺς πόθῳ ὑμνοῦντάς σε."

trMA.Ode9C2A1.ode = miscellanea_gr_GR_cog.misc.Ode9
trMA.Ode9C2A1.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode9C2A1.melody = he.h.m8_gr_GR_cog.MegalynomenSeTinPanamomon.name
trMA.Ode9C2A1.incipit = "Δανιὴλ τοὺς θῆρας, ἐν τῷ…"
trMA.Ode9C2A1.text = "Δανιὴλ τοὺς θῆρας, ἐν τῷ λάκκῳ ἡμεροῖ, ἐγκρατείας κημῷ· καὶ ἡμεῖς δαμάσωμεν, τὰ πάθη νηστεύοντες."

trMA.Ode9C2H.ode = miscellanea_gr_GR_cog.misc.Ode9
trMA.Ode9C2H.mode = miscellanea_gr_GR_cog.misc.Mode8
trMA.Ode9C2H.melody = he.h.m8_gr_GR_cog.MegalynomenSeTinPanamomon.melody
trMA.Ode9C2H.incipit = he.h.m8_gr_GR_cog.MegalynomenSeTinPanamomon.incipit
trMA.Ode9C2H.text = he.h.m8_gr_GR_cog.MegalynomenSeTinPanamomon.text

trMA.AposIdiomelon1.mode = miscellanea_gr_GR_cog.misc.Mode4
trMA.AposIdiomelon1.melody = miscellanea_gr_GR_cog.misc.Idiomelon
trMA.AposIdiomelon1.incipit = "Τὸν χαλεπὸν τῶν πταισμάτων μου…"
trMA.AposIdiomelon1.text = "Τὸν χαλεπὸν τῶν πταισμάτων μου, ῥύπον ἐνδέδυμαι, καὶ τῆς χαρᾶς τοῦ νυμφῶνος ἐκβέβλημαι οἴκτειρόν με τῇ ἀφάτῳ σου εὐσπλαγχνίᾳ, ὡς τὸν Ἄσωτον παῖδα ὁ Θεός, καὶ ἐλέησόν με."

trMA.AposMartyric.mode = miscellanea_gr_GR_cog.misc.Mode4
trMA.AposMartyric.melody = ""
trMA.AposMartyric.incipit = "Τίς οὐκ ἐξίσταται ὁρῶν, ἅγιοι…"
trMA.AposMartyric.text = "Τίς οὐκ ἐξίσταται ὁρῶν, ἅγιοι Μάρτυρες, τὸν ἀγῶνα τὸν καλόν, ὃν ἠγωνίσασθε; πῶς ἐν σώματι ὄντες, τὸν ἀσώματον ἐχθρὸν ἑνικήσατε, Χριστὸν ὁμολογήσαντες, καὶ Σταυρῷ ὁπλισάμενοι; ὅθεν ἐπαξίως ἀνεδείχθητε, δαιμόνων φυγαδευταί, καὶ βαρβάρων πολέμιοι, ἀπαύστως πρεσβεύσατε, σωθῆναι τὰς ψυχας ἡμῶν."

trMA.AposTheotokion.mode = miscellanea_gr_GR_cog.misc.Mode4
trMA.AposTheotokion.melody = ""
trMA.AposTheotokion.incipit = "Ἡ λαμπὰς ἡ ἄσβεστος, ὁ…"
trMA.AposTheotokion.text = "Ἡ λαμπὰς ἡ ἄσβεστος, ὁ θρόνος τῆς δικαιοσύνης, ἄχραντε Δέσποινα πρέσβευε, τοῦ σωθῆναι τὰς ψυχὰς ἡμῶν."

trTE.Troparion.mode = miscellanea_gr_GR_cog.misc.Mode4
trTE.Troparion.melody = ""
trTE.Troparion.incipit = "Ὅτι ἀσθενεῖς ἡμεῖς, καὶ παρειμένοι…"
trTE.Troparion.text = "Ὅτι ἀσθενεῖς ἡμεῖς, καὶ παρειμένοι ταῖς ἁμαρτίαις, ἴασαι τὰ συντρίμματα ἡμῶν, ὁ ἰατρὸς τῶν ψυχῶν ἡμῶν, ὁ ἐπιστάμενος τὸν νοῦν τῶν ἀνθρώπων, Φιλάνθρωπε."
trTE.Troparion2.incipit = "ὁ ἐπιστάμενος τὸν νοῦν τῶν…"
trTE.Troparion2.text = "ὁ ἐπιστάμενος τὸν νοῦν τῶν ἀνθρώπων, Φιλάνθρωπε."
