meVE.Aposticha1.mode = misc_gr_GR_cog.Mode6
meVE.Aposticha1.melody = he.a.m6_gr_GR_cog.AiAngelikai.incipit
meVE.Aposticha1.incipit = "Λάμψας ὑπὲρ νοῦν, ἐκ Παρθένου"
meVE.Aposticha1.text = "Λάμψας ὑπὲρ νοῦν, ἐκ Παρθένου Παναγίας, Λόγε τοῦ Θεοῦ, τὸν Ἀδὰμ ὅλον φορέσας, τὴν πρὶν ἀμαυρωθεῖσαν, φύσιν πᾶσαν ἐλάμπρυνας, θείᾳ τῆς μορφῆς σου ἀλλοιώσει, μεταμορφωθεὶς ἐπὶ τοῦ ὄρους, Θαβὼρ Δέσποτα, πρὸ τοῦ τιμίου σου Σταυροῦ, Χριστὲ ὁ Θεὸς ἡμῶν."

meVE.Aposticha2.mode = misc_gr_GR_cog.Mode6
meVE.Aposticha2.melody = he.a.m6_gr_GR_cog.AiAngelikai.incipit
meVE.Aposticha2.incipit = "Ὄρος τὸ ποτέ, ὂν ζοφῶδες"
meVE.Aposticha2.text = "Ὄρος τὸ ποτέ, ὂν ζοφῶδες καὶ καπνῶδες, τίμιον ἰδού, νῦν καὶ ἅγιον ὑπάρχει· ὁ πρὶν τὸν Ἰσραὴλ γάρ, ὁδηγήσας ὡς γέγραπται, στύλῳ πυριμόρφῳ καὶ νεφέλῃ, σήμερον ἐξέλαμψεν ἀῤῥήτως, ὑπὲρ ἥλιον, Χριστὲ ἐν τούτῳ ὡς Θεός, φαιδρύνας τὰ σύμπαντα."

meVE.Aposticha3.mode = misc_gr_GR_cog.Mode6
meVE.Aposticha3.melody = he.a.m6_gr_GR_cog.AiAngelikai.incipit
meVE.Aposticha3.incipit = "Νῦν δουλοπρεπῶς, συλλαλοῦντες τῷ Δεσπότῃ,"
meVE.Aposticha3.text = "Νῦν δουλοπρεπῶς, συλλαλοῦντες τῷ Δεσπότῃ, βλέπων ὁ Κηφᾶς, ἐν Θαβὼρ τοὺς δύο ἄνδρας· Καλὸν ἐστιν ἐβόα, ὧδε εἶναι· ποιήσωμεν, τρεῖς σκηνὰς εἰ θέλεις ἀνὰ μέρος, σοὶ Χριστὲ Μωσεῖ τε καὶ Ἠλίᾳ, τοῖς οἰκέταις σου, ὅπερ ἐλάλει μὴ εἰδώς· θάμβει γὰρ συνείχετο."

meVE.AposGlory.mode = misc_gr_GR_cog.Mode2
meVE.AposGlory.melody = ""
meVE.AposGlory.incipit = me.m08.d05_gr_GR_cog.meVE.AposGlory.incipit
meVE.AposGlory.text = me.m08.d05_gr_GR_cog.meVE.AposGlory.text

meVE.Apolytikion1.poet = poets_gr_GR_cog.Gerasimou
meVE.Apolytikion1.mode = misc_gr_GR_cog.Mode3
meVE.Apolytikion1.melody = he.a.m3_gr_GR_cog.TheiasPisteos.incipit
meVE.Apolytikion1.incipit = "Θείῳ Πνεύματι, κεκληρωμένος, συνεπλήρωσας, τῶν"
meVE.Apolytikion1.text = "Θείῳ Πνεύματι, κεκληρωμένος, συνεπλήρωσας, τῶν Ἀποστόλων, τὴν δωδεκάριθμον φάλαγγα ἔνδοξε· μεθ' ὧν κηρύξας τοῦ Λόγου τὴν κένωσιν, ἐθαυμαστώθης Ματθία Ἀπόστολε. Ἀλλὰ πρέσβευε, δοθῆναι τοῖς σὲ γεραίρουσι, πταισμάτων ἱλασμὸν καὶ μέγα ἔλεος."

meMA.Kathisma11.mode = misc_gr_GR_cog.Mode4
meMA.Kathisma11.melody = he.a.m4_gr_GR_cog.KateplagiIosif.incipit
meMA.Kathisma11.incipit = me.m08.d06_gr_GR_cog.meMA.Kathisma21.incipit
meMA.Kathisma11.text = me.m08.d06_gr_GR_cog.meMA.Kathisma21.text

meMA.Kathisma21.mode = misc_gr_GR_cog.Mode4
meMA.Kathisma21.melody = he.a.m4_gr_GR_cog.KateplagiIosif.incipit
meMA.Kathisma21.incipit = "Τῇ ἀπροσίτῳ Ἰησοῦ, δόξῃ τοῦ"
meMA.Kathisma21.text = "Τῇ ἀπροσίτῳ Ἰησοῦ, δόξῃ τοῦ θείου σου φωτός, μεταμορφούμενος τοῖς σοῖς, ἔλαμψας θείοις Μαθηταῖς, τῷ Ἰωάννῃ καὶ Πέτρῳ καὶ Ἰακώβῳ, ἐξέστησας αὐτούς, τῇ θείᾳ χάριτι· φωνῆς γὰρ πατρικῆς, ἀγαπητὸν σε Υἱόν, προσμαρτυρούσης ἤκουον, καὶ εἶδον, τὴν τοῦ προσώπου σου δόξαν φρικτήν, Σωτὴρ ὁ πάντας, θέλων σωθῆναι, φώτισον τὰς ψυχὰς ἡμῶν."

meMA.C1.acrostic = "οὗ ἡ Ἀκροστιχίς· Ὕμνοις σε μέλπω Ματθία Χριστοῦ φίλε. Θεοφάνους."

