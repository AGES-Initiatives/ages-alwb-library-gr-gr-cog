A_Resource_Whose_Name = ho.ho26_gr_GR_cog

hoCA.title1 = "Κανὼν Ἰκετήριος εἰς τὸν Ἄγγελον"
hoCA.title2 = "τὸν Φύλακα τῆς τοῦ Ἀνθρώπου Ζωῆς"
hoCA.title3 = "Κανὼν Ἰκετήριος εἰς τὸν Φύλακα Ἄγγελον"
hoCA.poet = "Ποίημα Ἰωάννου Μοναχοῦ τοῦ Μαυρόποδος"
hoCA.translator = ""

hoCA.acrostic.title = "Ὁ Κανών,"
hoCA.acrostic.notice1 = "οὗ ἡ ἀκροστιχίς·"
hoCA.acrostic.text = "Τὸν Ἄγγελον μέλπω σε τὸν φύλακά μου. ᾨδὴ Μοναχοῦ Ἰωάννου."
hoCA.title = "Κανὼν τοῦ Φύλακος Ἀγγέλου."
hoCA.verse = "Ἄγγελε τοῦ Θεοῦ, πρέσβευε ὑπὲρ ἡμῶν."

