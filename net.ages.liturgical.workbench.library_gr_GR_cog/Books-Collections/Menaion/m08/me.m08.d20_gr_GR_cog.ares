meMA.Aposticha1.text = me.m08.d16_gr_GR_cog.meVE.AposGlory.text

meMA.Aposticha2.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Aposticha2.melody = he.a.m2_gr_GR_cog.heAU.OikosTouEfratha.name
meMA.Aposticha2.incipit = me.m08.d16_gr_GR_cog.meVE.Aposticha2.incipit
meMA.Aposticha2.text = me.m08.d16_gr_GR_cog.meVE.Aposticha2.text

meMA.Aposticha3.mode = miscellanea_gr_GR_cog.misc.Mode2
