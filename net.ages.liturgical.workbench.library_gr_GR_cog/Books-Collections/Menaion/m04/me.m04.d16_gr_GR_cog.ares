meMA.Ode1C11.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C11.mode = misc_gr_GR_cog.Mode8
meMA.Ode1C11.melody = he.h.m8_gr_GR_cog.AsomenToKyrio.name
meMA.Ode1C11.incipit = "Χάριν μοι οὐρανόθεν, Μάρτυρες Κυρίου…"
meMA.Ode1C11.text = "Χάριν μοι οὐρανόθεν, Μάρτυρες Κυρίου νῦν αἰτήσασθε, τὴν ὑμῶν εὐφημοῦντι, ἐπὶ γῆς φωτοφόρον πανήγυριν."

meMA.Ode1C12.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C12.mode = misc_gr_GR_cog.Mode8
meMA.Ode1C12.melody = he.h.m8_gr_GR_cog.AsomenToKyrio.name
meMA.Ode1C12.incipit = "Ῥήξασαι προσπαθείας, σαρκικῆς δεσμὰ ἐν…"
meMA.Ode1C12.text = "Ῥήξασαι προσπαθείας, σαρκικῆς δεσμὰ ἐν θείῳ Πνεύματι, τῷ Χριστῷ αἱ Παρθένοι, συνεδέθησαν σθένει Θεότητος."

meMA.Ode1C13.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C13.mode = misc_gr_GR_cog.Mode8
meMA.Ode1C13.melody = he.h.m8_gr_GR_cog.AsomenToKyrio.name
meMA.Ode1C13.incipit = "Ἴσχυσεν ὁ Βελίαρ, ἀπατῆσαι πρὶν…"
meMA.Ode1C13.text = "Ἴσχυσεν ὁ Βελίαρ, ἀπατῆσαι πρὶν τὴν Εὔαν σεπταὶ Μάρτυρες, ἀλλὰ νῦν κατῃσχύνθη, τοῖς ποσὶν ὑμῶν ὄντως πατούμενος."

meMA.Ode1C14.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C14.mode = misc_gr_GR_cog.Mode8
meMA.Ode1C14.melody = he.h.m8_gr_GR_cog.AsomenToKyrio.name
meMA.Ode1C14.incipit = "Σώμασιν ἡγνισμένοις, ἐκ Παρθένου Κόρης…"
meMA.Ode1C14.text = "Σώμασιν ἡγνισμένοις, ἐκ Παρθένου Κόρης τὸν ἐκλάμψαντα, θεραπεύσασαι Λόγον, αἱ Παρθένοι, σὺν τούτῳ εὐφραίνονται."

meMA.Ode1C1T.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C1T.mode = misc_gr_GR_cog.Mode8
meMA.Ode1C1T.melody = he.h.m8_gr_GR_cog.AsomenToKyrio.name
meMA.Ode1C1T.incipit = "Τίκτεις ἀνερμηνεύτως, ὃν Πατὴρ ἀῤῥεύστως…"
meMA.Ode1C1T.text = "Τίκτεις ἀνερμηνεύτως, ὃν Πατὴρ ἀῤῥεύστως ἀπεγέννησε, καὶ μαζοῖς διατρέφεις, τὸν τὰ σύμπαντα τρέφοντα Δέσποινα."

meMA.Ode3C1H.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C1H.mode = misc_gr_GR_cog.Mode8
meMA.Ode3C1H.melody = he.h.m8_gr_GR_cog.SyEiToStereoma.melody
meMA.Ode3C1H.incipit = he.h.m8_gr_GR_cog.SyEiToStereoma.incipit
meMA.Ode3C1H.text = he.h.m8_gr_GR_cog.SyEiToStereoma.text

meMA.Ode3C11.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C11.mode = misc_gr_GR_cog.Mode8
meMA.Ode3C11.melody = he.h.m8_gr_GR_cog.SyEiToStereoma.name
meMA.Ode3C11.incipit = "Ὄμβροις θείου Πνεύματος, καταρδευόμεναι στάχυας…"
meMA.Ode3C11.text = "Ὄμβροις θείου Πνεύματος, καταρδευόμεναι στάχυας, μαρτυρικούς, θείᾳ γεωργίᾳ, αἱ Παρθένοι ἐξήνθησαν."

meMA.Ode3C12.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C12.mode = misc_gr_GR_cog.Mode8
meMA.Ode3C12.melody = he.h.m8_gr_GR_cog.SyEiToStereoma.name
meMA.Ode3C12.incipit = "Ὑπὲρ τὰ ὁρώμενα, τὰ μὴ…"
meMA.Ode3C12.text = "Ὑπὲρ τὰ ὁρώμενα, τὰ μὴ βλεπόμενα στέργουσαι, τοὺς ὁρατούς, καὶ τοὺς ἀοράτους, ἐτροπώσαντο δαίμονας."

meMA.Ode3C13.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C13.mode = misc_gr_GR_cog.Mode8
meMA.Ode3C13.melody = he.h.m8_gr_GR_cog.SyEiToStereoma.name
meMA.Ode3C13.incipit = "Πῦρ οὐκ ἑπτοήθητε, προσαναπτόμεναι Πνεύματος…"
meMA.Ode3C13.text = "Πῦρ οὐκ ἑπτοήθητε, προσαναπτόμεναι Πνεύματος, θείῳ πυρί· ὅθεν ἰαμάτων, θείαν δρόσον πηγάζετε."

meMA.Ode3C1T.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C1T.mode = misc_gr_GR_cog.Mode8
meMA.Ode3C1T.melody = he.h.m8_gr_GR_cog.SyEiToStereoma.name
meMA.Ode3C1T.incipit = "Ῥῆξον τῶν πταισμάτων μου, δεινὸν…"
meMA.Ode3C1T.text = "Ῥῆξον τῶν πταισμάτων μου, δεινὸν χειρόγραφον Δέσποινα, ἡ τὰ δεσμά, λύσασα τοῦ, ᾍδου, τῇ ἀσπόρῳ κυήσει σου."

meMA.Ode3Kathisma1.mode = misc_gr_GR_cog.Mode8
meMA.Ode3Kathisma1.melody = he.a.m8_gr_GR_cog.TinSofianKaiLogon.name
meMA.Ode3Kathisma1.incipit = "Νυμφευθεῖσαι Κυρίῳ θεοπρεπῶς, ὥσπερ προῖκα…"
meMA.Ode3Kathisma1.text = "Νυμφευθεῖσαι Κυρίῳ θεοπρεπῶς, ὥσπερ προῖκα τὸ αἷμα καὶ τὴν αὐτῷ σφαγήν, προσηνέγκατε, ἀθληφόροι Νεάνιδες, καὶ νυμφῶνος θείου, ἀξίως ἐτύχετε, φωτισμοῦ ἀῤῥήτου, ἀπαύστως πληρούμεναι· ὅθεν τὴν ἁγίαν, καὶ σεπτὴν ὑμῶν μνήμην, τελοῦντες ἐν πνεύματι, τὸν Σωτῆρα δοξάζομεν, καὶ πιστῶς ἀνακράζομεν· Πρεσβεύσατε Χριστῷ τῷ Θεῷ, τῶν πταισμάτων ἄφεσιν δωρήσασθαι, τοῖς ἑορτάζουσι πόθῳ, τὴν ἁγίαν μνήμην ὑμῶν."

meMA.Ode3KathismaT.mode = misc_gr_GR_cog.Mode8
meMA.Ode3KathismaT.melody = he.a.m8_gr_GR_cog.TinSofianKaiLogon.name
meMA.Ode3KathismaT.incipit = he.a.m8_gr_GR_cog.TinSofianKaiLogon.incipit
meMA.Ode3KathismaT.text = he.a.m8_gr_GR_cog.TinSofianKaiLogon.text

meMA.Ode3KathismaSt.mode = misc_gr_GR_cog.Mode8
