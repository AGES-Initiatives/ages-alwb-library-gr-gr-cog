meMA.Ode3C1H.text = he.h.m2_gr_GR_cog.heHE.StereosonImasEnSoiKyrie.text

meMA.Ode3C11.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C11.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode3C11.greekmelody = he.h.m2_gr_GR_cog.heHE.StereosonImasEnSoiKyrie.greekname
meMA.Ode3C11.melody = he.h.m2_gr_GR_cog.heHE.StereosonImasEnSoiKyrie.name
meMA.Ode3C11.incipit = "Ἀμέμπτως τῷ Θεῷ πολιτευσάμενοι, τὴν…"
meMA.Ode3C11.text = "Ἀμέμπτως τῷ Θεῷ πολιτευσάμενοι, τὴν πάντων ἐκύησαν σωτηρίαν, οἱ θεόφρονες γεννήτορες, τῆς τὸν Κτίστην τεκούσης καὶ Θεὸν ἡμῶν."

meMA.Ode3C12.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C12.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode3C12.melody = he.h.m2_gr_GR_cog.heHE.StereosonImasEnSoiKyrie.name
meMA.Ode3C12.incipit = "Ὁ πᾶσι τὴν ζωὴν πηγάζων…"
meMA.Ode3C12.text = "Ὁ πᾶσι τὴν ζωὴν πηγάζων Κύριος, ἐκ στείρας προήγαγε τὴν Παρθένον· ἣν εἰσδῦναι κατηξίωσε, μετὰ τόκον φυλάξας ἀδιάφθορον."

meMA.Ode3C13.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C13.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Ode3C13.melody = he.h.m2_gr_GR_cog.heHE.StereosonImasEnSoiKyrie.name
meMA.Ode3C13.incipit = "Τῆς Ἄννης τὸν καρπὸν Μαρίαν…"
meMA.Ode3C13.text = "Τῆς Ἄννης τὸν καρπὸν Μαρίαν σήμερον, τὴν βότρυν κυήσασαν ζωηφόρον, ὡς Θεοτόκον ἀνυμνήσωμεν, προστασίαν τε πάντων καὶ βοήθειαν."

meMA.Ode3C2H.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C2H.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C2H.greekmelody = he.h.m8_gr_GR_cog.heHE.EstereothiIKardiaMouEnKyrio.greekname
meMA.Ode3C2H.melody = he.h.m8_gr_GR_cog.heHE.EstereothiIKardiaMouEnKyrio.melody
meMA.Ode3C2H.incipit = he.h.m8_gr_GR_cog.heHE.EstereothiIKardiaMouEnKyrio.incipit
meMA.Ode3C2H.text = he.h.m8_gr_GR_cog.heHE.EstereothiIKardiaMouEnKyrio.text

meMA.Ode3C21.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C21.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C21.greekmelody = he.h.m8_gr_GR_cog.heHE.EstereothiIKardiaMouEnKyrio.greekname
meMA.Ode3C21.melody = he.h.m8_gr_GR_cog.heHE.EstereothiIKardiaMouEnKyrio.name
meMA.Ode3C21.incipit = "Εὐλογημένη ἡ κοιλία σου σώφρων…"
meMA.Ode3C21.text = "Εὐλογημένη ἡ κοιλία σου σώφρων Ἄννα· καρπὸν γὰρ ἤνθησε παρθενίας, τὴν ἀσπόρως τὸν τροφέα τῆς κτίσεως, τεκοῦσαν καὶ λυτρωτὴν Ἰησοῦν."

meMA.Ode3C22.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C22.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C22.melody = he.h.m8_gr_GR_cog.heHE.EstereothiIKardiaMouEnKyrio.name
meMA.Ode3C22.incipit = "Σὲ μακαρίζει Ἀειπάρθενε πᾶσα κτίσις…"
meMA.Ode3C22.text = "Σὲ μακαρίζει Ἀειπάρθενε πᾶσα κτίσις, ἐξ Ἄννης σήμερον γεννηθεῖσαν, τὴν ἐκ ῥίζης Ἰεσσαὶ ῥάβδον ἄχραντον, τὸ ἄνθος Χριστὸν βλαστήσασαν."

meMA.Ode3C23.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C23.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C23.melody = he.h.m8_gr_GR_cog.heHE.EstereothiIKardiaMouEnKyrio.name
meMA.Ode3C23.incipit = "Σὲ ἀνωτέραν πάσης κτίσεως Θεοτόκε…"
meMA.Ode3C23.text = "Σὲ ἀνωτέραν πάσης κτίσεως Θεοτόκε, δεικνύων ἄχραντε ὁ Υἱός σου, τὴν ἐξ Ἄννης μεγαλύνει σου γέννησιν, καὶ πάντας εὐφραίνει σήμερον."

meMA.Ode3C24.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C24.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C24.melody = he.h.m8_gr_GR_cog.heHE.EstereothiIKardiaMouEnKyrio.name
meMA.Ode3C24.incipit = "Ἀνατραφεῖσα εἰς τὰ Ἅγια τῶν…"
meMA.Ode3C24.text = "Ἀνατραφεῖσα εἰς τὰ Ἅγια τῶν Ἁγίων, Παρθένε ἄχραντε Θεοτόκε, ἀνωτέρα ἀνεδείχθης τῆς κτίσεως, τὸν Κτίστην σαρκὶ κυήσασα."

meMA.Ode3C2Tr.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C2Tr.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C2Tr.melody = he.h.m8_gr_GR_cog.heHE.EstereothiIKardiaMouEnKyrio.name
meMA.Ode3C2Tr.incipit = "Σὲ προσκυνοῦμεν Πάτερ ἄναρχε τῇ…"
meMA.Ode3C2Tr.text = "Σὲ προσκυνοῦμεν Πάτερ ἄναρχε τῇ οὐσίᾳ, ὑμνοῦμεν ἄχρονον τὸν Υἱόν σου, καὶ τὸ Πνεῦμα συναΐδιον σέβομεν, ὡς ἕνα τὰ τρία φύσει Θεόν."

meMA.Ode3C2T.ode = miscellanea_gr_GR_cog.misc.Ode3
meMA.Ode3C2T.mode = miscellanea_gr_GR_cog.misc.Mode8
meMA.Ode3C2T.melody = he.h.m8_gr_GR_cog.heHE.EstereothiIKardiaMouEnKyrio.name
meMA.Ode3C2T.incipit = "Τὸν φωτοδότην καὶ ἀρχίζωον τῶν…"
meMA.Ode3C2T.text = "Τὸν φωτοδότην καὶ ἀρχίζωον τῶν ἀνθρώπων, τεκοῦσα ἄχραντε Θεοτόκε, ἀνεδείχθης θησαυρὸς τῆς ζωῆς ἡμῶν, καὶ πύλη τοῦ ἀπροσίτου φωτός."

meMA.Hypakoe.mode = miscellanea_gr_GR_cog.misc.Mode2
meMA.Hypakoe.melody = ""
meMA.Hypakoe.incipit = "Πύλην ἀδιόδευτον…"
meMA.Hypakoe.text = "Πύλην ἀδιόδευτον ὁ Προφήτης, μόνῳ τῷ Θεῷ ἡμῶν τηρουμένην, τὴν Ἁγίαν Παρθένον ἐκάλεσε, διʼ αὐτῆς διῆλθεν ὁ Κύριος, ἐξ αὐτῆς προῆλθεν ὁ Ὕψιστος, καὶ πάλιν ἐσφραγισμένην κατέλιπε, λυτρούμενος ἐκ φθορᾶς τὴν ζωὴν ἡμῶν."

meMA.Ode3Kathisma1.mode = miscellanea_gr_GR_cog.misc.Mode4
meMA.Ode3Kathisma1.greekmelody = he.a.m4_gr_GR_cog.heAU.KateplagiIosif.greekname
meMA.Ode3Kathisma1.melody = he.a.m4_gr_GR_cog.heAU.KateplagiIosif.name
meMA.Ode3Kathisma1.incipit = "Ἡ Παρθένος Μαριάμ, καὶ Θεοτόκος…"
meMA.Ode3Kathisma1.text = "Ἡ Παρθένος Μαριάμ, καὶ Θεοτόκος ἀληθῶς, ὡς νεφέλη τοῦ φωτός, σήμερον ἔλαμψεν ἡμῖν, καὶ ἐκ Δικαίων προέρχεται εἰς δόξαν ἡμῶν. Οὐκ ἔτι ὁ Ἀδὰμ κατακρίνεται· ἡ Εὔα τῶν δεσμῶν ἠλευθέρωται· καὶ διὰ τοῦτο κράζομεν βοῶντες, ἐν παῤῥησίᾳ τῇ μόνῃ Ἁγνῇ· Χαρὰν μηνύει, ἡ γέννησίς σου, πάσῃ τῇ οἰκουμένῃ."

