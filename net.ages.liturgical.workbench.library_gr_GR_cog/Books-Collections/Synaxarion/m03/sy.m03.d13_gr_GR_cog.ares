syMA.commemoration04.couplet1.verse2 = ""

syMA.commemoration05.text = "Τῇ αὐτῇ ἡμέρᾳ, ἡ Ἁγία Μάρτυς Χριστίνα ἡ ἐκ Περσίδος, μαστιζομένη, τελειοῦται."
