peMA.Ode7C11.ode = miscellanea_gr_GR_cog.misc.Ode7
peMA.Ode7C11.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode7C11.greekmelody = he.h.m3_gr_GR_cog.heHE.OTinFloga.greekname
peMA.Ode7C11.melody = he.h.m3_gr_GR_cog.heHE.OTinFloga.name
peMA.Ode7C11.incipit = "Ἁπλωθέντα σε ξύλῳ ὡς ἑώρα…"
peMA.Ode7C11.text = "Ἁπλωθέντα σε ξύλῳ ὡς ἑώρα, φῶς συνέστειλεν ἥλιος μὴ σθένων, τῷ κόσμῳ φαίνειν, σοῦ ἑκουσίως δύναντος Παμβασιλεῦ, εἰς φαῦσιν πάντων ἐθνῶν."

peMA.Ode7C12.ode = miscellanea_gr_GR_cog.misc.Ode7
peMA.Ode7C12.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode7C12.melody = he.h.m3_gr_GR_cog.heHE.OTinFloga.name
peMA.Ode7C12.incipit = "Ἐξανέστης κενώσας τὰ μνημεῖα, καὶ…"
peMA.Ode7C12.text = "Ἐξανέστης κενώσας τὰ μνημεῖα, καὶ σκυλεύσας τὸν ᾍδην δυναστείᾳ παντοδυνάμῳ· διὸ ὑμνοῦμεν τὴν σεπτήν σου Χριστὲ καὶ θείαν Ἔγερσιν."

peMA.Ode7C13.ode = miscellanea_gr_GR_cog.misc.Ode7
peMA.Ode7C13.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode7C13.melody = he.h.m3_gr_GR_cog.heHE.OTinFloga.name
peMA.Ode7C13.incipit = "Ὡς νεκρὸν τί τὸν ζῶντα…"
peMA.Ode7C13.text = "Ὡς νεκρὸν τί τὸν ζῶντα ἐκζητεῖτε; ἐξηγέρθη, οὐκ ἔστιν ἐν τῷ τάφῳ, ταῖς Μυροφόροις ἐβόα πάλαι, ἐξαστράπτων μορφῇ ὁ θεῖος Ἄγγελος."

peMA.Ode7C1F1.ode = miscellanea_gr_GR_cog.misc.Ode7
peMA.Ode7C1F1.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode7C1F1.greekmelody = he.h.m3_gr_GR_cog.heHE.OTinFloga.greekname
peMA.Ode7C1F1.melody = he.h.m3_gr_GR_cog.heHE.OTinFloga.name
peMA.Ode7C1F1.incipit = "Κατακείμενον χρόνοις…"
peMA.Ode7C1F1.text = "Κατακείμενον χρόνοις ἐπὶ πλείστοις, τὸν Παράλυτον λόγῳ ὑγιώσας, ἐβόας· Ἆρον τὸν κράββατόν σου, καὶ πορεύου ὑμνῶν τὰ μεγαλεῖα τοῦ Θεοῦ."

peMA.Ode7C1F2.ode = miscellanea_gr_GR_cog.misc.Ode7
peMA.Ode7C1F2.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode7C1F2.melody = he.h.m3_gr_GR_cog.heHE.OTinFloga.name
peMA.Ode7C1F2.incipit = "Μιχαὴλ Ἀρχιστράτηγε Κυρίου…"
peMA.Ode7C1F2.text = "Μιχαὴλ Ἀρχιστράτηγε Κυρίου, τοὺς πιστῶς ἐν τῷ θείῳ σου τεμένει, ἀθροιζομένους Θεοῦ εἰς αἶνον, ὁδήγει, σκέπε, ἐκ παντοίων κακῶν τῇ μεσιτείᾳ σου."

peMA.Ode7C1Tr.ode = miscellanea_gr_GR_cog.misc.Ode7
peMA.Ode7C1Tr.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode7C1Tr.melody = he.h.m3_gr_GR_cog.heHE.OTinFloga.name
peMA.Ode7C1Tr.incipit = "Ὦ Τριάς, τοὺς πίστει σε…"
peMA.Ode7C1Tr.text = "Ὦ Τριάς, τοὺς πίστει σε ὑμνοῦντας, ὡς Θεὸν τῶν ἁπάντων καὶ Δεσπότην, ἀπὸ παντοίων κινδύνων σῷζε, καὶ τῶν σῶν ἀγαθῶν μετόχους ποίησον."

peMA.Ode7C1T.ode = miscellanea_gr_GR_cog.misc.Ode7
peMA.Ode7C1T.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode7C1T.melody = he.h.m3_gr_GR_cog.heHE.OTinFloga.name
peMA.Ode7C1T.incipit = "Παρθενεύεις τεκοῦσα…"
peMA.Ode7C1T.text = "Παρθενεύεις τεκοῦσα ὑπὲρ λόγον, τὸν πρὸ πάντων αἰώνων γεννηθέντα, ἐκ τοῦ ἀνάρχου Πατρὸς ἀῤῥεύστως· διὰ τοῦτο Ἁγνὴ σὲ μακαρίζομεν."

peMA.Ode8C1H.ode = miscellanea_gr_GR_cog.misc.Ode8
peMA.Ode8C1H.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode8C1H.greekmelody = he.h.m3_gr_GR_cog.heHE.TonYpAngelon.greekname
peMA.Ode8C1H.melody = he.h.m3_gr_GR_cog.heHE.TonYpAngelon.melody
peMA.Ode8C1H.incipit = he.h.m3_gr_GR_cog.heHE.TonYpAngelon.incipit
peMA.Ode8C1H.text = he.h.m3_gr_GR_cog.heHE.TonYpAngelon.text

peMA.Ode8C11.ode = miscellanea_gr_GR_cog.misc.Ode8
peMA.Ode8C11.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode8C11.greekmelody = he.h.m3_gr_GR_cog.heHE.TonYpAngelon.greekname
peMA.Ode8C11.melody = he.h.m3_gr_GR_cog.heHE.TonYpAngelon.name
peMA.Ode8C11.incipit = "Τὸ καταπέτασμα ἐσχίσθη, σταυρωθέντος σου…"
peMA.Ode8C11.text = "Τὸ καταπέτασμα ἐσχίσθη, σταυρωθέντος σου ὁ Σωτὴρ ἡμῶν, καὶ ἀπεδίδου τοὺς νεκρούς, οὓς κατέπιεν ὁ θάνατος, καὶ ᾍδης ἐγυμνοῦτο, σὲ καθορῶν ἐν κατωτάτοις τῆς γῆς γεγονότα."

peMA.Ode8C12.ode = miscellanea_gr_GR_cog.misc.Ode8
peMA.Ode8C12.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode8C12.melody = he.h.m3_gr_GR_cog.heHE.TonYpAngelon.name
peMA.Ode8C12.incipit = "Ποῦ σοῦ τὸ κέντρον ἐστὶ…"
peMA.Ode8C12.text = "Ποῦ σοῦ τὸ κέντρον ἐστὶ θάνατε; ποῦ σου ᾍδη νῦν τὸ νῖκος; τῷ ἀναστάντι Βασιλεῖ ἐνεκρώθης καὶ ἀπόλωλας, οὐκ ἔτι βασιλεύεις· ὁ κραταιὸς ἀφείλετο γὰρ οὕς εἶχες δεσμίους."

peMA.Ode8C13.ode = miscellanea_gr_GR_cog.misc.Ode8
peMA.Ode8C13.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode8C13.melody = he.h.m3_gr_GR_cog.heHE.TonYpAngelon.name
peMA.Ode8C13.incipit = "Δράμετε τάχος ἀπαγγείλατε, Ἀποστόλοις τὴν…"
peMA.Ode8C13.text = "Δράμετε τάχος ἀπαγγείλατε, Ἀποστόλοις τὴν ἀνάστασιν, ταῖς μυροφόροις, ὁ φανεὶς νεανίας ἀπεφθέγγετο· Ἀνέστη ὁ Δεσπότης, καὶ σὺν αὐτῷ, οἱ ἀπʼ αἰῶνος νεκροὶ παραδόξως."

peMA.Ode8C1F1.ode = miscellanea_gr_GR_cog.misc.Ode8
peMA.Ode8C1F1.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode8C1F1.greekmelody = he.h.m3_gr_GR_cog.heHE.TonYpAngelon.greekname
peMA.Ode8C1F1.melody = he.h.m3_gr_GR_cog.heHE.TonYpAngelon.name
peMA.Ode8C1F1.incipit = "Ἔτεσι πλείστοις κατακείμενος…"
peMA.Ode8C1F1.text = "Ἔτεσι πλείστοις κατακείμενος ὁ Παράλυτος, ἐκραύγαζεν· Ἐλέησόν με Λυτρωτά, ἀπορίᾳ συνεχόμενον· ὁ δὲ προστάττει τούτῳ ἆραι σπουδῇ τὸν κράββατον, καὶ ὀρθῶς βηματίζειν."

peMA.Ode8C1F2.ode = miscellanea_gr_GR_cog.misc.Ode8
peMA.Ode8C1F2.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode8C1F2.melody = he.h.m3_gr_GR_cog.heHE.TonYpAngelon.name
peMA.Ode8C1F2.incipit = "Ὡς τῶν ἀΰλων προηγούμενος…"
peMA.Ode8C1F2.text = "Ὡς τῶν ἀΰλων προηγούμενος, Ἀρχιστράτηγε Δυνάμεων, σὺν ταύταις αἴτησαι ἡμῖν τῶν πταισμάτων ἀπολύτρωσιν, διόρθωσίν τε βίου, καὶ τῶν ἐκεῖ ἀπόλαυσιν ἀγαθῶν αἰωνίων."

peMA.Ode8C1Tr.ode = miscellanea_gr_GR_cog.misc.Ode8
peMA.Ode8C1Tr.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode8C1Tr.melody = he.h.m3_gr_GR_cog.heHE.TonYpAngelon.name
peMA.Ode8C1Tr.incipit = "Ἄκτιστον, ἄτμητον οὐσίαν…"
peMA.Ode8C1Tr.text = "Ἄκτιστον, ἄτμητον οὐσίαν, τρισυπόστατον θεότητα, Πατέρα ἄναρχον Θεόν, καὶ Υἱὸν καὶ Πνεῦμα ἅγιον, ὑμνήσωμεν συμφώνως, τῶν Σεραφὶμ κραυγάζοντες τὴν φρικτήν μελῳδίαν."

peMA.Ode8C1T.ode = miscellanea_gr_GR_cog.misc.Ode8
peMA.Ode8C1T.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode8C1T.melody = he.h.m3_gr_GR_cog.heHE.TonYpAngelon.name
peMA.Ode8C1T.incipit = "Τόμον σε πάλαι ἐθεάσατο…"
peMA.Ode8C1T.text = "Τόμον σε πάλαι ἐθεάσατο, Ἠσαΐας Ἀειπάρθενε, ἐν ᾧ δακτύλῳ τοῦ Πατρός, Λόγος ἄχρονος ἐγέγραπτο, ἐκ πάσης ἀλογίας σῴζων ἡμᾶς, τοὺς λόγοις σε ἱεροῖς ἀνυμνοῦντας."

peMA.Ode9C1H.ode = miscellanea_gr_GR_cog.misc.Ode9
peMA.Ode9C1H.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode9C1H.greekmelody = he.h.m3_gr_GR_cog.heHE.EnSinaioToOrei.greekname
peMA.Ode9C1H.melody = he.h.m3_gr_GR_cog.heHE.EnSinaioToOrei.melody
peMA.Ode9C1H.incipit = he.h.m3_gr_GR_cog.heHE.EnSinaioToOrei.incipit
peMA.Ode9C1H.text = he.h.m3_gr_GR_cog.heHE.EnSinaioToOrei.text

peMA.Ode9C11.ode = miscellanea_gr_GR_cog.misc.Ode9
peMA.Ode9C11.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode9C11.greekmelody = he.h.m3_gr_GR_cog.heHE.EnSinaioToOrei.greekname
peMA.Ode9C11.melody = he.h.m3_gr_GR_cog.heHE.EnSinaioToOrei.name
peMA.Ode9C11.incipit = "Ἰησοῦ ἐπὶ ξύλου ὑψούμενος, συνανύψωσας…"
peMA.Ode9C11.text = "Ἰησοῦ ἐπὶ ξύλου ὑψούμενος, συνανύψωσας ἡμᾶς, καὶ τεθεὶς ἑκουσίως ἐν μνήματι, ἐκ μνημάτων τοὺς νεκρούς, ἐξανέστησας πάντας, ὑμνοῦντας τὸ κράτος σου τὸ ἀκατάληπτον, καὶ τὴν δυναστείαν τὴν σὴν τὴν ἀήττητον."

peMA.Ode9C12.ode = miscellanea_gr_GR_cog.misc.Ode9
peMA.Ode9C12.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode9C12.melody = he.h.m3_gr_GR_cog.heHE.EnSinaioToOrei.name
peMA.Ode9C12.incipit = "Ὡραιότατος τάφου ἀνέτειλας, ὡς νυμφίος…"
peMA.Ode9C12.text = "Ὡραιότατος τάφου ἀνέτειλας, ὡς νυμφίος ἐκ παστοῦ, ὡραιότατε Λόγε, καὶ ἔλυσας τὸ τοῦ ᾍδου ἀμειδές, καὶ δεσμώτας ἐξῆρας, συμφώνως κραυγάζοντας· Δόξα τῇ δόξῃ σου, δόξα Ἰησοῦ ὁ Θεὸς τῇ Ἐγέρσει σου."

peMA.Ode9C13.ode = miscellanea_gr_GR_cog.misc.Ode9
peMA.Ode9C13.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode9C13.melody = he.h.m3_gr_GR_cog.heHE.EnSinaioToOrei.name
peMA.Ode9C13.incipit = "Στεναγμούς τε καὶ δάκρυα φέρουσαι…"
peMA.Ode9C13.text = "Στεναγμούς τε καὶ δάκρυα φέρουσαι, μετὰ μύρων ἐν σπουδῇ, τὸ πανάγιον μνῆμα κατέλαβον αἱ γυναῖκες, καὶ Χριστοῦ ἐδιδάσκοντο πίστει τὴν ἔνδοξον Ἔγερσιν, ἥν ἑορτάζομεν, ἐν ἀγαλλιάσει ψυχῆς εὐφραινόμενοι."

peMA.Ode9C1F1.ode = miscellanea_gr_GR_cog.misc.Ode9
peMA.Ode9C1F1.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode9C1F1.greekmelody = he.h.m3_gr_GR_cog.heHE.EnSinaioToOrei.greekname
peMA.Ode9C1F1.melody = he.h.m3_gr_GR_cog.heHE.EnSinaioToOrei.name
peMA.Ode9C1F1.incipit = "Ἠκολούθει ἡ ῥῶσις…"
peMA.Ode9C1F1.text = "Ἠκολούθει ἡ ῥῶσις τοῦ σώματος, τῇ προστάξει σου Χριστέ, καὶ ὡρᾶτο ὁ πρῴην Παράλυτος, διερχόμενος σπουδῇ, καὶ τὸν κράββατον φέρων ἐφʼ ὃν κατεκλίνετο, ἔτεσι πλείοσιν, ἀνυμνολογῶν τὴν πολλὴν δυναστείαν σου."

peMA.Ode9C1F2.ode = miscellanea_gr_GR_cog.misc.Ode9
peMA.Ode9C1F2.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode9C1F2.melody = he.h.m3_gr_GR_cog.heHE.EnSinaioToOrei.name
peMA.Ode9C1F2.incipit = "Φωτισμὸν ἡμῖν αἴτησαι…"
peMA.Ode9C1F2.text = "Φωτισμὸν ἡμῖν αἴτησαι μέγιστε, Ἀρχιστράτηγε, ἀεὶ τῷ μεγάλῳ φωτὶ παριστάμενος, καὶ εἰρήνευσον ἡμῶν, τὴν ζωὴν ἐπηρείαις, πάντοτε τοῦ ὄφεως, καὶ περιστάσεσι, βίου δονουμένην ἀεὶ ἀξιάγαστε."

peMA.Ode9C1Tr.ode = miscellanea_gr_GR_cog.misc.Ode9
peMA.Ode9C1Tr.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode9C1Tr.melody = he.h.m3_gr_GR_cog.heHE.EnSinaioToOrei.name
peMA.Ode9C1Tr.incipit = "Φῶς καὶ φῶτα ζωήν…"
peMA.Ode9C1Tr.text = "Φῶς καὶ φῶτα ζωήν τε δοξάζω σε, καὶ ζωὰς πανευσεβῶς, Πάτερ Λόγε, καὶ Πνεῦμα τὸ ἅγιον, τρισυπόστατε Μονάς, ἀδιαίρετον κράτος, Θεότης ἀσύγχυτε, Ἅγιος, Ἅγιος, Ἅγιος, βοῶν, σὺν ταῖς ἄνω δυνάμεσι."

peMA.Ode9C1T.ode = miscellanea_gr_GR_cog.misc.Ode9
peMA.Ode9C1T.mode = miscellanea_gr_GR_cog.misc.Mode3
peMA.Ode9C1T.melody = he.h.m3_gr_GR_cog.heHE.EnSinaioToOrei.name
peMA.Ode9C1T.incipit = "Φωτοφόρου προῆλθε…"
peMA.Ode9C1T.text = "Φωτοφόρου προῆλθε νηδύος σου, μέγας Ἥλιος Χριστός, καὶ τὸν κόσμον ἐφώτισεν Ἄχραντε, ἐπιλάμψεσι φαιδραῖς, καὶ τὸ σκότος ἐξῆρε, τὸ τῆς παραβάσεως· ὅθεν ὑμνοῦμέν σε, πάντων ὡς αἰτίαν καλῶν Θεονύμφευτε."

peMA.Exaposteilarion1.mode = miscellanea_gr_GR_cog.misc.Mode2
peMA.Exaposteilarion1.greekmelody = he.a.m2_gr_GR_cog.heAU.GynaikesAkoutisthite.greekname
peMA.Exaposteilarion1.melody = he.a.m2_gr_GR_cog.heAU.GynaikesAkoutisthite.name
peMA.Exaposteilarion1.incipit = "Ἐπέστη ὁ φιλάνθρωπος…"
peMA.Exaposteilarion1.text = "Ἐπέστη ὁ φιλάνθρωπος, καὶ πανοικτίρμων Κύριος, Προβατικῇ κολυμβήθρᾳ, τοῦ θεραπεῦσαι τὰς νόσους· εὗρε δὲ κατακείμενον, ἄνθρωπον πλείστοις ἔτεσι, καὶ πρός αὐτὸν ἐβόησεν· Ἆρον τὸν κράββατον, ἴθι, πρὸς τὰς ὁδοὺς τὰς εὐθείας."

peMA.Lauds1.mode = oc.m3.d1_gr_GR_cog.ocMA.Lauds1.mode
peMA.Lauds1.melody = oc.m3.d1_gr_GR_cog.ocMA.Lauds1.melody
